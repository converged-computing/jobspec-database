                "tensorboardX",
                "scipy",
                "ninja",
                "pandas",
                "torchdiffeq-0.2.2-py3-none-any.whl",
                "torchvision"
            ]
        },
        "script": "train_vampprior.py",
        "arguments": [
            "$DATADIR/particles.128.txt",
