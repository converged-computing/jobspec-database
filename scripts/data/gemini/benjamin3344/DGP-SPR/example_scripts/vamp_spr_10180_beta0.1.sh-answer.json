{
    "application": "SLURM",
    "details": {
        "resources": {
            "gres": "gpu:2",
            "account": "def-janehowe",
            "cpus_per_task": 6,
            "memory": "64000M",
            "time": "1-22:50"
        },
        "modules": [
            "StdEnv/2020",
            "python/3.7",
            "cuda",
            "cudnn"
