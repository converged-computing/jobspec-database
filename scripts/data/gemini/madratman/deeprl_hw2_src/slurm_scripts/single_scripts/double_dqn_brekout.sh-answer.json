{
    "application": "Slurm",
    "details": {
        "resource_manager": "Slurm",
        "runtime": "5 hours",
        "partition": "gpu",
        "gpu_count": 1,
        "output_file": "double_dqn_breakout_%j.out",
        "error_file": "double_dqn_breakout_%j.err",
        "node": "roberto",
        "docker_image": "madratman/deeprl_hw",
        "python_script": "dqn_atari.py",
        "script_arguments": [
            "--mode='double'",
            "--env='breakout'"
        ],
        "data_volume_mount": "/data/datasets:/data/datasets",
        "home_directory_mount": "/home/$USER:/home/$USER"
    }
}