{
    "application": "Python",
    "details": {
        "environment": "realtime_ocl (conda)",
        "compiler": "gcc/11.1.0",
        "resources": {
            "nodes": 1,
            "partition": "batch",
            "job_name": "RWalkT1",
            "output_file": "/path/to/output.%J.out",
            "error_file": "/path/to/error.%J.err",
            "time_limit": "1:00:00",
            "memory": "200G",
            "tasks": 1,
            "gpu": true,
            "cpus_per_task": 6
        },
        "script_arguments": {
            "dataset": "cifar10",
            "batch_size": 10,
            "lr": 0.001,
            "lr_type": "constant",
            "batch_delay": 1,
            "gradient_steps": 1,
            "output_dir": "/path/to/tensorboard/output",
            "workers": 4,
            "method": "RWalk",
            "RWalk_warmup": 0.05,
            "RWalk_update_freq": 1000,
            "RWalk_ewc_lambda": 2,
            "seed": 123,
            "size_replay_buffer": 100
        }
    }
}