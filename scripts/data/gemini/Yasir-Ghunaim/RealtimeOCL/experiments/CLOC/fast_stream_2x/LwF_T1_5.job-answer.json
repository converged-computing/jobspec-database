{
    "application": "Python",
    "details": {
        "environment": "conda realtime_ocl",
        "dependencies": [
            "gcc/11.1.0",
            "python",
            "main.py"
        ],
        "resources": {
            "nodes": 1,
            "partition": "batch",
            "job_name": "LwFT1_5",
            "output_file": "/path/to/output.%J.out",
            "error_file": "/path/to/error.%J.err",
            "walltime": "50:00:00",
            "memory": "200G",
            "tasks": 1,
            "gpus": "a100:2",
            "reservation": "A100",
            "cpus_per_task": 12
        },
        "arguments": {
            "dataset": "cloc",
            "batch_size": 128,
            "lr": 0.005,
            "lr_type": "constant",
            "batch_delay": 1.5,
            "gradient_steps": 1,
            "output_dir": "/path/to/tensorboard/output",
            "workers": 12,
            "method": "LwF",
            "LwF_warmup": 0.05,
            "LwF_update_freq": 1000,
            "seed": 123,
            "dataset_root": "/path/to/CLOC/release/",
            "size_replay_buffer": 40000,
            "pretrained": true
        }
    }
}