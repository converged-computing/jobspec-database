{
    "application": "MIR",
    "software": [
        "gcc/11.1.0",
        "conda",
        "python"
    ],
    "resources": {
        "nodes": 1,
        "partition": "batch",
        "job_name": "MIRT0",
        "output_file": "/path/to/output.%J.out",
        "error_file": "/path/to/error.%J.err",
        "time": "60:00:00",
        "memory": "200G",
        "tasks": 1,
        "gpus": 2,
        "cpus_per_task": 12
    }
}