{
    "application": "Python",
    "details": {
        "environment": "realtime_ocl",
        "libraries": [
            "gcc/11.1.0",
            "conda"
        ],
        "resources": {
            "nodes": 1,
            "partition": "batch",
            "job_name": "ERpp_PoLRS",
            "output_file": "/path/to/output.%J.out",
            "error_file": "/path/to/error.%J.err",
            "walltime": "55:00:00",
            "memory": "200G",
            "tasks": 1,
            "gpus": 2,
            "cpus_per_task": 12
        },
        "script": "main.py",
        "arguments": [
            "--dataset 'cloc'",
            "--batch_size 128",
            "--lr 0.005",
            "--lr_type 'constant'",
            "--batch_delay 0",
            "--gradient_steps 3",
            "--output_dir '/path/to/tensorboard/output'",
            "--workers 12",
            "--method 'ER'",
            "--seed 123",
            "--dataset_root '/path/to/CLOC/release/'",
            "--size_replay_buffer 40000",
            "--pretrained"
        ]
    }
}