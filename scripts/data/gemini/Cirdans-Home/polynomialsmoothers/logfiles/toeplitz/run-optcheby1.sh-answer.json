{
    "application": "anisopsblas",
    "software_requirements": [
        "gcc/12.2.0",
        "openmpi/4.1.4-gcc-12.2.0",
        "openblas/0.3.20-gcc-12.2.0",
        "metis/5.1.0-gcc-12.2.0",
        "suite-sparse/5.10.1-gcc-12.2.0",
        "libunwind/1.6.2-gcc-12.2.0",
        "papi/6.0.0.1-gcc-12.2.0",
        "binutils/2.33.1-gcc-12.2.0",
        "libxml2/2.10.1-gcc-12.2.0",
        "mumps/5.4.1-gcc-12.2.0"
    ],
    "resource_requirements": {
        "tasks": "@NTASK@",
        "threads_per_task": "@NTHREAD@",
        "partition": "all"
    },
    "environment_variables": [
        "LD_LIBRARY_PATH"
    ],
    "input_parameters": [
        "@SIZE@",
        "@THETA@",
        "@EPS@"
    ],
    "output_file": "soc1/log_vsvbm_optcheby1_l1jac_${idim}_task_@NTASK@_thr_@NTHREAD@.txt"
}