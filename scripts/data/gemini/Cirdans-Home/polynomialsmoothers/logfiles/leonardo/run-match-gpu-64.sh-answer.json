            "ordering_of_aggregation": "NATURAL",
            "coarsening_ratio": "-1.5",
            "filtering_of_matrix": "FILTER",
            "smoothed_aggregation_threshold": "-0.0100d0",
            "number_of_thresholds": "-2",
            "thresholds": "0.05 0.025",
            "coarsest_level_solver": "L1-JACOBI",
            "coarsest_level_subsolver": "L1-JACOBI",
            "coarsest_matrix_layout": "DIST",
            "fill_in_for_incompl_LU": 0,
            "threshold_for_ILUT": "1.d-2",
            "sweeps_for_GS_JAC_subsolver": 30,
            "dump_on_file": "F",
            "minimum_level_to_dump": 1,
            "maximum_level_to_dump": 10,
            "dump_coarse_matrices": "T",
            "dump_restrictors": "T",
            "dump_prolongators": "T",
            "dump_smoothers": "T",
            "dump_coarse_solver": "T",
            "dump_using_global_numbering": "T"
