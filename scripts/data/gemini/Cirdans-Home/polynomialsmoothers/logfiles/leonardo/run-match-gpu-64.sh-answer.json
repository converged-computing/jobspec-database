{
    "application": "Slurm",
    "details": {
        "job_name": "anisopb",
        "partition": "boost_usr_prod",
        "time": "01:00:00",
        "nodes": 16,
        "gres": "gpu:4",
        "ntasks": 64,
        "ntasks_per_node": 4,
        "account": "CNHPC_1465132",
        "modules": [
            "openmpi/4.1.6--gcc--12.2.0",
            "openblas/0.3.24--gcc--12.2.0",
            "gcc/12.2.0",
            "cuda/12.1"
