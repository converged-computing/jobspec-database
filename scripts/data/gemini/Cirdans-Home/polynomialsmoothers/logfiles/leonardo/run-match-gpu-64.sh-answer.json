            "inner_solver_sweeps": 1,
            "AINV_variant": "LLK",
            "fill_level_P_for_ILU_and_ILUT": 0,
            "inverse_fill_level_P_for_INVK": 1,
            "threshold_T_for_ILUT": "1.d-4",
            "second_smoother_type": "NONE",
            "post_smoother_sweeps": 1,
            "second_polynomial_smoother_degree": 4,
            "second_polynomial_variant": "POLY_LOTTES_BETA",
            "second_spectral_radius_estimation_algorithm": "POLY_RHO_EST_POWER",
            "second_spectral_radius_estimate": "1.0",
            "second_number_of_overlap_layers": 0,
            "second_AS_restriction_operator": "HALO",
            "second_AS_prolongation_operator": "NONE",
            "second_subdomain_solver": "L1-JACOBI",
            "second_inner_solver_sweeps": 1,
            "second_AINV_variant": "LLK",
            "second_fill_level_P_for_ILU_and_ILUT": 0,
            "second_inverse_fill_level_P_for_INVK": 8,
            "second_threshold_T_for_ILUT": "1.d-4",
            "AMG_cycle_type": "VCYCLE",
            "number_of_1lev_outer_sweeps": 1,
            "max_number_of_levels": "-3",
            "target_coarse_matrix_size_per_process": "-3",
            "aggregation_type": "SMOOTHED",
            "parallel_aggregation": "COUPLED",
            "aggregation_measure": "MATCHBOXP",
