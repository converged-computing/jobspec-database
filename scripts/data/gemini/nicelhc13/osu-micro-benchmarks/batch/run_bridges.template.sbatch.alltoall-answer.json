        "OMP_NUM_THREADS",
        "MV2_USE_CUDA",
        "MV2_USE_GDR",
        "MPI_THREAD_MULTIPLE",
        "MV2_CPU_MAPPING",
        "MV2_ENABLE_AFFINITY",
        "LD_PRELOAD",
        "MV2_DEBUG_SHOW_BACKTRACE",
        "MV2_SHOW_ENV_INFO"
    ]
