{
    "application": "SLURM",
    "software": [
        "nvidia-smi",
        "mpirun",
        "osu_alltoall",
        "osu_alltoall_sdh_hds",
        "generate_pbs_nodefile"
    ],
    "resources": [
        "gpu:p100:4",
        "8 cores per node",
        "2 nodes",
        "gpu partition",
