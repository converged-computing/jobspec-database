{
    "application": "Slurm",
    "details": {
        "job_name": "augmented",
        "partition": "gpu",
        "cpus_per_task": 8,
        "gres": "gpu:1",
        "ntasks": 1,
        "ntasks_per_node": 1,
        "hint": "nomultithread",
        "time": "03:00:00",
        "output": "tiling_%x.out",
        "error": "tiling_%x.out",
        "python_script": "build_dataset/main_tiling.py",
        "arguments": {
            "path_wsi": "./test_dataset/slides",
            "ext": "svs",
            "level": 1,
            "tiler": "imagenet",
            "size": 224,
            "model_path": "/path/of/the/model/if/using/ssl/for/encoding.pth",
            "path_outputs": "./test_dataset/encoded",
            "normalizer": "macenko",
            "Naug": 50,
            "Nt": 256,
            "NWSI": 10,
            "tiler_type": "SharedAugTiler"
        },
        "num_workers": 8
    }
}