{
    "application": "GEOS-Chem",
    "details": {
        "scheduler": "SLURM or LSF",
        "resources": {
            "cores": "24",
            "nodes": "1",
            "walltime": "0-03:30",
            "partition": "REQUESTED_PARTITION",
            "memory": "90GB",
            "docker": "registry.gsc.wustl.edu/sleong/esm:intel-2021.1.2"
        },
        "environment": {
            "bashrc": "~/.bashrc",
            "gcclassic.env": "${ptRoot}/${ENV_DIR}/gcclassic.env"
        },
        "software": {
            "GEOS-Chem Classic": "${ptRoot}/CodeDir/src/GEOS-Chem",
            "HEMCO": "${ptRoot}/CodeDir/src/HEMCO"
        },
        "scripts": {
            "commonFunctionsForTests.sh": "${ptRoot}/scripts/commonFunctionsForTests.sh",
            "cleanRunDir.sh": "${ptRoot}/${RUNDIRS_DIR}/*/cleanRunDir.sh"
        },
        "output": "${logsDir}/results.parallel.log",
        "logs": "${logsDir}/parallel.*.log"
    }
}