{
    "application": "SLURM",
    "details": {
        "partition": "gpu",
        "gpus": 1,
        "job_name": "CIFAR10_resnet18_random_experiment",
        "ntasks": 1,
        "cpus_per_task": 4,
        "time": "48:00:00",
        "output": "../out/slurm_output_%x.out",
        "modules": [
            "Anaconda3/2022.05"
        ],
        "environment": {
            "PROJECT_ENV_NAME": "environment variable",
            "PROJECT_DIR": "environment variable"
        },
        "python_script": "main.py",
        "arguments": [
            "--experiment",
            "--debug",
            "--strategy random",
            "--dataset cifar10",
            "--model resnet18",
            "--epochs 100",
            "--n_round 100",
            "--num_valid 300",
            "--n_init_labeled 100",
            "--n_query 10"
        ]
    }
}