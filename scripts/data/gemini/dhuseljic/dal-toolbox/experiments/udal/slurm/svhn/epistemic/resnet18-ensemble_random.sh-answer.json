{
    "application": "SLURM",
    "details": {
        "software": [
            "zsh",
            "python"
        ],
        "resources": {
            "memory": "32gb",
            "tasks": 1,
            "cpus_per_task": 8,
            "gpus": 1,
            "partition": "main",
            "array_range": "1-10%10",
            "job_name": "udal",
            "output_file": "/mnt/work/dhuseljic/logs/udal/active_learning/%A_%a_%x.log"
        },
        "libraries": [
            "~/envs/dal-toolbox/bin/activate"
        ],
        "datasets": [
            "SVHN",
            "CIFAR10",
            "CIFAR100"
        ],
        "models": [
            "resnet18_ensemble"
        ],
        "scripts": [
            "active_learning.py"
        ],
        "directories": [
            "/mnt/work/dhuseljic/logs/udal/active_learning",
            "/mnt/home/dhuseljic/projects/dal-toolbox/experiments/udal",
            "/mnt/home/dhuseljic/projects/dal-toolbox/experiments/udal/initial_pools/${dataset}",
            "/mnt/work/dhuseljic/results/udal/active_learning/${dataset}/${model}/${al_strat}/N_INIT${n_init}__ACQ_SIZE${acq_size}__N_ACQ${n_acq}/seed${random_seed}/",
            "/mnt/work/dhuseljic/datasets"
        ],
        "environment_variables": [
            "model",
            "dataset",
            "ood_datasets",
            "al_strat",
            "n_init",
            "acq_size",
            "n_acq",
            "random_seed",
            "init_pool_file",
            "output_dir",
            "SLURM_ARRAY_TASK_ID"
        ]
    }
}