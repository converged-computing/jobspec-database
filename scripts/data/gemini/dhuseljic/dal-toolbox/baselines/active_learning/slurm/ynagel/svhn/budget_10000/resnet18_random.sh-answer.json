{
    "application": "Slurm",
    "details": {
        "resource_requirements": {
            "memory": "32GB",
            "tasks": 1,
            "cpus_per_task": 8,
            "gpu": 1,
            "partition": "main",
            "array": "1-3%3",
            "job_name": "al_baselines"
        },
        "environment_setup": {
            "source": "/mnt/stud/home/ynagel/dal-toolbox/venv/bin/activate",
            "working_directory": "~/dal-toolbox/experiments/active_learning/"
        },
        "script": {
            "language": "python",
            "file": "active_learning.py",
            "arguments": {
                "model": "resnet18",
                "model.optimizer.lr": "1e-3",
                "model.optimizer.weight_decay": "5e-2",
                "dataset": "SVHN",
                "dataset_path": "/mnt/stud/home/ynagel/data",
                "al_strategy": "random",
                "al_cycle.n_init": "1000",
                "al_cycle.acq_size": "1000",
                "al_cycle.n_acq": "9",
                "random_seed": "$SLURM_ARRAY_TASK_ID",
                "output_dir": "/mnt/stud/home/ynagel/dal-toolbox/results/al_baselines/${dataset}/${model}/${al_strat}/budget_${budget}/seed${random_seed}/"
            }
        }
    }
}