{
    "application": "Slurm",
    "details": {
        "resource_requirements": {
            "memory": "32gb",
            "tasks": 1,
            "cpus_per_task": 8,
            "gpu": 1,
            "partition": "main",
            "array": "1-3%3",
            "job_name": "al_baselines",
            "output": "/mnt/work/dhuseljic/logs/al_baselines/%A_%a__%x.log"
        },
        "software_requirements": {
            "conda_environment": "dal-toolbox",
            "python_script": "active_learning.py",
            "python_module": "active_learning"
        },
        "model_parameters": {
            "model": "resnet18",
            "dataset": "CIFAR100",
            "al_strat": "entropy",
            "n_init": 1000,
            "acq_size": 1000,
            "n_acq": 9,
            "budget": 10000,
            "random_seed": "$SLURM_ARRAY_TASK_ID",
            "output_dir": "/mnt/work/deep_al/results/al_baselines/${dataset}/${model}/${al_strat}/budget_${budget}/seed${random_seed}/"
        },
        "python_arguments": {
            "model": "resnet18",
            "model.optimizer.lr": "1e-3",
            "model.optimizer.weight_decay": "5e-2",
            "dataset": "CIFAR100",
            "dataset_path": "/mnt/work/dhuseljic/datasets",
            "al_strategy": "entropy",
            "al_cycle.n_init": 1000,
            "al_cycle.acq_size": 1000,
            "al_cycle.n_acq": 9,
            "random_seed": "$SLURM_ARRAY_TASK_ID",
            "output_dir": "/mnt/work/deep_al/results/al_baselines/${dataset}/${model}/${al_strat}/budget_${budget}/seed${random_seed}/"
        }
    }
}