{
    "application": "OpenMPI",
    "details": {
        "modules": [
            "2015a/espressopp/adress",
            "bakery-github-dev"
        ],
        "resources": {
            "walltime": "72:00:00",
            "nodes": "1",
            "ppn": "20"
        },
        "environment": {
            "n_proc": "Number of processors (obtained from $PBS_NODEFILE)",
            "n_node": "Number of nodes (obtained from $PBS_NODEFILE)",
            "alpha": "Looping variable ranging over 0.001, 0.0001, 0.00001",
            "s": "Looping variable ranging over 5, 6, 7, 8",
            "rng": "Random integer between 12345 and 99999"
        },
        "commands": [
            "mpirun -n 18 start_backmapping.py @params --rng_seed ${rng}  --alpha $alpha --output_prefix data_s${s}/sim0 &> log_s${s}_${alpha}.log",
            "cp -v data_s${s}/sim0confout_final_aa_${alpha1}_* gmx_s${s}/${alpha}/conf.gro"
        ]
    }
}