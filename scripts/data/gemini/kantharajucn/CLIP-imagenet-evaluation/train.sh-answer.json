{
    "application": "Slurm",
    "details": {
        "job_name": "Resnet50-train-on-clip",
        "tasks": 1,
        "cpus_per_task": 16,
        "memory": "200G",
        "nodes": 1,
        "output_file": "ResNet50-clip%j.out",
        "error_file": "ResNet50-clip%j.err",
        "gpus": 4,
        "constraint": "ImageNet2012",
        "partition": "gpu-2080ti",
        "singularity_image": "docker://lukasschott/ifr:v8",
        "python_script": "train.py",
        "dataset_path": "/path/to/dataset/ImageNet2012",
        "model": "resnet50",
        "distributed_url": "tcp://127.0.0.1:1405",
        "distributed_backend": "nccl",
        "world_size": 1,
        "rank": 0,
        "workers": 8,
        "label_type": "soft_labels"
    }
}