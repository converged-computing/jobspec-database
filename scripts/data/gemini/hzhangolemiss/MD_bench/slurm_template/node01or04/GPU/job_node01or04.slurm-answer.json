{
    "application": "LAMMPS",
    "details": {
        "scheduler": "SLURM",
        "libraries": [
            "openmpi",
            "openblas",
            "fftw3",
            "lammps/gcc/sfft/openmpi/cuda/2Aug23.3"
        ],
        "gpu_accelerator": "NVIDIA MPS",
        "nodes": 1,
        "cores_per_node": 12,
        "gpus_per_node": 2,
        "memory_per_cpu": "200M",
        "mpi_processes": 12,
        "input_file": "input.lmps",
        "configuration": {
            "-sf": "gpu",
            "-pk": "gpu",
            "neigh": "yes",
            "newton": "on",
            "pair/only": "off",
            "split": 1.0
        }
    }
}