{
    "application": "MWA pipeline",
    "details": {
        "software": [
            "Singularity",
            "mwa_client",
            "track_task.py",
            "unzip"
        ],
        "resources": {
            "compute": "zeus",
            "queue": "copyq",
            "account": "mwasci",
            "time": "12:00:00",
            "nodes": 1
        },
        "dependencies": {
            "environment": "/group/mwasci/$pipeuser/GLEAM-X-pipeline/GLEAM-X-pipeline.profile"
        },
        "data": {
            "input": "${obslist}_manta.tmp",
            "output": [
                "${obsnum}_ms.zip",
                "${obsnum}_vis.zip"
            ]
        }
    }
}