{
    "application": "python",
    "software": [
        "gcc/9.1.0",
        "conda",
        "NumPy",
        "SciPy"
    ],
    "resources": {
        "walltime": "0:30:00",
        "cpus": 1,
        "gpus": 1,
        "memory": "4gb"
    },
    "libraries": [
        "sdecouplings"
    ],
    "script_arguments": {
        "srcpath": "/scratch/st-schieb-1/zsteve/wtf/src",
        "n_iter": 25,
        "outfile": "output_$SRAND",
        "r": 40,
        "srand": "$SRAND",
        "tol": "1e-4",
        "split": 5,
        "lamda": 5
    },
    "environment_variables": {
        "NUMBA_CACHE_DIR": "$PBS_O_WORKDIR",
        "SRAND": "$PBS_ARRAY_INDEX"
    },
    "job_name": "faces",
    "job_account": "st-schieb-1-gpu"
}