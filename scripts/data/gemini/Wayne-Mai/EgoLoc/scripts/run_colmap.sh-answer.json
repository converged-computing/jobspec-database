{
    "application": "COLMAP",
    "details": {
        "software": [
            "Python",
            "COLMAP",
            "CUDA (optional)",
            "Conda (optional)"
        ],
        "resources": {
            "SLURM": {
                "nodes": 1,
                "job_name": "colmap",
                "output_file": "slurm_logs/%x.%3a.%A.out",
                "error_file": "slurm_logs/%x.%3a.%A.err",
                "time_limit": "8:00:00",
                "memory": "100G",
                "cpus_per_task": 4,
                "array_tasks": "0-113"
            },
            "modules": [
                "gcc (optional)",
                "cuda (optional)"
            ],
            "conda_environment": "fd3d (optional)"
        },
        "input_data": "data/clips_frames",
        "output_data": "data/colmap_modesl"
    }
}