{
    "application": "SLURM",
    "details": {
        "runtime": "48 hours",
        "nodes": 1,
        "memory": "62 GB",
        "output_log": "log/%j.out",
        "job_name": "fiveDifTask",
        "requeue": true,
        "gpu": "a100 (1)",
        "partition": "a100-4",
        "python_script": "train.py",
        "model": "gpt2-large",
        "task": "fiveSimTask",
        "split": "train",
        "method": "channel",
        "learning_rate": "1e-2",
        "prefix_tokens": 10,
        "tensorize_dir": "tensorized",
        "output_dir": "checkpoints/$MODEL/$TASK-$SPLIT/prefix={$N_PREFIX}-{$METHOD}-lr={$LR}-initByVocab",
        "batch_size": 16,
        "training_steps": 10000
    }
}