{
    "application": "SLURM",
    "details": {
        "job_name": "{{ experiment_name }}",
        "node": "c0",
        "memory_per_cpu": "5G",
        "array_range": "{{ array_range }}",
        "error_log": "./results/heldout_experiment/errorlog-%j.err",
        "docker_image": "dncb-fac-image",
        "container_name": "dncb-fac-container",
        "mlflow_experiment_name": "heldout_experiment",
        "modules": "jq",
        "job_config_file": "{{ experiment_config_json }}",
        "data_path": "${data_path}",
        "uuid": "${uuid}",
        "data_volume": "/data/projects/dncbtd:/work/data",
        "tmp_volume": "results/heldout_experiment:/work/data/tmp",
        "python_script": "src/dncbfac/heldout_experiment.py"
    }
}