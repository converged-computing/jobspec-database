{
    "application": "SLURM",
    "details": {
        "scheduler": "SLURM",
        "account": "t3",
        "working_directory": "/work/areimers/workdir_slurm",
        "error_file_pattern": "%x-%A-%a.err",
        "output_file_pattern": "%x-%A-%a.out",
        "environment_variables": "NONE",
        "email_notification_type": "FAIL",
        "email_recipient": "arne.reimers@physik.uzh.ch",
        "code_folder": "LQDM/GENSIM",
        "cms_software_directory": "CMSSWDIR",
        "target_folder_name": "TARGETFOLDERNAME",
        "job_list_file": "JOBLIST",
        "cmssw_environment": {
            "cmsset_default_script": "$VO_CMS_SW_DIR/cmsset_default.sh",
            "scram_arch": "ARCHTAG",
            "cmssw_source_directory": "$CMSSWDIR/src"
        },
        "python_path": "$CODEFOLDER:$PYTHONPATH",
        "temporary_directory": "/scratch/$USER/tmpdir_${SLURM_JOB_ID}_${SLURM_ARRAY_TASK_ID}",
        "cms_run_command": "TASKCMD",
        "gfal_commands": {
            "mkdir": "LD_LIBRARY_PATH='' PYTHONPATH='' gfal-mkdir -p $TARGETFOLDERNAME; sleep 10s;",
            "copy": "LD_LIBRARY_PATH='' PYTHONPATH='' gfal-copy -f file:////$PWD/$FILENAME $TARGETFOLDERNAME"
        }
    }
}