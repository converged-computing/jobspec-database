{
    "application": "SLURM",
    "details": {
        "software": "Python",
        "resources": {
            "partition": "normal",
            "ntasks": 1,
            "mem": "1024",
            "job_name": "Pythonbatch",
            "output": "<output_file.log>",
            "mail_type": "ALL",
            "mail_user": "<email>",
            "time": "<length_of_time in DD:HH:MM:SS format>",
            "tmpdir": "/scratch/$USER"
        }
    }
}