{
    "application": "matlab",
    "details": {
        "version": "2016b",
        "resources": {
            "nodes": 1,
            "processors_per_node": 16,
            "walltime": "59:00:00",
            "queue": "your_queue_name"
        },
        "execution": {
            "command": "matlab -nodesktop -r \"your_function_name(`expr ${PBS_ARRAYID}`); exit\"",
            "output_file": "your_function_name_`expr ${PBS_ARRAYID}`.out",
            "error_file": "your_function_name_`expr ${PBS_ARRAYID}`.err",
            "input_file": "/dev/null",
            "array_jobs": "1-48"
        }
    }
}