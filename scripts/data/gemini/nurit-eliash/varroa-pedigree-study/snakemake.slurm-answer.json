{
    "application": "Snakemake",
    "details": {
        "scheduler": "Slurm",
        "resources": {
            "partition": "compute",
            "time": "2-00:00:00",
            "memory": "12G",
            "cpus-per-task": 1,
            "ntasks": 1
        },
        "other_parameters": {
            "restart-times": 1,
            "jobs": 500,
            "parallelism": true,
            "max-jobs-per-second": 1,
            "cluster-config": "cluster.json",
            "cluster_command": "sbatch  -p {cluster.partition} --cpus-per-task {cluster.cpus-per-task} -t {cluster.time} --mem {cluster.mem}",
            "rerun-incomplete": true,
            "notemp": true,
            "nolock": true,
            "latency-wait": 60
        }
    }
}