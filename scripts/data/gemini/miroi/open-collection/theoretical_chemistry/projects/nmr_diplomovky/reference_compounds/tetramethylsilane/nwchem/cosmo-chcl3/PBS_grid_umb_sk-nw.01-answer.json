{
    "application": "NWChem",
    "software": [
        "Intel Fortran/C/C++ compilers 2013",
        "Intel MKL 2013 math library",
        "PGI compiler",
        "libnuma",
        "OpenMPI-Intel 4.0.1",
        "Python"
    ],
    "resource_requirements": {
        "nodes": 1,
        "ppn": 12,
        "walltime": "200:00:00",
        "memory": "40g",
        "queue": "batch",
        "scratch_directory": "/mnt/local/$USER/$PBS_JOBID"
    },
    "environment_variables": [
        "LD_LIBRARY_PATH",
        "PATH",
        "TMPDIR",
        "NWCHEM_EXECUTABLE",
        "OMP_NUM_THREADS",
        "MKL_NUM_THREADS",
        "MKL_DYNAMIC",
        "OMP_DYNAMIC",
        "PMIX_MCA_gds"
    ],
    "input_files": [
        "$PBS_O_WORKDIR/TMS.start_geom.xyz",
        "$PBS_O_WORKDIR/$project.nw"
    ],
    "output_files": [
        "$PBS_O_WORKDIR/$project.out",
        "$PBS_O_WORKDIR/TMS_geom*"
    ]
}