{
    "application": "Dalton",
    "details": {
        "software": [
            "Intel Fortran/C/C++ compilers",
            "MKL library",
            "OpenMPI",
            "CMake"
        ],
        "resources": {
            "nodes": 1,
            "ppn": 8,
            "walltime": "04:00:00",
            "memory": "8g",
            "queue": "batch",
            "scratch_directory": "/mnt/local/$USER/$PBS_JOBID"
        },
        "environment_variables": [
            "MKLROOT",
            "LD_LIBRARY_PATH",
            "PATH",
            "DALTON_TMPDIR",
            "MKL_NUM_THREADS",
            "MKL_DYNAMIC",
            "OMP_NUM_THREADS",
            "OMP_DYNAMIC"
        ]
    }
}