{
    "application": "NWChem",
    "details": {
        "software": [
            "Intel 2013 compilers",
            "Intel MKL 2013 math library",
            "PGI compiler",
            "OpenMPI-Intel 4.0.1",
            "python",
            "ifort",
            "mpif90",
            "icc",
            "mpicc",
            "mpirun"
        ],
        "resources": {
            "nodes": 1,
            "processors_per_node": 12,
            "walltime": "900:00:00",
            "memory": "40g",
            "queue": "batch",
            "scratch_directory": "/mnt/local/$USER/$PBS_JOBID",
            "environment_variables": [
                "LD_LIBRARY_PATH",
                "PATH",
                "TMPDIR",
                "NWCHEM_EXECUTABLE",
                "OMP_NUM_THREADS",
                "MKL_DYNAMIC",
                "OMP_DYNAMIC",
                "MKL_NUM_THREADS"
            ]
        }
    }
}