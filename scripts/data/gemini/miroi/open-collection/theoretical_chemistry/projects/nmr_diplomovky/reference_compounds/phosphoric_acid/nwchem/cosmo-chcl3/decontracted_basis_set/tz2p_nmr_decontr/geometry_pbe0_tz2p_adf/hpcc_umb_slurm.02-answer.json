{
    "application": "Slurm",
    "details": {
        "software": [
            "Intel 2013 compilers",
            "Intel MKL 2013 math library",
            "PGI compilers",
            "OpenMPI-Intel local installation",
            "NWChem"
        ],
        "resources": {
            "nodes": 1,
            "cpus": 12,
            "memory": "42GB",
            "time": "3-10:00:00",
            "partition": "compute",
            "scratch_directory": "/mnt/local/$USER/SLURMjob-${SLURM_JOBID}"
        },
        "environment_variables": [
            "PROD_DIR",
            "MKLROOT",
            "LD_LIBRARY_PATH",
            "PATH",
            "PMIX_MCA_gds",
            "NWCHEM_EXECUTABLE",
            "TMPDIR",
            "MKL_NUM_THREADS",
            "MKL_DYNAMIC",
            "OMP_NUM_THREADS",
            "OMP_DYNAMIC"
        ]
    }
}