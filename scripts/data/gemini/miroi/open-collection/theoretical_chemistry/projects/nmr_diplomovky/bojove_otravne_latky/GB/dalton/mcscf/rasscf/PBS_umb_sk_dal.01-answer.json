{
    "application": "Dalton",
    "details": {
        "software": [
            "Intel Fortran/C/C++ compilers with MKL library",
            "OpenMPI",
            "CMake",
            "Python"
        ],
        "resources": {
            "nodes": 1,
            "cpus": 8,
            "memory": "8g",
            "walltime": "04:00:00",
            "queue": "batch",
            "scratch_directory": "/mnt/local/$USER/$PBS_JOBID"
        },
        "environment_variables": [
            "DALTON",
            "BUILD",
            "DALTON_TMPDIR",
            "MKL_NUM_THREADS",
            "MKL_DYNAMIC",
            "OMP_NUM_THREADS",
            "OMP_DYNAMIC",
            "PATH",
            "LD_LIBRARY_PATH"
        ]
    }
}