{
    "application": "Dirac",
    "details": {
        "software": {
            "name": "Dirac",
            "version": "23",
            "build": "gcc11_cuda_essl_lapack_release"
        },
        "resources": {
            "nodes": 8,
            "walltime": "2:00",
            "mpi_processes": 16,
            "project": "CHP109",
            "scratch_dir": "${MEMBERWORK}/${ProjectNumber}/${FILE_NAME}"
        },
        "modules": [
            "/gpfs/alpine/proj-shared/chp109/dirac/build-setup/modules"
        ],
        "environment": {
            "DIRAC_PATH": "/gpfs/alpine/proj-shared/chp109/dirac/dirac-public/build_gcc11_cuda_essl_lapack_release-23",
            "PYTHONPATH": "${PYTHONPATH}:${DIRAC_PATH}",
            "BASDIR": "${DIRAC_PATH}/basis:${DIRAC_PATH}/basis_dalton:${DIRAC_PATH}/basis_ecp"
        },
        "input": {
            "mol_file": "${MOL_NAME}.xyz",
            "inp_file": "${INP_NAME}.inp"
        },
        "output": {
            "dir": "ccsd_pt_8nodes_x2c_avdz_canonical",
            "files": [
                "qfdir",
                "${FILE_NAME}.out"
            ]
        }
    }
}