{
    "application": "PBS",
    "details": {
        "software": [
            "aprun",
            "fluidity",
            "interval",
            "module load PrgEnv-fluidity"
        ],
        "resource_requirements": {
            "walltime": "00:05:00",
            "nodes": 1,
            "cores_per_node": 24
        },
        "environment_variables": {
            "FLUIDITYDIR": "/work/z01/z01/mjf/FluidityProject/fluidity",
            "PBS_O_WORKDIR": "absolute path",
            "PATH": "$PATH:$FLUIDITYDIR/bin",
            "PYTHONPATH": "$PYTHONPATH:$FLUIDITYDIR/python"
        }
    }
}