{
    "application": "TensorPack",
    "details": {
        "software": [
            "Python",
            "TensorPack",
            "MPI (OpenMPI)",
            "NCCL",
            "CUDA"
        ],
        "resources": {
            "nodes": "4",
            "GPUs": "8",
            "CPUs": "80",
            "memory": "200GB",
            "data_path": "~/data/imagenet",
            "batch_size": "32"
        },
        "libraries": [
            "serve-data.py",
            "main.py"
        ],
        "environment_variables": [
            "TENSORPACK_PROGRESS_REFRESH",
            "TENSORPACK_SERIALIZE",
            "DATA_PATH",
            "BATCH",
            "CONFIG",
            "NCCL_IB_CUDA_SUPPORT",
            "NCCL_IB_DISABLE",
            "NCCL_DEBUG"
        ]
    }
}