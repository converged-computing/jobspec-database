{
    "application": "Slurm",
    "details": {
        "software": [
            "Slurm",
            "OpenMPI",
            "NCCL",
            "Python"
        ],
        "resources": {
            "nodes": 4,
            "gpus": 8,
            "cpus": 80,
            "memory": "200G"
        },
        "libraries": [
            "TensorPack"
        ],
        "environment_variables": {
            "TENSORPACK_PROGRESS_REFRESH": "20",
            "TENSORPACK_SERIALIZE": "msgpack",
            "DATA_PATH": "~/data/imagenet",
            "BATCH": "32",
            "CONFIG": "$1"
        },
        "command": "python ./main.py --eval --data $DATA_PATH --batch $BATCH $CONFIG"
    }
}