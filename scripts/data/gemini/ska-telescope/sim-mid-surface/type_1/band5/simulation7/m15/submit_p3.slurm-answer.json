{
    "application": "Dask",
    "details": {
        "scheduler": "dask-scheduler",
        "worker": "dask-worker",
        "environment": {
            "python": "python",
            "module": "purge",
            "pythonpath": "PYTHONPATH=$PYTHONPATH:$ARL",
            "dask_scheduler": "ARL_DASK_SCHEDULER=${scheduler}:8786"
        },
        "resources": {
            "nodes": 12,
            "tasks": 29,
            "memory": "50GB",
            "time": "23:59:59"
        },
        "launch": {
            "method": "ssh",
            "hostfile": "hostfile.$JOBID",
            "directory": "$SLURM_SUBMIT_DIR"
        },
        "script": "surface_simulation_elevation.py"
    }
}