{
    "application": "SLURM",
    "details": {
        "resource_requirements": {
            "time": "{jobtime}",
            "job_name": "{jobname}",
            "output": "{logfilename}",
            "error": "{errfilename}",
            "qos": "shared_short",
            "partition": "shared",
            "nodes": 1,
            "tasks_per_node": 1
        },
        "software_requirements": {
            "ic_setup": "$STORE/ic_setup.sh",
            "data_labelling_setup": "$HOME/data_labelling/setup.sh"
        },
        "commands": "{commands}"
    }
}