{
    "application": "SLURM",
    "details": {
        "resource_manager": "SLURM",
        "queue": "gpu-long",
        "gpus": 1,
        "runtime": "1 hour",
        "output_files": "jobs/sweep_%A_%a.stdout",
        "error_files": "jobs/sweep_%A_%a.stderr",
        "conda_environment": "usb",
        "python_version": "python --version",
        "cuda_version": "cuda/11.8",
        "script": "train.py",
        "script_arguments": "--min_epochs=10 --max_epochs=500 --batch_size=512 --target_label=class --num_classes=2"
    }
}