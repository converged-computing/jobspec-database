{
    "application": "SLURM",
    "details": {
        "job_name": "classifier-job",
        "nodes": 1,
        "tasks": 1,
        "time": "02:00:00",
        "partition": "gpu-8",
        "gres": "gpu:a6000:2",
        "output": "jobs/cls_%A_%a.stdout",
        "error": "jobs/cls_%A_%a.stderr",
        "environment": "conda",
        "conda_env": "usb2",
        "python_version": "python",
        "script": "classifier.py",
        "arguments": "--log --method=tsfresh --dataset=dataset_a --target_label=category --classifier=lstm --task=identification --batch_size=32",
        "libraries": [
            "cuda/11.8",
            "pytorch",
            "cudnn"
        ]
    }
}