{
    "application": "SLURM",
    "details": {
        "resource_requirements": {
            "memory": "500G",
            "cores": 128,
            "walltime": "4:00:00",
            "partition": "amdv100"
        },
        "software_requirements": {
            "modules": [
                "python/3.7.2",
                "CMake",
                "GCC"
            ],
            "conda_environment": "dev",
            "python_script": "~/uw-nlp/pos/pos.py"
