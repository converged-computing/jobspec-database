{
    "application": "Slurm",
    "details": {
        "resource_requirements": {
            "partition": "gpu",
            "gpus": 1,
            "job_name": "Exp",
            "ntasks": 1,
            "cpus_per_task": 18,
            "time": "00:15:00",
            "output": "run_%A.out"
        },
        "software_requirements": {
            "modules": [
                "2022",
                "Anaconda3/2022.05"
            ],
            "environment": {
                "CEConv": "source activate CEConv",
                "DATA_DIR": "./DATA",
                "WANDB_DIR": "$HOME/CEConvDL2/CEConv/WANDB",
                "OUT_DIR": "./output",
                "WANDB_API_KEY": "$YOUR_API_KEY",
                "WANDB_NAME": "$RUN_NAME_ON_WANDB"
            },
            "python_script": "experiments.classification.train"
        }
    }
}