{
    "application": "Julia",
    "software": [
        "OpenMPI",
        "Julia",
        "CUDA"
    ],
    "resources": [
        "GPU (volta)",
        "CPU cores",
        "SLURM"
    ],
    "details": {
        "OpenMPI": "version 3.1.2",
        "Julia": "version 1.0",
        "CUDA": "latest version",
        "SLURM": {
            "partition": "gpu",
            "nodes": 1,
            "tasks_per_node": 4,
            "cpus_per_task": 4,
            "gres": "gpu:volta:4"
        },
        "environment_variables": {
            "SRCDIR": "$SLURM_SUBMIT_DIR",
            "JULIA_DEPOT_PATH": "${HOME}/.julia",
            "OPENBLAS_NUM_THREADS": "1",
            "OMPI_MCA_btl": "self,tcp"
        }
    }
}