{
    "application": "Singularity",
    "details": {
        "software": "DEST",
        "version": "v2.6.1",
        "image": "dest_v2.6.1_latest.sif",
        "resources": {
            "cores": 10,
            "memory": "90G",
            "time": "72:00:00",
            "queue": "instructional",
            "account": "biol4559-aob2x"
        },
        "input": {
            "reads": [
                "${srr}_1.fastq.gz",
                "${srr}_2.fastq.gz"
            ],
            "sampleId": "${sampleId}",
            "reference": "$3"
        },
        "output": "/project/biol4559-aob2x/mapping_output/${sampleId}.sync.gz",
        "logs": {
            "stdout": "/project/biol4559-aob2x/mapping_scripts/COMPUTEID/logs/RunDest.%A_%a.out",
            "stderr": "/project/biol4559-aob2x/mapping_scripts/COMPUTEID/logs/RunDest.%A_%a.err"
        },
        "parameters": {
            "max_cov": 0.95,
            "min_cov": 4,
            "base_quality_threshold": 25,
            "num_flies": "${numFlies}",
            "do_poolsnp": true,
            "single_end": false
        }
    }
}