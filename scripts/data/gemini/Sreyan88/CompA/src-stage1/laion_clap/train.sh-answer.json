{
    "application": "torchrun",
    "details": {
        "script_interpreter": "bash",
        "job_scheduler": "SLURM",
        "cluster_partition": "gamma",
        "job_name": "mclap",
        "nodes": 3,
        "tasks_per_node": 8,
        "cpus_per_gpu": 6,
        "exclusive_mode": true,
        "modules": [
            "cuda/11.3.1",
            "mpi"
        ],
        "environment_variables": {
            "NCCL_PROTO": "simple",
            "FI_EFA_FORK_SAFE": 1,
            "FI_LOG_LEVEL": 1,
            "FI_EFA_USE_DEVICE_RDMA": 1,
            "NCCL_DEBUG": "info",
            "OMPI_MCA_mtl_base_verbose": 1,
            "FI_EFA_ENABLE_SHM_TRANSFER": 0,
            "FI_PROVIDER": "efa",
            "FI_EFA_TX_MIN_CREDITS": 64,
            "NCCL_TREE_THRESHOLD": 0,
            "WORLD_SIZE": 8,
            "OMP_NUM_THREADS": 20
        },
        "torchrun_arguments": {
            "nnodes": 1,
            "nproc_per_node": 8,
            "master_addr": "localhost",
            "master_port": 2120,
            "module": "training.main",
            "name": "ck_sim",
            "save_frequency": 5,
            "save_top_performance": 3,
            "save_most_recent": true,
            "dataset_type": "csv",
            "precision": "fp32",
            "batch_size": 24,
            "lr": 0.0005,
            "wd": 0.0,
            "epochs": 45,
            "workers": 1,
            "use_bn_sync": true,
            "amodel": "HTSAT-tiny",
            "tmodel": "t5",
            "warmup": 3200,
            "report_to": "wandb",
            "wandb_notes": "ck_wandb_logs",
            "train_data": "./final_train.csv",
            "val_data": "./val_grouped_new_2.csv",
            "top_k_checkpoint_select_dataset": "Clotho-test",
            "top_k_checkpoint_select_metric": "mAP@10",
            "openai_model_cache_dir": "./src-stage1/laion_clap",
            "logs": "/fsx/clap_logs",
            "seed": 3407,
            "gather_with_grad": true,
            "optimizer": "adam",
            "data_filling": "repeatpad",
            "data_truncating": "fusion",
            "enable_fusion": true,
            "fusion_type": "aff_2d",
            "pretrained_audio": "./CLAP/htsat.ckpt"
        }
    }
}