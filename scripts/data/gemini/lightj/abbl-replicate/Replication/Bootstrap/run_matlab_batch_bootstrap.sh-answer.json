{
    "application": "Matlab",
    "software": "Matlab",
    "resource_requirements": {
        "nodes": 1,
        "processors_per_node": 20,
        "memory": "40gb",
        "temp_directory": "~/matlabtmp/$PBS_JOBID"
    },
    "command": "matlab -nodesktop -nosplash -r \"run_bootstrap $PBS_ARRAYID spec_name 20 100 100\"",
    "output_file": "stdout_${PBS_ARRAYID}.log"
}