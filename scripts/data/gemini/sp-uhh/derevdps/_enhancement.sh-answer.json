{
    "application": "SLURM",
    "details": {
        "scheduler": "SLURM",
        "resources": {
            "nodes": 1,
            "tasks_per_node": 1,
            "cpus_per_task": 8,
            "partition": "all",
            "nodelist": "spgpu2",
            "job_name": "eval",
            "output_file": ".slurm/%x-%j.out",
            "error_file": ".slurm/%x-%j.err",
            "time_limit": "4-00:00",
            "memory": 0,
            "qos": "wimi-compute",
            "gpus": 1
        },
        "python_script": "enhancement.py",
        "python_version": "3",
        "libraries": [
            "scipy",
            "torch",
            "torchaudio",
            "numpy",
            "matplotlib",
            "soundfile"
        ]
    }
}