{
    "application": "Slurm",
    "details": {
        "software": [
            "Slurm",
            "Python",
            "PyTorch",
            "MPI"
        ],
        "resources": [
            "CPU (Haswell)",
            "MPI",
            "SCRATCH storage",
            "Python modules"
        ],
        "configuration": {
            "nodes": 1,
            "queue": "debug",
            "time": "30 minutes",
