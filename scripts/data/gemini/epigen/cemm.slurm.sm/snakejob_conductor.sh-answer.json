{
    "application": "Snakemake",
    "details": {
        "resource_manager": "Slurm",
        "resource_requirements": {
            "nodes": 1,
            "tasks": 1,
            "cpus_per_task": 1,
            "time": "16:00:00",
            "memory": "1000MB",
            "partition": "longq",
            "qos": "longq",
            "mail_type": "end",
            "mail_user": "<username>@cemm.at"
        },
        "environment": {
            "conda_environment": "<snakemake_environment_name>",
            "workflow_directory": "<path/to/workflow>"
        },
        "execution_options": {
            "parallelism": true,
            "conda_activation": true,
            "report_generation": false
        }
    }
}