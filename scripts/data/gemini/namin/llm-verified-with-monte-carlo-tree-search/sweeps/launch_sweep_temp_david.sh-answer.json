            "wandb_project": "$WANDB_PROJECT",
            "wandb_group": "$WANDB_GROUP",
            "wandb_name": "$WANDB_NAME",
            "widen_policy_value": "$widen_policy_value",
            "discovery_factor": "$discovery_factor",
            "remove_hints": "True",
