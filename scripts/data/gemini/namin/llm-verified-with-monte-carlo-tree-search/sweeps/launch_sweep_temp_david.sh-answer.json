{
    "application": "Slurm",
    "details": {
        "job_name": "vmcts",
        "output_file": "/n/holyscratch01/kempner_fellows/Users/dbrandfonbrener/vmcts/logs/%A_%a.out",
        "nodes": 1,
        "tasks_per_node": 1,
        "gpus_per_node": 1,
        "cpus_per_task": 16,
        "time": "1:00:00",
        "memory": "250GB",
        "account": "kempner_fellows",
