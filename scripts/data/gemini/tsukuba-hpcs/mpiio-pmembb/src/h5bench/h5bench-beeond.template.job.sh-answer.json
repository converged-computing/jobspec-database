{
    "application": "h5bench",
    "other_software": [
        "openmpi",
        "spack",
        "beegfs",
        "time_json",
        "rsync",
        "tree",
        "h5ls"
    ],
    "resource_requirements": {
        "mpi_version": "${NQSV_MPI_VER}",
        "walltime": "24:00:00",
        "storage": "BeeOND",
        "beeond_chunksize": "512*2**10",
        "nodes": "${NNODES}",
        "ppn": "32",
        "spack_env_name": "${SPACK_ENV_NAME}",
        "script_dir": "${SCRIPT_DIR}",
        "output_dir": "${OUTPUT_DIR}"
    }
}