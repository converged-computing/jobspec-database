{
    "application": "rdbench",
    "software": [
        "openmpi",
        "rdbench",
        "spack",
        "beegfs-ctl"
    ],
    "resources": [
        "PBS",
        "BeeOND",
        "Lustre"
    ],
    "environment": [
        "SPACK_ENV_NAME",
        "SCRIPT_DIR",
        "OUTPUT_DIR",
        "NQSV_MPI_VER",
