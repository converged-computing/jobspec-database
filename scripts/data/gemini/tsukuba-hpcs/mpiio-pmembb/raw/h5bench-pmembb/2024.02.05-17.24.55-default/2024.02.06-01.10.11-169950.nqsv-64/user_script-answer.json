{
    "application": "h5bench",
    "details": {
        "software_requirements": [
            "openmpi",
            "spack",
            "rpmbb",
            "lfs",
            "h5bench"
        ],
        "resource_requirements": {
            "pbs_options": [
                "elapstim_req=5:00:00",
                "T openmpi",
                "v NQSV_MPI_VER=5.0.0rc12-pmembb-eval-gcc-11.4.0-fevlzaz",
                "v USE_DEVDAX=pmemkv",
                "v NUM_DEVDAX=1"
