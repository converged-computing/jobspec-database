{
    "application": "PBS",
    "software_requirements": {
        "bash": "which bash",
        "picard": "$picardir",
        "samtools": "$samdir",
        "gatk": "$gatk",
        "tabix": "$tabixdir",
        "vcftools": "$vcftoolsdir",
        "launcher": "$launcherdir/scheduler.x",
        "java": "$javadir"
    },
    "resource_requirements": {
        "pbs_project_id": "$pbsprj",
        "pbs_queue": {
            "wgs": "$pbsqueueWGEN",
            "wes": "$pbsqueueEXOME"
        },
        "pbs_cpu": {
            "wgs": "$pbSCPUOtherWGEN",
            "wes": "$pbSCPUOtherEXOME"
        },
        "pbs_threads": "$pbSTHREADS",
        "nodes": {
            "launcher": "$numnodes",
            "per_sample": "1"
        }
    }
}