{
    "application": "matlab",
    "software": [
        "nixpkgs/16.09",
        "gcc/7.3.0",
        "openmpi/3.1.4",
        "matlab/2020a",
        "gromacs/2019.6"
    ],
    "resources": [
        {
            "name": "time",
            "value": "##HOURS##:0:00"
        },
        {
            "name": "nodes",
            "value": "##NODES##"
        },
        {
            "name": "ntasks",
            "value": "1"
        },
        {
            "name": "cpus-per-task",
            "value": "##CPUS##"
        },
        {
            "name": "mem-per-cpu",
            "value": "##MEMORY##"
        },
        {
            "name": "account",
            "value": "##ACCOUNT##"
        },
        {
            "name": "job-name",
            "value": "##MODEL_NAME##"
        },
        {
            "name": "error",
            "value": "##MODEL_NAME##.stde"
        },
        {
            "name": "output",
            "value": "##MODEL_NAME##.stdo"
        }
    ],
    "environment_variables": [
        {
            "name": "MATLAB_PREFDIR",
            "value": "$SLURM_TMPDIR/.matlab"
        }
    ]
}