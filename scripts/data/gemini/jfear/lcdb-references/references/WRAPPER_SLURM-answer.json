{
    "application": "Snakemake",
    "details": {
        "environment": "conda environment 'lcdb-references'",
        "cluster_config": "config/clusterconfig.yaml",
        "config_file": "config/config.yaml",
        "cluster_scheduler": "SLURM",
        "cluster_options": {
            "partition": "norm",
            "time": "24:00:00",
            "job_name": "lcdb-wf",
            "cpus_per_task": "{threads}"
        },
        "output_logs": "slurm_logs/{rule}.o.%j",
        "error_logs": "slurm_logs/{rule}.e.%j",
        "job_name_format": "s.{rulename}.{jobid}.sh",
        "max_jobs": 999,
        "logging_file": "Snakefile.log"
    }
}