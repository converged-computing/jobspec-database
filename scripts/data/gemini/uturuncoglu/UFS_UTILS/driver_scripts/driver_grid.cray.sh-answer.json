{
    "application": "fv3gfs",
    "software": [
        "PrgEnv-intel",
        "cfp-intel-sandybridge/1.1.0"
    ],
    "resources": {
        "queue": "debug",
        "memory": "2400M",
        "walltime": "00:30",
        "nodes": "1",
        "aprun": {
            "grid_gen": "aprun -n 1 -N 1 -j 1 -d 1 -cc depth",
            "sfc_climo_gen": "aprun -j 1 -n 24 -N 24"
        },
        "omp_num_threads": "6",
        "omp_stacksize": "2048m",
        "kmp_affinity": "disabled",
        "machine": "WCOSS_C"
    },
    "ulimits": {
        "unlimited": [
            "stacksize"
        ]
    },
    "script_description": "Driver script to create a cubic-sphere based model grid on Cray. Produces files containing lat/lon and other records, orography, and surface climate fields."
}