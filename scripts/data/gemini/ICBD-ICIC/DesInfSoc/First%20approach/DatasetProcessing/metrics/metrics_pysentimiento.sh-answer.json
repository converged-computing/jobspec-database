{
    "application": "Slurm",
    "details": {
        "job_name": "itrust-sentiments",
        "array_range": "0-58",
        "time_limit": "5 days",
        "output_file": "outputs2/sentiments-%A-%a.out",
        "error_file": "errors2/sentiments-%A-%a.err",
        "environment": ".sentiments_env",
        "python_script": "metrics_pysentimiento.py",
        "arguments": "${SLURM_ARRAY_TASK_ID}"
    }
}