{
    "application": "SLURM",
    "details": {
        "job_name": "itrust-context",
        "memory_per_cpu": "8G",
        "cpus_per_task": 2,
        "array_tasks": "0-10",
        "time_limit": "02-00:00:00",
        "output_file": "outputs/context-%A-%a.out",
        "error_file": "errors/context-%A-%a.err",
        "environment": ".context_env/bin/activate",
        "script": "context.py",
        "language": "python"
    }
}