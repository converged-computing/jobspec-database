{
    "application": "SLURM",
    "details": {
        "software": [
            "openmpi"
        ],
        "resources": {
            "nodes": 2,
            "tasks_per_node": 8,
            "cpus_per_task": 16,
            "constraints": "rome,ib",
            "partition": "ccb",
            "time": "00:10:00",
            "job_name": "mpi_omp_example4",
            "output_file": "mpi_omp_example4.log"
        },
        "environment": {
            "OMP_NUM_THREADS": "${SLURM_CPUS_PER_TASK}",
            "mpirun": "-np ${SLURM_NTASKS}"
        },
        "program": "mpi_omp_mockup"
    }
}