{
    "application": "SuperMUC",
    "software": [
        {
            "name": "OpenMPI",
            "version": "4.1.5-gcc12"
        },
        {
            "name": "IMPI",
            "version": "2021.9.0"
        },
        {
            "name": "TBB",
            "version": "2021.10.0"
        },
        {
            "name": "GCC",
            "version": "13.1.0"
        },
        {
            "name": "CMAKE",
            "version": "3.26.3"
        },
        {
            "name": "Boost",
            "version": "1.82.0-gcc12-impi"
        },
        {
            "name": "Spack",
            "version": "23.1.0"
        },
        {
            "name": "Intel Toolkit",
            "version": "2023.2.0"
        }
    ],
    "resource_requirements": {
        "nodes": {
            "type": "integer",
            "description": "Number of nodes to request for the job"
        },
        "tasks": {
            "type": "integer",
            "description": "Total number of tasks (MPI processes) to request for the job"
        },
        "cpus_per_task": {
            "type": "integer",
            "description": "Number of CPU cores per task"
        },
        "tasks_per_node": {
            "type": "integer",
            "description": "Number of tasks (MPI processes) to run per node"
        },
        "timelimit": {
            "type": "string",
            "description": "Time limit for the job, in HH:MM:SS format"
        },
        "partition": {
            "type": "string",
            "description": "Partition to submit the job to (micro, general, large)"
        },
        "mpi": {
            "type": "string",
            "description": "MPI implementation to use (OpenMPI, IMPI)"
        },
        "project": {
            "type": "string",
            "description": "Project account to use for the job"
        }
    }
}