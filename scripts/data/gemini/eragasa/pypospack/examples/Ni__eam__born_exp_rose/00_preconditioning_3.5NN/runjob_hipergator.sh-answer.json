{
    "application": "SLURM",
    "details": {
        "job_name": "Ni_rose",
        "mail_type": "NONE",
        "ntasks": 128,
        "cpus_per_task": 1,
        "distribution": "cyclic:cyclic",
        "mem_per_cpu": "3000mb",
        "time": "24:00:00",
        "output": "job.out",
        "error": "job.err",
        "qos": "phillpot-b",
        "modules": [
            "intel/2018.1.163",
            "openmpi/3.0.0"
        ],
        "environment_variables": [
            "OMPI_MCA_mpi_warn_on_fork=0",
            "PYTHONPATH",
            "PATH"
        ],
        "command": "srun --mpi=pmix_v1 python mc_iterative_sampler.py"
    }
}