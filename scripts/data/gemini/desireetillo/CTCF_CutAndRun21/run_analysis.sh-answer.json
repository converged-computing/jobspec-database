{
    "application": "Snakemake",
    "details": {
        "python_version": "3.5",
        "snakemake_version": "5.1.3",
        "cluster_config": "cluster.json",
        "slurm_partition": "ccr",
        "slurm_time_limit": "12:00:00",
        "slurm_mail_type": "ALL",
        "max_jobs": 100,
        "latency_wait": 10,
        "cluster_gres": "{cluster.gres}",
        "cluster_threads": "{cluster.threads}",
        "cluster_partition": "{cluster.partition}",
        "cluster_time": "{cluster.time}",
        "cluster_mem": "{cluster.mem}",
        "job_name": "{params.rname}"
    }
}