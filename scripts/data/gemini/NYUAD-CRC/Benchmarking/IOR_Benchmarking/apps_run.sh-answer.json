{
    "application": "IOR",
    "details": {
        "scheduler": "Slurm",
        "nodes": 2,
        "tasks": 64,
        "tasks_per_node": 32,
        "queue": "large",
        "modules": [
            "gcc",
            "openmpi"
        ],
        "libraries": [
            "ior"
        ],
        "output_files": [
            "slurm/%j.out",
            "slurm/%j.err"
        ],
        "working_directory": "$working_directory/bin",
        "output_directory": "$working_directory/$outdir",
        "log_directory": "$working_directory/$outdir/log"
    }
}