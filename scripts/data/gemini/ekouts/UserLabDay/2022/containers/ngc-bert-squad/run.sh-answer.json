{
    "application": "SLURM",
    "details": {
        "scheduler": "SLURM",
        "job name": "bert-squad-finetune",
        "runtime": "50 minutes",
        "nodes": 8,
        "tasks per core": 1,
        "tasks per node": 1,
        "cpus per task": 12,
        "partition": "debug",
        "constraint": "gpu",
        "account": "usup",
        "modules": [
            "daint-gpu",
            "sarus",
            "OpenMPI/4.1.2"
        ],
        "environment variable": "OMP_NUM_THREADS",
        "environment variable value": "$SLURM_CPUS_PER_TASK",
        "runtime environment": "nvcr.io/nvidia/pytorch:22.08-py3",
        "python script": "3_squad_bert_deepspeed.py",
        "deepspeed configuration": "ds_config.json"
    }
}