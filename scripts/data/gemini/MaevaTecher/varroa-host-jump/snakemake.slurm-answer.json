{
    "application": "Snakemake",
    "details": {
        "environment": {
            "python_version": "3.5.2",
            "conda_environment": "popgenomics"
        },
        "resource_requirements": {
            "partition": "short",
            "time": "0-02:00:00",
            "memory": "12G",
            "cores": 1,
            "tasks": 1,
            "cluster_config": "cluster.json"
        },
        "snakemake_options": {
            "restart_times": 1,
            "jobs": 500,
            "parallel": true,
            "max_jobs_per_second": 1,
            "rerun_incomplete": true,
            "notemp": true,
            "nolock": true
        }
    }
}