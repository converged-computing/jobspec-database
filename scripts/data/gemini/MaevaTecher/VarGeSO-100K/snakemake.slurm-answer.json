{
    "application": "Snakemake",
    "details": {
        "resource_manager": "SLURM",
        "job_name": "snakemake",
        "partition": "compute",
        "time": "5-00:00:00",
        "memory": "12G",
        "cpus_per_task": 1,
        "ntasks": 1,
        "output_file": "%j.out",
        "conda_environment": "popgenomics",
        "python_version": "3.5.2",
        "snakemake_options": {
            "restart_times": 1,
            "jobs": 500,
            "parallel": true,
            "max_jobs_per_second": 1,
            "cluster_config": "cluster.json",
            "rerun_incomplete": true,
            "notemp": true,
            "nolock": true
        }
    }
}