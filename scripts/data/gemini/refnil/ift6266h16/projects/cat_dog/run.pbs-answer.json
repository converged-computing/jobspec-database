{
    "application": "Theano",
    "details": {
        "scheduler": "PBS",
        "walltime": "1 hour",
        "nodes": "1 node with 1 core",
        "queue": "@hades",
        "dependency": "after any job with token TOKEN",
        "output": "run.pbs.o$PBS_JOBID",
        "error": "run.pbs.o$PBS_JOBID",
        "module": "CUDA",
        "environment variables": {
            "THEANO_FLAGS": "device=gpu,floatX=float32,nvcc.fastmath=True,compiledir=$RAMDISK",
            "NAME": "$PBS_JOBID",
            "RESULT_PATH": "./result/$NAME"
        },
        "script": "run.py"
    }
}