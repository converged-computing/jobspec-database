{
    "application": "SLURM",
    "details": {
        "time_limit": "2 hours",
        "nodes": 4,
        "tasks": 4,
        "partition": "west",
        "output": "west4.out",
        "error": "west4.err",
        "environment": [
            "/etc/profile.d/modules.sh",
            "/etc/profile.d/wr-spack.sh",
            "spack load --dependencies mpi"
        ],
        "mpi_implementation": "MPICH",
        "mpi_network_module": "ib (for abu partition)",
        "executable": "../writefile"
    }
}