{
    "application": "Slurm",
    "details": {
        "resource_requirements": {
            "time_limit": "2 hours",
            "nodes": 2,
            "tasks_per_node": 5,
            "partition": "west",
            "output_file": "west2.out",
            "error_file": "west2.err"
        },
        "software_requirements": {
            "modules": {
                "profile": "/etc/profile.d/modules.sh",
                "spack": "/etc/profile.d/wr-spack.sh"
            },
            "spack_packages": {
                "mpi": true,
                "dependencies": true
            },
            "mpi_implementation": {
                "nemesis_netmod": "ib"
            }
        },
        "executable": {
            "path": "../writefile",
            "arguments": [
                "1000000000",
                "1",
                "3",
                "1000000000",
                "2",
                "3"
            ]
        }
    }
}