{
    "application": "axpby_bench",
    "software_requirements": [
        "PrgEnv/gcc12-openmpi-python",
        "likwid-pin"
    ],
    "resource_requirements": {
        "nodes": 1,
        "tasks": 1,
        "cores": 64,
        "partition": "amd",
        "threads_per_core": 1,
        "memory": "200G",
        "time": "3000",
        "output_format": "%x-%j.out",
        "ulimit_stack_size": "unlimited",
        "OMP_STACKSIZE": "100M"
    }
}