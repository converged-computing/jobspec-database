{
    "application": "Slurm",
    "details": {
        "resource_requirements": {
            "nodes": 2,
            "tasks_per_node": 8,
            "walltime": "15:00:00",
            "job_name": "tfrec_8gpu",
            "output_file": "outputs/tfrec_8gpu-%J.o",
            "error_file": "outputs/tfrec_8gpu-%J.o",
            "partition": "gpuq",
            "features": "c4140,m,32gb,v100"
        },
        "software_requirements": {
            "cuda": "10.0.130",
            "gcc": "7.2.0",
            "python_environment": "docker_pip2",
            "mpi": "openmpi-4.0.0-flags-ucx"
        },
        "python_script": "chexnet_densenet_tfrec.py",
        "command": "mpirun -np 8 --map-by socket  python chexnet_densenet_tfrec.py --batch_size=64  --epochs=10 --skip_eval=1 --write_weights=0",
        "alternative_command": "horovodrun -np 1 python $HOME/models/chexnet/official_resnet_tf_1.12.0.py  --batch_size=128  --epochs=10"
    }
}