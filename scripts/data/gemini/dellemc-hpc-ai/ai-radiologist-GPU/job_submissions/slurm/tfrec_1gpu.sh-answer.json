{
    "application": "Slurm",
    "details": {
        "resource_requirements": {
            "nodes": 1,
            "tasks_per_node": 1,
            "wall_time": "15:00:00",
            "job_name": "tfrec_1gpu",
            "output_file": "outputs/tfrec_1gpu-%J.o",
            "error_file": "outputs/tfrec_1gpu-%J.o",
            "partition": "gpuq",
            "features": "c4140,m,32gb,v100",
            "modules": [
                "cuda10.0/toolkit/10.0.130",
                "gcc/7.2.0"
            ],
            "conda_env": "docker_pip2"
