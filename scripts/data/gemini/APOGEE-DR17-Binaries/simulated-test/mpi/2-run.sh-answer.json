{
    "application": "Slurm",
    "details": {
        "job_name": "apogee-run",
        "output_file": "logs/apogee-run.o%j",
        "error_file": "logs/apogee-run.e%j",
        "nodes": 8,
        "tasks_per_node": 56,
        "time_limit": "36:00:00",
        "partition": "cca",
        "constraints": "rome",
        "environment": {
            "HQ_RUN_PATH": "defined in hq-config/init.sh",
            "CONDA_PREFIX": "assumed to be set"
        },
        "libraries": [
            "mpi4py",
            "thejoker"
        ],
        "language": "Python 3",
        "command": "hq run_thejoker -v --mpi"
    }
}