{
    "application": "Slurm",
    "details": {
        "job_name": "apogee-run",
        "output_file": "logs/apogee-run.o%j",
        "error_file": "logs/apogee-run.e%j",
        "nodes": 8,
        "tasks_per_node": 56,
        "time_limit": "36:00:00",
        "partition": "cca",
        "constraint": "rome",
        "environment": "hq-config/init.sh",
        "python_package": "mpi4py",
        "executable": "$CONDA_PREFIX/bin/hq run_thejoker",
        "arguments": "-v --mpi"
    }
}