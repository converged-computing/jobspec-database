{
    "application": "Slurm",
    "details": {
        "software": [
            "python3/anaconda/2019.10",
            "dbtool.py",
            "job.sh"
        ],
        "resources": {
            "nodes": "$SLURM_JOB_NUM_NODES",
            "tasks": "$SLURM_NTASKS",
            "cores_per_task": "$SLURM_CPUS_PER_TASK"
        },
        "command_line_parameters": [
            "--job-name",
            "--output",
            "--error",
            "--ntasks",
            "--export"
        ],
        "environment_variables": [
            "dataset",
            "n",
            "a",
            "e",
            "t",
            "s",
            "x",
            "SLURM_NTASKS"
        ],
        "data_files": [
            "results.db",
            "name.json"
        ]
    }
}