{
    "application": "mpiexec",
    "details": {
        "software": [
            "conda",
            "mpiexec",
            "python",
            "nvidia-smi"
        ],
        "resources": {
            "nodes": 10,
            "walltime": "1:00:00",
            "filesystems": [
                "home",
                "grand"
            ],
            "queue": "prod",
            "account": "STlearn",
            "environment": {
                "MPICH_GPU_SUPPORT_ENABLED": "1",
                "NCCL_COLLNET_ENABLE": "1",
                "NCCL_NET_GDR_LEVEL": "PHB",
                "OMP_NUM_THREADS": 8,
                "OMP_PLACES": "threads"
            }
        },
        "libraries": {
            "conda": "3DCNN"
        },
        "script_paths": [
            "/lus/grand/projects/STlearn/4D_fMRI_Transformer/main.py",
            "/lus/grand/projects/STlearn/4D_fMRI_Transformer/scripts/polaris/set_affinity_gpu_polaris.sh",
            "/lus/grand/projects/STlearn/HCP_MNI_to_TRs"
        ]
    }
}