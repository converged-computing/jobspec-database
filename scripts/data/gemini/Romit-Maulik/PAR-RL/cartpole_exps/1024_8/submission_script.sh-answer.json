{
    "application": "Ray",
    "details": {
        "runtime": "Python",
        "framework": "Ray",
        "library": "mpi4py",
        "resource_manager": "Cobalt",
        "compute_nodes": 1024,
        "walltime": "180",
        "account": "datascience",
        "modules": [
            "anaconda3",
            "mvapich2",
            "rllib_env"
        ],
        "environment_variables": [
            "COBALT_JOBID",
            "COBALT_JOBSIZE",
            "MPICH_GNI_FORK_MODE",
            "PMI_NO_FORK",
            "KMP_AFFINITY",
            "LC_ALL",
            "LANG",
            "PATH"
        ],
        "other_requirements": [
            "ssds=required",
            "ssd_size=128",
            "enable_ssh=1"
        ]
    }
}