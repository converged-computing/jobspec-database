{
    "application": "Ray",
    "software": [
        "OpenFOAM",
        "MVAPICH2",
        "IntelPython3.6",
        "RLLib"
    ],
    "resources": [
        "Cobalt",
        "MPI",
        "SSDs",
        "OpenMP"
    ],
    "environment_variables": [
        "COBALT_JOBID",
        "FOAM_LIBBIN",
        "LD_LIBRARY_PATH",
        "KMP_BLOCKTIME",
        "KMP_AFFINITY",
        "OMP_PROC_BIND",
        "OMP_NESTED",
        "MPICH_GNI_FORK_MODE",
        "PMI_NO_FORK",
        "LC_ALL",
        "LANG",
        "PATH"
    ],
    "modules": [
        "intelpython36",
        "mvapich2",
        "trackdeps",
        "darshan",
        "xalt"
    ],
    "commands": [
        "source /home/projects/OpenFOAM/OpenFOAM-5.x/etc/bashrc.ThetaIcc",
        "aprun -n $COBALT_JOBSIZE -N 1 --cc none python start_ray.py",
        "source activate <yourenvname>"
    ]
}