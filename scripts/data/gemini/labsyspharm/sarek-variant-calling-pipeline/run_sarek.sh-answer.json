{
    "application": "Nextflow",
    "software": [
        "Java JDK 11.0.11",
        "nf-core/sarek (version 3.4.0)",
        "Singularity"
    ],
    "resources": {
        "cores": 1,
        "runtime": "24 hours",
        "memory": "5GB",
        "partition": "medium",
        "output": "%x_%j.out",
        "error": "%x_%j.err"
    },
    "parameters": {
        "params_file": "nf-params.json",
        "config_file": "~/o2.config",
        "trace": true,
        "report": "report_${SLURM_JOB_ID}.html",
        "timeline": "timeline_${SLURM_JOB_ID}.html"
    }
}