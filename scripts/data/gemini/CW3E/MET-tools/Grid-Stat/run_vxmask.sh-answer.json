{
    "application": "MET",
    "details": {
        "software": [
            "MET",
            "Singularity"
        ],
        "resources": {
            "compute": {
                "nodes": 1,
                "cores": "not specified",
                "partition": "shared"
            },
            "memory": "120G",
            "time": "01:00:00",
            "storage": {
                "input": [
                    "/MSK_ROOT",
                    "/MSK_IN"
                ],
                "output": "/MSK_OUT"
            },
            "environment": {
                "variables": {
                    "USR_HME": "Path to MET-tools clone directory",
                    "OBS_F_IN": "Path to StageIV grid file",
                    "MSK_ROOT": "Path to root directory for landmasks",
                    "MSK_IN": "Path to lat-lon text files for mask generation",
                    "MSKS": "List of landmasks for verification region",
                    "MSK_OUT": "Output directory for landmasks",
                    "MET_SNG": "Path to MET singularity container"
                }
            }
        }
    }
}