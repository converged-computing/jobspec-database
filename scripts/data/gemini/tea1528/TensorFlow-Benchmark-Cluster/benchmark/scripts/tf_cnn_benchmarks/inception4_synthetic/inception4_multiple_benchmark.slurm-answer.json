{
    "application": "Tensorflow",
    "details": {
        "environment": "GCC Singularity git",
        "repository": "tensorflow/models",
        "container": "docker://tensorflow/tensorflow:latest-gpu",
        "script": "tf_cnn_benchmarks.py",
        "parameters": {
            "num_gpus": 1,
            "batch_size": 32,
            "model": "inception4",
            "variable_update": "parameter_server"
        },
        "slurm_resources": {
            "account": "sc3260",
            "partition": "maxwell",
            "gpu": 1,
            "memory": "10G",
            "time": "20:00:00",
            "output": "inception_multiple_gpu.out"
        }
    }
}