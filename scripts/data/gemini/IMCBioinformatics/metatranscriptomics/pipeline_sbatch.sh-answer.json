{
    "application": "Snakemake",
    "details": {
        "resource_manager": "SLURM",
        "partition": "synergy,cpu2019,cpu2021",
        "nodes": 1,
        "tasks": 1,
        "cpus_per_task": 8,
        "walltime": "7-00:00:00",
        "memory": "10G",
        "error_file": "record_sbatch_run.%J.err",
        "output_file": "record_sbatch_run.%J.out",
        "cluster_config_file": "cluster.json",
        "log_directory": "$(pwd)",
        "log_file": "logs/pipeline-analysis.log.txt",
        "number_of_jobs": 60
    }
}