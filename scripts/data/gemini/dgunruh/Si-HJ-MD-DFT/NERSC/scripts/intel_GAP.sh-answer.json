{
    "application": "LAMMPS",
    "software": [
        "LAMMPS",
        "OpenMPI",
        "Python 3.6.0",
        "PrgEnv-intel"
    ],
    "resource_requirements": {
        "queue": "regular",
        "constraint": "haswell",
        "time": "1-12:00",
        "nodes": 4,
        "tasks_per_node": 32,
        "cpus_per_task": 2,
        "memory": "118G",
        "openmp_threads": 1
    },
    "output_files": [
        "outputs/cSiaSiMD-%j.output",
        "aSi-GAP-$j-$t.xyz",
        "aSiBox-GAP-$j-$t.xyz"
    ]
}