{
    "application": "LAMMPS",
    "details": {
        "resource_requirements": {
            "nodes": 1,
            "cores_per_node": 64,
            "memory": "250G",
            "runtime": "2 days",
            "partition": "med2"
        },
        "software": {
            "openmpi": "module loaded",
            "lammps": "version 3Mar2020, MPI compiled"
        },
        "input_file": "heatStrip.in",
        "output_files": [
            "bulk_heatstrip-$displacement-$change.xyz",
            "box_heatstrip-$displacement-$change.xyz",
            "outputs/cSiaSiMD-%j.output"
        ],
        "environment_variables": {
            "OMP_NUM_THREADS": "$SLURM_CPUS_PER_TASK",
            "j": "$SLURM_JOB_ID",
            "s": "124248+$j+$t",
            "displacement": "2.97",
            "change": "3.14",
            "dumpA": "bulk_heatstrip-$displacement-$change.xyz",
            "dumpsnapA": "box_heatstrip-$displacement-$change.xyz"
        }
    }
}