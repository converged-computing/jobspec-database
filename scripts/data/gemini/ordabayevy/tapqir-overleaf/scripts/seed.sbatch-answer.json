{
    "application": "Slurm",
    "details": {
        "job_name": "seed",
        "output": "simulations/seed%a_%A.out",
        "gres": "gpu:1",
        "array": "0-16",
        "executable": "scripts/cosmos_simulations.py",
        "arguments": [
            "--seed ${SLURM_ARRAY_TASK_ID}",
            "--cuda",
            "--path simulations/seed${SLURM_ARRAY_TASK_ID}"
        ],
        "language": "python",
        "resource_requirements": {
            "gpu": 1
        }
    }
}