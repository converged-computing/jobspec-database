{
    "application": "SLURM",
    "details": {
        "job_name": "height",
        "output": "simulations/height%a_%A.out",
        "resources": {
            "gpu": 1
        },
        "array_tasks": 8,
        "executable": "python scripts/cosmos_simulations.py",
        "arguments": {
            "gain": 7,
            "pi": 0.15,
            "lamda": 0.15,
            "proximity": 0.2,
            "height": "dynamic based on array task ID",
            "cuda": true,
            "path": "simulations/height[height value]"
        },
        "environment": "bash"
    }
}