{
    "application": "Keras",
    "details": {
        "libraries": [
            "lib/x86_64-linux-gnu/ld-linux-x86-64.so.2",
            "PNETCDF_INSTALL_DIR/lib",
            "FAKE_SYSTEM_LIBS/lib",
            "FAKE_SYSTEM_LIBS/lib/x86_64-linux-gnu",
            "FAKE_SYSTEM_LIBS/usr/lib64/gconv",
            "FAKE_SYSTEM_LIBS/usr/lib64/audit",
            "LD_LIBRARY_PATH"
        ],
        "python_interpreter": "PYTHONHOME/bin/python",
        "script_path": "$PWD/keras_lenet3.py",
        "arguments": [
            "--train_batch",
            "$train_batch",
            "--epochs",
            "13"
        ],
        "environment_variables": [
            "FAKE_SYSTEM_LIBS",
            "PNETCDF_INSTALL_DIR",
            "PYTHONHOME",
            "LD_LIBRARY_PATH"
        ],
        "mpi_parameters": [
            "--map-by node",
            "-n $nodes",
            "--mca opal_event_include poll"
        ],
        "batch_size": "$train_batch",
        "epochs": "13",
        "nodes": "$nodes"
    }
}