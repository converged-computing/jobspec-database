{
    "application": "Qiskit",
    "details": {
        "environment": "SLURM",
        "python_version": "3.10.4",
        "compiler": "gcccore-11.3.0",
        "quantum_provider": "IBM Quantum",
        "script": "run_qaoa_domain_wall.py",
        "arguments": "--custom_mixer --recursive -initial_point 0.0 0.0 -reps $SLURM_ARRAY_TASK_ID -num_experiments 100 -num_initial_solutions 3",
        "resources": {
            "nodes": 1,
            "tasks_per_node": 2,
            "time": "6:00:00",
            "account": "$SLURM_ARRAY_TASK_ID",
            "partition": "$SLURM_ARRAY_TASK_ID"
        }
    }
}