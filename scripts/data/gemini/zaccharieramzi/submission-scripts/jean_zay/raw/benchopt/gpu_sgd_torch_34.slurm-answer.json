{
    "application": "SLURM",
    "details": {
        "job_name": "benchopt_run_sgd_torch",
        "nodes": 1,
        "tasks_per_node": 1,
        "gpus": 1,
        "cpus_per_task": 10,
        "walltime": "06:00:00",
        "output_file": "%x_%A_%a.out",
        "error_file": "%x_%A_%a.out",
        "qos": "qos_gpu-t3",
        "distribution": "block:block",
        "hints": "nomultithread",
        "modules": [
            "pytorch-gpu/py3/1.10.1"
        ],
        "python_environment": "$WORK/.local_torch",
        "working_directory": "$WORK/benchmark_resnet_classif",
        "command": "benchopt run . -o *34 -d cifar[*,random_state=42,with_validation=False] -r 1 -n 200 --timeout 10800 -s sgd-torch[batch_size=128,data_aug=True,*,lr_schedule=cosine,momentum=0.9,nesterov=True,weight_decay=0.0005]"
    }
}