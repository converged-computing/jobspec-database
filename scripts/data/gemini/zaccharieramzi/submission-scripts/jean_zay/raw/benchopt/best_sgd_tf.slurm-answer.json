{
    "application": "Slurm",
    "details": {
        "job_name": "benchopt_run_best_sgd",
        "tasks": 1,
        "tasks_per_node": 1,
        "gpu": 1,
        "cpus_per_task": 10,
        "time": "02:00:00",
