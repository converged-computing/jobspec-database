{
    "application": "Benchopt",
    "details": {
        "software": [
            "Benchopt",
            "PyTorch",
            "Python 3"
        ],
        "resources": {
            "SLURM": {
                "job_name": "restricted_benchopt_run",
                "ntasks": 1,
                "ntasks_per_node": 1,
                "gres": "gpu:1",
                "cpus_per_task": 10,
                "time": "02:00:00",
                "output": "%x_%A_%a.out",
                "error": "%x_%A_%a.out",
                "qos": "qos_gpu-t3",
                "distribution": "block:block",
                "hint": "nomultithread",
                "array": "7,15,23,31,39"
            },
            "modules": {
                "pytorch-gpu/py3/1.10.1": {
                    "load": true
                }
            },
            "environment": {
                "PYTHONUSERBASE": "$WORK/.local_torch",
                "PATH": "$WORK/.local_torch/bin:$PATH"
            }
        }
    }
}