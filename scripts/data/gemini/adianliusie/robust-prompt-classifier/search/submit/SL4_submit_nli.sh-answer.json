{
    "application": "SLURM",
    "software": [
        "SLURM",
        "OpenMPI",
        "OpenMP"
    ],
    "resources": [
        "AMD EPYC 7763",
        "ConnectX-6",
        "A100",
        "GPU",
        "CPU"
    ],
