{
    "application": "Spark",
    "details": {
        "modules": [
            "spark/2.3.2-hadoop2.7",
            "python/2.7.15"
        ],
        "configuration": {
            "nodes": 1,
            "memory": "100G",
            "output": "slurm-logs/hm.%j.out",
            "error": "slurm-logs/hm.%j.err",
            "exclusive": true,
            "spark_master": "local[40]",
            "py_files": "preprocessor.py",
            "executor_memory": "100g",
            "spark_driver_maxResultSize": "10G",
            "spark_network_timeout": "1000s",
            "driver_memory": "100g",
            "ulimit": 10000
        },
        "files": [
            "heatMap.py",
            "data/RDDs/$3",
            "data/$G1/graph",
            "data/$G2/graph"
        ],
        "output_directory": "HMArrays/$3"
    }
}