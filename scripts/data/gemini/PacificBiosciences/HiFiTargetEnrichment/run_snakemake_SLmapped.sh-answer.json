{
    "application": "Snakemake",
    "details": {
        "workflow_management": "Snakemake",
        "cluster_manager": "Slurm",
        "environment": "Conda/Mamba",
        "containerization": "Singularity",
        "gpu_support": true,
        "resource_requirements": {
            "cpus": 4,
            "jobs": 500,
            "max_jobs_per_second": 1,
            "partition": "compute",
            "nodes": 1
        },
        "configuration_file": "workflow/config.yaml",
        "cluster_configuration_file": "workflow/cluster.yaml",
        "snakefile": "workflow/Snakefile_SLmapped",
        "scripts_directory": "workflow/scripts"
    }
}