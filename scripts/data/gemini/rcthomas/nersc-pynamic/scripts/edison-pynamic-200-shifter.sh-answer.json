{
    "application": "Slurm",
    "details": {
        "resource_manager": "Slurm",
        "image": "docker:registry.services.nersc.gov/pynamic:2.6a1",
        "job_name": "edison-pynamic-200-shifter",
        "nodes": 200,
        "tasks_per_node": 24,
        "partition": "regular",
        "qos": "normal",
        "walltime": "25",
        "modules": [
            "PrgEnv-gnu",
            "python_base",
            "mysql",
            "mysqlpython",
            "shifter"
        ],
        "environment_variables": [
            "PMI_MMAP_SYNC_WAIT_TIME"
        ],
        "benchmark_scripts": [
            "report-benchmark.py"
        ],
        "executable": "/bench/pynamic-pyMPI /bench/pynamic_driver.py"
    }
}