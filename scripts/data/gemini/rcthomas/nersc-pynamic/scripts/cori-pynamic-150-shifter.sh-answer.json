{
    "application": "slurm",
    "details": {
        "scheduler": "slurm",
        "docker_image": "docker:registry.services.nersc.gov/pynamic:2.6a1",
        "nodes": 150,
        "tasks_per_node": 32,
        "partition": "regular",
        "qos": "normal",
        "time": "25",
        "modules": [
            "shifter",
            "python_base",
            "mysql",
            "mysqlpython"
        ],
        "benchmark_script": "/bench/pynamic_driver.py",
        "output_file": "latest-$SLURM_JOB_NAME.txt",
        "environment_variables": {
            "PMI_MMAP_SYNC_WAIT_TIME": "300"
        }
    }
}