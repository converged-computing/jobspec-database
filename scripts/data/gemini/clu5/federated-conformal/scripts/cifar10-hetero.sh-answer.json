{
    "application": "Slurm",
    "details": {
        "scheduler": "Slurm",
        "nodes": 1,
        "cpus_per_task": 16,
        "gpu": "volta:1",
        "modules": [
            "anaconda",
            "cuda"
        ],
        "python_script": "src/run_TCT.py",
        "data_dir": "../data",
        "save_dir": "new_experiments",
        "dataset": [
            "cifar10-2",
            "cifar10-3",
            "cifar10"
        ],
        "architecture": "small_resnet14",
        "hyperparameters": {
            "rounds_stage1": [
                200,
                100,
                100,
                100
            ],
            "rounds_stage2": [
                0,
                100,
                100,
                100
            ],
            "local_epochs_stage1": 5,
            "local_lr_stage1": 0.01,
            "samples_per_client": 10000,
            "local_steps_stage2": [
                null,
                500,
                500,
                500
            ],
            "local_lr_stage2": [
                null,
                0.0001,
                0.0001,
                0.0001
            ],
            "momentum": 0.9,
            "use_data_augmentation": true,
            "use_iid_partition": [
                false,
                false,
                false,
                true
            ]
        }
    }
}