{
    "application": "Magpie",
    "details": {
        "software": [
            "SLURM",
            "Ray",
            "MPI",
            "Python"
        ],
        "resources": {
            "nodes": "<my_node_count>",
            "time": "<my_time_in_minutes>",
            "partition": "<my_partition>",
            "job_name": "<my_job_name>",
            "scripts_home": "${HOME}/magpie",
            "local_dir": "/tmp/${USER}/magpie",
            "ray_path": "${HOME}/bin/ray",
            "ray_local_dir": "/tmp/${USER}/ray",
            "python": "/usr/bin/python"
        },
        "other_requirements": [
            "Passwordless SSH access between nodes",
            "NFS mount for shared files"
        ]
    }
}