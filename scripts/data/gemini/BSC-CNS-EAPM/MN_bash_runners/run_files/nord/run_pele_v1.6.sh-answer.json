{
    "application": "Pele_mpi",
    "software_requirements": [
        "intel",
        "gcc",
        "openmpi/1.8.1",
        "boost/1.63.0",
        "python/2.7.3",
        "MKL/11.3",
        "GTK+3/3.2.4"
    ],
    "resource_requirements": {
        "queue": "bsc_ls",
        "walltime": "48:00",
        "nodes": 1,
        "processors_per_node": 64
    },
    "environment_variables": {
        "MPLBACKEND": "Agg",
        "OMPI_MCA_coll_hcoll_enable": "0",
        "OMPI_MCA_mtl": "^mxm"
    },
    "configuration_file": "pele.conf"
}