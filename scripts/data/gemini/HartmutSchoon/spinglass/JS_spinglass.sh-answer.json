{
    "application": "Slurm",
    "details": {
        "job_name": "SG_L4",
        "output_file": "./logs/slurm-%j.out",
        "error_file": "./logs/slurm_error-%j.out",
        "mail_type": "ALL",
        "mail_user": "hartmut.schoon@uni-oldenburg.de",
        "partition": "carl.p",
        "time": "3-00:00",
        "nodes": 2,
        "cpus_per_task": 1,
        "mem": "1G",
        "mem_per_cpu": "1G",
        "ntasks": 10,
        "array": "1-10:1%4",
