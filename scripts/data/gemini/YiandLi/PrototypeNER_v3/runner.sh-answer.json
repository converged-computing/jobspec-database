{
    "application": "pytorch",
    "details": {
        "libraries": [
            "nvidia/cuda/10.0",
            "pytorch/1.0_python3.7_gpu"
        ],
        "resources": {
            "nodes": 1,
            "tasks_per_node": 1,
            "partition": "gpu",
            "gpus": 1,
            "requeue": false,
            "output_file": "print.log",
            "error_file": "log.log"
        },
        "script": "src/train.py",
        "arguments": [
            "--update_per_tasks 4",
            "--eval_per_update 4",
            "--do_source_train",
            "--support_test_epoch 5",
            "--inner_identify_loops 5",
            "--inner_classify_loops 10",
            "--encoder_type ../../pre_ckpts/bert-base-uncased"
        ]
    }
}