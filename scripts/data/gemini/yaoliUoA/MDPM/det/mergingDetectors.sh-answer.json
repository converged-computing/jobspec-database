{
    "application": "matlab",
    "software": [
        "matlab"
    ],
    "resources": {
        "nodes": 1,
        "processors_per_node": 1,
        "virtual_memory": "10G",
        "wall_time": "30:00:00"
    },
    "arguments": "-singleCompThread -nojvm -nosplash -nodesktop -r \"mergingDetectors(${PBS_ARRAYID}); quit;\"",
    "script_details": {
        "job_name": "mergingDetectors",
        "array_id": "${PBS_ARRAYID}",
        "output_file": "mergingDetectors_${PBS_ARRAYID}.log",
        "error_file": "mergingDetectors_${PBS_ARRAYID}.err"
    }
}