{
    "application": "Fluxion",
    "details": {
        "language": {
            "default": "en",
            "file_location": "$FLUXIONPath/language/$FluxionLanguage.sh"
        },
        "dependencies": [
            "aircrack-ng",
            "awk",
            "curl",
            "7zr",
            "iwconfig",
            "macchanger",
            "openssl",
            "xterm",
            "rfkill",
            "unzip",
            "route",
            "fuser",
            "killall"
        ],
        "optional_dependencies": [
            "pyrit"
        ],
        "resources": [
            "iptables",
            "network-manager",
            "systemctl",
            "service",
            "systemd-resolved",
            "tput"
        ],
        "subroutines": [
            "fluxion_startup",
            "fluxion_shutdown",
            "fluxion_kill_lineage",
            "fluxion_conditional_clear",
            "fluxion_conditional_bail",
            "fluxion_error_report",
            "fluxion_handle_abort_attack",
            "fluxion_handle_exit",
            "fluxion_handle_target_change",
            "fluxion_set_resolution",
            "fluxion_do",
            "fluxion_undo",
            "fluxion_done",
            "fluxion_done_reset",
            "fluxion_do_sequence",
            "fluxion_header",
            "fluxion_unset_language",
            "fluxion_set_language",
            "fluxion_deallocate_interface",
            "fluxion_allocate_interface",
            "fluxion_next_assignable_interface",
            "fluxion_get_interface",
            "fluxion_target_get_candidates",
            "fluxion_get_target",
            "fluxion_target_normalize_SSID",
            "fluxion_target_show",
            "fluxion_target_tracker_daemon",
            "fluxion_target_tracker_stop",
            "fluxion_target_tracker_start",
            "fluxion_target_unset_tracker",
            "fluxion_target_set_tracker",
            "fluxion_target_unset",
            "fluxion_target_set",
            "fluxion_hash_verify",
            "fluxion_hash_unset_path",
            "fluxion_hash_set_path",
            "fluxion_hash_get_path",
            "fluxion_unset_attack",
            "fluxion_set_attack",
            "fluxion_unprep_attack",
            "fluxion_prep_attack",
            "fluxion_run_attack",
            "fluxion_main"
        ],
        "configuration_files": [
            "$FLUXIONPath/preferences/preferences.conf",
            "$FLUXIONPath/attacks/$FluxionAttack/attack.conf"
        ],
        "workspace_directory": "$FLUXIONWorkspacePath",
        "log_file": "/tmp/fluxion.debug.log"
    }
}