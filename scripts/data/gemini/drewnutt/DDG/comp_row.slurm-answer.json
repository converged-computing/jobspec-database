{
    "application": "Slurm",
    "details": {
        "software": [
            "Python",
            "conda",
            "rsync"
        ],
        "resources": [
            "compute nodes",
            "scratch space",
            "gpu (optional, depending on compute_row.py)"
        ],
        "environment_variables": [
            "PATH",
            "LD_LIBRARY_PATH",
            "PYTHONPATH",
            "SCRATCH_DIR"
        ],
        "script_arguments": [
            "compute_row.py",
            "--pdbseqs",
            "cri_bindb_old.txt",
            "-r",
            "${SLURM_ARRAY_TASK_ID}",
            "--out",
            "row_${SLURM_ARRAY_TASK_ID}.out"
        ]
    }
}