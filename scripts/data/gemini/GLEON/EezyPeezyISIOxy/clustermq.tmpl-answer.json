{
    "application": "SLURM",
    "details": {
        "software": [
            "R (version 4.1.0)",
            "clustermq"
        ],
        "resources": {
            "memory": "4096 MB per CPU",
            "cores": "1 per task",
            "time": "60 minutes",
            "log_file": "specified in the script"
        },
        "dependencies": {
            "modules": "R/4.1.0 (optional)",
            "environment variables": "CMQ_AUTH"
        },
        "configuration": {
            "job name": "floating_hamster_{{ job_name }}",
            "array size": "1-{{ n_jobs }}",
            "master node": "{{ master }}"
        }
    }
}