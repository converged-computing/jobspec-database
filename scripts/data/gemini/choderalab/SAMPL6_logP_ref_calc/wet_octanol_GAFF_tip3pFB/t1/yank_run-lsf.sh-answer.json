{
    "application": "Yank",
    "details": {
        "software": [
            "conda",
            "python",
            "cuda 9.2"
        ],
        "resources": {
            "compute": {
                "nodes": 1,
                "cores": 1,
                "gpu": "1 (exclusive, shared mode)"
            },
            "memory": "8 GB",
            "time": {
                "walltime": "10:00",
                "exclusive_time": "8:30"
            },
            "queue": "gpuqueue",
            "environment": "SAMPL6_logP",
            "dependency": "module load cuda/9.2"
        },
        "output": {
            "stdout": "%J.stdout",
            "stderr": "%J.stderr",
            "requirements": "requirements.txt"
        },
        "array_jobs": {
            "number": 27,
            "indices_file": "job_indices.txt"
        },
        "working_directory": "$LS_SUBCWD"
    }
}