{
    "application": "OpenMM",
    "software": [
        "OpenMM",
        "Conda",
        "Python",
        "CUDA",
        "Nvidia-smi"
    ],
    "resources": {
        "compute": "GPU",
        "gpu_type": "A100 or A40",
        "gpu_count": 1,
        "memory": "8 GB",
        "cpu_cores": 1,
        "queue": "gpuqueue",
        "priority": "low",
        "walltime": "3 hours"
    },
    "dependencies": [
        "OpenEye Toolkit"
    ],
    "environment": {
        "OPENMM_CPU_THREADS": 1,
        "OE_LICENSE": "~/.openeye/oe_license.txt"
    }
}