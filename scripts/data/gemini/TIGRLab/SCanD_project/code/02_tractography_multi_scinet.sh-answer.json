{
    "application": "qsiprep",
    "software": [
        "qsiprep",
        "singularity",
        "mrtrix",
        "freesurfer"
    ],
    "resources": {
        "nodes": 1,
        "cpus": 40,
        "memory": "Not specified",
        "time": "12:00:00"
    },
    "environment_variables": [
        "SUB_SIZE",
        "CORES",
        "THREADS_PER_COMMAND",
        "BASEDIR",
        "BIDS_DIR",
        "QSIPREP_HOME",
        "SING_CONTAINER",
        "OUTPUT_DIR",
        "QSIPREP_DIR",
        "FREESURFER_DIR",
        "WORK_DIR",
        "LOGS_DIR",
        "project_id",
        "ORIG_FS_LICENSE"
    ],
    "input_data": [
        "participants.tsv",
        "bids data (in BIDS_DIR)"
    ],
    "output_data": [
        "qsiprep output (in OUTPUT_DIR)",
        "logs (in LOGS_DIR)"
    ],
    "dependencies": [
        "Singularity container with qsiprep 0.16.0RC3"
    ]
}