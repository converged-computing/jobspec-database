{
    "application": "Julia",
    "software": [
        "Julia",
        "SLURM"
    ],
    "resources": [
        "64 GB memory",
        "16 tasks",
        "1 CPU per task",
        "1 hour runtime",
        "serial partition",
        "serial QoS",
        "ACCOUNT account"
    ],
    "dependencies": [
        "julia.env"
    ],
    "commands": [
        "source julia.env",
        "export SRUN_CPUS_PER_TASK=$SLURM_CPUS_PER_TASK",
        "bin/julia --project -O3 --check-bounds=no precompile-no-run.jl"
    ]
}