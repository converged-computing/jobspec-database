{
    "application": "SLURM",
    "details": {
        "software": [
            "GCC",
            "OpenMPI",
            "CUDA",
            "MPI-settings",
            "Python",
            "HDF5",
            "PnetCDF",
            "libaio",
            "mpi4py",
            "torchrun",
            "deepspeed",
            "horovod"
        ],
        "resources": {
            "partition": "batch",
            "nodes": 2,
            "gpus_per_node": 4,
            "cpus_per_gpu": 4,
            "gres": "gpu:4",
            "time": "00:30:00",
            "environment": {
                "CUDA_VISIBLE_DEVICES": "0,1,2,3",
                "OMP_NUM_THREADS": 1
            }
        },
        "environment_variables": [
            "DIST_MODE",
            "RUN_NAME",
            "TRAINING_CMD",
            "PYTHON_VENV",
            "DEBUG",
            "NCCL_DEBUG",
            "MASTER_ADDR",
            "MASTER_PORT",
            "NCCL_IB_DISABLE",
            "NCCL_SOCKET_IFNAME"
        ]
    }
}