{
    "application": "SLURM",
    "details": {
        "resources": {
            "nodes": 2,
            "tasks": 2,
            "tasks_per_node": 1,
            "gpus_per_node": 8,
            "gres": "gpu:8",
            "gpus_per_task": 8,
            "exclusive": true,
            "wait_all_nodes": true
        },
        "environment": {
            "APPS_PATH": "/apps",
            "DATA_PATH": "/fsx",
            "IMAGE": "/apps/mosaicml-stable-diffusion.sqsh",
            "FSX_MOUNT": "/fsx:/fsx",
            "FI_EFA_USE_DEVICE_RDMA": 1,
            "FI_EFA_FORK_SAFE": 1,
            "FI_LOG_LEVEL": 1,
            "FI_PROVIDER": "efa",
            "FI_EFA_ENABLE_SHM_TRANSFER": 1,
            "NCCL_DEBUG": "INFO",
            "WANDB_MODE": "offline"
        },
        "software": {
            "composer": "MosaicML Composer",
            "streaming": "MosaicML Streaming",
            "python": "Python"
        },
        "output": {
            "stdout": "/apps/accelerate-pcluster-distributed-training/stable-diffusion/sd_out_%j.out",
            "stderr": "/apps/accelerate-pcluster-distributed-training/stable-diffusion/sd_err_%j.err"
        }
    }
}