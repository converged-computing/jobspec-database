{
    "application": "MPI",
    "details": {
        "resource_manager": "SLURM",
        "nodes": 2,
        "cores_per_task": 1,
        "tasks": 2,
        "partition": "compute",
        "memory_per_core": "1G",
        "time_limit": "0:01:00",
        "mpi_implementation": "OpenMPI",
        "mpi_configuration": {
            "btl_self_rndv_eager_limit": 256,
            "btl_sm_eager_limit": 256,
            "btl_vader_eager_limit": 256,
            "btl_sm_max_send_size": 256,
            "osc_rdma_buffer_size": 256,
            "cpu_binding": "verbose"
        },
        "executable": "deadlock"
    }
}