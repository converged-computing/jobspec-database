{
    "application": "Optuna",
    "details": {
        "framework": "Docker",
        "image": "terrain-gpu",
        "dockerfile": "DockerfileGPU",
        "python_script": "optuna_tuning.py",
        "resource_requirements": {
            "cpus": 4,
            "time": "4 hours",
            "gpu": "all",
            "dataset": "husky",
            "model": "SVM"
        },
        "environment_variables": {
            "CUDA_VISIBLE_DEVICES": "dependent on system configuration",
            "DATASET": "husky",
            "MODEL": "SVM"
        },
        "mounts": {
            "source": "/dev/shm",
            "target": "/dev/shm",
            "type": "bind"
        }
    }
}