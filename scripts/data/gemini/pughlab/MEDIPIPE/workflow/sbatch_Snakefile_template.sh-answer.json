{
    "application": "Snakemake",
    "details": {
        "environment": "MEDIPIPE conda environment",
        "resource_requirements": {
            "partition": "all",
            "time": "5-00:00:00",
            "memory": "16G",
            "job_name": "submit_snakemake_%j",
            "output_file": "submit_snakemake_%j.out",
            "error_file": "submit_snakemake_%j.err",
            "cluster_config": "/path/to/workflow/config/cluster_std_err.json",
            "cluster_partition": "himem",
            "cluster_cores": 12,
            "cluster_memory": "60G",
            "cluster_output": "{cluster.std}",
            "cluster_error": "{cluster.err}",
            "latency_wait": 60,
            "jobs": 4
        },
        "config_file": "/path/to/test/config_template.yaml",
        "workflow_file": "/path/to/workflow/Snakefile"
    }
}