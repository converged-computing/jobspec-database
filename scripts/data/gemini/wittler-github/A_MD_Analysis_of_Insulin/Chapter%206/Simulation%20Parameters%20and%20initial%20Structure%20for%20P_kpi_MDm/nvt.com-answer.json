{
    "application": "GROMACS",
    "details": {
        "version": "5.0.4",
        "compiler": "gcc",
        "MPI implementation": "openmpi",
        "resource requirements": {
            "walltime": "15 hours",
            "memory": "2GB",
            "nodes": "1",
            "processors per node": "16"
        },
        "input files": [
            "nvt.mdp",
            "em.gro",
            "topol.top"
        ],
        "output files": [
            "nvtout.mdp",
            "nvt.tpr",
            "nvt.trr",
            "nvt.edr",
            "nvt.log",
            "nvt.xtc",
            "nvt.cpt"
        ],
        "commands": [
            "gmx grompp",
            "mdrun"
        ],
        "environment variables": [
            "PBS_N",
            "PBS_R",
            "PBS_J",
            "PBS_O",
            "PBS_L",
            "PBS_JOBNAME",
            "PBS_QUEUE"
        ]
    }
}