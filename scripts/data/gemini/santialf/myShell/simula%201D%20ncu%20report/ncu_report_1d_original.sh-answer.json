        "time_limit": "0-15:00",
        "output_file": "slurm.%N.%j.out",
        "error_file": "slurm.%N.%j.err",
        "gpus_per_node": 1,
        "software": [
            "nvcc",
            "ncu",
            "gunzip",
            "make"
