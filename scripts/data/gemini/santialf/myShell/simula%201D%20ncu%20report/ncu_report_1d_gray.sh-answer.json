{
    "application": "SLURM",
    "details": {
        "partition": "a100q",
        "time_limit": "0-15:00",
        "stdout": "slurm.%N.%j.out",
        "stderr": "slurm.%N.%j.err",
        "gpus_per_node": 1,
        "environment_variables": [
            "PATH=$PATH:/usr/local/cuda/bin"
        ],
        "tools": [
            "nvidia-smi",
            "nvcc",
            "make",
            "gunzip",
            "mkdir",
            "srun",
            "ncu",
            "rm"
        ],
        "libraries": [
            "CUDA"
        ],
        "input_data": [
            "matrices from /global/D1/homes/james/sparcity/suitesparse/mtx/gray-v2/"
        ],
        "output_data": [
            "reports/report_1D_gray_$base_filename"
        ],
        "temporary_files": [
            "/work/santiago/tmp2/$base_filename"
        ],
        "execution_environment": {
            "operating_system": "Linux",
            "resources": {
                "GPU": "NVIDIA A100"
            }
        }
    }
}