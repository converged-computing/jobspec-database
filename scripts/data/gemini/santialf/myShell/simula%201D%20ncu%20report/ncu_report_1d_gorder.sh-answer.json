{
    "application": "SLURM",
    "details": {
        "scheduler": "SLURM",
        "partition": "a100q",
        "time_limit": "0-15:00",
        "output_file": "slurm.%N.%j.out",
        "error_file": "slurm.%N.%j.err",
        "gpus_per_node": 1,
        "environment_variables": {
            "PATH": "/usr/local/cuda/bin"
        },
        "tools": [
            "nvidia-smi",
            "nvcc",
            "make",
            "gunzip",
            "srun",
            "ncu"
        ],
        "libraries": [
            "cuBLAS"
        ],
        "input_data": "Matrix files in .mtx.gz format",
        "output_data": "Performance reports in text files"
    }
}