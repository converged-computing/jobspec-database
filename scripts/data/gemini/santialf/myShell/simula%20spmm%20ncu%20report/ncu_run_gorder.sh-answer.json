{
    "application": "SLURM",
    "details": {
        "partition": "a100q",
        "time_limit": "0-15:00",
        "stdout_file": "slurm.%N.%j.out",
        "stderr_file": "slurm.%N.%j.err",
        "gpus_per_node": 1,
        "cuda_path": "/usr/local/cuda/bin",
        "matrix_directory": "/global/D1/homes/santiago/reorderings/gorder2/",
        "temporary_directory": "/work/$USER/tmp3",
        "program": "./spmm_csr_example",
        "ncu_options": {
            "output_file": "reports/report_spmm_32_gorder_$base_filename",
            "kernel": "load_balancing_kernel",
            "cores": 1,
            "full_trace": true
        },
        "software_dependencies": [
            "nvcc",
            "make",
            "gunzip",
            "ncu"
        ]
    }
}