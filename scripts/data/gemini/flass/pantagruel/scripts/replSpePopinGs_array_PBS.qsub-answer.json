{
    "application": "PBS",
    "details": {
        "language": "bash",
        "modules": "modulefile",
        "environment_variables": [
            "tasklist",
            "dtag",
            "colalinexuscodedir",
            "collapsecond",
            "coltreechains",
            "speciestree",
            "ncpus",
            "verbosereplSpePopinGs",
            "chaintype",
            "mlgenetrees",
            "replmethod",
            "repllogs",
            "ptgscripts"
        ],
        "input_files": [
            "${tasklist}_${dtag}_taskchunks",
            "${colalinexuscodedir}/${collapsecond}",
            "${coltreechains}/${collapsecond}",
            "${speciestree}.lsd.nwk",
            "${speciestree%.*}_populations",
            "${speciestree%.*}_collapsedPopulations.nwk",
            "${speciestree%.*}_interNodeDistPopulations"
        ],
        "output_files": [
            "${coltreechains}/${collapsecond}",
            "${repllogs}_${replrun}.log"
        ],
        "software": [
            "python2.7",
            "awk"
        ],
        "script_name": "replSpePopinGs",
        "job_name": "$PBS_JOBNAME",
        "job_id": "$PBS_JOBID",
        "array_task_id": "$PBS_ARRAY_INDEX",
        "host": "$HOSTNAME",
        "processes": "$PBS_NUM_PPN",
        "workers": "$PBS_NODEFILE"
    }
}