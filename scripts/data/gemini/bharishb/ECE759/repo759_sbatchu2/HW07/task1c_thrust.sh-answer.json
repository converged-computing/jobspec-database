{
    "application": "CUDA",
    "details": {
        "runtime": "zsh",
        "scheduler": "SLURM",
        "partition": "instruction",
        "job_name": "task1_thrust",
        "output_file": "task1_thrust.out",
        "error_file": "%x.err",
        "time_limit": "0-00:02:00",
        "gpu_resources": "1",
        "cuda_version": "11.8.0",
        "compiler": "nvcc",
        "optimization_flags": "-O3",
        "warning_flags": "-Wall",
        "ptxas_optimization_flags": "-O3",
        "c++_standard": "c++17",
        "executable": "task1_thrust",
        "input_data_size": "2**i (where i ranges from 10 to 20)",
        "log_file": "task1c_thrust.log"
    }
}