{
    "application": "CLOVER",
    "details": {
        "environment": "anaconda3/personal",
        "package": "clover",
        "resources": {
            "walltime": "{WALLTIME}:00:00",
            "nodes": "1",
            "cpus": "8",
            "memory": "11800Mb"
        },
        "dependencies": [
            "python",
            "src.clover.scripts.hpc"
        ],
        "arguments": [
            "--runs",
            "{RUNS_FILE}",
            "--walltime",
            "{WALLTIME}",
            "{VERBOSE}"
        ]
    }
}