{
    "application": "SLURM",
    "details": {
        "software": [
            "Python",
            "PyTorch",
            "SLURM",
            "srun",
            "module load",
            "bash"
        ],
        "resources": {
            "CPU": "Haswell",
            "nodes": 1,
            "queue": "regular",
            "time": "3:00:00",
            "OMP_NUM_THREADS": "32",
            "KMP_AFFINITY": "granularity=fine,compact,1,0",
            "KMP_BLOCKTIME": "1",
            "BENCHMARK_RESULTS_PATH": "$SCRATCH/pytorch-benchmarks/results/hsw-$version-$backend-n${SLURM_JOB_NUM_NODES}",
            "models": "alexnet resnet50 lstm cnn3d transformer",
            "version": "1.7.1",
            "backend": "mpi",
            "clean": "false"
        },
        "files": [
            "train.py",
            "configs/${model}.yaml",
            "parse.py"
        ]
    }
}