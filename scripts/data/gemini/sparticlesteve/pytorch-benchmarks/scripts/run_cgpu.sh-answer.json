{
    "application": "slurm",
    "details": {
        "software": [
            "pytorch/$version-gpu"
        ],
        "resources": [
            "gpu",
            "ntasks-per-node=8",
            "gpus-per-task=1",
            "exclusive",
            "singleton",
            "c=10",
            "t=30",
            "SCRATCH",
            "python",
            "train.py",
            "configs/${model}.yaml",
            "parse.py"
        ]
    }
}