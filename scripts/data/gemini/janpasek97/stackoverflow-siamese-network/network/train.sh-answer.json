        "environment_variables": {
            "PYTHONUSERBASE": "/storage/brno3-cerit/home/pasekj/.local",
            "PATH": "$PYTHONUSERBASE/bin:$PATH",
            "PYTHONPATH": "$SCRATCHDIR/SiameseSearch_BP:$PYTHONPATH"
