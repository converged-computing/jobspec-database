{
    "application": "valgrind",
    "details": {
        "compiler": "gcc/11.4.0",
        "performance_monitoring_tools": "papi/7.0.1",
        "benchmark": "benchmark_mp",
        "threads": 32,
        "valgrind_options": [
            "--leak-check=full",
            "--show-leak-kinds=all",
            "--track-origins=yes",
            "--verbose",
            "--log-file=valgrind-out.txt"
        ],
        "output_files": [
            "valgrind.out",
            "valgrind.err",
            "valgrind-out.txt"
        ]
    }
}