{
    "application": "snakemake",
    "details": {
        "environment": "python 3.9.6",
        "requirements": "config/requirements.txt",
        "workflow": "snakefiles/sqtl.snakefile",
        "configuration": "config/rna_prcoess.yaml",
        "cluster_configuration": "config/cluster.yaml",
        "slurm_parameters": {
            "job_name": "SQTL",
            "time": "3-00:00:00",
            "nodes": 1,
            "tasks_per_node": 1,
            "cores_per_task": 1,
            "partition": "general",
            "memory": "2gb",
            "output": "%x-%j.out"
        },
        "snakemake_parameters": {
            "jobs": 460,
            "max_jobs_per_second": 5,
            "max_status_checks_per_second": 0.2,
            "rerun_incomplete": true,
            "latency_wait": 500,
            "dry_run": true
        }
    }
}