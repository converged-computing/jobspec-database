{
    "application": "PBS",
    "details": {
        "software": [
            "nosofs.v3.6.0",
            "envvar",
            "PrgEnv-intel",
            "craype",
            "intel",
            "cray-pals",
            "libjpeg",
            "python",
            "prod_envir",
            "prod_util",
            "grib_util",
            "awscli"
        ],
        "resources": {
            "queue": "dev_transfer",
            "nodes": 1,
            "cpus": 8,
            "walltime": "1:30:00",
            "place": "vscatter"
        },
        "environment": [
            "KEEPDATA=YES",
            "SENDCOM=NO",
            "SENDDBN=NO",
            "SENDSMS=NO",
            "PACKAGEROOT=/lfs/h1/nos/nosofs/noscrub/$LOGNAME/packages",
            "COMPATH=/lfs/h1/ops/prod/com",
            "COMROOT=/lfs/h1/nos/ptmp/$LOGNAME/com",
            "DCOMROOT=/lfs/h1/ops/prod/dcom",
            "DATAROOT=/lfs/h1/nos/ptmp/$LOGNAME/work/${nosofs_ver}/${OFS}",
            "envir=dev",
            "OFS=lsofs",
            "cyc=18",
            "job=lsofs_aws_18",
            "platform=ptmp"
        ],
        "job_script": "/lfs/h1/nos/nosofs/noscrub/$LOGNAME/packages/nosofs.${nosofs_ver}/jobs/JNOS_OFS_AWS"
    }
}