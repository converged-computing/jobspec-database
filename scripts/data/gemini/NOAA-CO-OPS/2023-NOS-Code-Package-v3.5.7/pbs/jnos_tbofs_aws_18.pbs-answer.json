{
    "application": "PBS",
    "details": {
        "software": [
            "nosofs",
            "PrgEnv-intel",
            "craype",
            "intel",
            "cray-pals",
            "libjpeg",
            "python",
            "prod_envir",
            "prod_util",
            "grib_util",
            "awscli"
        ],
        "resource_requirements": {
            "nodes": 1,
            "cpus": 8,
            "walltime": "1:30:00",
            "place": "vscatter",
            "queue": "dev_transfer"
