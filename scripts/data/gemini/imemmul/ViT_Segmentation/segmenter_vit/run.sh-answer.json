{
    "application": "Slurm",
    "details": {
        "job_name": "eddy_segmenter",
        "account": "users",
        "nodes": 1,
        "tasks": 4,
        "partition": "main",
        "nodelist": "nova[32]",
        "gres": "gpu:1",
        "time": "7-0",
        "output": "./output/%j-slurm.out",
        "modules": [
            "lmod",
            "conda",
            "emirdlp"
        ],
        "commands": [
            "nvidia-smi",
            "pwd",
            "ulimit -s unlimited",
            "ulimit -l unlimited",
            "ulimit -a",
            "python main.py"
        ]
    }
}