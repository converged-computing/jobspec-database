{
    "application": "proteus",
    "details": {
        "queue": "standard",
        "walltime": "12 hours",
        "cpus": 128,
        "job_name": "wigley",
        "output_type": "both",
        "email_options": "error, begin, abort",
        "email_address": "cekees@gmail.com",
        "modules": [
            "proteus"
        ],
        "environment": "garnet",
        "memory_per_node": "62 GB",
        "mpich_reordering": "enabled",
        "work_directory": "$PBS_O_WORKDIR",
        "output_directory": "$WORKDIR/wigley.$PBS_JOBID",
        "command": "aprun -n 64 -N 16 parun wigley_so.py -l 7 -v -M 3.8 -O ../inputTemplates/petsc.options.schur_upper_a11_asm_boomeramg -D $WORKDIR/wigley.$PBS_JOBID"
    }
}