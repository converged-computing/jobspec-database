{
    "application": "proteus",
    "details": {
        "resource_requirements": {
            "walltime": "72 hours",
            "ncpus": 4096,
            "application": "proteus"
        },
        "environment_setup": {
            "modules": "/opt/modules/default/etc/modules.sh",
            "proteus_config": "${PROTEUS}/envConfig/garnet.gnu.bash"
        },
        "execution": {
            "workdir": "$PBS_O_WORKDIR",
            "mpich_rank_reorder_method": 2,
            "output_directory": "$WORKDIR/dtmb.$PBS_JOBID",
            "command": "aprun -n 1024 -N 8 -S 2  parun dtmb_so.py -l 7 -v -m -O ../inputTemplates/petsc.options.schur_upper_a11_asm_boomeramg -D $WORKDIR/dtmb.$PBS_JOBID"
        },
        "other_software": {
            "aprun": "A tool for launching parallel applications on a cluster",
            "parun": "A tool for running MPI programs on a cluster",
            "dtmb_so.py": "A Python script that likely runs a simulation using the Proteus framework"
        }
    }
}