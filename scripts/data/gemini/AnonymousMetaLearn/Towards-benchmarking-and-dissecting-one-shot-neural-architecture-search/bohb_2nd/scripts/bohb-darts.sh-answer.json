{
    "application": "SLURM",
    "details": {
        "queue": "bosch_gpu-rtx2080",
        "gpu_count": 1,
        "array_size": 16,
        "job_name": "bohb-darts-2nd",
        "working_directory": ".",
        "output_file": "./logs/%A-%a.o",
        "error_file": "./logs/%A-%a.e",
        "environment": "tensorflow-stable",
        "python_script": "src/darts_master.py",
        "arguments": {
            "array_id": "$SLURM_ARRAY_TASK_ID",
            "total_num_workers": 16,
            "num_iterations": 64,
            "run_id": "$SLURM_ARRAY_JOB_ID",
            "working_directory": "./bohb_output/",
            "min_budget": 25,
            "max_budget": 100,
            "seed": 1,
            "space": "$1"
        }
    }
}