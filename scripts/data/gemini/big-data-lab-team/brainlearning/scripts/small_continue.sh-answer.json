{
    "application": "TensorFlow",
    "details": {
        "environment": {
            "modules": [
                "cuda",
                "cudnn",
                "python/3.6.3"
            ],
            "virtualenv": "$HOME/tensorflow/bin/activate"
        },
        "script": "$HOME/brainlearning/brainlearning/operations.py",
        "arguments": {
            "mode": "continue",
            "model": "small",
            "model_dir": "small/",
            "batch_size": 10,
            "steps_per_epoch": 1,
            "epochs": 250,
            "save_each_epochs": 50,
            "images_dir_path": "../project/ml-bet/",
            "verbose": 2
        },
        "resources": {
            "gpu": 2,
            "cpu": 6,
            "memory": "64000M",
            "time": "0-03:00"
        }
    }
}