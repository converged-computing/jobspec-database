{
    "application": "Slurm",
    "details": {
        "software": [
            "Intel oneAPI MPI",
            "WRF",
            "mpiexec.hydra"
        ],
        "resources": {
            "compute": {
                "nodes": 2,
                "tasks_per_node": 16
            },
            "environment": {
                "OMP_NUM_THREADS": 6,
                "FI_PROVIDER": "efa",
                "I_MPI_FABRICS": "ofi",
                "I_MPI_OFI_PROVIDER": "efa",
                "I_MPI_PIN_DOMAIN": "omp",
                "KMP_AFFINITY": "compact",
                "I_MPI_DEBUG": 4
            }
        },
        "data": {
            "input": "conus_12km directory",
            "output": [
                "conus-%j.out",
                "wrf.exit.code"
            ]
        }
    }
}