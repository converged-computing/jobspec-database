{
    "application": "DeepSpeed",
    "details": {
        "framework": "PyTorch",
        "model": "Megatron-LM",
        "library": "Megatron-DeepSpeed",
        "resources": {
            "memory": "30GB",
            "gpus": "4 A6000",
            "partition": "babel-shared-long",
            "time": "1-00:00:00",
            "environment": {
                "conda": "Miniconda",
                "environment_name": "${TOWERLLM_ENV_NAME}"
            }
        },
        "logging": "WandB",
        "distributed_training": "DeepSpeed",
        "data_format": "mmap",
        "tokenizer": "PretrainedFromHF",
        "optimizer": "Adam",
        "learning_rate_scheduler": "cosine",
        "mixed_precision": "FP16",
        "attention": "Flash Attention v2",
        "activation": "SwiGLU",
        "normalization": "RMSNorm",
        "position_embeddings": "Rotary Position Embeddings",
        "seed": "${seed}"
    }
}