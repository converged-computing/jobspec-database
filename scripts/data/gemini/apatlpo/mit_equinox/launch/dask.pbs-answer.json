{
    "application": "Dask",
    "details": {
        "scheduler": "PBS",
        "software": {
            "dask": "dask-scheduler",
            "dask-worker": "dask-worker"
        },
        "environment": "source ~/.bashrc; export PATH=$HOME/.miniconda3/bin:$PATH; source activate equinox",
        "resources": {
            "nodes": "8",
            "cores": "28",
            "memory": "100 GB",
            "walltime": "24:00:00",
            "interface": "ib0"
        },
        "options": {
            "OMP_NUM_THREADS": "1",
            "NUMEXPR_NUM_THREADS": "1",
            "MKL_NUM_THREADS": "1",
            "OPENBLAS_NUM_THREADS": "1",
            "NCPUS": "14",
            "MEMORY_LIMIT": "100e9",
            "INTERFACE": "--interface ib0"
        }
    }
}