{
    "application": "Snakemake",
    "details": {
        "environment": "varroa",
        "cluster_config": "cluster.json",
        "cluster_submission_command": "sbatch -p {cluster.partition} --cpus-per-task {cluster.cpus-per-task} -t {cluster.time} --mem {cluster.mem}",
        "parallelism": "2 processes",
        "resources": {
            "partition": "compute",
            "time": "10 seconds",
            "memory": "2 GB",
            "cpus_per_task": 1,
            "ntasks": 1
        }
    }
}