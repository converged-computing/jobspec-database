{
    "application": "Jupyter",
    "details": {
        "software": [
            "Singularity",
            "Python",
            "virtualenv",
            "pip",
            "ipykernel",
            "ipyparallel",
            "Jupyter Notebook"
        ],
        "resources": [
            "GPU",
            "SLURM"
        ],
        "environment": {
            "VENV": "$HOME/.envs/venv_cuda_${ULHPC_CLUSTER}",
            "JUPYTER_CONFIG_DIR": "$HOME/jupyter_sing/$SLURM_JOBID/",
            "JUPYTER_PATH": "$VENV/share/jupyter\":\"$HOME/jupyter_sing/$SLURM_JOBID/jupyter_path",
            "JUPYTER_DATA_DIR": "$HOME/jupyter_sing/$SLURM_JOBID/jupyter_data",
            "JUPYTER_RUNTIME_DIR": "$HOME/jupyter_sing/$SLURM_JOBID/jupyter_runtime",
            "IPYTHONDIR": "$HOME/ipython_sing/$SLURM_JOBID",
            "IP_ADDRESS": "$(hostname -I | awk '{print $1}')",
            "XDG_RUNTIME_DIR": ""
        }
    }
}