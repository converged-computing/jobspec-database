{
    "application": "Spark",
    "software": [
        "Spark",
        "SLURM"
    ],
    "resource_requirements": {
        "time": "1 hour",
        "partition": "batch",
        "qos": "normal",
        "nodes": 3,
        "tasks_per_node": 1,
        "cpus_per_task": 28,
        "memory": "4096m"
    },
    "dependencies": [
        "devel/Spark",
        "module"
    ],
    "environment_variables": [
        "SPARK_HOME",
        "SPARK_LOG_DIR",
        "SPARK_WORKER_DIR",
        "SPARK_LOCAL_DIRS",
        "SPARK_WORKER_CORES",
        "SPARK_DAEMON_MEMORY",
        "SPARK_WORKER_MEMORY",
        "SPARK_EXECUTOR_MEMORY",
        "SPARK_MASTER_WEBUI_PORT",
        "SPARK_MASTER_HOST",
        "SPARK_MASTER_OPTS",
        "SPARK_WORKER_OPTS",
        "SPARK_IDENT_STRING",
        "SRUN_CPUS_PER_TASK"
    ]
}