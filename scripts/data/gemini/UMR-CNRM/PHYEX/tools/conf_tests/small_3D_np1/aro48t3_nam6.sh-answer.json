{
    "application": "SURFEX",
    "software": [
        "MASTERODB",
        "openmpi",
        "grib_api",
        "eccodes",
        "rttov12",
        "radiation_params.47r1_light.tgz"
    ],
    "resources": [
        "data/aro/$case/ELSCFFCSTALBC000${N}_l15",
        "data/aro/$case/ICMSHFCSTINIT_l15",
        "data/aro/$case/ICMSHFCSTINIT.sfx",
        "data/surfex/v8/ecoclimapI_covers_param.bin",
        "data/surfex/v8/ecoclimapII_eu_covers_param.bin",
        "data/surfex/v8/ecoclimapII_af_covers_param.bin",
        "data/aro/$case/Const.Clim",
        "data/aro/$case/Const.Clim.sfx",
        "data/aro/$case/const.clim.FRANGP0025",
        "data/rttov12/rtcoef_meteosat_9_seviri.dat",
        "data/rttov12/rtcoef_meteosat_10_seviri.dat",
        "data/rttov12/sccldcoef_meteosat_9_seviri.dat",
        "data/rtm/radiation_params.47r1_light.tgz"
    ],
    "environment": {
        "MYLIB": "The gmkpack pack name",
        "TESTDIR": "The test directory",
        "OUTPUTDIR": "Directory for results (default: current)",
        "HOMEPACK": "$HOME/pack",
        "SOURCE": "$HOMEPACK/$MYLIB/src/local",
        "LOADIR": "$HOMEPACK/$MYLIB/bin",
        "TMPDIR": "$HOME/tmp",
        "TMPLOC": "$TMPDIR/rundir.$$",
        "TMPWAIT": "$TMPDIR/wait_queue.$$",
        "RTTOV_COEFDIR": "$PWD",
        "CNMEXP": "FPOS",
        "NPROC": "1",
        "NSTRIN": "1",
        "NSTROUT": "1",
        "OMP_NUM_THREADS": "1",
        "DR_HOOK": "1",
        "DR_HOOK_NOT_MPI": "1",
        "DR_HOOK_SILENT": "1",
        "EC_PROFILE_HEAP": "0",
        "EC_PROFILE_MEM": "0",
        "EC_MPI_ATEXIT": "0",
        "DR_HOOK_SHOW_PROCESS_OPTIONS": "0",
        "EC_MEMINFO": "0",
        "TVSEARCHPATH": "$SOURCE",
        "GRIB_SAMPLES_PATH": "$GRIB_API/share/grib_api/ifs_samples/grib1",
        "GRIB_DEFINITION_PATH": "$GRIB_API/share/grib_api/definitions",
        "ECCODES_SAMPLES_PATH": "$ECCODES/share/eccodes/ifs_samples/grib1",
        "ECCODES_DEFINITION_PATH": "$rekchemin/data/eccodes_extras_definitions:$ECCODES/share/eccodes/definitions",
        "MPILIB": "(determined dynamically)",
        "MPIRUN": "(determined dynamically)",
        "GRIB_API": "(determined dynamically)",
        "ECCODES": "(determined dynamically)"
    },
    "system": {
        "SBATCH": {
            "nodes": "1",
            "tasks": "1",
            "memory": "20000",
            "time": "00:10:00",
            "partition": "normal256"
        }
    }
}