{
    "application": "SURFEX",
    "software": [
        "MASTERODB",
        "openmpi",
        "grib_api",
        "eccodes",
        "rttov12"
    ],
    "resource": [
        "2 cores",
        "20 GB RAM",
        "10 minutes runtime",
        "normal256 partition",
        "export variables: MYLIB, HOME, HOMEPACK, TMPDIR, OUTPUTDIR, TESTDIR",
        "OMP_NUM_THREADS=1",
        "DR_HOOK=1",
        "DR_HOOK_NOT_MPI=1",
        "DR_HOOK_SILENT=1",
        "EC_PROFILE_HEAP=0",
        "EC_PROFILE_MEM=0",
        "EC_MPI_ATEXIT=0",
        "DR_HOOK_SHOW_PROCESS_OPTIONS=0",
        "EC_MEMINFO=0",
        "RTTOV_COEFDIR=$PWD",
        "HOMEPACK=${HOMEPACK:=$HOME/pack}",
        "SOURCE=$HOMEPACK/$MYLIB/src/local",
        "LOADIR=$HOMEPACK/$MYLIB/bin",
        "TMPDIR=${TMPDIR:=$HOME/tmp}",
        "TMPLOC=$TMPDIR/rundir.$$",
        "TMPWAIT=$TMPDIR/wait_queue.$$"
    ]
}