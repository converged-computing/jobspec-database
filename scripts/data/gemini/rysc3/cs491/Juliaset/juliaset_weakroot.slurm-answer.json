{
    "application": "julia",
    "details": {
        "scheduler": "Slurm",
        "job_name": "weakroot_julia",
        "partition": "general",
        "tasks": 1,
        "cpus_per_task": 32,
        "time_limit": "5:00",
        "output_file": "julia_weakroot_%a.out",
        "array_range": "1-7",
        "email": "mfricke@unm.edu",
        "environment_variable": "OMP_NUM_THREADS",
        "executable": "juliaset",
        "input_arguments": [
            "FRACTAL_HEIGHT",
            "FRACTAL_WIDTH",
            "OUTPUT_PATH"
        ],
        "output_format": "tga"
    }
}