{
    "application": "SLURM",
    "details": {
        "scheduler": "SLURM",
        "job_name": "strong_julia",
        "partition": "debug",
        "tasks": 1,
        "cpus_per_task": 1,
        "time": "0:01",
        "output": "julia_strong_%a.out",
        "array_range": "1-7",
        "mail_user": "rscherbarth@unm.edu",
        "mail_type": "all",
        "executable": "./juliaset",
        "environment_variables": {
            "OMP_NUM_THREADS": "SCALE_FACTOR",
            "FRACTAL_HEIGHT": 10000,
            "FRACTAL_WIDTH": 2000
        },
        "output_path": "juliaset_weak_$SLURM_ARRAY_TASK_ID.tga",
        "scale_factor": "2 ** ($SLURM_ARRAY_TASK_ID-1)"
    }
}