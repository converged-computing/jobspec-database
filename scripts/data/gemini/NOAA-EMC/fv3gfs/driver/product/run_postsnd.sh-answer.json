{
    "application": "LSF",
    "software": [
        "craype-hugepages16M",
        "PrgEnv-intel",
        "ESMF-intel-haswell/3_1_0rp5",
        "cfp-intel-sandybridge",
        "iobuf",
        "craype-haswell",
        "prod_envir",
        "prod_util/1.0.4",
        "grib_util/1.0.3",
        "JGFS_POSTSND"
    ],
    "resource_requirements": {
        "queue": "dev",
        "project": "FV3GFS-T2O",
        "walltime": "02:30",
        "memory": "500M",
        "nodes": "1",
        "cores_per_node": "12",
        "processors": "1",
        "environment_variables": {
            "IOBUF_PARAMS": "sigf*:size=128M:count=20:prefetch=0:verbose,gfs_collectiv*:size=128M:count=2:prefetch=0:verbose,*.snd:size=128M:count=3:prefetch=0:verbose,*.sfc:size=32M:count=3:prefetch=0:verbose,bufr.*:size=8M:count=20:prefetch=0:verbose",
            "OMP_NUM_THREADS": "1",
            "KMP_AFFINITY": "disabled",
            "OMP_STACKSIZE": "1024m",
            "MP_LABELIO": "yes",
            "MP_STDOUTMODE": "ordered",
            "envir": "prod",
            "gfs_bufrsnd_ver": "v1.0.2",
            "gsm_ver": "v12.0.0",
            "util_ver": "v1.0.0",
            "pid": "$$",
            "DATA_IN": "/gpfs/hps3/ptmp/$USER",
