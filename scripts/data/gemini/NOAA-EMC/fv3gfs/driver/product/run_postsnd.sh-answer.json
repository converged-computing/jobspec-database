            "pcom": "$DATA_IN/pcom/gfs",
            "HOMEbufrsnd": "/gpfs/hps3/emc/meso/noscrub/Guang.Ping.Lou/fv3gfs",
            "COMIN": "/gpfs/hps3/emc/meso/noscrub/Guang.Ping.Lou/fv3data/${RUN}.${PDY}/$cyc",
            "COMOUT": "$DATA_IN/com2/${NET}/${envir}/${RUN}.${PDY}",
            "COMAWP": "$DATA_IN/com2/nawips/${envir}/${RUN}.${PDY}"
