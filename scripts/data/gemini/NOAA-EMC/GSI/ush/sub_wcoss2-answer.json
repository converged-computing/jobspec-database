{
    "application": "qsub",
    "details": {
        "software": [
            "gsi_wcoss2.intel",
            "envvar/1.0",
            "cray-pals/1.2.2"
        ],
        "resources": {
            "queue": "dev",
            "time_limit": "01:20:00",
            "nodes": "nodes",
            "procs": "procs",
            "threads": "threads",
            "ncpus": "ncpus",
            "memory": "rmem",
            "environment_variables": [
                "OMP_NUM_THREADS",
                "ntasks",
                "ppn",
                "threads"
