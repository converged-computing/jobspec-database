{
    "application": "LSF",
    "details": {
        "software": [
            "BASH",
            "prod_envir",
            "prod_util",
            "grib_util/1.0.5",
            "PrgEnv-intel",
            "cfp-intel-sandybridge",
            "python/3.6.3",
            "nwps_python"
        ],
        "resources": {
            "nodes": 1,
            "memory": "1700M",
            "walltime": "6:00",
            "queue": "dev",
            "project": "NWPS-T2O"
        },
        "environment_variables": {
            "NODES": "1",
            "FORT_BUFFERED": "true",
            "NWPSplatform": "DEVWCOSS",
            "MP_PGMMODEL": "mpmd",
            "MP_CSS_INTERRUPT": "yes",
            "WFO": "%WFO%",
            "siteid": "${WFO}",
            "SITEID": "${WFO^^}",
            "N": "n",
            "KEEPDATA": "YES",
            "model": "nwps",
            "MODEL": "${model^^}",
            "envir": "para",
            "DEVWCOSS_USER": "$(whoami)",
            "COMROOT": "/gpfs/hps3/ptmp/${DEVWCOSS_USER}/data/retro/com",
            "job": "pnwps_${WFO}_po_cgn_ret",
            "jobid": "pnwps_${WFO}_po_cgn_ret.$$",
            "jlogfile": "${COMROOT}/logs/${envir}/jlogfile.${job}",
            "WCOSS_SYSTEM": "$( echo $COMROOTp1 | cut -c7 )d1",
            "WCOSS_SYSTEM_COM": "$( echo $COMROOTp1 | cut -c7-9 )",
            "NWROOT": "${NWPSdir}"
        }
    }
}