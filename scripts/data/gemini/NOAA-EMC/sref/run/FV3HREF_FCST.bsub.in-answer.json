        "MKL_CBWR": "AVX2",
        "RUN_ENVIR": "para",
        "envir": "prod",
        "job": "$$",
        "CONFIG_FILE": "_FV3HREFDIR_/parm/fv3href_para_config_cray",
        "PDY": "_PDY_",
        "cyc": "_CYC_",
        "FLENGTH": "_FLENGTH_",
        "MEMBER": "_MEMBER_",
        "MACHINE": "_MACHINE_",
        "APRUN": "aprun -n 1728 -N 12 -j 1 -d 2 -cc depth"
    },
    "job_script": "JFV3HREF_FCST"
}