{
    "application": "LSF",
    "software": [
        "PrgEnv-intel",
        "intel/16.3.210",
        "cray-mpich",
        "craype-hugepages4M",
        "NetCDF-intel-sandybridge/4.2",
        "pmi/5.0.11",
        "prod_util"
    ],
    "resources": {
        "nodes": 144,
        "memory": 256,
        "cores": 1728,
        "threads": 2,
        "stack_size": "1024m",
