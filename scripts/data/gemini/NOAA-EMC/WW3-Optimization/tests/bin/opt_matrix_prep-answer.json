{
    "application": "Bash",
    "details": {
        "software": [
            "Intel Compiler",
            "MPI",
            "NetCDF",
            "Matlab",
            "Parmetis",
            "Jasper",
            "Zlib",
            "PNG",
            "HDF5",
            "Bacio",
            "G2",
            "W3NCO",
            "ESMF"
        ],
        "resource_requirements": {
            "batch_queue": "slurm",
            "nodes": 440,
            "threads_per_node": 6,
            "walltime": "03:00:00",
            "account": "marine-cpu",
            "partition": "orion"
        },
        "environment_variables": {
            "HOME_PATH": "${PWD}",
            "OPTDIR": "${PWD%/*}",
            "WW3_DIR": "${PWD}/model",
            "WW3_BINDIR": "${WW3_DIR}/bin",
            "WW3_TMPDIR": "${WW3_DIR}/tmp",
            "WW3_EXEDIR": "${WW3_DIR}/exe",
            "WWATCH3_ENV": "${WW3_BINDIR}/wwatch3.env",
            "WW3_COMP": "intel",
            "WW3_CC": "icc",
            "WW3_F90": "ifort",
            "PRINTER": "printer",
            "WWATCH3_NETCDF": "NC4",
            "NETCDF_CONFIG": "/apps/netcdf/4.6.1/intel/16.1.150/bin/nc-config",
            "WW3_PARCOMPN": 4,
            "mpi": "mpirun",
            "rtst": "${HOME}/bin/run_test -c $cmplr -S -T -b $batchq",
            "ww3": "${WW3_DIR}"
        }
    }
}