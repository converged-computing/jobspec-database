{
    "application": "obsproc",
    "details": {
        "software": [
            "libjpeg",
            "grib_util",
            "netcdf",
            "intel",
            "craype",
            "cray-mpich",
            "cray-pals",
            "cfp",
            "prod_util",
            "prod_envir"
        ],
        "resource_requirements": {
            "walltime": "00:15:00",
            "nodes": "1",
            "cpus": "7",
            "memory": "500MB",
            "queue": "dev",
            "account": "OBSPROC-DEV",
            "place": "vscatter",
            "debug": "TRUE"
        },
        "environment_variables": {
            "envir": "prod",
            "cyc": "%CC%",
            "DESC": "%DESC%",
            "JTYP": "%JTYP%",
            "NET": "%JTYP%",
            "RUN": "%JTYP%",
            "job": "${JTYP}_dump_alert_$cyc",
            "jobid": "$job.$PBS_JOBID",
            "PDY": "%PDY%",
            "userROOT": "/lfs/h2/emc/obsproc/noscrub/$USER",
            "obsproc_ver": "v1.0",
            "obsproc_ver_pckg": "v1.0.0",
            "obsNET": "obsproc",
            "PACKAGEROOTpara": "/lfs/h1/ops/para/packages",
            "HOMEobsproc": "${PACKAGEROOT}/obsproc.$obsproc_ver",
            "VERSION_FILE": "${HOMEobsproc}/versions/run.ver",
            "DATAROOT": "/lfs/h2/emc/ptmp/${USER}",
            "DEBUG_LEVEL": "3",
            "KEEPDATA": "YES",
            "SENDECF": "NO",
            "SENDDBN": "NO",
            "SENDSDM": "NO",
            "COMIN_ROOT": "/lfs/h2/emc/stmp/$USER/CRON/${DESC}/com"
        }
    }
