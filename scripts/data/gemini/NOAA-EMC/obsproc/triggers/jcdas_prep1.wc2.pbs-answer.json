{
    "application": "PBS",
    "details": {
        "job_name": "obsproc_%JTYP%_prep1_%PDY%_%CC%_%DESC%",
        "queue": "dev",
        "account": "OBSPROC-DEV",
        "walltime": "0:30:00",
        "nodes": "1",
        "cpus_per_node": "12",
        "memory": "6gb",
        "debug": "true",
        "software": {
            "grib_util": "${grib_util_ver}",
            "netcdf": "${netcdf_ver}",
            "intel": "${intel_ver}",
            "craype": "${craype_ver}",
            "cray-mpich": "${cray_mpich_ver}",
            "cray-pals": "${cray_pals_ver}",
            "cfp": "${cfp_ver}",
            "bufr_dump": "${bufr_dump_ver}",
            "prepobs": "${prepobs_ver}"
        },
        "environment_variables": [
            "envir=prod",
            "cyc=%CC%",
            "DESC=%DESC%",
            "JTYP=%JTYP%",
            "job=${JTYP}_prep1_$cyc",
            "jobid=$job.$PBS_JOBID",
            "PDY=%PDY%",
            "userROOT=/lfs/h2/emc/obsproc/noscrub/$USER",
            "obsproc_ver=v1.0",
            "obsproc_ver_pckg=v1.0.0",
            "prepobs_ver=1.0.0",
            "obsNET=obsproc",
            "PACKAGEROOTpara=/lfs/h1/ops/para/packages",
            "HOMEobsproc=${userROOT}/install/obsproc-rmprep",
            "VERSION_FILE=${HOMEobsproc}/versions/run.ver",
            "SENDECF=NO",
            "SENDSDM=NO",
            "SENDDBN=NO",
            "DATAROOT=/lfs/h2/emc/stmp/${USER}",
            "jlogfile=/lfs/h2/emc/stmp/${USER}/${JTYP}.$PDY.jlogfile",
            "COMIN_ROOT=/lfs/h2/emc/stmp/$USER/CRON/${DESC}/com",
            "COMOUT_ROOT=/lfs/h2/emc/stmp/$USER/CRON/${DESC}/com",
            "DEBUG_LEVEL=3",
            "KEEPDATA=YES",
            "LOUD=ON"
        ],
        "commands": [
            "$HOMEobsproc/jobs/JOBSPROC_CDAS_PREP1"
        ],
        "output_files": [
            "/u/$USER/$PBS_JOBNAME.o$pbsjobid"
        ]
    }
}