{
    "application": "obsproc",
    "software": [
        "libjpeg",
        "grib_util",
        "netcdf",
        "intel",
        "craype",
        "cray-mpich",
        "cray-pals",
        "cfp",
        "bufr_dump"
    ],
    "resources": {
        "walltime": "00:15:00",
        "memory": "3gb",
        "nodes": 1,
        "cpus": 1
    },
    "modules": [
        "libjpeg",
        "grib_util/${grib_util_ver}",
        "netcdf/${netcdf_ver}",
        "intel/${intel_ver}",
        "craype/${craype_ver}",
        "cray-mpich/${cray_mpich_ver}",
        "cray-pals/${cray_pals_ver}",
        "cfp/${cfp_ver}",
        "bufr_dump/${bufr_dump_ver}"
    ],
    "environment": [
        "NODES=1",
        "envir=prod",
        "cyc=%CC%",
        "type=%DESC%",
        "DESC=%DESC%",
        "JTYP=%JTYP%",
        "job=$(echo $JTYP | cut -c1-3)_dump_post_erly_$cyc",
        "job=$(echo $JTYP | cut -c1-3)_dump_post_pcyc_$cyc",
        "job=${JTYP}_dump_post_$cyc",
        "jobid=$job.$PBS_JOBID",
        "PDY=%PDY%",
        "PDYm1=$(date -d \"${PDY} 1 day ago\" +%Y%m%d)",
        "userROOT=/lfs/h2/emc/obsproc/noscrub/$USER",
        "obsproc_ver=v1.0",
        "obsproc_ver_pckg=v1.0.0",
        "bufr_dump_ver=1.0.0",
        "obsNET=obsproc",
        "PACKAGEROOTpara=/lfs/h1/ops/para/packages",
        "HOMEobsproc=${userROOT}/install/obsproc-rmprep",
        "VERSION_FILE=$HOMEobsproc/versions/run.ver",
        "SENDECF=NO",
        "SENDDBN=NO",
        "SENDSDM=NO",
        "DATAROOT=/lfs/h2/emc/stmp/$USER",
        "jlogfile=/lfs/h2/emc/stmp/$USER/${JTYP}.$PDY.jlogfile",
        "LOUD=ON",
        "KEEPDATA=YES",
        "DEBUG_LEVEL=3",
        "COMIN_ROOT=/lfs/h2/emc/stmp/$USER/CRON/${DESC}/com/",
        "COMOUT_ROOT=/lfs/h2/emc/stmp/$USER/CRON/${DESC}/com/",
        "COPY=YES",
        "HOMEarch=${COMOUT_ROOT}/${obsNET}/${obsproc_ver}/sdm_rtdm/rap",
        "AVGDarch_OUT=$HOMEarch/avgdata",
        "OBCNTarch=$HOMEarch/obcount_30day"
    ]
}