{
    "application": "PBS",
    "details": {
        "job_name": "obsproc_%JTYP%_dump_%PDY%_%CC%_%TMMARK%_%DESC%",
        "queue": "dev",
        "account": "OBSPROC-DEV",
        "walltime": "00:20:00",
        "nodes": "1",
        "cpus_per_node": "7",
        "memory": "6000MB",
        "debug": "true",
        "software": [
            "libjpeg",
            "grib_util",
            "netcdf",
            "intel",
            "craype",
            "cray-mpich",
            "cray-pals",
            "cfp",
            "bufr_dump"
        ],
        "environment_variables": [
            "envir=prod",
            "cyc=%CC%",
            "DESC=%DESC%",
            "JTYP=%JTYP%",
            "job=${JTYP}_dump_$cyc",
            "jobid=$job.$PBS_JOBID",
            "tmmark=%TMMARK%",
            "PDY=%PDY%",
            "userROOT=/lfs/h2/emc/obsproc/noscrub/$USER",
            "obsproc_ver=v1.0",
            "obsproc_ver_pckg=v1.0.0",
            "bufr_dump_ver=1.0.0",
            "obsNET=obsproc",
            "PACKAGEROOTpara=/lfs/h1/ops/para/packages",
            "HOMEobsproc=${userROOT}/install/obsproc-rmprep",
            "VERSION_FILE=$HOMEobsproc/versions/run.ver",
            "SENDECF=NO",
            "SENDDBN=NO",
            "SENDSDM=NO",
            "TANK=$DCOMROOT",
            "DATAROOT=/lfs/h2/emc/stmp/$USER",
            "jlogfile=/lfs/h2/emc/stmp/$USER/${JTYP}.$PDY.jlogfile",
            "COMOUT_ROOT=/lfs/h2/emc/stmp/$USER/CRON/${DESC}/com",
            "LOUD=ON",
            "KEEPDATA=YES",
            "DEBUG_LEVEL=3"
        ],
        "commands": [
            "module load libjpeg",
            "module load grib_util/${grib_util_ver}",
            "module load netcdf/${netcdf_ver}",
            "module load intel/${intel_ver}",
            "module load craype/${craype_ver}",
            "module load cray-mpich/${cray_mpich_ver}",
            "module load cray-pals/${cray_pals_ver}",
            "module load cfp/${cfp_ver}",
            "module unload bufr_dump",
            "module load bufr_dump/${bufr_dump_ver}",
            "$HOMEobsproc/jobs/JOBSPROC_NAM_DUMP"
        ]
    }
}