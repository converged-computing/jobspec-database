{
    "application": "obsproc",
    "details": {
        "software": {
            "obsproc": "v1.0.0",
            "grib_util": "${grib_util_ver}",
            "netcdf": "${netcdf_ver}",
            "intel": "${intel_ver}",
            "craype": "${craype_ver}",
            "cray-mpich": "${cray_mpich_ver}",
            "cray-pals": "${cray_pals_ver}",
            "cfp": "${cfp_ver}",
            "bufr_dump": "${bufr_dump_ver}"
        },
        "resources": {
            "walltime": "00:10:00",
            "memory": "3gb",
            "nodes": 1,
            "cpus": 1,
            "queue": "dev",
            "account": "OBSPROC-DEV"
        },
        "environment": {
            "envir": "prod",
            "job": "${JTYP}_${cyc}",
            "PDY": "%PDY%",
            "userROOT": "/lfs/h2/emc/obsproc/noscrub/$USER",
            "obsproc_ver": "v1.0",
            "obsproc_ver_pckg": "v1.0.0",
            "obsNET": "obsproc",
            "PACKAGEROOTpara": "/lfs/h1/ops/para/packages",
            "HOMEobsproc": "${userROOT}/install/obsproc-rmprep",
            "VERSION_FILE": "${HOMEobsproc}/versions/run.ver",
            "SENDECF": "NO",
            "SENDDBN": "NO",
            "SENDSDM": "NO",
            "DATAROOT": "/lfs/h2/emc/stmp/${USER}",
            "jlogfile": "/lfs/h2/emc/stmp/${USER}/${JTYP}.${PDY}.jlogfile",
            "COMIN_ROOT": "${DATAROOT}/CRON/${DESC}/com",
            "COMOUT_ROOT": "${DATAROOT}/CRON/${DESC}/com",
            "LOUD": "on",
            "DEBUG_LEVEL": 3,
            "KEEPDATA": "YES",
            "COPY": "YES",
            "HOMEarch": "${DATAROOT}/CRON/${DESC}/com/${obsNET}/$obsproc_ver/sdm_rtdm",
            "AVGDarch_IN": "${HOMEarch}/${JTYP}/avgdata",
            "AVGDarch_OUT": "${HOMEarch}/${JTYP}/avgdata",
            "OBCNTarch": "${HOMEarch}/${JTYP}/obcount_30day"
        }
    }
}