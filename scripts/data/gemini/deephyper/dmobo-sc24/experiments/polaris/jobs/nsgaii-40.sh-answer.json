{
    "application": "PBS",
    "details": {
        "software": [
            "Python",
            "MPI",
            "PostgreSQL"
        ],
        "resources": {
            "nodes": 40,
            "walltime": "03:10:00",
            "queue": "prod",
            "account": "datascience",
            "filesystems": "grand:home"
        },
        "environment": {
            "problem": "dhb_combo",
            "search": "NSGAII",
            "timeout": 10800,
            "random_state": 42,
            "NDEPTH": 16,
            "NRANKS_PER_NODE": 4,
            "NNODES": "calculated from $PBS_NODEFILE",
            "NTOTRANKS": "calculated from NNODES and NRANKS_PER_NODE",
            "OMP_NUM_THREADS": 16,
            "log_dir": "output/nsgaii-40",
            "OPTUNA_DB_DIR": "output/nsgaii-40/optunadb",
            "OPTUNA_DB_HOST": "$HOST"
        }
    }
}