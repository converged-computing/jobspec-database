{
    "application": "DeepHyper",
    "details": {
        "software": [
            "DeepHyper",
            "OpenMPI",
            "Python 3",
            "PostgreSQL"
        ],
        "resources": {
            "computing": {
                "nodes": 2,
                "cores_per_node": 4,
                "walltime": "01:00:00"
            },
            "storage": {
                "filesystem": "grand",
                "home_directory": true
            },
            "queue": "debug",
            "project": "datascience"
        },
        "environment": {
            "DEEPHYPER_LOG_DIR": "results/$problem-mpi-logs-$search-$NNODES-$SEED",
            "OPTUNA_DB_DIR": "$DEEPHYPER_LOG_DIR/optunadb",
            "OPTUNA_DB_HOST": "$HOST",
            "NRANKS_PER_NODE": 4,
            "NDEPTH": 16,
            "NNODES": "$NNODES",
            "NTOTRANKS": "$NNODES * $NRANKS_PER_NODE",
            "OMP_NUM_THREADS": "$NDEPTH"
        }
    }
}