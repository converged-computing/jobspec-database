{
    "application": "ScalBO",
    "details": {
        "software": [
            "COBALT",
            "MPI",
            "Python",
            "ScalBO",
            "gpustat"
        ],
        "resources": {
            "compute": {
                "nodes": 1,
                "ranks_per_node": 48,
                "cores": 48,
                "jobsize": 1
            },
            "memory": {
                "queue": "bigmem"
            },
            "time": {
                "timeout": 10800
            },
            "gpu": {
                "mps_daemon": true,
                "mps_pipe_directory": "/tmp/nvidia-mps",
                "mps_log_directory": "/tmp/nvidia-log"
            },
            "storage": {
                "log_dir": "output/$problem-$search-$sync_str-$acq_func-$strategy-$COBALT_JOBSIZE-$RANKS_PER_NODE-$timeout-$random_state"
            }
        }
    }
}