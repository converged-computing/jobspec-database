{
    "application": "SLURM",
    "details": {
        "scheduler": "SLURM",
        "resources": {
            "nodes": 1,
            "tasks_per_node": 4,
            "cpus_per_task": 12,
            "threads_per_core": 1,
            "gpus": 4,
            "partition": "booster",
            "time": "00:59:00"
        },
        "modules": [
            "nano",
            "CUDA",
            "GCC",
            "OpenMPI",
            "MPI-settings/CUDA"
        ],
        "environment": "jutil env activate -p chwu29",
        "execution": {
            "command": "srun --distribution=block:cyclic:fcyclic --cpus-per-task=${SLURM_CPUS_PER_TASK} dd_alpha_amg sample8to4.ini",
            "environment_variables": {
                "SRUN_CPUS_PER_TASK": "${SLURM_CPUS_PER_TASK}",
                "OMP_NUM_THREADS": "${SLURM_CPUS_PER_TASK}"
            }
        }
    }
}