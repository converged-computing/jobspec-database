{
    "application": "Slurm",
    "details": {
        "job_name": "struct_marl",
        "environment_variables": "all exported",
        "log_file": "logs/%j.log",
        "cpus_per_task": 2,
        "memory": "4G per CPU core",
        "max_execution_time": "12:00:00",
        "partition": "batch",
        "array_tasks": "1-2",
        "python_script": "run.sh",
        "script_arguments": "$1 $2 $SLURM_ARRAY_TASK_ID"
    }
}