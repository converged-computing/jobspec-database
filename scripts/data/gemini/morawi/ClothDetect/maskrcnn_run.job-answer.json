{
    "application": "Slurm",
    "details": {
        "partition": "compute",
        "gpu": "rtx2080ti",
        "memory": "20000MB",
        "cpus_per_task": 2,
        "python_script": "mask_rcnn.py",
        "script_arguments": {
            "lr": 0.005,
            "job_name": "$SLURM_JOB_NAME",
            "lr_scheduler": "StepLR",
            "batch_size": 2,
            "n_cpu": 2,
            "num_epochs": 1,
            "epoch": 0,
            "checkpoint_interval": 1,
            "HPC_run": "True",
            "redirect_std_to_file": "True",
            "pretrained_model": "True",
            "train_percentage": 0.5,
            "dataset_name": "Modanet"
        }
    }
}