{
    "application": "Ray",
    "details": {
        "cluster_type": "SLURM",
        "resources": {
            "nodes": 2,
            "gpus": "TitanX:8",
            "cpus": 16,
            "memory": "1GB per CPU",
            "partition": "guest-gpu",
            "qos": "low-gpu",
            "time_limit": "24:00:00",
            "job_name": "runs/${JOB_NAME}"
        },
        "environment": {
            "conda_environment": "trade",
            "redis_password": "longredispassword",
            "ip_head": "IP address of the head node:6380"
        },
        "libraries": {
            "ray": "https://github.com/ray-project/ray"
        },
        "command_placeholder": "${COMMAND_PLACEHOLDER}"
    }
}