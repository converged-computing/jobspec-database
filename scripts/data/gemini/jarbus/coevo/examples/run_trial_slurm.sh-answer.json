{
    "application": "Slurm",
    "details": {
        "job_name": "trial_job",
        "output_file": "logs/output_%A_%a.txt",
        "error_file": "logs/error_%A_%a.txt",
        "array_task_ids": "1-30",
        "nodes": 1,
        "tasks": 5,
        "time": "02:00:00",
        "partition": "your_partition_name",
        "modules": "julia",
        "script": "trial.jl",
        "random_seed_generation": "head /dev/urandom | tr -dc 0-9 | head -c 10",
        "environment_variables": {
            "SLURM_ARRAY_TASK_ID": "current array task ID"
        }
    }
}