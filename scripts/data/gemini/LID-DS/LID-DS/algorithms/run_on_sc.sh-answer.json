{
    "application": "ids_cluster.py",
    "details": {
        "environment": {
            "module": "matplotlib",
            "pip packages": [
                "pip",
                "../",
                "requirements.txt"
            ]
        },
        "resources": {
            "partition": "paula-gpu",
            "time": "10:00:00",
            "memory": "64G"
        },
        "parameters": [
            {
                "name": "base_path",
                "description": "Path to the base directory"
            },
            {
                "name": "scenario_name",
                "description": "Name of the scenario"
            },
            {
                "name": "embedding_size",
                "description": "Size of the embedding"
            },
            {
                "name": "ngram_length",
                "description": "Length of the ngram"
            },
            {
                "name": "window",
                "description": "Window size"
            },
            {
                "name": "thread_aware",
                "description": "Whether to use thread awareness"
            }
        ]
    }
}