{
    "application": "SLURM",
    "details": {
        "software": [
            "bash",
            "singularity"
        ],
        "resources": [
            "partition: cp3",
            "time: 01:20:00",
            "memory: 10000M",
            "environment variable: ALL",
            "scratch disk: /nfs/scratch/fynu or /scratch"
        ],
        "environment_variables": {
            "JOBID": "SLURM_ARRAY_JOB_ID",
            "TASKID": "SLURM_ARRAY_TASK_ID",
            "WORKDIR": "/nfs/scratch/fynu/$USER/$JOBID.$TASKID or /scratch/$USER/$JOBID.$TASKID",
            "SINGULARITYENV_PATH": "$PATH",
            "SINGULARITYENV_LD_LIBRARY_PATH": "$LD_LIBRARY_PATH"
        },
        "container_image": "/cvmfs/unpacked.cern.ch/registry.hub.docker.com/cmssw/el7:x86_64"
    }
}