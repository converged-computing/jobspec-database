{
    "application": "Dask",
    "details": {
        "scheduler": {
            "type": "dask-scheduler",
            "resources": {
                "nodes": 1,
                "cores": 6,
                "walltime": "02:00:00",
                "partition": "normal",
                "local_directory": "/gpfs/flash/users/$USER"
            }
        },
        "worker": {
            "type": "dask-worker",
            "resources": {
                "memory_limit": "0.15",
                "nthreads": 4,
                "nprocs": 5,
                "local_directory": "/gpfs/flash/users/$USER",
                "interface": "ib0"
            }
        },
        "environment": {
            "modules": "pangeo",
            "jupyter_runtime_dir": "$WORK",
            "jupyterlab_port": 8888
        }
    }
}