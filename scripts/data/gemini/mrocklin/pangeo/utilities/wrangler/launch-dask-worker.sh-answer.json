{
    "application": "Dask",
    "details": {
        "software": {
            "dask": "Dask distributed computing library",
            "pangeo": "Pangeo environment for scientific computing"
        },
        "resources": {
            "scheduler": "$HOME/scheduler.json",
            "local_directory": "/gpfs/flash/users/$USER",
            "memory_limit": "0.15",
            "nthreads": "4",
            "nprocs": "6",
            "interface": "ib0",
            "slurm": {
                "job_name": "dask-worker",
                "output_file": "worker.%j.out",
                "partition": "normal",
                "nodes": "1",
                "tasks_per_node": "6",
                "time_limit": "02:00:00"
            }
        }
    }
}