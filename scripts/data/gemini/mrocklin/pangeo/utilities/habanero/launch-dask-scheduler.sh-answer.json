{
    "application": "Dask",
    "details": {
        "scheduler_type": "MPI",
        "resources": {
            "nodes": 1,
            "cores_per_node": 6,
            "threads_per_core": 4,
            "memory_limit": "12GB",
            "local_directory": "/local"
        },
        "environment": {
            "anaconda": "loaded",
            "pangeo": "activated"
        },
        "scheduler_file": "~/scheduler.json",
        "communication_interface": "ib0 (not used in this script)"
    }
}