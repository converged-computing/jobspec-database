{
    "application": "Dask",
    "details": {
        "scheduler_file": "~/scheduler.json",
        "environment": {
            "module": "anaconda",
            "conda_environment": "pangeo"
        },
        "mpi": {
            "mpirun_command": "mpirun",
            "processes": 6,
            "threads_per_process": 4,
            "memory_limit": "12e9",
            "interface": "ib0",
            "local_directory": "/local"
        },
        "dask_mpi_options": {
            "no_scheduler": true
        }
    }
}