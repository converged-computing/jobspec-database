{
    "application": "SLURM",
    "details": {
        "scheduler": "SLURM",
        "job_name": "beff",
        "account": "T2BENCH-SL2-CPU",
        "nodes": 1,
        "tasks": 32,
        "time": "1:0:0",
        "partition": "skylake",
        "modules": [
            "rhel7/default-peta4",
            "fftw/intel/64/3.3.3"
        ],
        "application_executable": "hpcc",
        "application_options": "",
        "workdir": "$SLURM_SUBMIT_DIR",
        "openmp_threads": 1,
        "mpi_implementation": "Intel MPI",
        "mpi_tasks_per_node": "$mpi_tasks_per_node",
        "mpi_pinning_domain": "omp:compact",
        "mpi_pinning_order": "scatter",
        "command": "mpirun -ppn $mpi_tasks_per_node -np $np $application $options"
    }
}