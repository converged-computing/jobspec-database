{
    "application": "heat3d",
    "details": {
        "scheduler": "Slurm",
        "partition": "amdrome",
        "node": "amd1",
        "nodes": 1,
        "tasks": 1,
        "gpus_per_node": 1,
        "cpus_per_task": 64,
        "gpus_per_task": 1,
        "time": "00:10:00",
        "output_file": "./stdout_%J",
        "error_file": "./stderr_%J",
        "environment_variable": "OMP_NUM_THREADS",
        "environment_variable_value": "${SLURM_CPUS_PER_TASK}",
        "rocr_visible_devices": "1,2,3",
        "executable": "../build/miniapps/heat3d/openmp/heat3d",
        "arguments": "--nx 512 --ny 512 --nz 512 --nbiter 1000 --freq_diag 0"
    }
}