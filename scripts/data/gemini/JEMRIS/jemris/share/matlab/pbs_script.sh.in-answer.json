{
    "application": "pjemris",
    "software_requirements": [
        "mpirun",
        "PBS (Portable Batch System)"
    ],
    "resource_requirements": {
        "queue": "cpu",
        "job_name": "pjemris",
        "cpu_time": "300000000",
        "nodes": 4,
        "processors_per_node": 8,
        "architecture": "oct"
    },
    "environment_variables": [
        "PBS_O_WORKDIR",
        "PBS_NODEFILE",
        "prefix"
    ],
    "input_files": [
        "simu.xml",
        "PBS_NODEFILE"
    ],
    "output_files": [
        ".sim.out"
    ]
}