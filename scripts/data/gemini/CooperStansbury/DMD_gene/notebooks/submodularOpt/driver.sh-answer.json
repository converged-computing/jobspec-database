{
    "application": "MATLAB",
    "details": {
        "environment": "Slurm",
        "modules": "matlab",
        "resources": {
            "nodes": 1,
            "tasks_per_node": 1,
            "memory_per_cpu": "50G",
            "time": "48:00:00",
            "account": "indikar0",
            "partition": "standard"
        },
        "script_arguments": {
            "argument1": "$1",
            "argument2": "$2",
            "argument3": "$3",
            "array_task_id": "$SLURM_ARRAY_TASK_ID"
        },
        "matlab_function": "driver($1, $SLURM_ARRAY_TASK_ID, 100, $2, $3)"
    }
}