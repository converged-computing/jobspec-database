{
    "application": "Slurm",
    "details": {
        "resource_requirements": {
            "nodes": 1,
            "tasks_per_node": 12,
            "partition": "short",
            "memory": "2G"
        },
        "software_requirements": {
            "python3": "pip install plotly",
            "gurobi": "optional",
            "other": "Bash"
        },
        "job_description": {
            "name": "toptw",
            "working_directory": "/local/$MYUSER/$THISJOB",
            "data_directory": "$DATADIR",
            "solution_directory": "$SOLDIR",
            "script_directory": "$MYDIR"
        },
        "job_logic": {
            "main_script": "$MYDIR/main.py",
            "iterations": 10,
            "solver_types": [
                "F1",
                "F2",
                "F3",
                "F4"
            ]
        }
    }
}