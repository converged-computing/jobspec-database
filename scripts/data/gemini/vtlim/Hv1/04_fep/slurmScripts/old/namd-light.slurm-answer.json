{
    "application": "NAMD",
    "details": {
        "version": "2.11",
        "dependencies": [
            "OpenMPI",
            "gcc"
        ],
        "resources": {
            "partition": "mf_ilg2.3",
            "nodes": 1,
            "tasks_per_node": 64,
            "distribution": "block:cyclic",
            "tasks_per_core": 2,
            "threads_per_core": 2,
            "memory": "8gb",
            "time": "72:00:00",
            "exclude_nodes": "c-16-24"
        },
        "input_files": [
            "hosts.${SLURM_ARRAY_JOB_ID}_${SLURM_ARRAY_TASK_ID}",
            "$fname.inp"
        ],
        "output_files": [
            "$fname.log",
            "$fname.fepout"
        ]
    }
}