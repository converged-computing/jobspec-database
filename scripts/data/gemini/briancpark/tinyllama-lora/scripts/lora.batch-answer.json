{
    "application": "Ray",
    "details": {
        "resources": {
            "nodes": 6,
            "cpus_per_task": 16,
            "tasks_per_node": 1,
            "queue": "max",
            "gpu_type": "RTX 4060",
            "gpu_count": 1,
            "cpu_count": 32
        },
        "libraries": [
            "conda",
            "ray",
            "nvidia-smi"
        ],
        "environment": {
            "conda_environment": "csc542"
        },
        "script": {
            "main_script": "main.py",
            "hyperparameter_tuning": true
        },
        "configuration": {
            "slurm_job_name": "lora-tune",
            "output_file": "lora-tune.o%j",
            "error_file": "lora-tune.e%j"
        }
    }
}