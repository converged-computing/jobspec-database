{
    "application": "Slurm",
    "details": {
        "script_purpose": "Runs a NEMS simulation on a Slurm cluster",
        "slurm_options": {
            "account": "coastal",
            "job_name": "IKSH_sch_RUN",
            "time_limit": "02:00:00",
            "nodes": 1,
            "tasks": 4,
            "cpus_per_task": 1,
            "error_output": "IKSH_sch_RUN.err.log",
            "output": "IKSH_sch_RUN.out.log"
        },
        "environment": {
            "module_file": "MOD_FILE",
            "executable": "NEMS.x",
            "binary_directory": "BIN_DIR"
        }
    }
}