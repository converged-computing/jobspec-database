{
    "application": "Slurm",
    "details": {
        "script_purpose": "Runs NEMS.x model",
        "resource_requirements": {
            "account": "coastal",
            "job_name": "SCI_fvc_RUN",
            "wall_time": "2 hours",
            "nodes": 1,
            "tasks": 12,
            "cpus_per_task": 1,
            "error_file": "SCI_fvc_RUN.err.log",
            "output_file": "SCI_fvc_RUN.out.log",
            "module_file": "${MOD_FILE}"
        },
        "execution_command": "${BATCH_RUNEXE} ${BIN_DIR:+${BIN_DIR}/}NEMS.x"
    }
}