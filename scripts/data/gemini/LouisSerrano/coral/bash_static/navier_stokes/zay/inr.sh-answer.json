{
    "application": "Slurm",
    "details": {
        "resource_requirements": {
            "partition": "gpu_p2",
            "qos": "qos_gpu-t4",
            "gres": "gpu:1",
            "cpus-per-task": 10,
            "time": "100:00:00"
        },
        "software_requirements": {
            "pytorch-gpu": "1.10.1"
        },
        "python_package": "training.inr",
        "arguments": {
            "data.data_dir": "$ZAY_DATA_DIR",
            "data.dataset_name": "navier-stokes",
            "data.sub_tr": 1,
            "optim.batch_size": 128,
            "inr.latent_dim": 256,
            "inr.hidden_dim": 256,
            "optim.epochs": 10000,
            "data.data_to_encode": "both"
        }
    }
}