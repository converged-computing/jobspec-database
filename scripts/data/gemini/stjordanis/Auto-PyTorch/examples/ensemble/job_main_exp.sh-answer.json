{
    "application": "SLURM",
    "details": {
        "queue": "bosch_cpu-cascadelake",
        "runtime": "2 hours and 1 minute",
        "nodes": 1,
        "cores": 2,
        "array_size": 3,
        "output_log_dir": "logs/cluster",
        "job_name": "apt_test",
        "python_version": "3",
        "script": "test_ensemble.py",
        "environment": "env/bin/activate",
        "pythonpath": "$PWD",
        "arguments": [
            "--run_id",
            "$1",
            "--task_id",
            "$SLURM_ARRAY_TASK_ID",
            "--num_workers",
            "3",
            "--dataset_id",
            "$2",
            "--seed",
            "$3",
            "--ensemble_setting",
            "ensemble",
            "--portfolio_type",
            "greedy",
            "--num_threads",
            "2",
            "--test",
            "false"
        ]
    }
}