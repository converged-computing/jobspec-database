{
    "application": "JupyterLab",
    "details": {
        "environment": {
            "JOB_ACCOUNT": "required",
            "WORKDIR": "/glade/scratch/${USER}/tmp (optional, defaults to /glade/scratch/${USER}/tmp)",
            "LOAD_MATLAB": "optional",
            "MATLAB_VERSION": "optional (defaults to R2018a)"
        },
        "software": {
            "matlab": "optional (requires build-matlab-api script)",
            "python": "required (specific version depends on MATLAB_VERSION)",
            "jupyter lab": "required"
        },
        "resources": {
            "nodes": "optional (defaults to 1)",
            "cores": "optional (defaults to 1)",
            "memory": "optional (defaults to 8GB)",
            "walltime": "required",
            "queue": "optional (defaults to share)",
            "jlab_port": "optional (defaults to 8888)"
        }
    }
}