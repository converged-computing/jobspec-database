{
    "application": "SLURM",
    "details": {
        "resource_requirements": {
            "partition": "gpu",
            "gpus": "1",
            "cpus_per_gpu": "4"
        },
        "software_requirements": {
            "conda_environment": "dassl",
            "python_interpreter": "python",
            "modules": [
                "Anaconda3"
            ],
            "libraries": [],
            "scripts": [
                "train.py"
            ]
        },
        "data_requirements": {
            "datasets": {
                "PACS": [
                    "art_painting",
                    "cartoon",
                    "photo",
                    "sketch"
                ],
                "office_home_dg": [
                    "art",
                    "clipart",
                    "product",
                    "real_world"
                ],
                "digits_dg": [
                    "mnist",
                    "mnist_m",
                    "svhn",
                    "syn"
                ],
                "vlcs": [
                    "caltech",
                    "labelme",
                    "pascal",
                    "sun"
                ],
                "mini_domainnet": [
                    "clipart",
                    "painting",
                    "real",
                    "sketch"
                ]
            },
            "data_paths": {
                "DATA": "~/Code_causal_neural_style_transfer/Dataset",
                "DASSL": "~/Code_causal_neural_style_transfer/Code-CausalStyleTransfer/Dassl.pytorch"
            },
            "config_files": {
                "dataset_config_file": "${DASSL}/configs/datasets/dg/${DATASET}.yaml",
                "config_file": "configs/trainers/style_transfer/${DATASET}.yaml"
            },
            "output_directory": "output_style_transfer/${DATASET}/${T}/seed${SEED}"
        }
    }
}