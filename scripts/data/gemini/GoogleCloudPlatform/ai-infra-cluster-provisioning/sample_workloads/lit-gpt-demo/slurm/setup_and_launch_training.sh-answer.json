{
    "application": "litgpt",
    "software_requirements": [
        "gcloud",
        "docker",
        "litgpt_container.sh"
    ],
    "resource_requirements": {
        "partition": "a3",
        "exclusivity": "exclusive",
        "gpus_per_node": 8,
        "nodes": 4
    },
    "environment_variables": [
        "MODEL_NAME",
        "GCS_EXPERIMENT_BUCKET",
        "EXPERIMENT_ROOT_DIR",
        "UDS_PATH"
    ],
    "other": {
        "SLURM_JOB_ID": "used for UDS_PATH"
    }
}