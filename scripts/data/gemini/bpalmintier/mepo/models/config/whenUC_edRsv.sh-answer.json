    "application": "GAMS",
    "details": {
        "model": "StaticCapPlan",
        "model_dir": "${MODEL_DIR_BASE}/capplan",
        "config_dir": "${MODEL_DIR_BASE}/config",
        "output_dir": "${HOME}/projects/advpower/results/gams/whenUC_grid/",
        "options": {
            "common_io": " -errmsg=1 -lo=2  -inputdir=${MODEL_DIR} --out_dir=${OUT_DIR} ",
