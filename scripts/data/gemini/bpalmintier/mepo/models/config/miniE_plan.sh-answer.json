{
    "application": "GAMS",
    "software": [
        "GAMS",
        "bash",
        "sed"
    ],
    "resource_requirements": {
        "nodes": 1,
        "node_type": "nehalem",
        "memory": "4gb",
        "queue": "long",
        "walltime": "62:00:00"
    },
    "input_files": [
        "${CONFIG_DIR}/MEPO_run.sh",
        "${CONFIG_DIR}/miniE_plan_list.csv"
    ],
    "output_files": [
        "${OUT_DIR}/StaticCapPlan.lst",
        "${OUT_DIR}/StaticCapPlan.log",
        "${OUT_DIR}/StaticCapPlan.gdx",
        "${OUT_DIR}/StaticCapPlan.csv"
    ],
    "environment_variables": {
        "MODEL_DIR_BASE": "${HOME}/bryan/advpower/models",
        "CONFIG_DIR": "${MODEL_DIR_BASE}/config",
        "MODEL_DIR": "${MODEL_DIR_BASE}/capplan",
        "GAMS_MODEL": "StaticCapPlan",
        "OUT_DIR": "${HOME}/bryan/advpower/results/gams/miniE/",
        "COMMON_IO_OPT": " -errmsg=1 -lo=2  -inputdir=${MODEL_DIR} --out_dir=${OUT_DIR} ",
        "ALL_RUN_OPT": " --skip_cap_limit=1",
        "THIS_FILE_OPT": "${ALL_RUN_OPT} ",
        "LONG_OPT": " --max_solve_time=79200 "
    }
}