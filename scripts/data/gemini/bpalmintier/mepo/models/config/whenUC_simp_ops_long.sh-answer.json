{
    "application": "GAMS",
    "details": {
        "model": "UnitCommit",
        "model_directory": "${MODEL_DIR_BASE}/ops",
        "config_directory": "${MODEL_DIR_BASE}/config",
        "output_directory": "${HOME}/projects/advpower/results/gams/whenUC_grid/",
        "queue": "xlong",
        "walltime": "62:00:00",
        "memory": "5gb",
        "nodes": 1,
        "node_type": "nehalem",
        "solver_options": {
            "common": [
                "-errmsg=1",
                "-lo=2",
                "-inputdir=${MODEL_DIR}",
                "--out_dir=${OUT_DIR}"
            ],
            "all_runs": [
                "--sys=thesis_sys.inc",
                "--min_gen_size=0.2"
            ],
            "this_file": [
                "${ALL_RUN_OPT}",
                "--adj_rsrv_for_nse=on",
                "--demand=ercot2007_dem_yr_as_52wk.inc"
            ],
            "long": [
                "--max_solve_time=216000"
            ],
            "parallel": [
                "--par_threads=${PBS_NP}",
                "--lp_method=6",
                "--par_mode=-1",
                "--probe=2"
            ]
        },
        "configuration_file": "${CONFIG_DIR}/whenUC_opsrun.sh",
        "run_list_file": "${CONFIG_DIR}/whenUC_simp_list.csv"
    }
}