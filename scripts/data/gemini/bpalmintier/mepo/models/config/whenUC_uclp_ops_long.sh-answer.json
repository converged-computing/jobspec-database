{
    "application": "GAMS",
    "software": [
        "GAMS",
        "bash",
        "sed"
    ],
    "resource_requirements": {
        "nodes": "1:nehalem",
        "memory": "5gb",
        "queue": "xlong",
        "walltime": "62:00:00",
        "array_size": 17
    },
    "model_directory": "${MODEL_DIR_BASE}/ops",
    "config_directory": "${MODEL_DIR_BASE}/config",
    "output_directory": "${HOME}/projects/advpower/results/gams/whenUC_grid/",
    "model": "UnitCommit",
    "options": {
        "common": [
            " -errmsg=1 -lo=2  -inputdir=${MODEL_DIR} --out_dir=${OUT_DIR} ",
            " --sys=thesis_sys.inc --min_gen_size=0.2 "
        ],
        "this_file": " ${ALL_RUN_OPT} --adj_rsrv_for_nse=on --demand=ercot2007_dem_yr_as_52wk.inc ",
        "long": " --max_solve_time=216000 ",
        "parallel": " --par_threads=${PBS_NP} --lp_method=6 --par_mode=-1 --probe=2 "
    }
}