{
    "application": "snakemake",
    "details": {
        "resource_requirements": {
            "partition": "compute",
            "memory_per_cpu": "4G",
            "cpus_per_task": 4,
            "tasks": 1
        },
        "environment": {
            "bashrc": "$HOME/.bashrc",
            "conda_env": "~/sasha_env"
        },
        "cluster_config": "cluster.json",
        "cluster_command": "sbatch  -p {cluster.partition} -n {cluster.n}"
    }
}