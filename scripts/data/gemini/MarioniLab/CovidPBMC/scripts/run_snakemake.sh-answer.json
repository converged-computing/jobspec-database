{
    "application": "Snakemake",
    "details": {
        "cluster_config": "CRUK_cluster.json",
        "cluster_submission_command": "sbatch -c {cluster.nCPUs} --nodes={cluster.nodes} --job-name={cluster.name} --mem={cluster.memory} --output={cluster.output} --error={cluster.error}",
        "memory_limit": "12000",
        "nodes": 1,
        "cores": 1,
        "output_file": "/mnt/scratchb/jmlab/morgan02/Covid/logs/Snakemake_run.out",
        "error_file": "/mnt/scratchb/jmlab/morgan02/Covid/logs/Snakemake_run.err",
        "snakefile": "CovidPBMC/snakemake_files/Snakefile_CRUK",
        "max_jobs": 100,
        "latency_wait": 250,
        "verbosity": "verbose",
        "rerun_incomplete": true,
        "rule_target": "normalise_gex"
    }
}