{
    "application": "MATLAB",
    "details": {
        "version": "R2014a",
        "modules": [
            "matlab/R2014a"
        ],
        "environment_variables": {
            "MATLABPATH": "${MATLABPATH}:../../matlab"
        },
        "parallel_processing": {
            "method": "MPI",
            "nodes": 4,
            "command": "mpirun -n 4 matlab -r \"seek,exit\""
        },
        "script_language": "bash",
        "job_scheduler": "SLURM",
        "job_parameters": {
            "nodes": 4,
            "time": "00:05:00",
            "output_file": "test.out",
            "error_file": "test.err"
        },
        "matlab_function": "seek"
    }
}