{
    "application": "Slurm",
    "details": {
        "account": "csmpi",
        "partition": "csmpi_fpga_long",
        "cpus_per_task": 32,
        "gres": "gpu:nvidia_a30:1",
        "memory": "64G",
        "time": "4:00:00",
        "output": "cuda.out",
        "environment": "XILINX_XRT=/opt/xilinx/xrt",
        "makefile": "Makefile",
        "executable": "bin/mg.{class}",
        "benchmark_output": "{outfile}",
        "dependencies": [
            "make"
        ]
    }
}