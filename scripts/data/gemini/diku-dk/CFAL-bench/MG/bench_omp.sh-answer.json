{
    "application": "slurm",
    "details": {
        "account": "csmpi",
        "partition": "csmpi_fpga_long",
        "cpus_per_task": 32,
        "gpu": "nvidia_a30",
        "memory": 0,
        "time": "4:00:00",
        "output_file": "omp.out",
        "environment_variable": "XILINX_XRT",
        "environment_variable_value": "/opt/xilinx/xrt",
        "compiler": "gfortran",
        "makefile": "makefile",
        "benchmark_executable": "bin/mg.${class}.x",
        "benchmark_output_file": "${outfile}",
        "benchmark_output_format": "mean,stddev",
        "benchmark_output_data": "Mop/s total",
        "benchmark_output_data_processing": "awk '{print $1/1000}' /dev/stdin | variance"
    }
}