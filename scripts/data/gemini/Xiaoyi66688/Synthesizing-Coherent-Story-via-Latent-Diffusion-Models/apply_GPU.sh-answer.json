{
    "application": "SLURM",
    "details": {
        "job_name": "test",
        "output_file": "ret-%j.out",
        "error_file": "ret-%j.err",
        "partition": "r8nv-gpu-hw",
        "time_limit": "0-8:00:00",
        "nodes": 1,
        "gpus_per_node": 4,
        "gres_flags": "enforce-binding",
        "qos": "gpu-short",
        "nodelist": "r8a100-c01",
        "constraint": "A100",
        "modules": [
            "cluster-tools/v1.0",
            "slurm-tools/v1.0",
            "python3/3.8.16",
            "cuda-cudnn/11.6-8.4.1"
        ],
        "environment": "source /tools/module_env.sh",
        "virtualenv": "xxxxx/activate"
    }
}