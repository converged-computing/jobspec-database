{
    "application": "JuliaChem",
    "details": {
        "environment": {
            "JULIA_NUM_THREADS": 16
        },
        "slurm": {
            "job_name": "juliachem-runtest",
            "output": "juliachem-runtest.log",
            "error": "juliachem-runtest.err",
            "partition": "haswell",
            "nodes": 1,
            "ntasks_per_node": 1,
            "cpus_per_task": 16
        },
        "julia_flags": [
            "--check-bounds=no",
            "--math-mode=fast",
            "--optimize=3",
            "--inline=yes",
            "--compiled-modules=yes",
            "-E 'using Pkg; using JuliaChem; Pkg.test(\"JuliaChem\")'"
        ]
    }
}