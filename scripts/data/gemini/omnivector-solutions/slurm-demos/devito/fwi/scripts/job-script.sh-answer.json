{
    "application": "SLURM",
    "details": {
        "environment": "DEVITO",
        "runtime": "Singularity",
        "image": "devito.sif",
        "code": "fwi.py",
        "resources": {
            "partition": "aws",
            "tasks": 1,
            "time": "1:00:00",
            "output": "/nfs/R-%x.%j.out",
            "error": "/nfs/R-%x.%j.err",
            "work_dir": "/nfs/$SLURM_JOB_NAME-Job-$SLURM_JOB_ID"
        }
    }
}