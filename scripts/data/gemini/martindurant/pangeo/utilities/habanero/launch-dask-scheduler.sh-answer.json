{
    "application": "Dask",
    "details": {
        "scheduler": "dask-mpi",
        "resource_requirements": {
            "nodes": 1,
            "processes_per_node": 6,
            "threads_per_process": 4,
            "memory_limit": "0.15",
            "local_directory": "/local/$USER",
            "scheduler_file": "~/scheduler.json"
        },
        "environment": "mod_env_setup.sh",
        "runtime": "6 hours"
    }
}