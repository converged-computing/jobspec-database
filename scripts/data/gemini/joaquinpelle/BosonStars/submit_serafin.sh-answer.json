{
    "application": "julia",
    "details": {
        "environment_variables": {
            "JULIA_NUM_THREADS": "64",
            "SLURM_HINT": "multithread"
        },
        "resources": {
            "nodes": 1,
            "tasks_per_node": 64,
            "wall_time": "24:00:00"
        },
        "scheduler": "slurm",
        "partition": "multi",
        "job_name": "bosonstar",
        "working_directory": "./",
        "output_file": "logs/out.%j",
        "error_file": "logs/err.%j",
        "email_type": "ALL",
        "email_address": "<user-email>"
    }
}