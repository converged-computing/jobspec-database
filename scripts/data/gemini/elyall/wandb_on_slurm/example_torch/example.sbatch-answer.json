            "python_version": "determined by 'which python'",
            "script": "wandb_on_slurm.py"
        },
        "other_dependencies": {
            "config_file": "/nfs/code/wandb_on_slurm/example_torch/torch_params.yaml",
            "train_file": "/nfs/code/wandb_on_slurm/example_torch/pytorch_model.py",
            "project_name": "wandb_slurm_torch"
        }
    }
}