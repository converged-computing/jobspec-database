{
    "application": "julia",
    "other_software": [
        "vncserver",
        "mumax3"
    ],
    "resource_requirements": {
        "nodes": 1,
        "tasks_per_node": 128,
        "total_tasks": 128,
        "queue": "normal",
        "runtime": "24:00:00",
        "email_notification": "8326909459@tmomail.net",
        "email_type": "ALL",
        "account": "OTH21017"
    },
    "environment_variables": {
        "LD_LIBRARY_PATH": "",
        "LD_PRELOAD": "",
        "JULIA_NUM_THREADS": "ntot"
    },
    "modules": [
        "gcc",
        "cuda/9.0"
    ],
    "staging_files": {
        "folder_path": "$SCRATCH/mumax3/$FOLDERNAME",
        "copy_command": "cp -r * $FOLDERPATH"
    },
    "calculations": {
        "command": "ibrun mumax3 $FILENAME"
    },
    "postprocessing": {
        "copy_command": "cp -r $FOLDERPATH $mumaxOutputPath"
    }
}