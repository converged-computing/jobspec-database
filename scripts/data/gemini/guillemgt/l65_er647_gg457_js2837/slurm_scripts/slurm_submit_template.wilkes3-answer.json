{
    "application": "SLURM",
    "details": {
        "batch_system": "SLURM",
        "queue": "ampere",
        "nodes": 1,
        "tasks_per_node": 1,
        "gpus_per_node": 1,
        "walltime": "0:30:00",
        "modules": [
            "rhel8/default-amp",
            "cuda/11.8",
            "miniconda/3"
        ],
        "conda_environment": "L65",
        "application_executable": "python",
        "application_arguments": "mamba-test.py",
        "workdir": "$SLURM_SUBMIT_DIR",
        "openmp_threads": 1,
        "mpi_tasks_per_node": 1,
        "mpi_tasks_total": 1,
        "command": "python mamba-test.py"
    }
}