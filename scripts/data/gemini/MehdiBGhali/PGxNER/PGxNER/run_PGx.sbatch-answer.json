{
    "application": "SLURM",
    "details": {
        "job_name": "PGxCorpus_training",
        "nodes": 1,
        "partition": "gpu_prod_long",
        "time": "12:00:00",
        "output_file": "logslurms/PGxCorpus/slurm-%j.out",
        "error_file": "logslurms/PGxCorpus/slurm-%j.err",
        "conda_environment": "final_PGx_env",
        "python_version": "3.8",
        "requirements": [
            "rust",
            "setuptools_rust",
            "tokenizers",
            "transformers",
            "fastNLP",
            "fitlog",
            "torch",
            "clang",
            "cudatoolkit",
            "mkl",
            "mkl-include",
            "magma-cuda110"
        ],
        "training_script": "./BARTNER_adapted/train.py",
        "training_parameters": {
            "dataset_name": "PGxCorpus",
            "history_dir": "./training_history",
            "save_model": 1,
            "n_epochs": 100,
            "max_len": 10,
            "max_len_a": 1.6,
            "num_beams": 3,
            "bart_name": "facebook/bart-large"
        }
    }
}