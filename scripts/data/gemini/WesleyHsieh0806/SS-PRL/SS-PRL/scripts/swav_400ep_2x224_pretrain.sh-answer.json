            "crops_for_assign": [
                0,
                1
            ],
            "temperature": 0.1,
            "epsilon": 0.05,
            "sinkhorn_iterations": 3,
            "feat_dim": 128,
            "nmb_prototypes": 3000,
            "queue_length": 0,
            "epochs": 400,
            "batch_size": 64,
            "base_lr": 4.8,
            "final_lr": 0.0048,
            "freeze_prototypes_niters": 313,
            "wd": 1e-06,
            "warmup_epochs": 10,
            "start_warmup": 0.3,
            "arch": "resnet50",
            "use_fp16": true,
            "sync_bn": "apex"
        }
    }
}