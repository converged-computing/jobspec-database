{
    "application": "SLURM",
    "details": {
        "software": [
            "Python",
            "SLURM",
            "main_swav.py"
        ],
        "resources": {
            "nodes": 8,
            "gpus": 64,
            "tasks_per_node": 8,
            "cpus_per_task": 8,
            "memory": "450G",
            "time": "25:00:00",
            "dataset_path": "/path/to/imagenet/train"
        },
        "parameters": {
            "nmb_crops": 2,
            "size_crops": 224,
            "min_scale_crops": 0.08,
