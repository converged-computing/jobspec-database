{
    "application": "SLURM",
    "details": {
        "modules": [
            "anaconda/mini/4.9.2",
            "nvidia/cuda/11.3.1"
        ],
        "conda_environment": "minienv",
        "python_version": "determined by 'which python'",
        "cpu_cores": 24,
        "wall_time": "100:0:0",
        "output_files": [
            "log/slurm.%j.%N.out",
            "log/slurm.%j.%N.err"
        ],
        "job_name": "simdata",
        "script_arguments": [
            "$1",
            "$2"
        ],
        "script_function": "create_data.py"
    }
}