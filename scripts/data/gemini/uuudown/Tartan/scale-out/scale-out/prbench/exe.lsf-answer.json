{
    "application": "LSF",
    "details": {
        "resource_manager": "LSF",
        "project": "#PROJECTNUMBER",
        "walltime": "10 minutes",
        "nodes": 4,
        "modules": [
            "gcc/5.4.0",
            "hdf5",
            "cuda/8.0.54",
            "spectrum-mpi"
        ],
        "environment": "OLCF_SPECTRUM_MPI_ROOT/jsm_pmix/bin/export_smpi_env -gpu",
