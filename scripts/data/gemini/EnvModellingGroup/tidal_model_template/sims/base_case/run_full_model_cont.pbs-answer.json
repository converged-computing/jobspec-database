{
    "application": "firedrake",
    "details": {
        "modules": [
            "firedrake"
        ],
        "environment_variables": [
            "PYTHONPATH",
            "LD_LIBRARY_PATH",
            "OMP_NUM_THREADS",
            "OPENBLAS_NUM_THREADS"
        ],
        "resources": {
            "nodes": 1,
            "tasks": 32,
            "cpus_per_task": 1,
            "time_limit": "4:00:00"
        },
        "execution_command": "mpiexec -n 32 python tidal_model_cont.py"
    }
}