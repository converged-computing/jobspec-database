{
    "application": "Snakemake",
    "details": {
        "resource_manager": "SLURM",
        "modules": [
            "snakemake"
        ],
        "cluster_configuration": "config/cluster.json",
        "job_submission_options": {
            "account": "<account>",
            "qos": "<account qos>",
            "mail_type": "FAIL",
            "mail_user": "<email>",
            "ntasks": 1,
            "cpus_per_task": 1,
            "mem": "1gb",
            "time": "96:00:00",
            "output": "logs/%j_disp.log",
            "error": "logs/%j_disp.log"
        },
        "snakemake_options": {
            "jobs": 100,
            "latency_wait": 20,
            "rerun_incomplete": true,
            "use_envmodules": true
        }
    }
}