{
    "application": "SLURM",
    "details": {
        "software": [
            "Python",
            "CUDA",
            "NCCL",
            "srun"
        ],
        "resources": {
            "nodes": 2,
            "tasks_per_node": 2,
            "cpus_per_task": 12,
            "memory_per_cpu": "4G",
            "partition": "shared-cpu",
            "python_version": "3.10.4",
            "cuda_version": "11.7",
            "nccl_version": "2.12.12-CUDA-11.7.0",
            "venv_path": "$HOME/venv/slurm-pytorch-ddp-boilerplate"
        },
        "environment_variables": [
            "MASTER_PORT",
            "MASTER_ADDR",
            "WORLD_SIZE",
            "nproc_per_node",
            "CUDA_VISIBLE_DEVICES",
            "PYTHONFAULTHANDLER",
            "NCCL_SOCKET_IFNAME",
            "NCCL_DEBUG"
        ],
        "command": "python3 ../main.py --cpu"
    }
}