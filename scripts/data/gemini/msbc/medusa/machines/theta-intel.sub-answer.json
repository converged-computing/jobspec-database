{
    "application": "COBALT",
    "details": {
        "scheduler": "COBALT",
        "modules": [
            "cray-hdf5-parallel"
        ],
        "mpi": {
            "implementation": "OpenMPI",
            "ranks_per_node": 64,
            "total_ranks": "Calculated based on nodes and ranks per node"
        },
        "openmp": {
            "threads_per_rank": "@NUM_THREADS@"
        },
        "execution_environment": {
            "mcdram": {
                "cache": "enabled",
                "numa": "quad"
            }
        },
        "resource_allocation": {
            "walltime": "@WALLTIME@",
            "nodes": "@NODES@",
            "allocation": "@ALLOCATION@"
        },
        "executable": "@EXECUTABLE@",
        "parameter_file": "@PARFILE@"
    }
}