{
    "application": "Dask",
    "details": {
        "software": [
            "Dask",
            "Miniconda3",
            "RASCIL"
        ],
        "resources": {
            "nodes": 8,
            "tasks": 16,
            "memory": "110000 MB",
            "wallclock_time": "01:00:00",
            "scheduler_host": "hostname of first node",
            "scheduler_port": 8786,
            "worker_interface": "ib0",
            "worker_memory_limit": "100GB",
            "worker_nprocs": 16,
            "worker_nthreads": 1
        },
        "environment": {
            "RASCIL": "/mnt/storage-ssd/${USER}/work/rascil",
            "PYTHONPATH": "/mnt/storage-ssd/${USER}/work/rascil:$PYTHONPATH",
            "RASCIL_DASK_SCHEDULER": "${scheduler}:8786"
        }
    }
}