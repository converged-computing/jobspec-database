{
    "application": "Singularity",
    "details": {
        "container_image": "pytorch/pytorch:1.7.0-cuda11.0-cudnn8-runtime",
        "container_mount": "/workspace",
        "container_entrypoint": "/workspace/bin/exec-setup.sh",
        "resource_requirements": {
            "nodes": 1,
            "tasks": 1,
            "cpus_per_task": 8,
            "gpus_per_task": 0,
            "time": "5:00",
            "output_file": "logs/NER-BERT-setup-%J.out",
            "error_file": "logs/NER-BERT-setup-%J.err",
            "job_name": "NER-BERT-setup"
        }
    }
}