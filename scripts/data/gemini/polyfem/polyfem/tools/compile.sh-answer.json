{
    "application": "Slurm",
    "details": {
        "job_submission_script": "compile.sh",
        "job_parameters": {
            "nodes": 1,
            "tasks_per_node": 1,
            "cpus_per_task": 8,
            "time": "2:00:00",
            "memory": "16GB"
        },
        "software_modules": [
            "gcc/10.2.0",
            "cmake/3.18.4",
            "intel/19.1.2",
            "boost/intel/1.74.0"
        ],
        "environment_variables": [
            "CC",
            "CXX",
            "PARDISO_LIC_PATH",
            "PARDISO_INSTALL_PREFIX",
            "OMP_NUM_THREADS",
            "CMAKE_INCLUDE_PATH",
            "CMAKE_LIBRARY_PATH"
        ],
        "build_system": "CMake",
        "build_type": "Release (default), Debug (optional)",
        "compiler": "gcc",
        "parallel_build": "make -j8"
    }
}