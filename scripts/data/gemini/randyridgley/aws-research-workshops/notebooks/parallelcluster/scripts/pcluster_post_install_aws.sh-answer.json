{
    "application": "Slurm",
    "details": {
        "software": [
            "Slurm",
            "Slurmrestd",
            "HDF5",
            "libjwt",
            "pmix",
            "munge",
            "aws cli",
            "htop"
        ],
        "resource_requirements": [
            "yum",
            "openssl",
            "wget",
            "gcc",
            "make",
            "python3",
            "aws secretsmanager",
            "sed",
            "bash"
        ],
        "libraries": [
            "libyaml",
            "http-parser",
            "json-c",
            "hdf5-devel",
            "libjwt-devel"
        ],
        "configuration_files": [
            "/opt/slurm/etc/slurm.conf",
            "/etc/munge/munge.key",
            "/etc/ld.so.conf.d/slurmrestd.conf"
        ],
        "environment_variables": [
            "LD_LIBRARY_PATH",
            "PCLUSTER_RDS_HOST",
            "PCLUSTER_RDS_PORT",
            "PCLUSTER_RDS_USER",
            "PCLUSTER_RDS_PASS",
            "PCLUSTER_NAME",
            "REGION",
            "slurm_version",
            "FEDERATION_NAME",
            "host_name",
            "CORES",
            "lower_name"
        ],
        "services": [
            "slurmctld",
            "slurmdbd"
        ],
        "other": [
            "cfnconfig",
            "imds-access.sh"
        ]
    }
}