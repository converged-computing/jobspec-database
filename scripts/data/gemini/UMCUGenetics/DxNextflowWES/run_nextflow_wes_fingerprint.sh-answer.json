{
    "application": "Nextflow",
    "details": {
        "workflow": "WES_Fingerprint.nf",
        "configuration": "WES.config",
        "runtime_environment": {
            "java_version": "1.8.0_60",
            "slurm_resources": {
                "time": "2:00:00",
                "nodes": "1",
                "memory": "5G",
                "tmpspace": "10G",
                "account": "diaggen"
            },
            "profile": "slurm",
            "options": [
                "-resume",
                "-ansi-log false"
            ]
        },
        "output_handling": {
            "zip_work_directory": true,
            "remove_work_directory": true,
            "change_permissions": true,
            "permission_level": "775"
        },
        "status_tracking": {
            "running_file": "workflow.running",
            "done_file": "workflow.done",
            "failed_file": "workflow.failed"
        }
    }
}