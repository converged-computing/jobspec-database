        "output_file": "ret-%j.err",
        "error_file": "ret-%j.err",
        "partition": "nv-gpu",
        "time_limit": "0-00:10:00",
        "nodes": 1,
        "gpus_per_node": 1,
        "gpu_type": "Volta",
        "qos": "gpu-short",
        "modules": [
            "cmake/3.15.7",
            "git/2.17.1",
            "vim/8.1.2424",
            "gcc/7.5.0",
            "python3/3.6.8",
            "conda/winograd"
        ],
        "environment_files": [
            "~/.bashrc",
            "/tools/module_env.sh"
        ],
        "command": "nvprof python network.py"
    }
}