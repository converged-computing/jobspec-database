{
    "application": "PBS",
    "details": {
        "job_name": "obsproc_%JTYP%_dump_%PDY%_%CC%_%DESC%",
        "queue": "dev",
        "walltime": "00:15:00",
        "resources": {
            "nodes": "1",
            "cpus": "3",
            "memory": "4gb",
            "debug": "true"
        },
        "software": {
            "modules": [
                "libjpeg",
                "grib_util/${grib_util_ver}",
                "netcdf/${netcdf_ver}",
                "intel/${intel_ver}",
                "craype/${craype_ver}",
                "cray-mpich/${cray_mpich_ver}",
                "cray-pals/${cray_pals_ver}",
                "cfp/${cfp_ver}",
                "bufr_dump/${bufr_dump_ver}"
            ],
            "environment_variables": {
                "envir": "prod",
                "cyc": "%CC%",
                "cycM": "%CCM%",
                "DESC": "%DESC%",
                "JTYP": "%JTYP%",
                "PDY": "%PDY%",
                "obsproc_ver": "v1.1",
                "obsproc_ver_pckg": "v1.1.2",
                "bufr_dump_ver": "1.0.0",
                "obsNET": "obsproc",
                "PACKAGEROOTpara": "/lfs/h1/ops/para/packages",
                "HOMEobsproc": "${userROOT}/install/obsproc-rel.v1.0.0",
                "VERSION_FILE": "${HOMEobsproc}/versions/run.ver",
                "SENDECF": "NO",
                "SENDDBN": "NO",
                "SENDSDM": "NO",
                "TANK": "$DCOMROOT",
                "DATAROOT": "/lfs/h2/emc/stmp/${USER}",
                "jlogfile": "/lfs/h2/emc/stmp/${USER}/${JTYP}.${PDY}.jlogfile",
                "COMOUT_ROOT": "/lfs/h2/emc/stmp/${USER}/CRON/${DESC}/com",
                "COMOUT": "${DATAROOT}/CRON/${DESC}/com/${obsNET}/$obsproc_ver/${JTYP}.${PDY}",
                "LOUD": "on",
                "DEBUG_LEVEL": "3",
                "KEEPDATA": "YES"
            }
        }
    }
}