{
    "application": "PBS",
    "details": {
        "job_name": "obsproc_%JTYP%_prep_%PDY%_%CC%_%DESC%",
        "queue": "dev",
        "account": "OBSPROC-DEV",
        "walltime": "00:15:00",
        "resources": {
            "nodes": "1",
            "cpus_per_node": "1",
            "memory": "40gb",
            "prepost": "true",
            "debug": "true"
        },
        "environment_variables": [
            "envir=prod",
            "cyc=%CC%",
            "JTYP=%JTYP%",
            "type=%DESC%",
            "DESC=%DESC%",
            "job",
            "jobid",
            "PDY=%PDY%",
            "userROOT=/lfs/h2/emc/obsproc/noscrub/$USER",
            "obsproc_ver=v1.1",
            "obsproc_ver_pckg=v1.1.2",
            "prepobs_ver=1.0.0",
            "obsNET=obsproc",
            "PACKAGEROOTpara=/lfs/h1/ops/para/packages",
            "HOMEobsproc=${userROOT}/install/obsproc-rel.v1.0.0",
            "VERSION_FILE=${HOMEobsproc}/versions/run.ver",
            "SENDECF=NO",
            "SENDSDM=NO",
            "DATAROOT=/lfs/h2/emc/stmp/$USER",
            "jlogfile=/lfs/h2/emc/stmp/$USER/${JTYP}.${PDY}.jlogfile",
            "COMIN_ROOT=/lfs/h2/emc/stmp/$USER/CRON/${DESC}/com",
            "COMOUT_ROOT=/lfs/h2/emc/stmp/$USER/CRON/${DESC}/com/",
            "KEEPDATA=YES",
            "DEBUG_LEVEL=3",
            "LOUD=ON",
            "POE=NO",
            "BACK=NO",
            "NSPLIT=1"
        ],
        "modules": [
            "grib_util/${grib_util_ver}",
            "netcdf/${netcdf_ver}",
            "bufr_dump/${bufr_dump_ver}",
            "intel/${intel_ver}",
            "craype/${craype_ver}",
            "cray-mpich/${cray_mpich_ver}",
            "cray-pals/${cray_pals_ver}",
            "cfp/${cfp_ver}",
            "prepobs/${prepobs_ver}"
        ],
        "software": [
            "obsproc",
            "prepobs"
        ]
    }
}