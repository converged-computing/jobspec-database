{
    "application": "obsproc",
    "software": [
        "grib_util",
        "netcdf",
        "intel",
        "craype",
        "cray-mpich",
        "cray-pals",
        "cfp",
        "bufr_dump"
    ],
    "resources": [
        "walltime=00:10:00",
        "place=vscatter,select=1:ncpus=1:mem=2500MB",
        "debug=true"
    ],
    "environment_variables": [
        "envir=prod",
        "cyc=%CC%",
        "cycM=%CCM%",
        "DESC=%DESC%",
        "JTYP=%JTYP%",
        "job=${JTYP}_dump_post_$cyc",
        "job=${JTYP}_ru_dump_post_${cyc}${cycM}",
        "PDY=%PDY%",
        "userROOT=/lfs/h2/emc/obsproc/noscrub/$USER",
        "obsproc_ver=v1.1",
        "obsproc_ver_pckg=v1.1.2",
        "obsNET=obsproc",
        "PACKAGEROOTpara=/lfs/h1/ops/para/packages",
        "HOMEobsproc=${userROOT}/install/obsproc-rel.v1.0.0",
        "VERSION_FILE=${HOMEobsproc}/versions/run.ver",
        "SENDECF=NO",
        "SENDDBN=NO",
        "SENDSDM=NO",
        "DATAROOT=/lfs/h2/emc/stmp/${USER}",
        "jlogfile=/lfs/h2/emc/stmp/${USER}/${JTYP}.${PDY}.jlogfile",
        "jlogfile=/lfs/h2/emc/stmp/${USER}/${JTYP}_ru.${PDY}.jlogfile",
        "COMOUT_ROOT=/lfs/h3/emc/stmp/${USER}/CRON/${DESC}/com",
        "COMIN_ROOT=/lfs/h3/emc/stmp/${USER}/CRON/${DESC}/com",
        "COMIN=${DATAROOT}/CRON/${DESC}/com/${obsNET}/$obsproc_ver/${JTYP}.${PDY}",
        "COMIN=${DATAROOT}/CRON/${DESC}/com/${obsNET}/$obsproc_ver/${JTYP}_ru.${PDY}",
        "COMOUT=${DATAROOT}/CRON/${DESC}/com/${obsNET}/$obsproc_ver/${JTYP}.${PDY}",
        "COMOUT=${DATAROOT}/CRON/${DESC}/com/${obsNET}/$obsproc_ver/${JTYP}_ru.${PDY}",
        "LOUD=on",
        "DEBUG_LEVEL=3",
        "KEEPDATA=YES"
    ],
    "execution_path": "$HOMEobsproc/jobs/JOBSPROC_RTMA_DUMP_POST"
}