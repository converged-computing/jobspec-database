{
    "application": "neurograph_import",
    "details": {
        "modules": [
            "PrgEnv-gnu",
            "cray-hdf5-parallel/1.8.16"
        ],
        "resources": {
            "nodes": 32,
            "tasks_per_node": 32,
            "partition": "regular",
            "walltime": "2:00:00",
            "file_system": "SCRATCH",
            "architecture": "haswell"
        },
        "environment_variables": {
            "prefix": "$SCRATCH/dentate/Full_Scale_Control/",
            "forest_connectivity_path": "$prefix/DGC_forest_connectivity_20170508.h5",
            "connectivity_output_path": "$prefix/dentate_Full_Scale_GC_20170728.h5"
        },
        "command": "./build/neurograph_import -f hdf5:syn -s 128 -d $forest_connectivity_path:/Populations/$post/Connectivity $pre $post $connectivity_output_path"
    }
}