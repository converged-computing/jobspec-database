{
    "application": "SOLO",
    "details": {
        "compiler": "intel",
        "environment_file": "${BUILDDIR}/site/environment.${COMPILER}.sh",
        "OMP_STACKSIZE": "256m",
        "executable": "${BUILDDIR}/Build/bin/SOLO_${TYPE}.${COMP}.${MODE}.${COMPILER}.x",
        "npx": "129",
        "npy": "129",
        "npz": "63",
        "layout_x": "4",
        "layout_y": "4",
        "io_layout": "1,1",
        "nthreads": "2",
        "days": "1",
        "hours": "0",
        "minutes": "0",
        "seconds": "0",
        "dt_atmos": "600",
        "ecmwf_ic": ".F.",
        "mountain": ".F.",
        "external_ic": ".F.",
        "warm_start": ".F.",
        "na_init": "1",
        "curr_date": "0,0,0,0",
        "make_nh": ".T.",
        "hydrostatic": ".F.",
        "phys_hydrostatic": ".F.",
        "use_hydro_pressure": ".F.",
        "consv_te": "1.",
        "hyperthread": ".true.",
        "div": "2",
        "skip": "1",
        "npes": "96",
        "LAUNCHER": "srun",
        "SLURM_CPU_BIND": "verbose",
        "run_cmd": "srun --label --ntasks=96 --cpus-per-task=1 $CONTAINER ./SOLO_nh.repro.64bit.intel.x",
        "WORKDIR": "${SCRATCHDIR:-${BUILDDIR}}/CI/BATCH-CI/${GRID}.${MEMO}/",
        "RESTART": "${WORKDIR}/RESTART",
        "INPUT": "${WORKDIR}/INPUT"
    }
}