{
    "application": "SLURM",
    "details": {
        "environment": "anaconda/2021.11",
        "conda_environment": "pt",
        "gpu_count": 2,
        "python_version": "unknown",
        "python_script": "final.py",
        "dataset": "abc",
        "experiment_name": "7input_pretrained",
        "experiment_directory": "exp/final/7input_pretrained",
        "model_directory": "exp/final/7input_pretrained/model",
        "config_file": "config/abc/final.yaml",
        "additional_files": [
            "tool/run.sh",
            "model/pointtransformer/pointtransformer.py",
            "tool/final.py"
        ],
        "logging": {
            "log_directory": "log_[job_id]",
            "gpu_log_file": "log_[job_id]/gpu.log"
        },
        "monitoring": {
            "interval": 1,
            "monitoring_script": "gpus_collection"
        },
        "other_requirements": [
            "nvidia-smi"
        ]
    }
}