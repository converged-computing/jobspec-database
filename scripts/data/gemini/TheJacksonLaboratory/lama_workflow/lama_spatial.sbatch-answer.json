{
    "application": "Slurm",
    "details": {
        "job_name": "lama_spatial",
        "output_file": "lama_spatial.out",
        "error_file": "lama_spatial.err",
        "walltime": "1 hour",
        "nodes": 1,
        "cores_per_node": 16,
        "memory_per_node": "20GB",
        "singularity_image": "LAMA.sif",
        "singularity_command": "lama_workspace/spatially_normalise_data.sh",
        "error_output_file": "lama_workspace/lama_spatial_$SLURM_ARRAY_TASK_ID.err",
        "standard_output_file": "lama_workspace/lama_spatial_$SLURM_ARRAY_TASK_ID.out"
    }
}