{
    "application": "nextflow",
    "details": {
        "software": [
            "nextflow (version 23.10.1)"
        ],
        "resources": {
            "partition": "compute",
            "queue": "batch",
            "time": "72:00:00",
            "memory": "1G",
            "tasks": 1
        },
        "pipeline": "rnaseq",
        "configuration": {
            "profile": "sumner2",
            "sample_folder": "<PATH_TO_YOUR_SEQUENCES>",
            "gen_org": "mouse",
            "genome_build": "GRCm38",
            "pubdir": "/flashscratch/${USER}/outputDir",
            "workdir": "/flashscratch/${USER}/outputDir/work",
            "comment": "This script will run rna sequencing analysis on mouse samples using default mm10"
        }
    }
}