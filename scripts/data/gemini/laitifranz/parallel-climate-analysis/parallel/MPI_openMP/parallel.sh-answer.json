{
    "application": "MPI",
    "details": {
        "resource_manager": "PBS",
        "walltime": "0:05:00",
        "queue": "short_cpuQ",
        "modules": [
            "mpich-3.2",
            "hdf5-1.10.5--gcc-9.1.0",
            "netcdf-4.7.0--gcc-9.1.0"
        ],
        "executable": "parallel.out",
        "arguments": [
            "${THREADS}"
        ],
        "mpi_options": {
            "processes": "${PROCESSES}",
            "mapping": "socket",
            "binding": "core"
        },
        "debugging_tools": [
            "valgrind",
            "leak-check",
            "track-origins"
        ]
    }
}