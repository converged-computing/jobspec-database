{
    "application": "MPI",
    "details": {
        "environment": {
            "modules": [
                "intel",
                "intelmpi"
            ],
            "variables": {
                "NNODES": 2
            }
        },
        "resources": {
            "nodes": 2,
            "cpus_per_task": 2,
            "time_limit": "00:05:00",
            "output_file": "stdout.out",
            "error_file": "error.out"
        },
        "execution": {
            "command": "mpirun -n 2 singularity exec matrixMul.sif /opt/main",
            "image": "matrixMul.sif"
        }
    }
}