{
    "application": "fmriprep",
    "software": [
        "Singularity",
        "fmriprep",
        "FreeSurfer"
    ],
    "resources": [
        "CPU (40 cores)",
        "RAM (12GB)",
        "disk space (for input, output, and temporary files)"
    ],
    "dependencies": [
        "BIDS dataset",
        "templateflow",
        "freesurfer license"
    ],
    "environment_variables": [
        "OPENNEURO_DSID",
        "BASEDIR",
        "BIDS_DIR",
        "FMRIPREP_HOME",
        "SING_CONTAINER",
        "OUTPUT_DIR",
        "WORK_DIR",
        "LOGS_DIR",
        "SINGULARITYENV_TEMPLATEFLOW_HOME",
        "SINGULARITYENV_FS_LICENSE"
    ],
    "script_purpose": "Run fmriprep on a BIDS dataset using singularity container",
    "other_notes": [
        "The script uses SLURM for job management and resource allocation.",
        "The script uses a ramdisk for temporary files."
    ]
}