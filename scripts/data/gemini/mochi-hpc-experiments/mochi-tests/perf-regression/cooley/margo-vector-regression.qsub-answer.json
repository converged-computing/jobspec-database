{
    "application": "margo-p2p-vector",
    "details": {
        "environment": {
            "COBALT": {
                "nodes": 2,
                "time": 20,
                "mode": "script",
                "account": "radix-io",
                "queue": "ibleaf3-debug",
                "attributes": "filesystems=home"
            },
            "HOME": "$SANDBOX",
            "FI_MR_CACHE_MAX_COUNT": 0,
            "FI_OFI_RXM_USE_SRX": 1
        },
        "libraries": {
            "spack": {
                "activated_environment": "mochi-regression"
            },
            "mpirun": {
                "node_file": "$COBALT_NODEFILE",
                "processes": 2,
                "numa_control": {
                    "node": 1,
                    "memory": 1
                }
            }
        },
        "executable": "./margo-p2p-vector"
    }
}