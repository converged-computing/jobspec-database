{
    "application": "SLURM",
    "details": {
        "cluster": "UF HiPerGator",
        "partition": "hpg-ai",
        "nodes": 8,
        "tasks": 8,
        "tasks_per_node": 1,
        "gpus_per_task": 8,
        "cpus_per_task": 12,
        "memory": "1024gb",
        "time": "48:00:00",
        "container_runtime": "Singularity",
        "container": "/apps/nvidia/containers/monai/core/0.9.0",
        "python_interpreter": "python3",
        "training_script": "main_amp.py",
        "training_command": "main_amp.py -a resnet50 --b 224 --workers 4 --opt-level O2 ./",
        "launch_utilities_path": "utils",
        "launch_script": "./utils/run_on_node.sh",
        "environment_variables": [
            "NCCL_DEBUG=WARN"
        ]
    }
}