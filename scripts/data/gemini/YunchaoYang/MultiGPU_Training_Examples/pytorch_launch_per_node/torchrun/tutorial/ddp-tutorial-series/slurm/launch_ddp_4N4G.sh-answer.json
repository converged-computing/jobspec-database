{
    "application": "SLURM",
    "details": {
        "cluster_manager": "SLURM",
        "container_runtime": "Singularity",
        "framework": "PyTorch",
        "library": "PyTorch Lightning",
        "environment_module": "conda",
        "environment": "pytorch_lightning",
        "resource_requirements": {
            "nodes": 4,
            "tasks": 4,
            "tasks_per_node": 1,
            "gpus_per_task": 1,
            "cpus_per_task": 4,
            "memory": "24gb",
            "partition": "hpg-ai",
            "reservation": "el8-hpgai",
            "time": "48:00:00"
        },
        "communication_backend": "NCCL",
        "communication_debug_level": "WARN",
        "launch_command": "torchrun",
        "launch_arguments": {
            "nnodes": 4,
            "nproc_per_node": 1,
            "rdzv_id": "$RANDOM",
            "rdzv_backend": "c10d",
            "rdzv_endpoint": "$head_node_ip:29500"
        }
    }
}