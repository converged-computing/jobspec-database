{
    "application": "Snakemake",
    "details": {
        "software": [
            "Snakemake",
            "Conda",
            "Python",
            "sbatch"
        ],
        "resources": {
            "partition": "synergy",
            "nodes": 1,
            "tasks": 1,
            "cpus": 1,
            "time": "7-00:00:00",
            "memory": "5G",
            "output_file": "metqc_run.%J.out",
            "error_file": "metqc_run.%J.err",
            "cluster_config": "cluster.json"
        },
        "environment": {
            "bash": true,
            "conda_environment": "snakemake"
        },
        "parameters": {
            "jobs": 60,
            "latency_wait": 15,
            "restart_times": 10,
            "max_inventory_time": 20
        },
        "files": [
            "config.yaml",
            "Snakefile",
            "cluster.json",
            "metqc_sbatch.sh",
            "logs",
            "utils",
            "list_files"
        ]
    }
}