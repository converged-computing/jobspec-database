{
    "application": "OpenMP",
    "software": [
        "openMPI",
        "mpicc",
        "mpirun"
    ],
    "resources": {
        "partition": "EPYC",
        "nodes": 1,
        "tasks": 1,
        "ntasks_per_node": 1,
        "cpus_per_task": 64,
        "time": "02:00:00",
        "output_file": "summary.out"
    },
    "modules": [
        "architecture/AMD",
        "openMPI/4.1.5/gnu/12.2.1"
    ],
    "environment_variables": {
        "OMP_PLACES": "cores",
        "OMP_PROC_BIND": "close",
        "OMP_NUM_THREADS": "64"
    }
}