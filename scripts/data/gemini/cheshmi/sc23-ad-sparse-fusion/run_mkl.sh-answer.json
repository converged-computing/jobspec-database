{
    "application": "Slurm",
    "details": {
        "job_name": "fusion",
        "output_file": "fusion_gs.%j.%N.out",
        "partition": "skx-normal",
        "nodes": 1,
        "tasks_per_node": 1,
        "time_limit": "11:00:05",
        "environment": "ALL",
        "singularity_version": "3.7.2",
        "bin_path": "singularity exec --env LD_LIBRARY_PATH=/opt/intel/oneapi/mkl/latest/lib/intel64 artifact.sif /source/fusion/build/demo/",
        "logs_path": "./logs/",
        "script_path": "./",
        "ufdb_path": "./mm/",
        "matlist_path": "./spd_list.txt",
        "threads": 20,
        "openmp_num_threads": 20,
        "mkl_executables": [
            "mkl_mv_trsv",
            "mkl_trsv_mv",
            "mkl_trsv_trsv",
            "mkl_demo",
            "mkl_dad_spilu0_demo",
            "mkl_trsv",
            "mkl_mv_mv"
        ]
    }
}