{
    "application": "SLURM",
    "details": {
        "script_name": "run_probes.sh",
        "output_log": "log/%j-%a-run_probes.log",
        "memory_per_cpu": "4G",
        "nodes": 1,
        "cores": 8,
        "array_task_ids": "1-6",
        "environment_variables": [
            "ORDINAL_PROBING_ROOT",
            "HF_DATASETS_OFFLINE",
            "TRANSFORMERS_OFFLINE",
            "RESULTS_DIR",
            "FEATURE_DATASET_DIR",
            "TRANSFORMERS_CACHE",
            "HF_DATASETS_CACHE",
            "HF_HOME"
        ],
        "conda_environment": "ord",
        "python_script": "train_probes.py",
        "model_name": "pythia-70m",
        "experiment_name": "rank_tune_2",
        "experiment_type": "rank_tune",
        "entity_types": [
            "hex_color",
            "city",
            "token",
            "color"
        ],
        "features": [
            "red",
            "latitude",
            "num",
            "len",
            "alpha",
            "blue"
        ],
        "normalization_types": [
            "layer_norm",
            "centering"
        ]
    }
}