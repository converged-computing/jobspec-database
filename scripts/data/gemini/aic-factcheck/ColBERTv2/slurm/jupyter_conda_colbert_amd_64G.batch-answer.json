{
    "application": "jupyter-lab",
    "software": [
        "Anaconda3",
        "colbert_cpu"
    ],
    "resources": {
        "time": "24 hours",
        "nodes": 1,
        "tasks_per_node": 1,
        "cpus_per_task": 4,
        "partition": "amd",
        "memory": "64 GB",
        "output_file": "../logs/jupyter.%j.out"
    },
    "environment_variables": [
        "PROJECT_DIR",
        "PYTHONPATH",
        "XDG_RUNTIME_DIR",
        "port",
        "node",
        "user"
    ]
}