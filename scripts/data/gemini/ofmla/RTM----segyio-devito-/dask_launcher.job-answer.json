{
    "application": "Dask",
    "details": {
        "scheduler": "Dask",
        "language": "Python",
        "libraries": [
            "Devito",
            "NumPy",
            "SciPy",
            "Sympy"
        ],
        "resources": {
            "CPU": {
                "cores": 10,
                "threads": 1
            },
            "memory": "unlimited",
            "time": "168 hours"
        },
        "modules": [
            "gcc/7.3.0",
            "anaconda3/2020.02"
        ],
        "environment": {
            "TMPDIR": "$PWD/codes_devito",
            "OMP_NUM_THREADS": 10,
            "DEVITO_LANGUAGE": "openmp",
            "DEVITO_DEBUG_COMPILER": 0,
            "DEVITO_LOGGING": "DEBUG",
            "SYMPY_USE_CACHE": "no"
        }
    }
}