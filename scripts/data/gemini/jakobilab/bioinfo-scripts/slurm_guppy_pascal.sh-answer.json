{
    "application": "guppy",
    "software": [
        "guppy",
        "nvidia-smi",
        "SLURM"
    ],
    "resources": [
        "GPU (Tesla)",
        "CPU",
        "Memory (60GB)"
    ],
    "requirements": {
        "SLURM": {
            "partition": "gpu",
            "nodes": 1,
            "tasks": 1,
            "cpus_per_task": 10,
            "gres": "gpu:pascal:1"
        },
        "guppy": {
            "num_callers": 4,
            "chunks_per_runner": 768,
            "chunk_size": 500,
            "gpu_runners_per_device": 8,
            "cpu_threads_per_caller": 10
        }
    }
}