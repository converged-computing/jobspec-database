{
    "application": "CUDA",
    "details": {
        "runtime": "zsh",
        "resource_manager": "SLURM",
        "partition": "instruction",
        "time_limit": "10 minutes",
        "tasks": 1,
        "cpus_per_task": 1,
        "gpu": 1,
        "output_file": "task1.out",
        "memory": "20GB",
        "cuda_version": "11.8.0",
        "compiler": "nvcc",
        "optimization_flags": "-O3",
        "warning_flags": "-Wall",
        "ptxas_optimization_flags": "-O3",
        "libraries": "-lcublas",
        "c++_standard": "c++17",
        "executable": "task1"
    }
}