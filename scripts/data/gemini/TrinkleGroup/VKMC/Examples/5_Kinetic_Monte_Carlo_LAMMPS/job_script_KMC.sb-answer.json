{
    "application": "LAMMPS",
    "details": {
        "environment": "conda",
        "module": "avalaiable_conda_module_in_the_HPC_facility",
        "conda_environment": "My_fav_Env",
        "resources": {
            "partition": "debug",
            "time": "02:00:00",
            "nodes": 1,
            "tasks": 1,
            "cpus_per_task": 60,
            "job_name": "MyKMCJob",
            "output": "MyKMCJob_%j.out",
            "error": "MyKMCJob_%j.err"
        },
        "libraries": {
            "VKMC": "/path/to/the/local/copy/of/the/VKMC/repository",
            "LMP_KMC": "/path/to/the/local/copy/of/the/VKMC/repository/Utils/MEAM_KMC",
            "potpath": "/path/to/the/local/copy/of/the/VKMC/repository/Utils/pot",
            "CD": "/path/to/the/local/copy/of/the/VKMC/repository/CrysDat_FCC/CrystData_ortho_5_cube.h5",
            "IF": "InitStates/statesAll_${T}.npy"
        },
        "parameters": {
            "T": 1073,
            "ni": 11,
            "etol": 0.0,
            "ftol": 0.001,
            "k": 10.0,
            "p": 10.0,
            "u": 5,
            "ns": 2,
            "bs": 1,
            "cs": 1,
            "dmp": true,
            "dpf": true
        }
    }
}