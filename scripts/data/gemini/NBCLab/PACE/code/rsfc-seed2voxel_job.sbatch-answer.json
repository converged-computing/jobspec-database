{
    "application": "rsfc-seed2voxel",
    "software": [
        "singularity",
        "afni",
        "python"
    ],
    "resource_requirements": {
        "compute": {
            "nodes": 1,
            "tasks": 1,
            "cpus_per_task": 4
        },
        "memory": {
            "per_cpu": "4gb"
        },
        "partition": "bluemoon",
        "time": "03:00:00"
    },
    "dependencies": [
        "mriqc",
        "fmriprep"
    ],
    "output_format": "tsv",
    "output_columns": [
        "subject",
        "SLURM_ARRAY_TASK_ID",
        "exitcode"
    ]
}