{
    "application": "slurm",
    "software": [
        "python",
        "torch.distributed.launch",
        "main/train_vlp_ddp.py"
    ],
    "resources": {
        "nodes": 1,
        "tasks_per_node": 1,
        "gpus_per_node": 8,
        "cpus_per_task": 96,
        "partition": "learnai4rl",
        "account": "all"
    },
    "environment": {
        "NCCL_SOCKET_IFNAME": "ens32",
        "NCCL_NSOCKS_PERTHREAD": 4,
        "NCCL_SOCKET_NTHREADS": 2
    }
}