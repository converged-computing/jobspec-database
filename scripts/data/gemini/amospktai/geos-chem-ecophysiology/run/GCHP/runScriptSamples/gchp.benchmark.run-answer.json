{
    "application": "GEOS-Chem",
    "software": [
        "GEOS-Chem",
        "SLURM",
        "OpenMPI",
        "PMIx"
    ],
    "resources": {
        "cores": 48,
        "nodes": 2,
        "memory": "MaxMemPerNode",
        "partition": "huce_intel",
        "walltime": "0-20:00",
        "mpi_implementation": "pmix",
        "environment": {
            "gchp.env": "Symbolic link to an environment file (see script for instructions)"
        }
    },
    "configuration_files": [
        "GCHP.rc",
        "runConfig.sh"
    ],
    "output_files": [
        "gchp.log",
        "gcchem_internal_checkpoint.restart.*.nc4",
        "cap_restart"
    ]
}