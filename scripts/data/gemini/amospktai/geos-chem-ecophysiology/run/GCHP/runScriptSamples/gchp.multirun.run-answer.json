{
    "application": "GEOS-Chem",
    "software": [
        "GEOS-Chem",
        "SLURM",
        "MAPL"
    ],
    "resource_requirements": {
        "cores": 30,
        "nodes": 1,
        "exclusive": true,
        "runtime": "0-0:30",
        "queue": "huce_intel",
        "memory": "MaxMemPerNode",
        "mail_type": "ALL",
        "output_file": "slurm-%j.out",
        "error_file": "slurm-%j.err",
        "environment_file": "gchp.env",
        "configuration_file": "runConfig.sh",
        "restart_file": "gcchem_internal_checkpoint.restart.${restart_datetime}.nc4"
    }
}