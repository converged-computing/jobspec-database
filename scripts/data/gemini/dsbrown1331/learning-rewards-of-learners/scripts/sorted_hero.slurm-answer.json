{
    "application": "SLURM",
    "details": {
        "scheduler": "SLURM",
        "queue": "gpu",
        "nodes": 1,
        "tasks": 1,
        "time": "02:00:00",
        "modules": [
            "launcher",
            "tacc-singularity"
        ],
        "environment_variables": [
            "TACC_LAUNCHER_PPN=1",
            "EXECUTABLE=$TACC_LAUNCHER_DIR/init_launcher",
            "WORKDIR=$WORK/Code/learning-rewards-of-learners/learner/",
            "CONTROL_FILE=commands_hero",
            "TACC_LAUNCHER_NPHI=0",
            "TACC_LAUNCHER_PHI_PPN=8",
            "PHI_WORKDIR=$WORK",
            "PHI_CONTROL_FILE=phiparamlist",
            "TACC_LAUNCHER_SCHED=interleaved"
        ],
        "launcher": {
            "executable": "$TACC_LAUNCHER_DIR/init_launcher",
            "control_file": "commands_hero",
            "scheduling_method": "interleaved"
        },
        "phi": {
            "enabled": false,
            "control_file": "phiparamlist"
        },
        "resources": {
            "workdir": "$WORK/Code/learning-rewards-of-learners/learner/",
            "phi_workdir": "$WORK"
        }
    }
}