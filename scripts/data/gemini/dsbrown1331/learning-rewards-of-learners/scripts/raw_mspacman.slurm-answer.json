{
    "application": "SLURM",
    "details": {
        "scheduler": "SLURM",
        "launcher": "TACC Launcher",
        "libraries": [
            "launcher",
            "tacc-singularity"
        ],
        "environment_variables": [
            "TACC_LAUNCHER_PPN",
            "EXECUTABLE",
            "WORKDIR",
            "CONTROL_FILE",
            "TACC_LAUNCHER_NPHI",
            "TACC_LAUNCHER_PHI_PPN",
            "PHI_WORKDIR",
            "PHI_CONTROL_FILE",
            "TACC_LAUNCHER_SCHED"
        ],
        "job_submission_command": "paramrun SLURM $EXECUTABLE $WORKDIR $CONTROL_FILE $PHI_WORKDIR $PHI_CONTROL_FILE"
    }
}