{
    "application": "SLURM",
    "software": [
        "Bash",
        "Conda",
        "AllenNLP",
        "rsync"
    ],
    "resources": {
        "nodes": 1,
        "tasks": 1,
        "gpu": 1,
        "memory": 0,
        "cpus": 4,
        "scratch_paths": [
            "/disk/scratch/${STUDENT_ID}",
            "/disk/scratch_big/${STUDENT_ID}",
            "/disk/scratch1/${STUDENT_ID}",
            "/disk/scratch2/${STUDENT_ID}",
            "/disk/scratch_fast/${STUDENT_ID}",
            "${CLUSTER_HOME}/scratch/${STUDENT_ID}"
        ],
        "environment_variables": [
            "STUDENT_ID",
            "SCRATCH_HOME",
            "NLTK_DATA",
            "EXP_ROOT",
            "ALLENNLP_CACHE_ROOT",
            "SERIAL_DIR",
            "DATASET_PATH",
            "PREDICTION_STORY_ID_FILE",
            "PREDICTION_ONLY_ANNOTATION_STORIES",
            "PREDICTION_LEVELS_TO_ROLLOUT",
            "PREDICTION_GENERATE_PER_BRANCH",
            "PREDICTION_SAMPLE_PER_BRANCH",
            "PREDICTION_BEAM_SIZE",
            "PREDICTION_SAMPLE_TOP_K_WORDS",
            "PREDICTION_WINDOWING",
            "PREDICTION_SENTIMENT_WEIGHTING",
            "PREDICTION_SENTIMENT_POSITIVE_WEIGHTING",
            "PREDICTION_SENTIMENT_NEGATIVE_WEIGHTING",
            "PREDICTION_MARKED_SENTENCE"
        ]
    }
}