{
    "application": "mirgeDriver",
    "software": [
        "gcc",
        "spectrum-mpi",
        "conda",
        "mpi4py",
        "python",
        "jsrun"
    ],
    "resources": [
        "nodes: 1",
        "group: uiuc",
        "queue: pbatch",
        "walltime: 720",
        "gpu: tesla",
        "cache: /tmp/$USER/xdg-scratch",
        "pocl_cache: /tmp/$USER/pocl-cache"
    ],
    "libraries": [
        "gcc/8.3.1",
        "spectrum-mpi",
        "mirgeDriver.Y2prediction"
    ],
    "files": [
        "restart_data/prediction-*.pkl",
        "run_params.yaml",
        "restart_data/prediction-000000",
        "prediction.py",
        "runOutput.txt",
        "$HOME/bin/getProgOutName.sh"
    ]
}