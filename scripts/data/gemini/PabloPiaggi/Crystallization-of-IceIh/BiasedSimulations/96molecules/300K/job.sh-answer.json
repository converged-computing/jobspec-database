{
    "application": "LAMMPS",
    "details": {
        "software": [
            "LAMMPS",
            "SLURM",
            "Intel MPI",
            "Intel Compiler"
        ],
        "resources": {
            "nodes": 4,
            "cores_per_task": 16,
            "memory_per_core": "400M",
            "runtime": "24:00:00",
            "constraints": "haswell|broadwell|skylake|cascade"
        },
        "environment_variables": [
            "OMP_NUM_THREADS",
            "PLUMED_NUM_THREADS"
        ],
        "input_files": [
            "Restart.lmp",
            "start.lmp",
            "job.sh"
        ],
        "output_files": [
            "log.lammps.*",
            "restart2.lmp.*",
            "restart.lmp.*",
            "data.final.*",
            "runno"
        ]
    }
}