{
    "application": "LAMMPS",
    "details": {
        "software": [
            "LAMMPS",
            "SLURM",
            "Intel MPI"
        ],
        "resources": {
            "nodes": 1,
            "tasks": 4,
            "cpus-per-task": 20,
            "memory-per-cpu": "200M",
            "time": "24:00:00",
            "job-name": "280K-fc"
        },
        "environment": {
            "OMP_NUM_THREADS": "$SLURM_CPUS_PER_TASK",
            "PLUMED_NUM_THREADS": "$SLURM_CPUS_PER_TASK"
        },
        "input_files": [
            "Restart.lmp",
            "start.lmp"
        ],
        "output_files": [
            "log.lammps.*",
            "restart2.lmp.*",
            "restart.lmp.*",
            "data.final.*"
        ]
    }
}