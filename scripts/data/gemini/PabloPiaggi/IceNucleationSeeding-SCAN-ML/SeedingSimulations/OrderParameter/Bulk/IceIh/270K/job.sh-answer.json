{
    "application": "LAMMPS",
    "details": {
        "software": [
            "LAMMPS",
            "DeepMD-Kit",
            "TensorFlow",
            "OpenMPI",
            "CUDA Toolkit",
            "cuDNN"
        ],
        "resources": {
            "nodes": 1,
            "tasks": 2,
            "tasks_per_node": 2,
            "cpus_per_task": 7,
            "memory_per_cpu": "2G",
            "gpus_per_node": 2,
            "time_limit": "24:00:00",
            "environment_variables": [
                "OMP_NUM_THREADS",
                "PLUMED_NUM_THREADS",
                "SLURM_WHOLE"
            ]
        },
        "modules": [
            "rh/devtoolset/4",
            "cudatoolkit/10.0",
            "cudnn/cuda-10.0/7.6.3",
            "openmpi/gcc/3.1.3/64"
        ],
        "input_files": [
            "Restart.lmp",
            "start.lmp",
            "job.sh"
        ],
        "output_files": [
            "restart2.lmp",
            "restart.lmp",
            "data.final",
            "log.lammps",
            "runno"
        ]
    }
}