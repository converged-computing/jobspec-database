{
    "application": "mumax3",
    "details": {
        "runtime": "2 hours",
        "memory": "1500 MB",
        "gpu": "1",
        "cuda_version": "11.0",
        "script_language": "bash",
        "slurm_array_tasks": "1-11",
        "output_file": "SOT2_orginal_${randomSeed}.mx3",
        "output_folder": "SOT2_orginal_${randomSeed}.out",
        "simulation_parameters": {
            "grid_size": "1024, 1024, 1",
            "cell_size": "2.5e-9, 2.5e-9, 0.8e-9",
            "periodic_boundary_conditions": "32, 0, 0",
            "output_format": "OVF2_BINARY",
            "magnetization_initial_state": "NeelSkyrmion(1, -1).transl(-1000e-9,0,0)",
            "saturation_magnetization": "900e3",
            "exchange_constant": "6.5e-12",
            "anisotropy_axis": "vector(0, 0, 1)",
            "anisotropy_constant": "0.70e6",
            "damping_constant": "0.1",
            "dmi_constant": "2.0e-3",
            "grain_size": "10e-9",
            "max_region": "255",
            "anisotropy_variation_multiplier": "0.1",
            "fixed_timestep": "5e-14",
            "hbar": "1.0545718e-34",
            "e": "1.6021766e-19",
            "AlphaH": "0.15",
            "d": "0.8e-9",
            "Ms": "900e3",
            "p": "Constvector(0,-1,0)",
            "xi_SOT": "-2.0",
            "J_SOT": "abs(-2.0e11)",
            "aj": "Const(J_SOT*(hbar/2.*AlphaH/e/d/Ms))",
            "bj": "Mul(aj,Const(xi_SOT))",
            "dampinglike": "Mul(aj,Cross(m,p))",
            "fieldlike": "Mul(bj,p)",
            "B_ext": "vector(0, 0, 200.0e-3)",
            "autosave_frequency": "5e-10",
            "snapshot_frequency": "5e-10",
            "table_autosave_frequency": "5.0e-10",
            "simulation_duration": "25e-9"
        },
        "software": {
            "mumax3_version": "3.10",
            "mumax3_executable": "$WRKDIR/mumax3.10_linux_cuda11.0/mumax3",
            "cache_location": "$WRKDIR/SOT_simulations/Cache",
            "project_location": "$WRKDIR/SOT_simulations/$PROJECTNAME"
        }
    }
}