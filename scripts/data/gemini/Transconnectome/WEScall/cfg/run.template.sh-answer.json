{
    "application": "Snakemake",
    "details": {
        "software": [
            "Snakemake",
            "Torque (PBS)",
            "MongoDB (optional)",
            "dotkit (optional)",
            "python"
        ],
        "resources": {
            "memory": "4g",
            "queue": "cu",
            "working_directory": "{WORKING_DIR}",
            "log_directory": "{LOGDIR}",
            "snakefile": "{SNAKEFILE}",
            "default_slave_queue": "{DEFAULT_SLAVE_Q}",
            "cluster_config": "cluster.yaml",
            "config_file": "conf.yaml",
            "master_log": "{MASTERLOG}"
        },
        "environment_variables": {
            "DEBUG": "0",
            "DRMAA_OFF": "1",
            "EXTRA_SNAKEMAKE_ARGS": "optional",
            "SLAVE_Q": "optional"
        }
    }
}