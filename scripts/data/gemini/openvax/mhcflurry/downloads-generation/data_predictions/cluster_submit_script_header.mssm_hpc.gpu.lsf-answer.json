{
    "application": "LSF",
    "details": {
        "scheduler": "LSF",
        "job_name": "MHCf-{work_item_num}",
        "queue": "gpu",
        "gpu_count": 1,
        "nodes": 1,
        "cores": 1,
        "walltime": "46:00",
        "memory": "30000 MB",
        "output_log": "{work_dir}/%J.stdout",
        "error_log": "{work_dir}/STDERR",
        "environment": "/bin/bash",
        "conda_env": "py36b",
        "python_version": "3.6",
        "cuda_version": "10.0.130",
        "cudnn_version": "7.1.1",
        "netmhc_bundle_home": "$HOME/sinai/git/netmhc-bundle",
        "netmhc_bundle_tmpdir": "/local/JOBS/netmhctmp-{work_item_num}"
    }
}