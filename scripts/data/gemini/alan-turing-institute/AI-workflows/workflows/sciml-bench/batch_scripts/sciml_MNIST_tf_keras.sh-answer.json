{
    "application": "Slurm",
    "details": {
        "resource_requirements": {
            "nodes": 1,
            "wall_time": "01:00:00",
            "gpus": 1
        },
        "modules": "%modules",
        "container": "sciml-bench_cu11.sif",
        "container_command": "sciml-bench run MNIST_tf_keras --output_dir=./output_$job_id --dataset_dir=/scratch_mount/MNIST",
        "run_command": "singularity exec\n  --nv\n  --bind $scratch:/scratch_mount\n  --pwd /scratch_mount\n  $container\n  $container_command",
        "inputs": "datasets/MNIST",
        "outputs": "output_$job_id"
    },
    "other_software": [
        "nvidia-smi",
        "singularity"
    ]
}