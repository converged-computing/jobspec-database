{
    "application": "Slurm",
    "details": {
        "scheduler": "Slurm",
        "container": "Singularity",
        "container_image": "sciml-bench_cu11.sif",
        "container_command": "sciml-bench run slstr_cloud --output_dir=./output_$job_id --dataset_dir=/scratch_mount/slstr_cloud_ds1",
        "gpu_monitoring": "nvidia-smi dmon",
        "resources": {
            "nodes": 1,
            "gpu": 1,
            "wallclock_time": "23:00:00"
        },
        "input_data": "datasets/slstr_cloud_ds1",
        "output_data": "output_$job_id"
    }
}