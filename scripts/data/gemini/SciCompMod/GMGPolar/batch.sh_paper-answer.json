{
    "application": "GMGPolar",
    "details": {
        "software": {
            "name": "GMGPolar",
            "version": "unspecified",
            "build_dir": "build"
        },
        "resources": {
            "scheduler": "SLURM",
            "nodes": 1,
            "cores_per_node": 4,
            "memory": "unspecified",
            "wall_time": "6000",
            "libraries": [
                "mumps/mumps-5.4.1/mumps-5.4.1-gcc-11.3.0-openmpi-s7bj6hi"
            ],
            "other": [
                "openMP (4 threads)"
            ]
        },
        "grid_files": {
            "radii_files_directory": "radii_files/Rmax${R}/aniso${fac_ani}",
            "angles_files_directory": "angles_files/Rmax${R}/aniso${fac_ani}",
            "grid_file_format": "divide${divideBy2}.txt"
        },
        "output_files": {
            "output_directory": "outputs",
            "output_file_format": "job.out_${fac_ani}_${mod_pk}_${prob}_${beta_coeff}_${extrapolation}_${divideBy2}_${rel_red_conv}.txt",
            "error_file_format": "job.err_${fac_ani}_${mod_pk}_${prob}_${beta_coeff}_${extrapolation}_${divideBy2}_${rel_red_conv}.txt"
        }
    }
}