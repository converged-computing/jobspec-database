{
    "application": "SLURM",
    "details": {
        "job_name": "satdamp",
        "output_file": "./study/satdamp/%A.log",
        "partition": "normal",
        "cpus_per_task": 8,
        "time_limit": "36:00:00",
        "memory_per_cpu": "6000mb",
        "environment_variables": [
            "MKL_NUM_THREADS",
            "OMP_NUM_THREADS",
            "NUMEXPR_NUM_THREADS",
            "PYTHONPATH"
        ],
        "python_script": "./study/vowel_train.py",
        "configuration_file": "./study/satdamp/satdamp.yml",
        "arguments": [
            "--num_threads",
            "$SLURM_CPUS_PER_TASK",
            "--name",
            "$SLURM_JOB_ID",
            "--savedir",
            "./study/satdamp/"
        ]
    }
}