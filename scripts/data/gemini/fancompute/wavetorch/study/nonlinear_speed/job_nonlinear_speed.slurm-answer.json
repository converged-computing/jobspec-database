{
    "application": "Slurm",
    "details": {
        "job_name": "nonlinear_speed",
        "output_file": "./study/nonlinear_speed/%A.log",
        "partition": "normal",
        "cpus_per_task": 8,
        "time": "36:00:00",
        "memory_per_cpu": "4500mb",
        "environment_variables": {
            "MKL_NUM_THREADS": "$SLURM_CPUS_PER_TASK",
            "OMP_NUM_THREADS": "$SLURM_CPUS_PER_TASK",
            "NUMEXPR_NUM_THREADS": "$SLURM_CPUS_PER_TASK",
            "PYTHONPATH": "$(pwd)"
        },
        "python_script": "./study/vowel_train.py",
        "configuration_file": "./study/nonlinear_speed/nonlinear_speed.yml",
        "arguments": {
            "num_threads": "$SLURM_CPUS_PER_TASK",
            "name": "$SLURM_JOB_ID",
            "savedir": "./study/nonlinear_speed/"
        }
    }
}