{
    "application": "SLURM",
    "details": {
        "job_name": "Epidemic Grid Simulation with Agent",
        "tasks_per_node": 15,
        "nodes": 1,
        "walltime": "00:45:00",
        "output": "tracing_runs/job_output/slurm/slurm-%A_%a.out",
        "error": "tracing_runs/job_output/slurm/slurm-%A_%a.err",
        "conda_environment": "contact",
        "python_module": "ct_simulator.run_tracing",
        "python_arguments": [
            "-a",
            "temp/agent_$TASK_ID.json",
            "-r",
            "temp/amodel_$TASK_ID.json",
            "--exp-id",
            "slurm_agent_$JOB_ID",
            "--nettype",
            "barabasi:5:1",
            "--reindex",
            "False",
            "--netsize",
            "100",
            "--k",
            "2.8",
            "--p",
            ".1",
            "--rem_orphans",
            "False",
            "--use_weights",
            "True",
            "--dual",
            "0",
            "--control_schedule",
            "2",
            "0.5",
            "0.99",
            "--control_after",
            "${gridentry[1]}",
            "--control_after_inf",
            ".05",
            "--control_initial_known",
            "${gridentry[0]}",
            "--control_gpu",
            "0",
            "--first_inf",
            "5",
            "--taut",
            "0",
            "--taur",
            "0",
            "--sampling_type",
            "min",
            "--presample",
            "10000",
            "--model",
            "seir",
            "--spontan",
            "False",
            "--pa",
            ".2",
            "--update_after",
            "1",
            "--summary_splits",
            "20",
            "--noncomp",
            "0",
            "--noncomp_after",
            "10000",
            "--avg_without_earlystop",
            "True",
            "--multip",
            "0",
            "--nnets",
            "1",
            "--niters",
            "1",
            "--infseed",
            "-1",
            "--seed",
            "$TASK_ID",
            "--netseed",
            "11",
            "--animate",
            "0",
            "--summary_print",
            "3"
        ],
        "libraries": [
            "conda",
            "matplotlib",
            "torch",
            "cudnn"
        ],
        "environment_variables": [
            "MPLBACKEND=Agg",
            "CUBLAS_WORKSPACE_CONFIG=':4096:8'"
        ]
    }
}