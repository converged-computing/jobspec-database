{
    "application": "SLURM",
    "details": {
        "job_name": "nemo-curator:example-script",
        "nodes": 2,
        "exclusive": true,
        "time": "04:00:00",
        "base_job_dir": "pwd/nemo-curator-jobs",
        "job_dir": "base_job_dir/SLURM_JOB_ID",
        "log_dir": "job_dir/logs",
        "profiles_dir": "job_dir/profiles",
        "scheduler_file": "log_dir/scheduler.json",
        "scheduler_log": "log_dir/scheduler.log",
        "done_marker": "log_dir/done.txt",
        "device": "cpu",
        "script_path": "/path/to/script.py",
        "script_command": "python script_path --scheduler-file scheduler_file --device device",
        "container_image": "/path/to/container",
        "base_dir": "pwd",
        "mounts": "base_dir:base_dir",
        "container_entrypoint": "base_dir/examples/slurm/container-entrypoint.sh",
        "interface": "eth0",
        "protocol": "tcp",
        "cpu_worker_memory_limit": 0,
        "rapids_no_initialize": "1",
        "cudf_spill": "1",
        "rmm_scheduler_pool_size": "1GB",
        "rmm_worker_pool_size": "72GiB",
        "libcudf_cufile_policy": "OFF"
    }
}