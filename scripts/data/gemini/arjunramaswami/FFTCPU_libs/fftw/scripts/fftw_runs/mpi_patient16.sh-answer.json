{
    "application": "MPI",
    "details": {
        "software": [
            "CMake",
            "gompi",
            "FFTW"
        ],
        "resources": {
            "cluster": "SLURM",
            "nodes": 2,
            "tasks": 16,
            "tasks_per_node": 8,
            "time": "30:00",
            "partition": "all",
            "account": "pc2-mitarbeiter",
            "switches": 1
        },
        "environment": {
            "OMP_DISPLAY_AFFINITY": "TRUE",
            "OMP_PLACES": "cores",
            "OMP_PROC_BIND": "close"
        },
        "output": {
            "directory": "../data/patient/mpi2nodes/result",
            "wisdom_directory": "../wisdom/mpi2nodes"
        },
        "execution": {
            "iterations": 10,
            "batch": 1,
            "threads_per_process": 4,
            "mpi_mapping": "ppr:4:socket"
        }
    }
}