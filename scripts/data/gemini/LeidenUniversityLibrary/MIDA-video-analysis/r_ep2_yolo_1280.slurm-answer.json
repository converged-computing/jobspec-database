{
    "application": "YOLOv5",
    "software": [
        "Python",
        "Torchvision (0.8.2)",
        "PyTorch (1.7.1)",
        "CUDA",
        "SLURM",
        "tar",
        "zip"
    ],
    "resource_requirements": {
        "memory": "15G",
        "time": "01:30:00",
        "partition": "gpu-short",
        "tasks": 1,
        "gpus": 1
    },
    "environment": {
        "virtual_environment": "yolo_env",
        "modules": [
            "torchvision/0.8.2-fosscuda-2020b-PyTorch-1.7.1"
        ],
        "scratch_directory": "$SCRATCH"
    },
    "input_data": {
        "datasets": [
            "ep2_0f.tar"
        ],
        "model": [
            "yolo_1280.pt"
        ]
    },
    "output_data": {
        "results_directory": "$PROJECTDIR",
        "archive": "ep2_$SLURM_JOB_ID.zip"
    }
}