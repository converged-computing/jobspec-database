{
    "application": "SLURM",
    "details": {
        "job_name": "GN_coeff",
        "output_file": "GN_coeff_%A_%a.out",
        "error_file": "GN_coeff_%A_%a.err",
        "array_range": "1-6",
        "time_limit": "1-0",
        "partition": "normal",
        "tasks": 1,
        "memory_per_cpu": "4000 MB",
        "cpus_per_task": 8,
        "qos": "long",
        "matlab_module": "matlab",
        "matlab_command": "GN_model_coeff_slurm 50 $SLURM_ARRAY_TASK_ID"
    }
}