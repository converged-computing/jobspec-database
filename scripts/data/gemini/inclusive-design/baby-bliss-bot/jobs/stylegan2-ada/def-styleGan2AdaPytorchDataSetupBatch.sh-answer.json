{
    "application": "StyleGAN2-ADA",
    "details": {
        "software": [
            "StyleGAN2-ADA",
            "Pytorch",
            "CUDA",
            "cuDNN"
        ],
        "resources": [
            "GPU",
            "CPU",
            "Memory",
            "SLURM"
        ],
        "dependencies": [
            "nixpkgs/16.09",
            "intel/2018.3",
            "cuda/10.0.130",
            "cudnn/7.5"
        ],
        "environment": {
            "virtual_environment": "BlissStyleGAN/StyleGAN2/pytorch",
            "temporary_directory": "$SLURM_TMPDIR"
        },
        "input": {
            "data_file": "preppedBlissSinceCharGrey.tar",
            "data_format": ".png",
            "data_dimensions": "256x256",
            "data_type": "grayscale"
        },
        "output": {
            "data_file": "preppedBliss4Pytorch.tar",
            "data_format": ".png",
            "data_location": "home directory"
        },
        "script_purpose": "Prepare dataset for StyleGAN2-ADA training"
    }
}