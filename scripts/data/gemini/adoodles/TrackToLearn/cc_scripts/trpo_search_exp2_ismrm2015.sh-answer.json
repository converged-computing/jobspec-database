{
    "application": "TractoRL",
    "software": [
        "python",
        "TrackToLearn",
        "TRPO",
        "Tractometer",
        "Comet.ml"
    ],
    "resources": {
        "compute": {
            "cores": 12,
            "gpu": 1
        },
        "memory": "10G",
        "storage": {
            "home": "/home/$USER/projects/$SALLOC_ACCOUNT/$USER/TractoRL",
            "dataset": {
                "folder": "${HOME}/projects/$SALLOC_ACCOUNT/$USER/tracktolearn",
                "files": [
                    "datasets/${SUBJECT_ID}/${SUBJECT_ID}.hdf5",
                    "datasets/${VALIDATION_SUBJECT_ID}/${VALIDATION_SUBJECT_ID}.hdf5",
                    "datasets/${VALIDATION_SUBJECT_ID}/masks/${VALIDATION_SUBJECT_ID}_wm.nii.gz"
                ]
            },
            "experiments": {
                "folder": "${DATASET_FOLDER}/experiments"
            }
        }
    }
}