{
    "application": "Julia",
    "software_requirements": [
        "intel/19.0.5-fasrc01",
        "openmpi/4.0.2-fasrc01",
        "fftw/3.3.8-fasrc01",
        "cmake/3.12.1-fasrc01",
        "Anaconda3/2019.10",
        "python/3.7.7-fasrc01"
    ],
    "resource_requirements": {
        "cores": 8,
        "nodes": 1,
        "runtime": "8 hours",
        "partition": "shared",
        "memory_per_cpu": "2048 MB"
    },
    "environment_variables": [
        "OMP_PROC_BIND=spread",
        "OMP_PLACES=threads",
        "JULIA_NUM_THREADS=$SLURM_CPUS_ON_NODE"
    ],
    "input_files": [
        "./cell.510001.surfs",
        "./DS2FF.500000.DAT"
    ],
    "output_files": [
        "particles_omega_600_job_%j.out",
        "particles_omega_600_job_%j.err",
        "./stats_omega_600.csv",
        "./exitstats_omega_600.csv"
    ]
}