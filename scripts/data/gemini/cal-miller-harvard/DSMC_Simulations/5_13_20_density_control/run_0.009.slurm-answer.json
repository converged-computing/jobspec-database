{
    "application": "julia",
    "details": {
        "environment": "SLURM",
        "modules": [
            "intel/19.0.5-fasrc01",
            "openmpi/4.0.2-fasrc01",
            "fftw/3.3.8-fasrc01",
            "cmake/3.12.1-fasrc01",
            "Anaconda3/2019.10",
            "python/3.7.7-fasrc01"
        ],
        "resources": {
            "cores": 8,
            "runtime": "0-08:00 (hours:minutes)",
            "partition": "shared",
            "memory_per_cpu": "2048 MB",
            "open_mode": "append",
            "output_file": "logs/dsmc_job_%j.out",
            "error_file": "logs/dsmc_job_%j.err"
        },
        "environment_variables": {
            "OMP_PROC_BIND": "spread",
            "OMP_PLACES": "threads",
            "JULIA_NUM_THREADS": "$SLURM_CPUS_ON_NODE"
        }
    }
}