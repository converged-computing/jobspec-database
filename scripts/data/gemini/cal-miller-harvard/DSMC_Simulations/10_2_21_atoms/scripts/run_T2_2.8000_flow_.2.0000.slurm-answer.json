{
    "application": "Julia",
    "details": {
        "environment": "Slurm",
        "modules": [
            "intel/19.0.5-fasrc01",
            "openmpi/4.0.2-fasrc01",
            "fftw/3.3.8-fasrc01",
            "cmake/3.12.1-fasrc01",
            "Anaconda3/2019.10",
            "python/3.7.7-fasrc01"
        ],
        "resources": {
            "cores": 2,
            "runtime": "08:00",
            "partition": "shared",
            "memory_per_cpu": "1024 MB",
            "open_mode": "append",
            "output_file": "logs/dsmc_job_%j.out",
            "error_file": "logs/dsmc_job_%j.err"
        },
        "environment_variables": [
            "OMP_PROC_BIND=spread",
            "OMP_PLACES=threads",
            "JULIA_NUM_THREADS=$SLURM_CPUS_ON_NODE"
        ],
        "script": "RunCells.jl",
        "arguments": [
            "--T1",
            "2.0000",
            "--T2",
            "2.8000",
            "-l",
            "0.0025",
            "-L",
            "0.0400",
            "--flow",
            "2.0000"
        ]
    }
}