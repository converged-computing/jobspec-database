{
    "application": "GCHP",
    "details": {
        "software": [
            "gchp",
            "mpirun",
            "runConfig.sh"
        ],
        "resources": {
            "queue": "rvmartin",
            "nodes": 720,
            "walltime": "36:00",
            "priority": 163,
            "memory": "300GB",
            "memory_per_node": "2TB",
            "docker_image": "registry.gsc.wustl.edu/sleong/base-engineering",
            "environment_variables": {
                "OMP_NUM_THREADS": "1",
                "I_MPI_ADJUST_GATHERV": "3",
                "I_MPI_ADJUST_ALLREDUCE": "12",
                "I_MPI_DAPL_UD": "enable",
                "I_MPI_SHM_HEAP_VSIZE": "512"
            }
        }
    }
}