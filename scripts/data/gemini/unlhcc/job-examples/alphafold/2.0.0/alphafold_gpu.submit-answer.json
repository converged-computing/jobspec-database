{
    "application": "AlphaFold",
    "details": {
        "container_runtime": "apptainer",
        "container_image": "docker://unlhcc/alphafold:2.0.0",
        "resources": {
            "nodes": 1,
            "tasks_per_node": 8,
            "memory": "50gb",
            "time": "168:00:00",
            "partition": "gpu",
            "gres": "gpu"
        },
        "databases": {
            "uniref90": "/data/uniref90/uniref90.fasta",
            "mgnify": "/data/mgnify/mgy_clusters.fa",
            "uniclust30": "/data/uniclust30/uniclust30_2018_08/uniclust30_2018_08",
            "bfd": "/data/bfd/bfd_metaclust_clu_complete_id30_c90_final_seq.sorted_opt",
            "pdb70": "/data/pdb70/pdb70",
            "template_mmcif": "/data/pdb_mmcif/mmcif_files",
            "obsolete_pdbs": "/data/pdb_mmcif/obsolete.dat"
        },
        "input": "input.fasta",
        "output": "output_directory",
        "models": "model_1,model_2,model_3,model_4,model_5",
        "preset": "casp14",
        "max_template_date": "2021-09-09"
    }
