{
    "application": "SLURM",
    "details": {
        "job_name": "cifar10",
        "output_file": "slurm_%j.out",
        "nodes": 2,
        "tasks_per_node": 1,
        "time": "00:05:00",
        "partition": "gpu-compute-ondemand",
        "nodelist": "gpu-compute-ondemand-dy-g4dnxlarge-1",
        "container_name": "ignite-vision",
        "container_image": "/shared/enroot_data/pytorchignite+vision+latest.sqsh",
        "container_workdir": "current working directory",
        "command": "pip install --upgrade git+https://github.com/pytorch/ignite.git && python cifar10-distributed.py run --backend=nccl",
        "libraries": [
            "pip",
            "python",
            "pytorch",
            "ignite"
        ],
        "resources": [
            "GPU",
            "network (for nccl backend)"
        ]
    }
}