{
    "application": "Singularity",
    "details": {
        "runtime": "Singularity",
        "image": "nvcr.io/nvidia/tensorflow:19.06-py3",
        "framework": "TensorFlow",
        "model": "ResNet",
        "dataset": "ImageNet",
        "queue": "dgx",
        "nodes": "1",
        "cpus": "40",
        "gpus": "8",
        "walltime": "0:30:00",
        "job_scheduler": "PBS",
        "environment": {
            "PATH": "/app/pbs/bin:/opt/pbs/bin"
        },
        "python_package": "nvidia-examples/cnn",
        "script": "run.pbs",
        "input": "stdin",
        "output": "stdout.$PBS_JOBID",
        "error": "stderr.$PBS_JOBID"
    }
}