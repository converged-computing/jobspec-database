{
    "application": "Snakemake",
    "details": {
        "software": [
            "bash",
            "python 3.8.5",
            "singularity 3.9.2",
            "snakemake"
        ],
        "resources": {
            "memory": "4 GB",
            "queue": "rna",
            "jobs": 100,
            "threads": "not specified",
            "drmaa_args": {
                "output": "{log}.out",
                "error": "{log}.err",
                "job_name": "{params.job_name}",
                "memory": "{params.memory} span[hosts=1]",
                "nodes": 1
            }
        }
    }
}