{
    "application": "SLURM",
    "details": {
        "job_name": "painn_train",
        "partition": "gpu",
        "nodes": 1,
        "tasks_per_node": 1,
        "cpus_per_task": 16,
        "memory_per_cpu": "3700M",
        "gpu": "quadro_rtx_6000",
        "gpu_count": 1,
        "time_limit": "01:00:00",
        "modules": [
            "GCC",
            "OpenMPI",
            "torchvision/0.13.1-CUDA-11.7.0"
        ],
        "python_environment": "painn01",
        "python_script": "train-painn-example.py",
        "script_arguments": [
            "--cutoff",
            "5.0",
            "--features",
            "32",
            "--max_epochs",
            "100",
            "--layer",
            "2",
            "--split",
            "$split"
        ],
        "output_file": "output_training.out"
    }
}