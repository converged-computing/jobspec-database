{
    "application": "Snakemake",
    "details": {
        "scheduler": "SLURM",
        "partition": "<partition-name>",
        "job_name": "<job-name>",
        "tasks": 2,
        "memory": "64G",
        "email": "<your-email>",
        "time": "03-12:00:00",
        "output_file_pattern": "%j_%x.out",
        "error_file_pattern": "%j_%x.err",
        "conda_environment": "/path/to/baseenv",
        "config_file": "config/analysis.yaml",
        "cores": 2
    }
}