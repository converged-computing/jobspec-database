{
    "application": "Snakemake",
    "details": {
        "resource_manager": "SLURM",
        "partition": "<partition-name>",
        "job_name": "<job-name>",
        "tasks": 2,
        "memory": "64G",
        "time": "03-12:00:00",
        "output_file": "%j_%x.out",
        "error_file": "%j_%x.err",
        "conda_environment": "/path/to/baseenv",
        "config_file": "config/analysis.yaml",
        "cores": 1,
        "execution_mode": "all",
        "run_options": [
            "unlock",
            "rerun-incomplete"
        ]
    }
}