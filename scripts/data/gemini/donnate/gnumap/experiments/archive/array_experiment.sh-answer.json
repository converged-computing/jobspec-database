{
    "application": "SLURM",
    "details": {
        "job_name": "array",
        "output_file": "logs/array_%A_%a.out",
        "error_file": "logs/array_%A_%a.err",
        "array_size": "1-5",
        "runtime": "35:00:00",
        "partition": "caslake",
        "tasks": "1",
        "memory": "10G",
        "account": "pi-cdonnat",
        "modules": [
            "R"
        ],
        "conda_env": "pytorch_geom3.9",
        "python_version": "3",
        "scripts": [
            "experiment_set_DGI.py",
            "experiment_set_MVGRL.py",
            "experiment_set_GNUMAP.py",
            "experiment_set_GRACE.py",
            "experiment_set_CCA-SSG.py",
            "experiment_set_BGRL.py"
        ],
        "arguments": [
            "--dataset",
            "--name_file",
            "--seed"
        ],
        "scratch_directory": "$SCRATCH/$USER/gnumap/experiments"
    }
}