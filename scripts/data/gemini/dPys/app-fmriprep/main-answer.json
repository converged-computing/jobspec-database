{
    "application": "fmriprep",
    "details": {
        "software": [
            "fmriprep",
            "bl2bids",
            "jq",
            "singularity",
            "freesurfer"
        ],
        "resources": {
            "nodes": 1,
            "ppn": 16,
            "walltime": "9:00:00",
            "vmem": "28gb",
            "memory_gb": 14,
            "n_procs": 16,
            "omp_nthreads": 16,
            "nthreads": 16
        },
        "filesystems": {
            "lustre": "avoided",
            "nvme": "/scratch/$USER/job_$SLURM_JOB_ID/work"
        },
        "dependencies": {
            "bids_funcs.sh": "source",
            "config.json": "used for configuration",
            "license.txt": "used for freesurfer license"
        },
        "output": {
            "outdir": "out",
            "workdir": "work",
            "templateflow": "templateflow",
            "debug": "debug"
        }
    }
}