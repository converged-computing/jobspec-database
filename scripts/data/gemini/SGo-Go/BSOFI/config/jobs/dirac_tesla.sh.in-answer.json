{
    "application": "PBS",
    "details": {
        "queue": "dirac_small",
        "nodes": 1,
        "processors_per_node": 8,
        "gpu_type": "tesla",
        "walltime": "02:00:00",
        "job_name": "my_job",
        "error_file": "job.$PBS_JOBID.err",
        "output_file": "job.$PBS_JOBID.out",
        "environment_variables": "PBS_O_WORKDIR",
        "modules": [
            "cuda",
            "intel",
            "mkl",
            "python"
        ],
        "command": "@cmd@"
    }
}