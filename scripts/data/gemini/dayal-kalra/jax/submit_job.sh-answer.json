{
    "application": "SLURM",
    "details": {
        "scheduler": "SLURM",
        "job_name": "_warmup",
        "array_size": 1,
        "tasks": 1,
        "tasks_per_core": 1,
        "time_limit": "12:00:00",
        "error_file": "err/%A_%a.err",
        "output_file": "out/%A_%a.out",
        "memory": "10000",
        "gpu": "a100:1",
        "partition": "gpu",
        "script": "train_wide_resnet_linear_warmup.py",
        "arguments": {
            "dataset": "cifar10",
            "loss_name": "xent",
            "abc": "sp",
            "width": 16,
            "widening_factor": 4,
            "depth": 16,
            "scale": 0.0,
            "varw": 2.0,
            "act": "relu",
            "lr_exp_start": 5.5,
            "warm_steps": 512,
            "num_epochs": 200,
            "init_seed": 1,
            "momentum": 0.0,
            "augment": "False"
        }
    }
}