{
    "application": "SLURM",
    "details": {
        "job_name": "GFP",
        "partition": "gpu",
        "tasks": 1,
        "cpus_per_task": 6,
        "memory_per_cpu": "8G",
        "array_range": "0-596%15",
        "time_limit": "3-12:00:00",
        "gpu_resources": "gpu:1",
        "conda_environment": "corel-env",
        "python_script": "${COREL_DIR}/experiments/run_cold_warm_start_experiments_gfp_bo.py",
        "script_arguments": [
            "-b",
            "${batchsize}",
            "-s",
            "${seed}",
            "-p",
            "${task}",
            "-n",
            "${n_start}",
            "-m",
            "${iterations}",
            "--model",
            "${model}"
        ]
    }
}