{
    "application": "Axolotl",
    "details": {
        "environment": {
            "conda_environment": "axolotl",
            "bash_configuration": "/fsx/user02/.bashrc",
            "slurm_partition": "dev",
            "slurm_nodes": 1,
            "slurm_tasks_per_node": 1,
            "slurm_exclusive": true,
            "slurm_output": "/fsx/user02/logs/%x-%j.out",
            "slurm_error": "/fsx/user02/logs/%x-%j.err"
        },
        "configuration": {
            "axolotl_config_file": "sharegpt-base-original.yaml",
            "wandb_entity": "augmxnt",
            "wandb_project": "shisa-v2",
            "gpus_per_node": 8,
            "accelerate_mixed_precision": "bf16",
            "accelerate_dynamo_backend": "no",
            "accelerate_max_restarts": 0,
            "accelerate_rdzv_conf": "rdzv_backend=c10d"
        },
        "software": {
            "python": "version required for axolotl",
            "accelerate": "latest version",
            "transformers": "latest version",
            "nccl": "latest version",
            "wandb": "latest version"
        },
        "resources": {
            "gpu": true,
            "storage": {
                "logs_path": "/fsx/user02/logs",
                "axolotl_path": "/fsx/user02/axolotl"
            }
        }
    }
}