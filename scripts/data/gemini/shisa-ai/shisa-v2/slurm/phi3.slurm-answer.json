{
    "application": "Axolotl",
    "software": [
        "Axolotl",
        "accelerate",
        "mamba",
        "wandb",
        "srun",
        "CUDA",
        "Python"
    ],
    "resource_requirements": {
        "compute": {
            "nodes": 2,
            "tasks_per_node": 1,
            "GPUs_per_node": 8
        },
        "storage": {
            "logs_path": "/fsx/user02/logs"
        },
        "environment": {
            "partition": "dev",
            "exclusive": true,
            "bashrc": "/fsx/user02/.bashrc",
            "axolotl_path": "/fsx/user02/axolotl",
            "conda_env": "axolotl",
            "HF_HUB_ENABLE_HF_TRANSFER": 1,
            "ACCELERATE_LOG_LEVEL": "info",
            "TRANSFORMERS_VERBOSITY": "info",
            "NCCL_ASYNC_ERROR_HANDLING": 1,
            "WANDB_ENTITY": "augmxnt",
            "WANDB_PROJECT": "shisa-v2"
        }
    }
}