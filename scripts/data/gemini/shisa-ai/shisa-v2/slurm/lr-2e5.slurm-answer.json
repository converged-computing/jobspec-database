{
    "application": "Axolotl",
    "details": {
        "framework": "PyTorch",
        "library": "accelerate",
        "config_file": "lr-2e5.yaml",
        "logging": {
            "output": "/fsx/user02/logs/%x-%j.out",
            "error": "/fsx/user02/logs/%x-%j.err",
            "main_log": "/fsx/user02/logs/main_log.txt"
        },
        "environment": {
            "conda_environment": "axolotl",
            "hardware": "GPU",
            "gpus_per_node": 8,
            "slurm_partition": "dev",
            "slurm_nodes": 1,
            "slurm_tasks_per_node": 1,
            "slurm_exclusive": true
        },
        "wandb": {
            "entity": "augmxnt",
            "project": "shisa-v2"
        },
        "distributed_training": {
            "method": "accelerate",
            "strategy": "multi_gpu",
            "mixed_precision": "bf16",
            "communication_backend": "c10d"
        }
    }
}