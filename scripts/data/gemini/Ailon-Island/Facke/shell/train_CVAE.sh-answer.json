{
    "application": "SLURM",
    "details": {
        "job_name": "Facke_CVAE_New_Generator",
        "partition": "gpu",
        "nodes": 1,
        "output_file": "log.out",
        "time_limit": "10-10:00:00",
        "gpu_resources": "2",
        "module_load": "anaconda3/2019.07",
        "conda_environment": "pytorch_1.11",
        "python_script": "./train_CVAE.py",
        "script_arguments": {
            "model": "CVAE",
            "batchSize": 32,
            "name": "CVAE_GAN",
            "display_freq": 2000,
            "no_intra_ID_random": true,
            "print_freq": 2400
        }
    }
}