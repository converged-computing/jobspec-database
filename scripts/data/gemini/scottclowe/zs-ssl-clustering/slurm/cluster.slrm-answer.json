{
    "application": "Slurm",
    "details": {
        "partition": "cpu",
        "nodes": 1,
        "tasks_per_node": 1,
        "cpus_per_task": 2,
        "memory": "2G",
        "time": "96:00:00",
        "output": "slogs/%x__%A_%a.out",
        "array": 0,
        "open_mode": "append",
        "job_name": "zs-ssl-clus",
        "conda_environment": "zs-ssl-clustering",
        "python_script": "cluster.py",
        "arguments": [
            "--seed=$SEED",
            "--log-wandb",
            "--run-name=$SLURM_JOB_NAME",
            "--run-id=$JOB_ID"
        ]
    }
}