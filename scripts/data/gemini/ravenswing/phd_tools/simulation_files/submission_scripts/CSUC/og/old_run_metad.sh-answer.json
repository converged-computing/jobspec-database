{
    "application": "Gromacs",
    "details": {
        "version": "2023",
        "plumed_version": "2.9",
        "gpu_requirements": "1 GPU",
        "cpu_requirements": {
            "cores": 24,
            "nodes": 1
        },
        "runtime": "4 hours",
        "simulation_steps": 50,
        "simulation_time": "500 ns",
        "input_files": [
            "step10.mdp",
            "i.ndx",
            "md.gro",
            "md.cpt",
            "plumed_${FN}.dat",
            "${FN}.top"
        ],
        "output_files": [
            "run.log",
            "${traj}.cpt",
            "${traj}_step*.cpt",
            "${traj}*.tpr"
        ],
        "environment_variables": {
            "GMX": "gmx_mpi",
            "GMX_DISABLE_GPU_TIMING": "yes",
            "OMP_NUM_THREADS": 24
        },
        "modules": "apps/gromacs/2023_plumed_2.9"
    }
}