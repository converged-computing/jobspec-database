{
    "application": "makeflow",
    "details": {
        "scheduler": "PBS",
        "queue": "windfall",
        "resources": {
            "nodes": 1,
            "cpus": 16,
            "memory": "62GB",
            "pcmem": "4GB",
            "walltime": "24:00:00",
            "cputime": "384:00:00"
        },
        "group": "lyons-lab",
        "placement": "pack:shared",
        "modules": [
            "singularity",
            "unsupported",
            "ferng/glibc"
        ],
        "environment_variables": {
            "CCTOOLS_HOME": "/rsgrps/ericlyons/phytooracle/cctools-release-7.1.2"
        },
        "working_directory": "/rsgrps/ericlyons/phytooracle/PSII/",
        "singularity_images": [
            "docker://acicarizona/ps2top-bin2png:1.0",
            "docker://acicarizona/ps2top-img_segmentation:1.0",
            "docker://acicarizona/ps2top-fluorescence_aggregation:1.0"
        ],
        "resource_monitor": {
            "output": "log-ps2-makeflow",
            "interval": 2
        },
        "makeflow_arguments": {
            "workflow": "main_workflow.jx",
            "arguments": "arg_exmaple.json"
        }
    }
}