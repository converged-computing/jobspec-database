{
    "application": "DeepVariant",
    "software": [
        "bwa",
        "samtools",
        "singularity",
        "DeepVariant"
    ],
    "resource_requirements": {
        "walltime": "10:00:00",
        "cpus": 2,
        "memory": "16gb",
        "conda_environment": "AnnotateVariantsEnvironment",
        "genome_index": "/mnt/common/DATABASES/REFERENCES/GRCh38/GENOME/GRCh38-lite.fa",
        "singularity_image": "docker://google/deepvariant:1.0.0"
    },
    "input_files": [
        "Sample1_R1.fastq",
        "Sample1_R2.fastq"
    ],
    "output_files": [
        "Sample1.sam",
        "Sample1.bam",
        "Sample1.sorted.bam",
        "Sample1.sorted.bam.bai",
        "Sample1_DeepVariant.vcf.gz"
    ]
}