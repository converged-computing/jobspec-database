{
    "application": "Python",
    "details": {
        "python_version": "3.x (specific version not specified)",
        "environment_variables": {
            "GCCVER": "5.4.0",
            "CC": "${PREFIX}gcc",
            "CPP": "${PREFIX}cpp",
            "CXX": "${PREFIX}c++",
            "LIBS": "",
            "LDSHARED": "${PREFIX}gcc -pthread -shared",
            "PYMSSQL_BUILD_WITH_BUNDLED_FREETDS": "1",
            "PATH": "/tools/bin:$PATH"
        },
        "dependencies": [
            "readline",
            "pyfiglet",
            "textfsm",
            "pytest-timeout",
            "gitpython",
            "ansible-core",
            "jinja2",
            "psutil",
            "prettytable",
            "tabulate",
            "pycryptodome",
            "natsort",
            "redis",
            "requests",
            "jsonpatch",
            "rpyc",
            "Pyro4",
            "netaddr",
            "crc16",
            "autoflake",
            "pyang",
            "pyparsing",
            "pyangbind",
            "yapf",
            "pexpect",
            "pytz",
            "docker==2.7.0",
            "pdbpp",
            "pylint-pytest",
            "pylint-protobuf",
            "ruff",
            "pylint>=1.9.5",
            "paramiko==2.11.0",
            "netmiko==2.4.2",
            "pytest>=4.4.1,<=4.6.5",
            "pytest-xdist==1.28.0",
            "future>=0.16.0",
            "cryptography >= 2.5",
            "scapy==2.4.3rc1",
            "protobuf>=3.15.6,<=3.20.*",
            "deepdiff==3.3.0",
            "grpcio>=1.8.3,<=1.20.1",
            "grpcio-tools>=1.8.3,<=1.20.1",
            "regex<2022.1.18",
            "pylint>=2.7.2",
            "netmiko>=3.3.3,<=3.4.0",
            "pytest<=6.2.5",
            "pytest-xdist<=2.5.0",
            "future>=0.16.0",
            "cryptography >= 2.5",
            "scapy>=2.4.4",
            "protobuf>=3.15.6,<=3.20.3",
            "pyopenssl",
            "deepdiff>=5.5.0",
            "grpcio>=1.8.3",
            "grpcio-tools>=1.8.3",
            "black",
            "pyenchant",
            "sys-prctl",
            "openpyxl"
        ],
        "install_options": [
            "--upgrade",
            "--force-reinstall",
            "--verbose",
            "--no-cache-dir"
        ],
        "build_system": "pip",
        "commands": [
            "mkdir -p /tmp/$USER",
            "exec &> >(tee /tmp/$USER/upgrade-$SPYTEST_PYTHON_VERSION-requirements.log)",
            "cd $(dirname $0)",
            "pwd -P",
            "source $dir/env",
            "$SPYTEST_PYTHON -V 2>&1 | grep \"Python 3\"",
            "$SPYTEST_PYTHON -m pip install --upgrade pip",
            "$SPYTEST_PYTHON -m pip install --upgrade pyopenssl",
            "$SPYTEST_PYTHON -m pip uninstall -y regex bitarray pyang pyangbind",
            "$SPYTEST_PYTHON -m pip install wheel",
            "$SPYTEST_PYTHON -m pip install sqlite3",
            "$SPYTEST_PYTHON -m pip install -r $TMPFILE.0",
            "$SPYTEST_PYTHON -m pip install --upgrade git+https://github.com/sachinholla/pyangbind.git@0.8.1+spytest.20220216#egg=pyangbind",
            "$SPYTEST_PYTHON -V 2>&1 | cut -d\\  -f 2",
            "$SPYTEST_PYTHON -m pip uninstall -y pytest pytest-xdist",
            "$SPYTEST_PYTHON -m pip install pytest-xdist==1.28.0 pytest==5.4.3",
            "$SPYTEST_PYTHON -m pip install --upgrade git+https://github.com/ramakristipatibrcm/pytest-xdist.git@spytest#egg=pytest-xdist",
            "$SPYTEST_PYTHON -m compileall $SCID_PYTHON_BIN/..",
            "chmod -R go+r $SCID_PYTHON_BIN/.."
        ]
    }
}