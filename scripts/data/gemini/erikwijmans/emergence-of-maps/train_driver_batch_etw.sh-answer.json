{
    "application": "SLURM",
    "details": {
        "job_name": "navigation-analysis-habitat",
        "output_file": "/checkpoint/%u/jobs/job.%A_%a.out",
        "error_file": "/checkpoint/%u/jobs/job.%A_%a.err",
        "gpu_resources": "8",
        "nodes": 1,
        "cpus_per_task": 10,
        "tasks_per_node": 8,
        "memory_per_cpu": "5625MB",
        "partition": "learnfair,scavenge",
        "time_limit": "24:00:00",
        "signal": "USR1@600",
        "open_mode": "append",
        "constraint": "volta32gb",
        "conda_environment": "nav-analysis-base",
        "python_module": "nav_analysis.train_hrl",
        "config_files": [
            "nav_analysis/configs/experiments/transfer/flee_scratch.yaml",
            "configs/experiments/transfer/use_gibson_2plus_se_resneXt50_rgb_weights.yaml"
        ],
        "options": [
            "logging.log_file=${EXP_DIR}/log.txt",
            "logging.checkpoint_folder=${CHECKPOINT}",
            "logging.tensorboard_dir=runs/${ENV_NAME}",
            "task.task_config=tasks/gibson-public.hrl.pointnav.yaml"
        ],
        "software_modules": [
            "cuda/10.0",
            "cudnn/v7.6-cuda.10.0",
            "NCCL/2.4.7-1-cuda.10.0"
        ],
        "environment_variables": {
            "LD_LIBRARY_PATH": "/usr/lib/x86_64-linux-gnu/nvidia-opengl:${LD_LIBRARY_PATH}",
            "GLOG_minloglevel": 2,
            "MAGNUM_LOG": "quiet",
            "MASTER_ADDR": "$(srun --ntasks=1 hostname 2>&1 | tail -n1)"
        }
    }
}