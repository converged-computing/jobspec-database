{
    "application": "SLURM",
    "details": {
        "resource_manager": "SLURM",
        "partition": "gpu",
        "gpus": 1,
        "cpus_per_gpu": 2,
        "memory_per_cpu": "2G",
        "time_limit": "120",
        "modules": [
            "python/3.9.0",
            "cuda/11.2.0",
            "cudnn/8.1.1.33"
        ],
        "environment": "env",
        "python_script": "main.py",
        "input_encoding_options": [
            "fourier_feature (scale: 0.01, 0.1, 1.0)",
            "sinusoidal",
            "affine"
        ]
    }
}