{
    "application": "SLURM",
    "details": {
        "modules": [
            "StdEnv/2020",
            "python/3.7",
            "gurobi/9.1.0"
        ],
        "cpu_cores": 8,
        "memory": "32GB",
        "time_limit": "12 hours",
        "python_version": "3.7",
        "gurobi_version": "9.1.0",
        "libraries": [
            "torch",
            "numpy",
            "scipy",
            "networkx",
            "numcodecs",
            "zarr",
            "gurobipy"
        ],
        "virtual_environment": "l2p-env",
        "script": "run.py",
        "data_format": "zarr",
        "data_paths": [
            "$SCRATCH/learn2price/experiments/exp21/tars/train.tar",
            "$SCRATCH/learn2price/experiments/exp21/tars/valid.tar"
        ],
        "output_file": "se.out"
    }
}