{
    "application": "Snakemake",
    "details": {
        "software": [
            "Snakemake",
            "Python 3.8.3",
            "dot (Graphviz)"
        ],
        "resources": {
            "nodes": 1,
            "ppn": 12,
            "mem": "60gb",
            "walltime": "11:55:00",
            "cluster": {
                "config_file": "cluster.json",
                "latency_wait": 20,
                "use_envmodules": true
            },
            "qsub": {
                "options": "-N {cluster.N} -l nodes={cluster.nodes}:ppn={cluster.ppn},walltime={cluster.walltime},mem={cluster.mem} -M {cluster.email} -m ae -j oe"
            }
        },
        "workflow_files": [
            "jw_bc4.smk",
            "cluster.json"
        ],
        "output_files": [
            "dag.pdf",
            "rulegraph.pdf"
        ]
    }
}