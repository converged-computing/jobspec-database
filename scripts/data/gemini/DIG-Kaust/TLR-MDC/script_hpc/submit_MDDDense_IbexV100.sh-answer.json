{
    "application": "MDDDense.py",
    "software": [
        "intel/2022.3",
        "gcc/11.3.0",
        "openmpi/4.1.4/gnu11.2.1-cuda11.8",
        "cmake/3.24.2/gnu-11.2.1",
        "cuda/11.8",
        "python",
        "mpirun"
    ],
    "resource_requirements": {
        "nodes": 1,
        "tasks": 4,
        "partition": "batch",
        "job_name": "mdd",
        "output_file": "mdd.%J.out",
        "error_file": "mdd.%J.err",
        "walltime": "01:00:00",
        "memory": "300G",
        "gpus": "v100:4"
    },
    "environment_variables": [
        "TLRMDCROOT",
        "TLRMVMROOT",
        "PYTLRROOT",
        "PYTHONPATH",
        "PYTHONDONTWRITEBYTECODE",
        "STORE_PATH",
        "FIG_PATH"
    ]
}