{
    "application": "gamer",
    "software": [
        "gcc/12.2.0-fasrc01",
        "mpich",
        "hdf5",
        "cuda"
    ],
    "resources": {
        "nodes": 2,
        "cores_per_node": 4,
        "runtime": "0-03:10",
        "partition": "itc_gpu",
        "memory_per_cpu": "4000",
        "gpus_per_node": 2,
        "environment_variables": [
            "OMP_NUM_THREADS=4",
            "CUDA_VISIBLE_DEVICES=0,1,2,3",
            "MPI_TYPE_MAX=655360",
            "MPI_TYPE_DEPTH=32",
            "MPI_MSGS_MAX=10485760",
            "MPI_BUFS_PER_PROC=256",
            "MPI_BUFS_PER_HOST=512",
            "MPI_USE_CUDA=1",
            "MPI_DSM_DISTRIBUTE=0",
            "KMP_AFFINITY=disabled",
            "HDF5_DISABLE_VERSION_CHECK=1"
        ]
    }
}