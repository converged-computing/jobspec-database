{
    "application": "SLURM",
    "details": {
        "partition": "A100",
        "nodes": 1,
        "gpus_per_node": 8,
        "ntasks_per_node": 8,
        "job_name": "your_job_name",
        "account": "your_account_name",
        "modules": [
            "openmpi",
            "cuda/11.8"
        ],
        "python_environment": "/path/to/your/python/environment",
        "python_script": "train/train_c_lora.py",
        "config_file": "configs/training/finetune_c_3b_lora.yaml",
        "directory": "/path/to/your/directory",
        "other_environment_variables": {
            "NCCL_PROTO": "simple",
            "FI_EFA_FORK_SAFE": 1,
            "FI_LOG_LEVEL": 1,
            "FI_EFA_USE_DEVICE_RDMA": 1,
            "NCCL_DEBUG": "info",
            "PYTHONFAULTHANDLER": 1,
            "CUDA_LAUNCH_BLOCKING": 0,
            "OMPI_MCA_mtl_base_verbose": 1,
            "FI_EFA_ENABLE_SHM_TRANSFER": 0,
            "FI_PROVIDER": "efa",
            "FI_EFA_TX_MIN_CREDITS": 64,
            "NCCL_TREE_THRESHOLD": 0,
            "PYTHONWARNINGS": "ignore",
            "CXX": "g++",
            "MASTER_ADDR": "master_addr",
            "MASTER_PORT": 33751,
            "PYTHONPATH": "./StableWurst"
        }
    }
}