{
    "application": "translation",
    "software": [
        "accelerate",
        "translate.py",
        "facebook/nllb-200-3.3B"
    ],
    "resources": {
        "cpu": 8,
        "gpu": 1,
        "memory": "64G",
        "environment": "pytorch-tximista",
        "libraries": [
            "openmpi"
        ],
        "environment_variables": [
            "LC_ALL",
            "LANG",
            "LANGUAGE",
            "TOKENIZERS_PARALLELISM",
            "OMP_NUM_THREADS",
            "PMI_SIZE",
            "OMPI_COMM_WORLD_SIZE",
            "MV2_COMM_WORLD_SIZE",
            "WORLD_SIZE",
            "TRANSFORMERS_NO_ADVISORY_WARNINGS",
            "PATH",
            "LD_LIBRARY_PATH"
        ]
    }
}