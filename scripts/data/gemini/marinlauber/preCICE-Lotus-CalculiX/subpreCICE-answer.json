{
    "application": "Allrun",
    "software": [
        "openmpi",
        "gcc",
        "python",
        "boost"
    ],
    "resources": {
        "nodes": 1,
        "tasks_per_node": 2,
        "walltime": "60:00:00",
        "partition": "batch"
    },
    "environment_setup": [
        "module load openmpi/3.0.0/gcc-6.4.0",
        "module load gcc/6.4.0",
        "module load python/2.7.14",
        "module load boost/1.76.0",
        "source ~/.profile"
    ]
}