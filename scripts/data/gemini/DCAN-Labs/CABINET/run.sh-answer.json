{
    "application": "cabinet",
    "software": [
        "singularity",
        "python",
        "conda (pytorch_1.11.0_agate)"
    ],
    "resources": [
        {
            "type": "time",
            "value": "24 hours"
        },
        {
            "type": "nodes",
            "value": "1"
        },
        {
            "type": "cores",
            "value": "24"
        },
        {
            "type": "gpu",
            "value": "1"
        },
        {
            "type": "memory",
            "value": "240GB"
        },
        {
            "type": "partition",
            "value": "v100"
        }
    ],
    "logs": [
        {
            "type": "stdout",
            "path": "/path/to/logs/%A_cabinet.out"
        },
        {
            "type": "stderr",
            "path": "/path/to/logs/%A_cabinet.err"
        }
    ]
}