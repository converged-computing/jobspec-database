{
    "application": "NAMD",
    "details": {
        "scheduler": "PBS",
        "queue": "Std-Long",
        "nodes": 4,
        "processors_per_node": 2,
        "walltime": "24:00:00",
        "email_options": "abe",
        "email_address": "user.name@domain.name",
        "error_file": "./namd2.err.pbs",
        "output_file": "./namd2.out.pbs",
        "module": "namd2",
        "charmrun_arguments": {
            "executable": "namd2",
            "processors": "$PBS_NPROCS",
            "nodelist": "$NODELIST",
            "input_file": "equilibrate_mobile.namd",
            "output_file": "solveq.log"
        }
    }
}