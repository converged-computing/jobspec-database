{
    "application": "SLURM",
    "details": {
        "job_name": "nianet-cae",
        "output_file": "nianet-cae-%j.out",
        "error_file": "nianet-cae-%j.err",
        "nodes": 1,
        "tasks": 1,
        "partition": "gpu",
        "memory_per_gpu": "32GB",
        "gpus": 1,
        "time": "72:00:00",
        "singularity_image": "docker://spartan300/nianet:cae",
        "python_script": "main.py",
        "mounted_volumes": [
            "/app/logs",
            "/app/data",
            "/app/configs"
        ]
    }
}