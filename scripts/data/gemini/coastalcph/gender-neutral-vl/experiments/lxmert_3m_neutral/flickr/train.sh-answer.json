{
    "application": "SLURM",
    "details": {
        "job_name": "flickr_lxmert3m_neutral",
        "resources": {
            "nodes": 1,
            "cpus": 1,
            "memory": "60G",
            "gpu": 1,
            "time": "2-00:00:00"
        },
        "output_file": "train.out",
        "error_file": "train.err",
        "environment": {
            "python_path": "../",
            "conda_environment": "genvlm",
            "modules": [
                "anaconda3/5.3.1",
                "cuda/11.3"
            ]
        },
        "script": "LXMERT/train_task.py",
        "script_arguments": {
            "config_file": "volta/config/lxmert.json",
            "from_pretrained": "${OUTS_DIR}/Pretrain_CC3M_neutral/lxmert_3m/pytorch_model_0.bin",
            "tasks_config_file": "volta/config_tasks/all_trainval_tasks.yml",
            "task": 8,
            "adam_epsilon": 1e-06,
            "adam_betas": "0.9 0.999",
            "adam_correct_bias": true,
            "weight_decay": 0.01,
            "warmup_proportion": 0.1,
            "clip_grad_norm": 5.0,
            "output_dir": "${OUTS_DIR}/${task_name}/${name}",
            "logdir": "logs/${task_name}/${name}",
            "use_wandb": true,
            "wandb_project": "${WANDB_PROJ}",
            "wandb_entity": "${WANDB_ENT}",
            "wandb_run_name": "${name}-${task_name}"
        }
    }
}