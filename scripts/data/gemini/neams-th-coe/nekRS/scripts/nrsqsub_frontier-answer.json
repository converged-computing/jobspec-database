{
    "application": "nekrs",
    "details": {
        "software": [
            "PrgEnv-gnu",
            "craype-accel-amd-gfx90a",
            "cray-mpich",
            "rocm",
            "sbcast",
            "srun",
            "sbatch",
            "nekrs",
            "nvidia-smi"
        ],
        "resources": {
            "compute_nodes": "nodes",
            "gpus_per_node": 8,
            "cores_per_numa": 7,
            "queue": "QUEUE",
            "project_id": "PROJ_ID",
            "time": "hh:mm:ss",
            "nvme_home": "/mnt/bb/$USER/",
            "nekrs_home": "$HOME/.local/nekrs",
            "nekrs_cache_bcast": 1,
            "nekrs_skip_build_only": 0,
            "striping_unit": 16777216,
            "max_striping_factor": 400,
            "striping_factor": "nodes/2 (up to max_striping_factor)"
        },
        "libraries": [
            "libnekrs.so",
            "libocca.so",
            "libnekrs-hypre-device.so",
            "libnekrs-hypre.so"
        ],
        "environment_variables": [
            "MPICH_GPU_SUPPORT_ENABLED",
            "PE_MPICH_GTL_DIR_amd_gfx90a",
            "PE_MPICH_GTL_LIBS_amd_gfx90a",
            "NEKRS_HOME",
            "NEKRS_GPU_MPI",
            "NVME_HOME",
            "MPICH_MPIIO_HINTS",
            "MPICH_MPIIO_STATS",
            "MPICH_OFI_NIC_POLICY",
            "NEKRS_CACHE_BCAST",
            "FI_CXI_RX_MATCH_MODE",
            "PMI_MMAP_SYNC_WAIT_TIME",
            "NEKRS_LOCAL_TMP_DIR",
            "LD_LIBRARY_PATH",
            "LD_PRELOAD"
        ]
    }
}