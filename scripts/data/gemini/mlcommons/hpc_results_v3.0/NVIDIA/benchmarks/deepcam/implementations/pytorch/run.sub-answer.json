{
    "application": "MLPerf",
    "details": {
        "framework": "PyTorch",
        "model": "deepcam",
        "slurm_allocation": "mlperf",
        "container_image": "CONT",
        "dgx_system": "DGXSYSTEM",
        "mlperf_ruleset": "3.0.0",
        "mlperf_cluster_name": "unknown",
        "num_experiments": "NEXP",
        "num_instances": "NUM_INSTANCES",
        "data_dir": "DATADIR",
        "log_dir": "LOGDIR",
        "power_command_dir": "POWERCMDDIR",
        "api_log_dir": "API_LOG_DIR",
        "cudnn_v8_api_enabled": "1",
        "nccl_async_error_handling": "0",
        "nccl_test": "1",
        "nccl_bisect": "0",
        "wireup_method": "nccl-slurm",
        "additional_srun_args": "ADDITIONAL_SRUN_ARGS",
        "comp_clock": "BASE_COMP_CLOCK",
        "mem_clock": "BASE_MEM_CLOCK",
        "dropcache_command": "sudo /sbin/sysctl vm.drop_caches=3",
        "set_maxq_clk": "0",
        "set_minedp_clk": "0",
        "maxq_clk": "BASE_COMP_CLOCK",
        "minedp_clk": "BASE_COMP_CLOCK",
        "dgx_nodes": "DGXRUNNODES",
        "dgx_gpus": "DGXNGPU",
        "local_batch_size": "LOCAL_BATCH_SIZE",
        "api_logging": "0",
        "jet": "0",
        "enable_gds": "0",
        "sbatch_network": "sharp",
        "sharp_debug": "0",
        "seed": "SEED",
        "logfile_base": "LOGDIR/slurm_${DATESTAMP}",
        "cont_name": "MODEL_NAME_${SLURM_JOB_ID}",
        "cont_mounts": "DATADIR:/data:ro,${LOGDIR}:/results:rw,/raid/scratch:/scratch:rw",
        "sprefix": "MODEL_NAME_MODEL_FRAMEWORK_DGXNNODESxDGXNGPUxLOCAL_BATCH_SIZExDATESTAMP",
        "json_model_name": "MLPERF_MODEL_NAME_MODEL_FRAMEWORK_train",
        "json_readme_link": "README_PREFIX/MODEL_NAME/MODEL_FRAMEWORK/README.md",
        "json_fmt": "{model_name: $mn, readme_link: $rl, configs: {($dt): [$bs]}, sweep: {($dt): [$bs]}}",
        "json_output": "JSON_MODEL_NAME.cudnn.json",
        "jet_dir": "JET_DIR",
        "jet_create": "JET_CREATE",
        "jet_upload": "JET_UPLOAD"
    }
}