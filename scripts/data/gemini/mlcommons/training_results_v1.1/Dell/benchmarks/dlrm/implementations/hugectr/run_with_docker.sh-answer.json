{
    "application": "DLRM",
    "details": {
        "software": [
            "Docker",
            "Python",
            "Bash",
            "mlperf_logging",
            "mlperf_logger"
        ],
        "resources": {
            "hardware": "DGX system",
            "gpu": "all",
            "network": "host",
            "volumes": [
                "/raid/datasets:/raid/datasets",
                "/gpfs/fs1:/gpfs/fs1",
                "/raid/datasets/criteo/mlperf/40m.limit_preshuffled/",
                "/workspace/dlrm/"
            ],
            "environment": {
                "DGXSYSTEM": "required",
                "CONT": "required",
                "NEXP": "optional, default: 5",
                "DATESTAMP": "optional, default: current date and time",
                "CLEAR_CACHES": "optional, default: 1",
                "MOUNTS": "optional, default: '/raid/datasets:/raid/datasets,/gpfs/fs1:/gpfs/fs1'",
                "LOGDIR": "optional, default: './results'",
                "DLRM_BIND": "optional, default: null"
            }
        }
    }
}