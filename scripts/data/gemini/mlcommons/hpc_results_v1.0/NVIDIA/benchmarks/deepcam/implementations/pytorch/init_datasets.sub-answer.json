{
    "application": "SLURM",
    "details": {
        "container_image": "mlperf-deepcam:v1.0",
        "dataset_input_dir": "<hdf5-dataset-path>",
        "dataset_output_dir": "<numpy-dataset-path>",
        "ranks_per_node": 1,
        "container_name": "mlperf_deepcam_convert_dataset",
        "container_mounts": "${DATASET_INPUT_DIR}:/data_in:ro,${DATASET_OUTPUT_DIR}:/data_out:rw",
        "python_script": "/opt/utils/convert_hdf52npy.py",
        "mpi_library": "pmix",
        "cluster_management_system": "SLURM",
        "container_runtime": "pyxis/enroot"
    }
}