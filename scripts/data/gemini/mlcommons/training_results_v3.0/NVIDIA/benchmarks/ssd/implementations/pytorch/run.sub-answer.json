{
    "application": "MLPerf",
    "details": {
        "framework": "PyTorch",
        "model": "Single Stage Detector",
        "resource_requirements": {
            "DGXSYSTEM": "DGXSYSTEM (environment variable)",
            "CONT": "CONTAINER (environment variable)",
            "MLPERF_RULESET": "3.0.0 (default)",
            "NEXP": "5 (default)",
            "DATESTAMP": "YYYYMMDDHHMMSSNNNN (default)",
            "CLEAR_CACHES": "1 (default)",
            "CHECK_COMPLIANCE": "1 (default)",
            "WORK_DIR": "/workspace/ssd (default)",
            "CONT_NAME": "single_stage_detector_${SLURM_JOB_ID} (default)",
            "LOGDIR": "./results (default)",
            "ABSLOGDIR": "${PWD}/results (default)",
            "POWERCMDDIR": "' ' (default)",
            "DROPCACHE_CMD": "sudo /sbin/sysctl vm.drop_caches=3 (default)",
            "SCRATCH_SPACE": "/raid/scratch (default)",
            "NVTX_FLAG": "0 (default)",
            "TIME_TAGS": "0 (default)",
            "NCCL_TEST": "1 (default)",
            "SYNTH_DATA": "0 (default)",
            "EPOCH_PROF": "0 (default)",
            "DISABLE_CG": "0 (default)",
            "API_LOGGING": "0 (default)",
            "API_LOG_DIR": "./api_logs (default)"
        },
        "container_mounts": {
            "DATADIR": "/datasets/open-images-v6",
            "LOGDIR": "/results",
            "BACKBONE_DIR": "/root/.cache/torch",
            "NVMLPERF_NSIGHT_LOCATION": "/nsight"
        },
        "requirements": {
            "Slurm": "For job management and resource allocation",
            "Python": "For running the MLPerf benchmark",
            "PyTorch": "For the deep learning framework",
            "NCCL": "For efficient communication between GPUs",
            "NVIDIA Nsight Systems": "For performance profiling (optional)",
            "Power monitor script": "For measuring power consumption (optional)"
        }
    }
}