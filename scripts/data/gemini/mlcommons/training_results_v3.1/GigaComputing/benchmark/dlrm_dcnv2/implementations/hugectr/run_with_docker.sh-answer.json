{
    "application": "DLRM",
    "details": {
        "software": [
            "Docker",
            "Python 3",
            "mlperf_logging",
            "HugeCTR"
        ],
        "resources": {
            "gpu": "all",
            "network": "host",
            "uts": "host",
            "ipc": "host",
            "volumes": [
                "/data",
                "/data_val",
                "/results",
                "/workspace/dlrm"
            ],
            "environment_variables": [
                "DGXSYSTEM",
                "CONT",
                "DATADIR",
                "NEXP",
                "DATESTAMP",
                "CLEAR_CACHES",
                "CHECK_COMPLIANCE",
                "MLPERF_RULESET",
                "MOUNTS",
                "LOGDIR",
                "DLRM_BIND",
                "MLPERF_SUBMISSION_ORG",
                "MLPERF_SUBMISSION_PLATFORM",
                "DATASET_TYPE"
            ]
        }
    }
}