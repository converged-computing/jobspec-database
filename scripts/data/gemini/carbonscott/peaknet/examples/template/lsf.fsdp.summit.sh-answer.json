{
    "application": "SLURM",
    "details": {
        "software": [
            "jsrun",
            "torchrun"
        ],
        "resources": {
            "nodes": {
                "type": "gpudefault",
                "count": "{{ num_nodes }}"
            },
            "cpus": {
                "per_task": "{{ cpu_per_task }}"
            },
            "gpus": {
                "per_node": "{{ num_gpus }}"
            },
            "environment_variables": [
                "OMP_NUM_THREADS",
                "NCCL_DEBUG",
                "TORCH_NCCL_BLOCKING_WAIT",
                "LOGLEVEL"
            ]
        },
        "libraries": [
            "c10d"
        ]
    }
}