{
    "application": "PIConGPU",
    "other_software": [
        "boost",
        "cupti",
        "papi/4.2.0",
        "cuda/5.0.35",
        "gdb",
        "pngwriter",
        "cmake",
        "hdf5/1.8.5-threadsafe",
        "gcc/4.6.2",
        "openmpi/1.6.2-gnu",
        "vampirtrace/gpu-gnu-cuda5.0"
    ],
    "resource_requirements": {
        "gpu_architecture": "fermi",
        "queue": "workq",
        "gpus_per_node": 4,
        "cores_per_node": 4,
        "nodes": "(( ( TBG_tasks + TBG_gpusPerNode -1 ) / TBG_gpusPerNode))",
        "walltime": "TBG_wallTime",
        "job_name": "TBG_jobNameShort",
        "mail_settings": "TBG_mailSettings",
        "mail_address": "TBG_mailAddress",
        "output_path": "TBG_dstPath/stdout",
        "error_path": "TBG_dstPath/stderr",
        "mpi_root": "MPI_ROOT",
        "program_params": "TBG_programParams",
        "author": "TBG_author"
    }
}