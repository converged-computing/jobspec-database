{
    "application": "srun",
    "details": {
        "scheduler": "Slurm",
        "job_name": "heat",
        "tasks": 1,
        "cpus_per_task": 32,
        "constraints": "AMD",
        "output_file": "out/run_32_cores_1024.log",
        "reservation": "fri",
        "time_limit": "00:10:00",
        "environment_variables": {
            "OMP_PLACES": "cores",
            "OMP_PROC_BIND": "TRUE",
            "OMP_NUM_THREADS": "32"
        },
        "executable": "prog",
        "arguments": "1024 1024 1 1"
    }
}