{
    "application": "HPCC",
    "details": {
        "software": {
            "HPCC": "1.5.0",
            "MPI": {
                "selection": "mpich",
                "debug": 1
            },
            "compiler": {
                "selection": "ARM/CLANG",
                "vendor": "ARM"
            }
        },
        "resources": {
            "nodes": 2,
            "partition": "arm8xlarge",
            "tasks_per_node": 64,
            "cpus_per_task": 1,
            "constraint": "c7g.8xlarge",
            "environment": {
                "OMP_NUM_THREADS": 1,
                "MKL_NUM_THREADS": 1
            },
            "ulimit": {
                "stacksize": "unlimited"
            }
        }
    }
}