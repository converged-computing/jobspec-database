{
    "application": "SLURM",
    "details": {
        "scheduler": "SLURM",
        "queue": "v100",
        "nodes": 2,
        "mpi_tasks": 8,
        "runtime": "00:30:00",
        "modules": [
            "conda",
            "py36pt"
        ],
        "libraries": [
            "mpiexec",
            "ibrun",
            "python"
        ],
        "scripts": [
            "cp_imagenet_to_temp_bin.sh",
            "examples/pytorch_imagenet_resnet.py"
        ],
        "commands": [
            "pwd",
            "date",
            "scontrol show hostnames $SLURM_NODELIST",
            "cat /tmp/hostfile"
        ]
    }
}