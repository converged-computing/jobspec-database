{
    "application": "SLURM",
    "details": {
        "resource_requirements": {
            "partition": "gpu20",
            "time_limit": "6 hours",
            "gpus": 2,
            "array_jobs": "1-5 (with 1% increment)"
        },
        "environment": {
            "conda_environment": "<path-to-conda-env>"
        },
        "script": {
            "name": "ddp_train_nerf.py",
            "config_file": "configs/noise/1.txt"
        },
        "logging": {
            "output_file": "<absolute-path-to-code>/slurmlogs/%A-%a.out"
        }
    }
}