{
    "application": "SLURM",
    "details": {
        "resource_manager": "SLURM",
        "partition": "gpu20",
        "time_limit": "6 hours",
        "gpu_count": 2,
        "array_task_ids": "1-5 (with step 1)",
        "output_file": "<absolute-path-to-code>/slurmlogs/%A-%a.out",
        "conda_environment": "<path-to-conda-env>",
        "python_script": "ddp_train_nerf.py",
        "config_file": "configs/ablation/noldist.txt",
        "other_tools": [
            "nvidia-smi"
        ]
    }
}