{
    "application": "sbatch",
    "details": {
        "software": [
            "python",
            "gcc",
            "java",
            "GeneWalk"
        ],
        "resources": [
            "priority queue",
            "32 GB memory",
            "3 days runtime",
            "8 threads"
        ],
        "modules": [
            "gcc/6.2.0",
            "python/3.6.0",
            "java/jdk-1.8u112"
        ],
        "environment": {
            "genewalkenv": "virtual environment",
            "PYTHONPATH": "includes the current directory"
        },
        "files": [
            "genewalk/cli_REV3TRAN.py",
            "REV3TRAN_MGIforINDRA_stmts.pkl",
            "REV3TRAN_HGNCidForINDRA.pkl",
            "REV3TRAN_forGW.csv"
        ],
        "output": {
            "log": "${baseDir}/LogErr/${jobname}.log",
            "error": "${baseDir}/LogErr/${jobname}.err",
            "python_environment": "${baseDir}/LogErr/${jobname}py.out"
        }
    }
}