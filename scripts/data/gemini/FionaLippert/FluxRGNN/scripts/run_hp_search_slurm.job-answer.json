{
    "application": "SLURM",
    "details": {
        "scheduler": "SLURM",
        "partition": "gpu_shared or gpu_short",
        "gres": "gpu:1",
        "job_name": "hp_search",
        "time": "120:00:00",
        "output": "slurm_output_%A_%a.out",
        "mail_type": "BEGIN,END",
        "mail_user": "f.lippert@uva.nl",
        "modules": [
            "fluxrgnn",
            "CUDA/11.0.2-GCC-9.3.0",
            "cuDNN/8.0.3.33-gcccuda-2020a",
            "NCCL/2.7.8-gcccuda-2020a"
        ],
        "python_script": "run.py",
        "dependencies": [
            "python",
            "hydra",
            "fluxrgnn"
        ],
        "data_path": "$WDIR/data/preprocessed",
        "config_path": "$CONFIGPATH",
        "hyperparameter_file": "$HPARAM_FILE"
    }
}