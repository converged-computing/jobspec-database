{
    "application": "SLURM",
    "details": {
        "software": [
            "SLURM",
            "Bash",
            "Nvidia-smi",
            "Jax"
        ],
        "resources": {
            "nodes": 1,
            "tasks": 1,
            "cpus": 4,
            "gpus": 1,
            "memory": "16G",
            "partition": "a40",
            "qos": "normal",
            "network": "NCCL",
            "network_options": {
                "NCCL_IB_DISABLE": 1,
                "NCCL_SOCKET_IFNAME": "bond0"
            }
        },
        "environment_variables": {
            "MASTER_ADDR": "hostname -s",
            "MASTER_PORT": 52069,
            "LOG_DIR": "input argument",
            "LOG_PATH": "dynamically constructed",
            "EXP_TYPE": "input argument",
            "TASK": "input argument",
            "MODEL_PATH": "input argument",
            "LR": "input argument",
            "LEN": "input argument"
        },
        "scripts": {
            "main_script": "input argument",
            "srun_script": "bash <main_script> ... "
        }
    }
}