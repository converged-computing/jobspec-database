{
    "application": "vllm",
    "details": {
        "requirements": {
            "cuda": "cuda-12.3",
            "python": "3",
            "virtual environment": "vllm venv",
            "libraries": [
                "vllm",
                "openai"
            ],
            "resources": {
                "cpus": 16,
                "memory": "64G",
                "time": "4 hours",
                "gpus": "NUM_GPUS"
            }
        },
        "configuration": {
            "model weights": "${VLLM_MODEL_WEIGHTS}",
            "data type": "${VLLM_DATA_TYPE}",
            "max logprobs": "${VLLM_MAX_LOGPROBS}",
            "server address": "http://${hostname}:${vllm_port_number}/v1"
        }
    }
}