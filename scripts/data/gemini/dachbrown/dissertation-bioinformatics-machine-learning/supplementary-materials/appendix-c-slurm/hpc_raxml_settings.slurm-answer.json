{
    "application": "RAxML",
    "details": {
        "software": "RAxML version 8.2.12 (or 8.2.4, or 7.4.2)",
        "resource_requirements": {
            "partition": "Pisces",
            "nodes": 1,
            "tasks_per_node": 6,
            "memory": "375GB",
            "time": "14-00:00:00",
            "modules": "raxml/${RAXML_VER}-mpi"
        },
        "input_files": [
            "core_gene_alignment.aln",
            "RAxML_bestTree.with_split"
        ],
        "output_files": [
            "900_ws_core_opt_PAIRWISE_DIST"
        ],
        "command_line_arguments": [
            "-s",
            "core_gene_alignment.aln",
            "-n",
            "900_ws_core_opt_PAIRWISE_DIST",
            "-N",
            "$SLURM_NTASKS_PER_NODE",
            "-f",
            "x",
            "-p",
            "1234",
            "-m",
            "GTRGAMMA",
            "-t",
            "RAxML_rootedTree.900_ws_core_opt.ROOTED"
        ]
    }
}