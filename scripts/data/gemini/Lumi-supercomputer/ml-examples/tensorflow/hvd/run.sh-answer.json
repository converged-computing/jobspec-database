{
    "application": "TensorFlow",
    "details": {
        "framework": "TensorFlow",
        "version": "2.10",
        "distribution": "Horovod",
        "accelerator": "ROCm 5.4.1",
        "resource_manager": "Slurm",
        "nodes": 4,
        "tasks_per_node": 8,
        "gpus_per_node": 8,
        "time_limit": "10 minutes",
        "partition": "standard-g",
        "account": "<project>",
        "modules": [
            "LUMI/22.08",
            "partition/G",
            "singularity-bindings",
            "aws-ofi-rccl",
            "OpenMPI"
        ],
        "environment_variables": [
            "NCCL_DEBUG",
            "NCCL_SOCKET_IFNAME",
            "MIOPEN_USER_DB_PATH",
            "MIOPEN_CUSTOM_CACHE_DIR",
            "CXI_FORK_SAFE",
            "CXI_FORK_SAFE_HP",
            "FI_CXI_DISABLE_CQ_HUGETLB",
            "SINGULARITYENV_LD_LIBRARY_PATH"
        ],
        "container": "tensorflow_rocm5.4.1-tf2.10-dev.sif",
        "benchmark": "tensorflow2_synthetic_benchmark.py",
        "batch_size": 512
    }
}