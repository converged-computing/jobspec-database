{
    "application": "Snudda",
    "details": {
        "scheduler": "Slurm",
        "partition": "main",
        "nodes": 2,
        "tasks": 256,
        "cpus_per_task": 2,
        "memory_per_cpu": "930M",
        "runtime": "1:59:00",
        "modules": "snic-env",
        "environment": {
            "OMP_STACKSIZE": "128G",
            "IPNWORKERS": 100,
            "IPYTHONDIR": "/cfs/klemming/scratch/${USER:0:1}/$USER/.ipython",
            "IPYTHON_PROFILE": "default",
            "FI_CXI_DEFAULT_VNI": "(generated randomly)"
        },
        "software": {
            "ipcontroller_new.sh": "Starts an IPython controller",
            "ipengine": "Starts IPython engines",
            "Dardel_runSnudda_lateral.sh": "Runs the Snudda application"
        }
    }
}