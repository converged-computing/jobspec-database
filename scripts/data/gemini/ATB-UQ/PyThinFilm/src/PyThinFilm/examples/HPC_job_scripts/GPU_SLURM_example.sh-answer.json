{
    "application": "PyThinFilm",
    "details": {
        "environment": {
            "modules": [
                "PrgEnv-cray",
                "rocm craype-accel-amd-gfx90a",
                "gcc/12.1.0",
                "gromacs-amd-gfx90a/2022.3.amd1_174",
                "hpc-python-collection/2022.11-py3.9.15"
            ],
            "variables": {
                "OMP_NUM_THREADS": "unset",
                "GMX_MAXBACKUP": "-1"
            }
        },
        "resources": {
            "slurm": {
                "partition": "gpu",
                "nodes": 1,
                "ntasks_per_node": 1,
                "gpus_per_node": 1,
                "sockets_per_node": 1,
                "cpus_per_task": 8,
                "time": "01:00:00",
                "account": "m72-gpu"
            },
            "mpi": {
                "template": "srun -l -u -c 8"
            },
            "mdrun": {
                "template": "{GMX_EXEC} mdrun -s {tpr} -x {xtc} -e {edr} -g {log} -cpo {cpo} -c {final} -nb gpu -bonded gpu -pin on -update gpu -ntomp 8 -ntmpi 1"
            }
        }
    }
}