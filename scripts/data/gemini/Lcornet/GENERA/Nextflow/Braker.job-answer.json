{
    "application": "Nextflow",
    "details": {
        "version": "21.08.0",
        "workflow": "Braker.nf",
        "resources": {
            "time": "20 days, 1 hour",
            "tasks": 1,
            "cpus_per_task": 20,
            "memory_per_cpu": "7500 MB",
            "partition": "bio",
            "environment": {
                "OMP_NUM_THREADS": 20,
                "MKL_NUM_THREADS": 20
            }
        },
        "inputs": {
            "genome": "<genome.fna>",
            "protein_database": "<BrakerDB>",
            "mode": "<mode>"
        }
    }
}