{
    "application": "fmriprep",
    "details": {
        "software": [
            "Singularity",
            "fmriprep",
            "FreeSurfer"
        ],
        "resources": {
            "nodes": 1,
            "cpus": 8,
            "memory": "24GB",
            "time": "24 hours"
        },
        "environment_variables": [
            "OPENNEURO_DSID",
            "BASEDIR",
            "BIDS_DIR",
            "FMRIPREP_HOME",
            "SING_CONTAINER",
            "OUTPUT_DIR",
            "WORK_DIR",
            "LOGS_DIR",
            "SINGULARITYENV_TEMPLATEFLOW_HOME",
            "SINGULARITYENV_FS_LICENSE"
        ],
        "dependencies": [
            "templateflow",
            "FreeSurfer license"
        ],
        "folders": [
            "bids",
            "derived",
            "work",
            "logs"
        ],
        "commands": [
            "singularity run",
            "sed",
            "head",
            "tail",
            "mkdir",
            "find",
            "rm",
            "echo"
        ]
    }
}