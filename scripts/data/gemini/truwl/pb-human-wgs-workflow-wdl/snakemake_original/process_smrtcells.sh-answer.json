{
    "application": "Snakemake",
    "details": {
        "software": [
            "Snakemake",
            "Conda"
        ],
        "resources": {
            "cluster": {
                "nodes": 1,
                "cores": 1,
                "cpus_per_task": 1,
                "account": "{cluster.account}",
                "partition": "{cluster.partition}",
                "cpus": "{cluster.cpus}",
                "output": "{cluster.out}",
                "extra": "{cluster.extra}"
            },
            "local": {
                "cores": 1,
                "jobs": 500,
                "max_jobs_per_second": 1
            },
            "latency_wait": 120
        }
    }
}