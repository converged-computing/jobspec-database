{
    "application": "jacobi",
    "details": {
        "environment": "Cray or AMD",
        "modules": [
            "PrgEnv-cray",
            "amd-mixed",
            "craype-accel-amd-gfx90a",
            "PrgEnv-amd"
        ],
        "gpu": {
            "type": "AMD gfx90a",
            "binding": "closest",
            "per_task": 4
        },
        "threads": {
            "total": 4,
            "binding": "spread"
        },
        "profiling": {
            "tool": "rocprof"
        },
        "build_system": [
            "cmake",
            "make"
        ]
    }
}