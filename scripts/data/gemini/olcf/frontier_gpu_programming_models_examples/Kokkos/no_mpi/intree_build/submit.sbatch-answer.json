{
    "application": "rocprof",
    "details": {
        "slurm_allocation": "stf007uanofn",
        "slurm_partition": "batch",
        "slurm_time_limit": "00:05:00",
        "slurm_nodes": 1,
        "modules": [
            "PrgEnv-cray",
            "amd-mixed"
        ],
        "environment_variables": [
            "OMP_PROC_BIND=spread",
            "OMP_PLACES=threads",
            "OMP_NUM_THREADS=4"
        ],
        "srun_arguments": [
            "-N1",
            "-n1",
            "-c4",
            "--gpus-per-task=1",
            "--gpu-bind=closest"
        ],
        "executable": "./make_build_dir/kokkos_example"
    }
}