{
    "application": "rocprof",
    "details": {
        "batch_system": "Slurm",
        "partition": "batch",
        "time": "00:05:00",
        "nodes": 1,
        "modules": [
            "PrgEnv-cray",
            "amd-mixed"
        ],
        "environment_variables": [
            "OMP_PROC_BIND=spread",
            "OMP_PLACES=threads",
            "OMP_NUM_THREADS=4"
        ],
        "srun_options": [
            "-N1",
            "-n4",
            "-c4",
            "--gpus-per-task=1",
            "--gpu-bind=closest"
        ],
        "executable": "./cmake_build_dir/kokkos_example",
        "output_directory": "results"
    }
}