{
    "application": "Jupyter Notebook",
    "software": [
        "dask",
        "jupyter",
        "dask-mpi"
    ],
    "resources": {
        "nodes": 4,
        "tasks": 128,
        "tasks_per_node": 32,
        "cpus_per_task": 1,
        "partition": "workq",
        "time": "02:00:00",
        "memory_limit": "4GB"
    },
    "modules": [
        "PrgEnv-cray",
        "PrgEnv-gnu",
        "dask/2.22.0",
        "jupyter/1.0.0"
    ],
    "environment_variables": [
        "XDG_RUNTIME_DIR",
        "LC_ALL",
        "LANG"
    ],
    "other": [
        "ssh tunneling",
        "dask dashboard",
        "SLURM job management system"
    ]
}