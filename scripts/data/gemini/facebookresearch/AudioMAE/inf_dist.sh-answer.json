            "/checkpoint/berniehuang/ast/egs/audioset/data/datafiles/eval_19k.json",
            "/checkpoint/berniehuang/ast/egs/audioset/data/class_labels_indices.csv"
        ],
        "model": "vit_base_patch16",
        "arguments": [
            "--log_dir",
            "/checkpoint/berniehuang/mae/as_exp/$SLURM_JOB_ID",
            "--output_dir",
            "/checkpoint/berniehuang/mae/as_exp/$SLURM_JOB_ID",
            "--dataset",
            "audioset",
            "--data_train",
            "/checkpoint/berniehuang/ast/egs/audioset/data/datafiles/train.json",
            "--data_eval",
            "/checkpoint/berniehuang/ast/egs/audioset/data/datafiles/eval_19k.json",
