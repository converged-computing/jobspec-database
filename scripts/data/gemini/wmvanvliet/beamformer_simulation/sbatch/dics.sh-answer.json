{
    "application": "SLURM",
    "details": {
        "resource_requests": {
            "time": "3:00:00",
            "memory_per_cpu": "8G",
            "number_of_nodes": 1,
            "job_name": "dics",
            "array_tasks": "0-3756",
            "output_file": "dics.out",
            "open_mode": "append"
        },
        "environment_setup": {
            "python_environment": "anaconda",
            "blas_threads": 1
        },
        "script_execution": {
            "script_path": "../dics.py",
            "arguments": "-v $SLURM_ARRAY_TASK_ID -n 0.1",
            "logging": {
                "log_file": "logs/dics.log",
                "prefix": "$VERTEX_NUMBER:  "
            }
        }
    }
}