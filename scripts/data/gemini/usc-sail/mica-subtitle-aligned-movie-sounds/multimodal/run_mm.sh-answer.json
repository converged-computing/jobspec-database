{
    "application": "SLURM",
    "details": {
        "scheduler": "SLURM",
        "nodes": 1,
        "tasks": 1,
        "cpus_per_task": 32,
        "memory": "32GB",
        "time": "1-00:00:00",
        "partition": "gpu",
        "gpus": "v100:2",
        "account": "shrikann_35",
        "modules": [
            "nvidia-hpc-sdk",
            "conda"
        ],
        "conda_environment": "/home1/rajatheb/.conda/envs/astmm2",
        "python_script": "./run_mm.py",
        "arguments": {
            "--data-train": "${tr_data}",
            "--data-val": "${val_data}",
            "--data-eval": "${test_data}",
            "--exp-dir": "$exp_dir",
            "--label-csv": "${label_csv}",
            "--n-class": "${n_class}",
            "--posembed": "${pe}",
            "--lr": "$lr",
            "--n-epochs": "${epoch}",
            "--batch-size": "$batch_size",
            "--save_model": "True",
            "--freqm": "$freqm",
            "--timem": "$timem",
            "--mixup": "${mixup}",
            "--tstride": "$tstride",
            "--fstride": "$fstride",
            "--imagenet_pretrain": "$imagenetpretrain",
            "--audioset_pretrain": "$audiosetpretrain",
            "--num-workers": "${num_workers}"
        },
        "output_file": "${exp_dir}/log.txt"
    }
}