{
    "application": "MPI",
    "details": {
        "batch_system": "Slurm",
        "partition": "debug",
        "tasks": 12,
        "tasks_per_node": 4,
        "mail_user": "<USERNAME>@alaska.edu",
        "mail_type": "BEGIN, END, FAIL",
        "output_file": "p4pdes.o.%j",
        "ulimit": "-s unlimited, -l unlimited",
        "mpi_implementation": "mpich2",
        "machinefile": "nodes.$SLURM_JOB_ID",
        "memory_requirements": "less than 16 Gb",
        "run_time_requirements": "less than 15 minutes for P >= 4 processes",
        "petsc_configuration": "--with-debugging=0",
        "dependencies": "PETSc"
    }
}