{
    "application": "Slurm",
    "details": {
        "resource_requirements": {
            "nodes": 1,
            "time": "1-00:00:00",
            "memory_per_cpu": "40G"
        },
        "environment_variables": {
            "PYTHONPATH": "...",
            "OE_LICENSE": ".../oe_license.txt"
        },
        "software": {
            "python": "./scripts/new_smirks.py",
            "mmpdb": [
                "fragment",
                "index"
            ]
        },
        "file_operations": {
            "input": [
                "$OUTPUT_DIR/pred_smiles_epoch_${EPOCH_NUM}.csv",
                "$DATA_DIR/${DATA_ID}_counted.csv"
            ],
            "output": [
                "$OUTPUT_DIR/pred_smiles_epoch_${EPOCH_NUM}_small.csv",
                "$OUTPUT_DIR/pred_smiles_epoch_${EPOCH_NUM}_mmpdb.csv",
                "$OUTPUT_DIR/pred_smiles_epoch_${EPOCH_NUM}.fragments",
                "$OUTPUT_DIR/pred_smiles_epoch_${EPOCH_NUM}_pairs.csv",
                "$OUTPUT_DIR/pred_smiles_epoch_${EPOCH_NUM}_pairs_unique.csv",
                "$OUTPUT_DIR/pred_smiles_epoch_${EPOCH_NUM}_parsed.csv",
                "$OUTPUT_DIR/pred_smiles_epoch_${EPOCH_NUM}_smirks.csv",
                "$OUTPUT_DIR/pred_smiles_epoch_${EPOCH_NUM}_newsmirks.csv"
            ],
            "directories": [
                "$OUTPUT_DIR",
                "$PNG_DEST"
            ]
        }
    }
}