{
    "application": "DrQA",
    "details": {
        "environment": {
            "cuda": "9.0.176",
            "cudnn": "9.0v7.0.5",
            "gcc": "6.3.0",
            "python": "3.6.3",
            "pytorch": "0.3.0_4",
            "corenlp": "installed and accessible"
        },
        "resources": {
            "nodes": 1,
            "tasks_per_node": 1,
            "cpus_per_task": 1,
            "memory": "64GB",
            "wall_time": "6:00:00",
            "gpu": 1
        },
        "script": {
            "name": "train.py",
            "location": "$SCRATCH/DrQA/scripts/reader/",
            "arguments": "--model-dir=$SCRATCH/DrQA/models"
        },
        "output": "output/train_%j.out"
    }
}