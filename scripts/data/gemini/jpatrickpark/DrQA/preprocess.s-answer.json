{
    "application": "SLURM",
    "details": {
        "resources": {
            "nodes": 1,
            "tasks_per_node": 1,
            "cpus_per_task": 4,
            "time": "4:00:00",
            "memory": "64GB"
        },
        "modules": [
            "cuda/9.0.176",
            "cudnn/9.0v7.0.5",
            "gcc/6.3.0"
        ],
        "environment_variables": {
            "RUNDIR": "$SCRATCH/my_project/run-${SLURM_JOB_ID/.*}",
            "SRCDIR": "$SCRATCH/DrQA/scripts/reader/",
            "CLASSPATH": "$CLASSPATH:$SCRATCH/data/corenlp/*"
        },
        "script": {
            "name": "preprocess.py",
            "path": "$SRCDIR/preprocess.py",
            "arguments": [
                "$SCRATCH/DrQA/data/datasets/",
                "$SCRATCH/DrQA/data/processed"
            ]
        }
    }
}