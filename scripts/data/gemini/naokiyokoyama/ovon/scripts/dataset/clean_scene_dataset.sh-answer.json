{
    "application": "SLURM",
    "details": {
        "resource_requirements": {
            "gpus": 1,
            "nodes": 1,
            "cpus_per_task": 7,
            "ntasks_per_node": 1,
            "signal": "USR1@100",
            "requeue": true,
            "constraint": "a40|rtx_6000|2080_ti",
            "partition": "short",
            "exclude": "calculon,alexa,cortana,bmo,c3po,ripl-s1,t1000,hal,irona,fiona"
        },
        "environment_variables": {
            "GLOg_minloglevel": 2,
            "HABITAT_SIM_LOG": "quiet",
            "MAGNUM_LOG": "quiet"
        },
        "python_environment": {
            "conda_environment": "ovon"
        },
        "script_execution": {
            "script_path": "ovon/dataset/clean_episodes.py",
            "arguments": "--path $scene_path --output-path $output_path"
        }
    }
}