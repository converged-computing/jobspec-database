{
    "application": "Gromacs",
    "details": {
        "environment": {
            "modules": [
                "modules/2.2-20230808",
                "openmpi/cuda-4.0.7",
                "gromacs/mpi-2023.1"
            ]
        },
        "slurm": {
            "nodes": 2,
            "tasks_per_node": 60,
            "cpus_per_task": 2,
            "constraints": "rome,ib",
            "partition": "ccb",
            "time": "01:00:00",
            "job_name": "slurm_gromacs_examplerun2",
            "output": "slurm_gromacs_examplerun2.log"
        },
        "openmp": {
            "threads_per_process": "$SLURM_CPUS_PER_TASK"
        },
        "system_information": [
            "lscpu",
            "nvidia-smi",
            "numactl -H"
        ],
        "compilation": {
            "command": "mpirun -np 1 gmx_mpi grompp -f gromacs_examplerun1.mdp -o gromacs_examplerun2.tpr -c step6.6_equilibration.gro -p topol.top -n index.ndx"
        },
        "execution": {
            "command": "mpirun --map-by socket:pe=$OMP_NUM_THREADS gmx_mpi mdrun -v -deffnm gromacs_examplerun2"
        }
    }
}