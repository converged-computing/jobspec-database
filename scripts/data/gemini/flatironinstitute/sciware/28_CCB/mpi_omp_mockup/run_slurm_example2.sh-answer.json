{
    "application": "SLURM",
    "details": {
        "software": [
            "openmpi",
            "mpirun",
            "mpi_omp_mockup"
        ],
        "resources": {
            "nodes": 2,
            "tasks_per_node": 8,
            "cpus_per_task": 16,
            "constraints": "rome,ib",
            "partition": "ccb",
            "time_limit": "00:10:00",
            "job_name": "mpi_omp_example2"
        },
        "environment": {
            "modules": [
                "openmpi"
            ],
            "environment_variables": [
                "OMP_NUM_THREADS",
                "SLURM_CPUS_PER_TASK"
            ]
        }
    }
}