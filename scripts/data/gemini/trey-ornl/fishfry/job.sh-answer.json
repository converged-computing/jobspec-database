{
    "application": "fishfry",
    "software": [
        "rocm",
        "craype-accel-amd-gfx90a",
        "mpich",
        "rocfft"
    ],
    "resources": [
        "gpu",
        "srun"
    ],
    "gpu_details": {
        "gpus_per_node": 8,
        "gpu_bind": "closest"
    },
    "runtime": "5:00",
    "environment_variables": [
        "LD_LIBRARY_PATH",
        "ROCFFT_RTC_CACHE_PATH",
        "MPICH_GPU_SUPPORT_ENABLED",
        "MPICH_OFI_NIC_POLICY",
        "MPICH_VERSION_DISPLAY"
    ]
}