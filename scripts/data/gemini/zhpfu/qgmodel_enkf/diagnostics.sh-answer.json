{
    "application": "LSF",
    "details": {
        "job_scheduler": "LSF",
        "queue": "regular",
        "nodes": 1,
        "processors": 16,
        "walltime": "1:00",
        "matlab_version": "Not specified in script, but assumed to be available on the system.",
        "matlab_script": "diagnostics_$vtype",
        "matlab_arguments": {
            "nens": "$nens",
            "obs_thin": "$obs_thin",
            "expname": "$expname",
            "casename": [
                "$casename",
                "num2str(i)"
            ],
            "n1": "$n1",
            "nt": "$nt",
            "dt": "$dt",
            "vtype": "$vtype"
        }
    }
}