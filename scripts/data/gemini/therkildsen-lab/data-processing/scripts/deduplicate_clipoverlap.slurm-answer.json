{
    "application": "SLURM",
    "details": {
        "script": "deduplicate_clipoverlap.sh",
        "resources": [
            {
                "name": "SERVER",
                "description": "Server and directory to mount to the computing node",
                "example": "cbsunt246 workdir/"
            },
            {
                "name": "BAMLIST",
                "description": "Path to a list of merged bam files on the computing node",
                "example": "/fs/cbsunt246/workdir/cod/greenland-cod/sample_lists/bam_list_merged.txt"
            },
            {
                "name": "SAMPLETABLE",
                "description": "Path to a sample table on the computing node",
                "example": "/fs/cbsunt246/workdir/cod/greenland-cod/sample_lists/sample_table.tsv"
            },
            {
                "name": "JAVA",
                "description": "Path to Java",
                "example": "java"
            },
            {
                "name": "PICARD",
                "description": "Path to Picard",
                "example": "/programs/picard-tools-2.9.0/picard.jar"
            },
            {
                "name": "BAMUTIL",
                "description": "Path to BamUtil",
                "example": "/programs/bamUtil/bam"
            },
            {
                "name": "ARRAY_LENGTH",
                "description": "Number of array jobs to divide adapter clipping into",
