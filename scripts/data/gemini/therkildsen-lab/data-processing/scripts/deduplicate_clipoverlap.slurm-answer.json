            },
            {
                "name": "SCRIPT",
                "description": "Path to the deduplicate_clipoverlap.sh script on the computing node",
                "example": "/fs/cbsunt246/workdir/data-processing/scripts/deduplicate_clipoverlap.sh"
            }
        ],
        "software": [
            "labutils",
            "awk",
            "sed",
            "grep"
        ]
    }
