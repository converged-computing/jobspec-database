{
    "application": "slurm",
    "details": {
        "scheduler": "slurm",
        "resources": {
            "memory": "20G",
            "time": "99:0:0",
            "partition": "gpu",
            "gpus": 2,
            "tasks_per_node": 2,
            "cpus_per_task": 6
        },
        "environment": {
            "conda_environment": "anaconda-python",
            "bash_profile": "~/.bashrc",
            "gcc_version": "4.9.2",
            "cuda_version": "7.5",
            "cudnn_version": "5.0",
            "scratch_path": "/scratch/users/hmei2@jhu.edu",
            "theano_flags": "device=gpu"
        },
        "python_script": "train_models.py",
        "script_arguments": {
            "data_directory": "./data/data_$dataname/",
            "model": "$model",
            "max_epochs": "$maxepoch",
            "lstm_dimension": "$dimlstm",
            "train_ratio": "$trainratio",
            "metric_type": 1,
            "metric_duration": 10,
            "batch_size": "$sizebatch",
            "track_period": "$trackperiod",
            "prune": "$prune",
            "seed": "$seed"
        }
    }
}