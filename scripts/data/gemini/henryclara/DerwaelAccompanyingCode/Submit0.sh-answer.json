{
    "application": "ElmerSolver",
    "software_requirements": [
        "intel-oneapi-mpi@2021.5.0%intel@2021.5.0",
        "intel-oneapi-compilers@2022.0.1",
        "intel-oneapi-mkl@2022.0.1%gcc@11.2.0",
        "metis@5.1.0%intel@2021.5.0"
    ],
    "resource_requirements": [
        "SLURM",
        "MPI (OpenMPI)",
        "UCX (Unified Communication X)",
        "HDF5",
        "ModulesPlusPaths2LoadIntelMPI.sh",
        "libpmi2.so",
        "FreeSurfaceSolver.so",
        "Mesh/result*",
        "Forward.sif.bak",
        "Submit.sh"
    ],
    "environment_variables": [
        "ELMER_HOME",
        "ELMER_SOLVER_HOME",
        "OMPI_MCA_pml",
        "OMPI_MCA_btl",
        "OMPI_MCA_osc",
        "UCX_IB_ADDR_TYPE",
        "OMPI_MCA_coll",
        "OMPI_MCA_coll_hcoll_enable",
        "HCOLL_ENABLE_MCAST_ALL",
        "HCOLL_MAIN_IB",
        "UCX_NET_DEVICES",
        "UCX_TLS",
        "UCX_UNIFIED_MODE",
        "HDF5_USE_FILE_LOCKING",
        "OMPI_MCA_io",
        "UCX_HANDLE_ERRORS",
        "I_MPI_PMI_LIBRARY"
    ],
    "system_limits": [
        "ulimit -s 102400",
        "ulimit -c 0"
    ],
    "commands": [
        "spack load",
        "export",
        "cp",
        "echo",
        "cd",
        "source",
        "sed",
        "make",
        "srun",
        "sbatch"
    ]
}