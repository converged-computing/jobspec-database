{
    "application": "ElmerSolver",
    "software": [
        "intel-oneapi-mpi@2021.5.0%intel@2021.5.0",
        "intel-oneapi-compilers@2022.0.1",
        "intel-oneapi-mkl@2022.0.1%gcc@11.2.0",
        "metis@5.1.0%intel@2021.5.0",
        "ElmerSolver_mpi"
    ],
    "resource_requirements": {
        "partition": "compute",
        "nodes": 80,
        "time": "08:00:00",
        "environment": {
            "OMPI_MCA_pml": "ucx",
            "OMPI_MCA_btl": "self",
            "OMPI_MCA_osc": "pt2pt",
            "UCX_IB_ADDR_TYPE": "ib_global",
            "OMPI_MCA_coll": "^ml,hcoll",
            "OMPI_MCA_coll_hcoll_enable": "0",
            "HCOLL_ENABLE_MCAST_ALL": "0",
            "HCOLL_MAIN_IB": "mlx5_0:1",
            "UCX_NET_DEVICES": "mlx5_0:1",
            "UCX_TLS": "mm,knem,cma,dc_mlx5,dc_x,self",
            "UCX_UNIFIED_MODE": "y",
            "HDF5_USE_FILE_LOCKING": "FALSE",
            "OMPI_MCA_io": "romio321",
            "UCX_HANDLE_ERRORS": "bt",
            "I_MPI_PMI_LIBRARY": "/usr/lib64/libpmi2.so"
        },
        "ulimit": {
            "stack_size": "102400",
            "core_size": "0"
        }
    },
    "additional_details": {
        "SLURM_directives": {
            "output": "Logs/SLURM_job.%j.%N.out",
            "error": "Logs/SLURM_job.%j.%N.err",
            "work_dir": ".",
            "job_name": "Forward",
            "get_user_env": true,
            "account": "bm1164",
            "mail_user": "clara.henry@mpimet.mpg.de",
            "export": "NONE"
        }
    }
}