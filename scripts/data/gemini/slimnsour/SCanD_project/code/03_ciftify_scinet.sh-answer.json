{
    "application": "fmriprep",
    "software": [
        "GNU Parallel",
        "Singularity",
        "FreeSurfer",
        "fmriprep",
        "ciftify"
    ],
    "resources": [
        "SLURM",
        "RAM disk",
        "BIDS dataset",
        "FreeSurfer license",
        "TemplateFlow",
        "Singularity container"
    ],
    "environment_variables": [
        "BASEDIR",
        "OPENNEURO_DS",
        "SLURM_SUBMIT_DIR",
        "SLURM_TMPDIR",
        "BIDS_DIR",
        "SING_CONTAINER",
        "OUTPUT_DIR",
        "LOGS_DIR",
        "WORK_DIR",
        "LOCAL_FREESURFER_DIR",
        "SLURM_ARRAY_TASK_ID",
        "SUB_SIZE",
        "CORES",
        "THREADS_PER_COMMAND",
        "SINGULARITYENV_TEMPLATEFLOW_HOME",
        "ORIG_FS_LICENSE",
        "SINGULARITYENV_FS_LICENSE"
    ],
    "input": "BIDS_DIR",
    "output": [
        "logs/%x_%j.out",
        "${LOGS_DIR}/${SLURM_JOB_NAME}.${SLURM_ARRAY_JOB_ID}.tsv"
    ],
    "dependencies": [
        "GNU Parallel",
        "Singularity",
        "FreeSurfer",
        "fmriprep",
        "ciftify",
        "TemplateFlow"
    ]
}