{
    "application": "Redis",
    "details": {
        "software": [
            "Redis",
            "LSF (Load Sharing Facility)",
            "jsrun (Job Submission Runtime)",
            "Spectrum MPI",
            "PMIX (Portable Message Interface)",
            "Redis-trib (Redis Cluster Management Tool)"
        ],
        "resources": {
            "nodes": "4",
            "queue": "excl",
            "walltime": "4:00",
            "memory": "10240",
            "environment": {
                "REDISHOME": "$REDIS_HOME",
                "REDISDUMP": "$RDB_HOME",
                "NODE_TIMEOUT": "120",
                "REPLICAS": "0",
                "PW": "foobared",
                "PORT": "1601",
                "TIMEOUT": "5000",
                "SCRIPT_DIR": "`pwd`",
                "WORK_DIR": "`pwd`"
            }
        }
    }
}