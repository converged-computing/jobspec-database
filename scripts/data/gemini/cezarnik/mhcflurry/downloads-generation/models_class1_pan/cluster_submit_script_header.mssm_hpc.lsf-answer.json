{
    "application": "LSF",
    "details": {
        "job_name": "MHCf-{work_item_num}",
        "allocation_account": "acc_nkcancer",
        "queue": "gpu",
        "gpu_request": "1 exclusive GPU",
        "node_request": "one node",
        "cores_request": "1",
        "walltime": "46:00",
        "memory_request": "30000 mb",
        "output_log": "{work_dir}/%J.stdout",
        "error_log": "{work_dir}/STDERR",
        "environment_initialization": "/bin/bash",
        "environment_variables": {
            "TMPDIR": "/local/JOBS/mhcflurry-{work_item_num}",
            "PATH": "$HOME/.conda/envs/py36b/bin/:$PATH",
            "PYTHONUNBUFFERED": "1",
            "KMP_SETTINGS": "1"
        },
        "modules": "cuda/10.0.130 cudnn/7.1.1",
        "python_version": "3.6",
        "tensorflow": "GPU AVAILABLE",
        "working_directory": "{work_dir}"
    }
}