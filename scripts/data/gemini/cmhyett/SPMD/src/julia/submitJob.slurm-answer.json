{
    "application": "SLURM",
    "details": {
        "resource_manager": "SLURM",
        "job_name": "SPMD",
        "output_file": "%x.out",
        "account": "${ACCOUNT}",
        "partition": "standard",
        "tasks": 6,
        "nodes": 1,
        "memory_per_cpu": "4gb",
        "time_limit": "1:00:00",
        "array_tasks": "1-10",
        "modules": "julia",
        "project_path": "${HOME}/SPMD/src/julia/",
        "output_path": "${PROJECT_PATH}/results/${SLURM_ARRAY_TASK_ID}/",
        "max_epochs": 500,
        "script": "runScript.jl",
        "language": "Julia"
    }
}