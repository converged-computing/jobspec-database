{
    "application": "SLURM",
    "details": {
        "job_scheduler": "SLURM",
        "resource_requirements": {
            "cores_per_node": 36,
            "numa_per_rank": 2,
            "ram_per_node": 120000,
            "cuda": {
                "enabled": false
            }
        },
        "command_execution": {
            "command": "$COMMAND",
            "mpi_implementation": "pmix",
            "ranks": "$NUM_RANKS",
            "ranks_per_node": "$RANKS_PER_NODE",
            "cores_per_rank": "$CORES_PER_RANK"
        },
        "environment_variables": [
            "HTR_DIR",
            "QUEUE"
        ],
        "shared_script": "jobscripts/jobscript_shared.sh"
    }
}