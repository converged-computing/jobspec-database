{
    "application": "JupyterLab",
    "details": {
        "environment": {
            "conda": "ml-gpu",
            "modules": [
                "miniconda",
                "JupyterLab/3.1.6-GCCcore-11.2.0"
            ]
        },
        "resources": {
            "gpu": {
                "count": 2,
                "exclusive": true,
                "memory": "8G",
                "model": "NVIDIAA40"
            },
            "memory": "8000",
            "queue": "gpu-interactive"
        },
        "script": {
            "output_file": "pyScripts/BarModels/logs/Output_jupyterinterface-%J.out",
            "error_file": "pyScripts/BarModels/logs/Error_jupyterinterface-%J.err",
            "job_name": "gettingJupyterEnv"
        }
    }
}