{
    "application": "BeeGenN",
    "details": {
        "environment": {
            "cuda": "11.1",
            "singularity": "3.4.0"
        },
        "resources": {
            "nodes": 1,
            "cpus": 2,
            "mpi_processes": 10,
            "memory": "128GB",
            "walltime": "07:59:00",
            "queue": "short_gpuQ"
        },
        "dependencies": {
            "squashfs-tools": "installed from source",
            "mksquashfs": "installed in ~/.local"
        },
        "docker_image": "giacthephantom/beegenn:sleep",
        "data_directory": "$PBS_O_WORKDIR/genn-network-model/data",
        "output_directory": "$PBS_O_WORKDIR/outputs"
    }
}