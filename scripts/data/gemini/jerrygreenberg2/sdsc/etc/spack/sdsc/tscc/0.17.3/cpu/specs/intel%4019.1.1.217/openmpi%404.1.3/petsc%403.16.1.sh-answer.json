{
    "application": "spack",
    "software": "PETSc 3.16.1",
    "compiler": "Intel 19.1.1.217",
    "dependencies": [
        "python@3.8.12",
        "openblas@0.3.17",
        "hdf5@1.10.7",
        "parmetis@4.0.3",
        "openmpi@4.1.3"
    ],
    "scheduler": "slurm",
    "resources": {
        "nodes": 1,
        "tasks_per_node": 1,
        "cpus_per_task": 16,
        "memory": "32G",
        "time": "00:30:00"
    },
    "environment": [
        "SPACK_VERSION=0.17.3",
        "SPACK_INSTANCE_NAME=cpu",
        "SPACK_INSTANCE_DIR=$(HOME)/cm/shared/apps/spack/0.17.3/cpu",
        "SYSTEM_NAME=expanse",
        "SCHEDULER_MODULE=slurm/expanse/current"
    ]
}