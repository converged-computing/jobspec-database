{
    "application": "SLURM",
    "software": [
        {
            "name": "OpenMM",
            "version": "7.5.0",
            "dependencies": [
                "cuda@11.2.2",
                "py-numpy@1.20.3",
                "openblas@0.3.17",
                "fftw@3.3.10"
            ],
            "compiler": "gcc@10.2.0",
            "variants": "+cuda cuda_arch=70,80 ~ipo",
            "package_manager": "spack",
            "package_manager_version": "0.17.3"
        },
        {
            "name": "CUDA",
            "version": "11.2.2"
        },
        {
            "name": "gcc",
            "version": "10.2.0"
        }
    ],
    "resources": [
        {
            "name": "CPU",
            "cores": 10
        },
        {
            "name": "RAM",
            "size": "93G"
        },
        {
            "name": "GPU",
            "count": 1
        }
    ],
    "scheduler": "SLURM",
    "scheduler_partition": "hotel",
    "scheduler_account": "sdsc",
    "scheduler_time_limit": "00:30:00",
    "environment_variables": [
        "SPACK_ROOT",
        "SPACK_INSTANCE_DIR",
        "CMAKE_LIBRARY_PATH",
        "OPENMM_PLUGIN_DIR",
        "CUDA_CUDA_LIBRARY",
        "SPACK_PACKAGE",
        "SPACK_COMPILER",
        "SPACK_VARIANTS",
        "SPACK_DEPENDENCIES",
        "SPACK_SPEC",
        "SYSTEM_NAME",
        "LOCAL_TIME",
        "UNIX_TIME",
        "SLURM_JOB_SCRIPT",
        "SLURM_JOB_MD5SUM",
        "SCHEDULER_MODULE",
        "COMPILER_MODULE",
        "CUDA_MODULE"
    ]
}