{
    "application": "SLURM",
    "software": [
        "spack",
        "hdf5",
        "intel",
        "metis"
    ],
    "resources": {
        "scheduler": "slurm",
        "partition": "ind-gpu-shared",
        "nodes": 1,
        "tasks_per_node": 1,
        "cpus_per_task": 10,
        "memory": "93G",
        "gpus": 1,
        "time": "00:30:00",
        "intel_license_file": "40000@elprado.sdsc.edu:40200@elprado.sdsc.edu",
        "spack_version": "0.17.3",
        "spack_instance_name": "gpu",
        "spack_instance_dir": "${HOME}/cm/shared/apps/spack/${SPACK_VERSION}/${SPACK_INSTANCE_NAME}",
        "spack_package": "hdf5@1.10.7",
        "spack_compiler": "intel@19.1.1.217",
        "spack_variants": "+cxx +fortran +hl ~ipo ~java ~mpi +shared +szip ~threadsafe +tools",
        "spack_dependencies": ""
    }
}