{
    "application": "KMeans",
    "details": {
        "software": {
            "COMPSs": "A framework for parallel and distributed computing",
            "DataClay": "A distributed in-memory data management system",
            "Java": "Programming language used for the application"
        },
        "resources": {
            "nodes": {
                "client": "1 node",
                "LM": "1 node",
                "DS": "Remaining nodes"
            },
            "memory": {
                "jvm_workers_opts": "-Xms1024m,-Xmx8496m,-Xmn400m"
            },
            "network": "infiniband",
            "storage": {
                "type": "DataClay",
                "configuration": "storage.properties"
            }
        },
        "execution": {
            "environment_flags": "--worker_working_dir=${JOBDIR} --network=infiniband",
            "debug_flags": "--summary --log_level=debug --tracing=false --graph=false",
            "storage_flags": "--storage_conf=$STORAGEPROPERTIES --task_execution=compss"
        }
    }
}