{
    "application": "Nextflow",
    "details": {
        "environment": {
            "operating_system": "Linux",
            "slurm_partition": "shared",
            "modules": [
                "cpu/0.15.4",
                "gpu/0.15.4",
                "slurm",
                "anaconda3/2020.11",
                "singularitypro/3.11"
            ],
            "conda_environment": "/home/$USER/a/conda_envs/nextflow"
        },
        "resources": {
            "memory": "2000M",
            "cpus": 2,
            "nodes": 1,
            "tasks_per_node": 1,
            "time_limit": "00:50:00"
        },
        "software": {
            "nextflow_config": "~/a/CIP_Nextflow_on_HPC/examples/nextflow_slurm/example5/nextflow.config",
            "nextflow_workflow": "~/a/CIP_Nextflow_on_HPC/examples/nextflow_slurm/example5/test.nf",
            "singularity_image": "Not specified in the script"
        }
    }
}