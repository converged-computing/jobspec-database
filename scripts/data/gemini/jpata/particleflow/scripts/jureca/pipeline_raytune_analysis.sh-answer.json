{
    "application": "Ray Tune",
    "details": {
        "slurm_account": "raise-ctp2",
        "partition": "dc-gpu",
        "time_limit": "1:00:00",
        "nodes": 1,
        "tasks_per_node": 1,
        "gpus_per_node": 4,
        "cpus_per_task": 128,
        "exclusive": true,
        "job_name": "tuneAnalysis",
        "output_log_dir": "logs_slurm",
        "modules": [
            "GCC/11.2.0",
            "CMake",
            "NCCL",
            "CUDA",
            "cuDNN",
            "OpenMPI"
        ],
        "conda_environment": "tf2",
        "python_version": "python3",
        "script_path": "mlpf/pipeline.py",
        "script_arguments": [
            "raytune-analysis",
            "$1"
        ]
    }
}