{
    "application": "SLURM",
    "details": {
        "job_name": "train_adafac",
        "partition": "gpu_p5",
        "nodes": 1,
        "tasks_per_node": 1,
        "gres": "gpu:1",
        "cpus_per_task": 8,
        "time": "12:00:00",
        "output": "gpu_mlp_nod_meta%j.out",
        "error": "gpu_mlp_nod_meta%j.out",
        "modules": [
            "cpuarch/amd",
            "cudnn/10.1-v7.5.1.10",
            "python/3.11.5"
        ],
        "environment": "venv3",
        "python_version": "3.11",
        "script": "src/main.py",
        "config": "config/meta_train/meta_train_mlp_image-mlp-fmst_schedule_3e-3_10000_d3.py",
        "num_local_steps": 4,
        "num_grads": 8,
        "local_learning_rate": 0.5,
        "tfds_data_dir": "$STORE/tf_datasets",
        "wandb_dir": "$STORE/jax_wandb"
    }
}