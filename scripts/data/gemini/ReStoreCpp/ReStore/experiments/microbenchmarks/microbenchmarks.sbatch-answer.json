{
    "application": "Slurm",
    "details": {
        "resource_requirements": {
            "nodes": 16,
            "tasks_per_node": 48,
            "cpus_per_task": 1,
            "time": "00:30:00",
            "memory": "80000mb",
            "partition": "test",
            "account": "<CHANGE>",
            "output_file": "logs/%x.%j.out"
        },
        "software_requirements": {
            "modules": [
                "gnu",
                "openmpi"
            ],
            "environment_variables": [
                "PMIX_MCA_gds",
                "MODE",
                "BENCHMARK_FILTER",
                "NUM_REPETITIONS"
            ],
            "executables": [
                "mpirun",
                "./microbenchmarks-$MODE"
            ]
        }
    }
}