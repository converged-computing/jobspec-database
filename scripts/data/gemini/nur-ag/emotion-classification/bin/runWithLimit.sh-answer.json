{
    "application": "SLURM",
    "details": {
        "job_name": "Emotions-CPU",
        "partition": "high",
        "tasks": 1,
        "cpus_per_task": 1,
        "memory": "32g",
        "output_file": "output/logs/emotions-cpu-%A-%a-%J.%N.out",
        "error_file": "output/logs/emotions-cpu-%A-%a-%J.%N.err",
        "array_range": "1-255",
        "modules": [
            "CUDA/10.0.130",
            "PyTorch/1.4.0-foss-2017a-Python-3.6.4-CUDA-10.0.130"
        ],
        "python_packages": "requirements.txt",
        "script": "./bin/runExperiment.sh",
        "config_path": "$CONFIG_PATH"
    }
}