{
    "application": "Streamlit",
    "details": {
        "job_scheduler": "PBS",
        "queue": "cheminf",
        "resources": {
            "nodes": 1,
            "cpus": 8,
            "gpus": 1,
            "memory": "8gb",
            "walltime": "720:00:00"
        },
        "conda_environment": {
            "path": "$CONDA_ENV_PATH"
        },
        "remote_host": {
            "hostname": "$REMOTE_HOST",
            "port": "$PORT"
        },
        "scratch_directory": "/scratch/$USER/"
    }
}