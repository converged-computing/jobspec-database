{
    "application": "SLURM",
    "software": [
        "anaconda/2023.03",
        "cuda/11.8",
        "deepspeed",
        "wandb",
        "safe-rlhf",
        "python",
        "torch.distributed.run",
        "bash"
    ],
    "resources": {
        "nodes": 2,
        "tasks_per_node": 1,
        "gpus_per_node": 8,
        "memory_per_node": "500GB",
        "time": "40:00:00",
        "partition": "gpu",
        "nodelist": [
            "g0004",
            "g0010"
        ],
        "environment_variables": [
            "NCCL_IB_HCA",
            "NCCL_IB_DISABLE",
            "NCCL_SOCKET_IFNAME",
            "NCCL_DEBUG",
            "WANDB_MODE",
            "PYTHONPATH",
            "LOGLEVEL",
            "MASTER_PORT",
            "OUTPUT_DIR",
            "MODEL_NAME_OR_PATH",
            "ZERO_STAGE"
        ]
    }
}