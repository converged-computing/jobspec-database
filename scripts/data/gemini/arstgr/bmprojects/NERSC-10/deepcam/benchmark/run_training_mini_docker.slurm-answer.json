            "DeepCAM",
            "NCCL",
            "UCX",
            "SLURM",
            "Shifter",
            "Docker"
        ],
        "resources": {
            "compute": {
                "nodes": 1,
                "tasks_per_node": 8,
                "cpus_per_task": 12,
                "gpus_per_node": 8
