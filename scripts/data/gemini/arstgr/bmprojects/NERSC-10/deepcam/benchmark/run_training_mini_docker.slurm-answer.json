            "storage": {
                "mounts": [
                    "/opt/microsoft:/opt/microsoft",
                    "/mnt/resource_nvme/deepcam:/workspace",
                    "/mnt/resource_nvme/deepcam/benchmark:/benchmark",
                    "/mnt/resource_nvme/deepcam/data:/data",
                    "/opt/nccl-tests:/opt/nccl-tests"
                ]
            },
            "networking": "Infiniband"
        },
        "environment_variables": [
            "UCX_IB_PCI_RELAXED_ORDERING",
            "CUDA_DEVICE_ORDER",
            "NCCL_DEBUG",
            "NCCL_IB_PCI_RELAXED_ORDERING",
            "NCCL_TOPO_FILE",
            "NCCL_SOCKET_IFNAME",
            "UCX_NET_DEVICES",
            "OMPI_MCA_coll_hcoll_enable",
            "LD_LIBRARY_PATH",
            "N10_DEEPCAM"
