{
    "application": "CaMa-Flood",
    "software": [
        "MPI",
        "OpenMP",
        "NetCDF",
        "Fortran"
    ],
    "resources": [
        "runoff forcing",
        "river map",
        "topography"
    ],
    "details": {
        "forcing": {
            "types": [
                "plain binary",
                "netCDF"
            ],
            "frequency": "hourly"
        },
        "map": {
            "types": [
                "plain binary",
                "netCDF"
            ],
            "parameters": [
                "channel width",
                "channel depth",
                "manning coefficient",
                "groundwater delay"
            ]
        },
        "restart": {
            "types": [
                "plain binary",
                "netCDF"
            ],
            "frequency": "monthly"
        },
        "output": {
            "types": [
                "plain binary",
                "netCDF"
            ],
            "frequency": "hourly",
            "variables": [
                "outflw",
                "storge",
                "fldfrc",
                "rivdph"
            ]
        }
    }
}