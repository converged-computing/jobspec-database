{
    "application": "Slurm",
    "details": {
        "job_name": "psana_legion",
        "dependency": "singleton",
        "nodes": 3,
        "time": "03:00:00",
        "qos": "regular",
        "constraint": "knl,quad,cache",
        "core_spec": 4,
        "image": "docker:stanfordlegion/psana-mpi:latest",
        "mail_type": "ALL",
        "account": "m2859",
        "psana_location": "$HOME/psana_legion/psana-legion",
        "data_location": "$SCRATCH/noepics_c24_s1_data/reg",
        "environment_variables": {
            "SIT_PSDM_DATA": "$HOST_DATA_DIR/d/psdm",
            "EAGER": 1,
            "KERNEL_KIND": "memory_bound"
        },
        "executable": "shifter python $HOST_PSANA_DIR/psana_legion/mpi_rax.py",
        "parameters": {
            "rounds": [
                20,
                40,
                80,
                160
            ],
            "n": [
                2,
                1
            ],
            "c": [
                256,
                128,
                64,
                32,
                16
            ]
        },
        "output_file": "rax_rounds\"$rounds\"_n\"$n\"_c\"$c\".log"
    }
}