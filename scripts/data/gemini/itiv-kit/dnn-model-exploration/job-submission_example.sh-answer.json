{
    "application": "Slurm",
    "details": {
        "job_name": "model_exploration",
        "output": "results/exploration_%A_%a.out",
        "error": "results/exploration_%A_%a.err",
        "nodes": 1,
        "tasks": 16,
        "cpus_per_task": 1,
        "memory_per_cpu": "2048mb",
        "partition": "normal",
        "gpu": "4g.20gb:1",
        "time": "08:00:00",
        "array": "0-2",
        "runpath": "$HOME/projekte/mixed-precision-dnns",
        "venv": "$HOME/venvs/torch_exploration",
        "python_script": "model_explorer/scripts/evaluate_individual.py",
        "arguments": "workloads/resnet50.yaml dummy --progress -n 20"
    }
}