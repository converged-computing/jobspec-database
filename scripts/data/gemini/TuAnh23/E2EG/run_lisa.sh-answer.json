{
    "application": "SLURM",
    "details": {
        "partition": "gpu_shared",
        "gpu": "1",
        "tasks": "1",
        "cpus_per_task": "6",
        "job_name": "ExampleJob",
        "time": "12 hours",
        "output": "slurm_output_%A.out",
        "modules": [
            "Anaconda3/2021.05",
            "2021"
        ],
        "environment": "giant-xrt",
        "software": [
            "nvidia-smi",
            "bash",
            "python",
            "wandb",
            "multi_task_pipeline.sh",
            "encode_mtask.sh",
            "hyperparams_sweep.sh"
        ],
        "data": {
            "dataset": "ogbn-arxiv",
            "subset": "optional",
            "download_script": "download_data.sh",
            "processing_script": "proc_data_multi_task.sh"
        },
        "experiment_name": "mtask_roberta",
        "runs": "1",
        "params_path": "data/proc_data_multi_task/params_mtask_${dataset}${subset}.json"
    }
}