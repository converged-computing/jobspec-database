{
    "application": "Singularity",
    "details": {
        "environment": "TensorFlow 1.10.1-devel-gpu-py3",
        "resources": {
            "nodes": 1,
            "tasks_per_node": "{tasks_per_node}",
            "memory": "{mem}G",
            "gpu": 1,
            "partition": "{partition}",
            "time": "{time}",
            "comment": "{identifier}",
            "job_name": "mk13_{i}_{log_name}",
            "output_file": "/scratch/tmp/m_kais13/slogs/{log_name}_{identifier}.log",
            "mail_type": "ALL",
            "mail_user": "moritz.kaiser@uni-muenster.de"
        },
        "python_script": "{python_script}"
    }
}