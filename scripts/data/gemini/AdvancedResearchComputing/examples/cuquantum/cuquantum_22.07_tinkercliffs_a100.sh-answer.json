{
    "application": "cuQuantum",
    "software": [
        "apptainer",
        "python",
        "cuQuantum",
        "Nvidia cuQuantum Appliance"
    ],
    "resource_requirements": {
        "partition": "a100_normal_q",
        "nodes": 1,
        "gpus": 1,
        "cpus": 8,
        "memory": "256GB"
    },
    "container": "/global/arcsingularity/cuquantum-appliance_22.07-cirq.sif",
    "container_source": "docker://nvcr.io/nvidia/cuquantum-appliance_22.07-cirq",
    "bind_options": [
        "/home/$USER,/projects",
        "/globalscratch/$USER"
    ],
    "examples": [
        "ghz.py",
        "hidden_shift.py",
        "simon.py"
    ]
}