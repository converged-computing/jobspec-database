{
    "application": "cuquantum",
    "details": {
        "resource_requirements": {
            "partition": "a100_normal_q",
            "nodes": 1,
            "gpus": 1,
            "cpus": 8,
            "memory": "256GB"
        },
        "container_image": {
            "name": "cuquantum-appliance_23.06.sif",
            "source": "docker://nvcr.io/nvidia/cuquantum-appliance:23.06"
        },
        "container_runtime": "apptainer",
        "libraries": [
            "cuquantum"
        ],
        "examples_directory": "/home/cuquantum/examples",
        "examples_run": [
            "ghz.py",
            "hidden_shift.py",
            "simon.py"
        ],
        "output_files": [
            "ghz_out.$SLURM_JOBID.txt",
            "hidden_shift_out.$SLURM_JOBID.txt",
            "simon_out.$SLURM_JOBID.txt"
        ]
    }
}