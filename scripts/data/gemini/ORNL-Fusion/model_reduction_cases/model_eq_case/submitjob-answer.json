{
    "application": "SLURM",
    "details": {
        "partition": "regular",
        "account": "atom",
        "nodes": 10,
        "tasks_per_node": 1,
        "cpus_per_task": 64,
        "time": "48:00:00",
        "job_name": "ips_fastran",
        "error_file": "ips.err",
        "output_file": "ips.out",
        "image": "docker:registry.services.nersc.gov/rwp53/ips-massive-serial:latest",
        "volume": "/global/cscratch1/sd/rwp/tmpfiles:/tmp:perNodeCache=size=1G",
        "modules": "gcc",
        "conda_environment": "/global/common/software/atom/cori/adaptive/conda",
        "efit_bin_dir": "/global/common/software/atom/cori/binaries/efit/default",
        "efit_bin_name": "efitd90",
        "python_script": "ips.py",
        "platform_config": "platform.conf",
        "simulation_config": "ips.ml_train.config",
        "scratch_directory": "$SCRATCH/$WORK_DIRECTORY"
    }
}