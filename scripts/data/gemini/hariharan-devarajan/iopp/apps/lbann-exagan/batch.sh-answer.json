        "DISTCONV_OVERLAP_HALO_EXCHANGE",
        "LBANN_DISTCONV_HALO_EXCHANGE",
        "LBANN_DISTCONV_TENSOR_SHUFFLER",
        "LBANN_DISTCONV_CONVOLUTION_FWD_ALGORITHM",
        "LBANN_DISTCONV_CONVOLUTION_BWD_DATA_ALGORITHM",
        "LBANN_DISTCONV_CONVOLUTION_BWD_FILTER_ALGORITHM",
        "LBANN_DISTCONV_RANK_STRIDE",
        "LBANN_KEEP_ERROR_SIGNALS",
        "MV2_USE_RDMA_CM",
        "AL_PROGRESS_RANKS_PER_NUMA_NODE",
        "OMP_NUM_THREADS",
        "IBV_FORK_SAFE",
        "HCOLL_ENABLE_SHARP",
