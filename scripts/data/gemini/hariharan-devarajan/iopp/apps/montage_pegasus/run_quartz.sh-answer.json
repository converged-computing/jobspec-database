{
    "application": "montage-workflow",
    "details": {
        "software": [
            "Slurm",
            "spack",
            "mpifileutils",
            "htcondor",
            "montage-workflow-v3"
        ],
        "resources": [
            "/usr/workspace/iopp/install_scripts/bin/iopp-init",
            "/usr/workspace/iopp/install_scripts/bin/spack-init",
            "/usr/workspace/iopp/applications/montage-workflow-v3",
            "/usr/WS2/iopp/software/htcondor-V9.0.6/install/bin/condor.sh",
            "/usr/WS2/iopp/software/htcondor-V9.0.6/install/sbin/condor_master",
            "$pfs/job/$USER",
            "sites.quartz.yml",
            "pegasus.properties"
        ],
        "environment": {
            "CPWD": "$PWD",
            "JOB_DIR": "$pfs/job/$USER"
        }
    }
}