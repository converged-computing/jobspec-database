{
    "application": "DLIO",
    "details": {
        "software": [
            "COBALT",
            "aprun",
            "Python",
            "TensorFlow 2.2",
            "Darshan"
        ],
        "resources": {
            "nodes": "COBALT_JOBSIZE",
            "ranks_per_node": 1,
            "cores": 64,
            "threads_per_core": 2,
            "data_dir": "/projects/datascience/dhari/dlio_datasets",
            "darshan_preload": "/soft/perftools/darshan/darshan-3.1.8/lib/libdarshan.so"
        },
        "libraries": {
            "datascience/tensorflow-2.2": "module load datascience/tensorflow-2.2"
        },
        "environment_variables": [
            "PYTHONPATH",
            "OMP_NUM_THREADS",
            "DXT_ENABLE_IO_TRACE",
            "LD_PRELOAD"
        ]
    }
}