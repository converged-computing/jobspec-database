{
    "application": "SLURM",
    "details": {
        "resource_requirements": {
            "job_name": "mlcommons-cloudmask-{experiment.card_name}-{experiment.gpu_count}-{experiment.epoch}-{experiment.repeat}",
            "output": "cloudmask-%j.log",
            "error": "cloudmask-%j.error",
            "partition": "{system.partition}",
            "account": "{system.allocation}",
            "reservation": "{system.reservation}",
            "constraint": "{system.constraint}",
            "cpu_num": "{experiment.cpu_num}",
            "memory": "{experiment.mem}",
            "gpu": "{experiment.card_name}:{experiment.gpu_count}",
            "cpus_per_task": 1,
            "mail_user": "%u@virginia.edu",
            "mail_type": "ALL"
        },
        "software_requirements": {
            "modules": [
                "anaconda",
                "singularity tensorflow/2.8.0",
                "cudatoolkit/11.0.3-py3.8",
                "cuda/11.4.2",
                "cudnn/8.2.4.15",
                "anaconda/2020.11-py3.8",
                "gcc"
            ],
            "conda_environment": "cloudenv",
            "python_version": "3.8"
        }
    }
}