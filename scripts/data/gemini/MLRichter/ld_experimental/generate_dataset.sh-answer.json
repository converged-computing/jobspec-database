{
    "application": "SLURM",
    "details": {
        "modules": [
            "openmpi",
            "cuda/11.7"
        ],
        "environment_variables": [
            "NCCL_PROTO=simple",
            "FI_EFA_FORK_SAFE=1",
            "FI_LOG_LEVEL=1",
            "FI_EFA_USE_DEVICE_RDMA=1",
            "NCCL_DEBUG=info",
            "PYTHONFAULTHANDLER=1",
            "CUDA_LAUNCH_BLOCKING=0",
            "OMPI_MCA_mtl_base_verbose=1",
            "FI_EFA_ENABLE_SHM_TRANSFER=0",
            "FI_PROVIDER=efa",
            "FI_EFA_TX_MIN_CREDITS=64",
            "NCCL_TREE_THRESHOLD=0",
            "PYTHONWARNINGS=ignore",
            "CXX=g++",
            "MASTER_ADDR",
            "MASTER_PORT=33751"
        ],
        "conda_environment": true,
        "python_script": "wiring_coco_inference.py",
        "slurm_configuration": {
            "account": "laion",
            "partition": "g40x",
            "nodes": 1,
            "gres": "gpu:1",
            "ntasks_per_node": 1,
            "time": "12:00:00",
            "job_name": "dataset_generation"
        }
    }
}