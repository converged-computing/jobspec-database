{
    "application": "Python",
    "details": {
        "environment": {
            "operating_system": "Red Hat Enterprise Linux 8",
            "modules": [
                "rhel8/default-amp",
                "python/3.7",
                "cuda/11.1",
                "cudnn/8.0_cuda-11.1"
            ],
            "virtual_environment": ".venv-slurm"
        },
        "libraries": [
            "PyTorch",
            "NumPy",
            "SciPy"
        ],
        "resources": {
            "compute": {
                "nodes": 1,
                "cpus_per_task": 32,
                "gpus_per_node": 1,
                "openmp_threads": 1
            },
            "storage": {
                "workdir": "$SLURM_SUBMIT_DIR"
            },
            "queue": "ampere"
        }
    }
}