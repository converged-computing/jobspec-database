{
    "application": "zsh",
    "software": [
        "oh-my-zsh",
        "flux",
        "google-cloud-sdk",
        "kubectl",
        "yarn",
        "nvm",
        "go",
        "vim",
        "mvim",
        "docker",
        "git"
    ],
    "resources": [
        "~/.oh-my-zsh",
        "~/.local/bin",
        "~/go/bin",
        "~/.nvm",
        "~/.path",
        "~/.exports",
        "~/.aliases",
        "~/.functions",
        "~/.extra",
        "~/.kubectl_aliases",
        "/usr/local/bin",
        "/usr/local/google-cloud-sdk/path.bash.inc",
        "/usr/local/google-cloud-sdk/completion.bash.inc",
        "/snap/bin"
    ]
}