{
    "application": "hpcrun",
    "details": {
        "batch_system": "SLURM",
        "nodes": 1,
        "tasks_per_node": 8,
        "exclusive_execution": true,
        "time_limit": "00:05:00",
        "modules": [
            "OpenMPI"
        ],
        "environment_variables": [
            {
                "name": "OMP_NUM_THREADS",
                "value": "2"
            },
            {
                "name": "OMP_WAIT_POLICY",
                "value": "active"
            },
            {
                "name": "KMP_BLOCKTIME",
                "value": "infinite"
            }
        ],
        "executable": "./amg2006",
        "arguments": [
            "-P",
            "2",
            "2",
            "1",
            "-n",
            "2",
            "2",
            "4",
            "-r",
            "10",
            "10",
            "10"
        ],
        "output_file": "hpctoolkit-all-measurements"
    }
}