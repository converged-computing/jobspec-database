{
    "application": "MPI",
    "details": {
        "job_scheduler": "PBS",
        "resources": {
            "nodes": 4,
            "walltime": "1:15:00",
            "queue": "preemptable",
            "filesystems": "home"
        },
        "environment": {
            "spack": "polaris",
            "wandb": "offline"
        },
        "mpi_configuration": {
            "ranks_per_node": 4,
            "depth": 8,
            "threads_per_rank": 1,
            "cpu_binding": "depth"
        },
        "python_script": "main.py",
        "configuration_file": "experiments_polaris.yaml",
        "config": "baseline_naive_resnet_scale",
        "log_level": "info"
    }
}