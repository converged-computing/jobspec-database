{
    "application": "SLURM",
    "details": {
        "scheduler": "SLURM",
        "partition": "smp",
        "time_limit": "12 hours",
        "memory": "512GB",
        "nodes": 1,
        "tasks": 1,
        "cpus_per_task": 128,
        "threads_per_core": 1,
        "modules": [
            "apps/julia"
        ],
        "environment_variables": {
            "OMP_NUM_THREADS": 1,
            "JULIA_NUM_THREADS": 128
        },
        "code": "julia -t 128 --project=examples examples/Africa_run.jl"
    }
}