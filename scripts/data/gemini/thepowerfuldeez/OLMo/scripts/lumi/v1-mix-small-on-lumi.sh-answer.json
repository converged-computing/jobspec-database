{
    "application": "SLURM",
    "details": {
        "software": [
            "Singularity",
            "Python",
            "PyTorch",
            "ROCm",
            "NCCL",
            "MIOPEN",
            "libFabric",
            "CXI",
            "JSON-C"
        ],
        "resources": {
            "nodes": 32,
            "tasks_per_node": 8,
            "gpus_per_node": 8,
            "cpus_per_task": 6,
            "memory": "all",
            "time": "48:00:00",
            "time_min": "24:00:00",
            "partition": "standard-g"
        },
        "environment_variables": [
            "OLMO_CONTAINER",
            "OMP_NUM_THREADS",
            "MPICH_GPU_SUPPORT_ENABLED",
            "NCCL_SOCKET_IFNAME",
            "NCCL_NET_GDR_LEVEL",
            "MIOPEN_USER_DB_PATH",
            "MIOPEN_CUSTOM_CACHE_DIR",
            "CXI_FORK_SAFE",
            "CXI_FORK_SAFE_HP",
            "FI_CXI_DISABLE_CQ_HUGETLB",
            "FI_CXI_DEFAULT_CQ_SIZE",
            "NCCL_DEBUG",
            "PYTHONPATH",
            "ROCM_PATH",
            "SINGULARITYENV_LD_LIBRARY_PATH",
            "PYTORCH_HIP_ALLOC_CONF",
            "DATA_PATH",
            "CHECKPOINTS_PATH",
            "EVAL_DATA_PATH"
        ],
        "script": "scripts/train.py configs/v1-mix-small.yaml --run_name=${SLURM_JOB_ID} ${@}"
    }
}