{
    "application": "Snakemake",
    "details": {
        "software": [
            "conda",
            "cookiecutter",
            "snakemake",
            "singularity",
            "bsub",
            "dot"
        ],
        "resources": {
            "cluster": {
                "queue": "oversubscribed",
                "group": "team152",
                "memory": "10000 MB",
                "cores": "1",
                "memlimit": "True",
                "span": "hosts=1"
            },
            "snakemake": {
                "jobs": "50",
                "latency_wait": "90",
                "default_resources": {
                    "threads": "1",
                    "mem_mb": "2000"
                },
                "cluster_config": "config_re_embed_after_within_lineage.yaml"
            }
        }
    }
}