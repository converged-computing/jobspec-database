{
    "application": "SLURM",
    "details": {
        "software": [
            "diploSHIC",
            "diploSHIC_NORMALIZATION",
            "python"
        ],
        "resources": {
            "partition": "phillips",
            "job_name": "12stats",
            "walltime": "200:00:00",
            "nodes": 1,
            "tasks": 1,
            "array": "0-4999%100",
            "cpus_per_task": 1,
            "environment": {
                "OMP_NUM_THREADS": "$SLURM_CPUS_PER_TASK"
            }
        },
        "input": {
            "vcf_files": [
                "sim30rep",
                "sim70rep",
                "simmut15",
                "neutral_extra",
                "balancing",
                "decay",
                "decay_balancing",
                "exponent",
                "fluctuations"
            ]
        },
        "output": {
            "files": [
                "12stats.txt",
                "12stats_1mb_25.txt",
                "12stats_1mb_25_short.txt"
            ]
        }
    }
}