{
    "application": "SLURM",
    "details": {
        "job_name": "JetsTrain%j",
        "output_file": "slurm_out/JetsTrain%j.out",
        "error_file": "slurm_out/JetsTrain%j.err",
        "time_limit": "hh:mm:ss",
        "gpu_request": "--gres=gpu:1",
        "partition": "gpu",
        "nodes": 1,
        "memory_per_node": "12000",
        "email_notifications": "END,FAIL",
        "email_address": "henrion@nyu.edu",
        "python_script": "train.py",
        "python_arguments": "$PYTHONARGS",
        "environment_variables": {
            "SRCDIR": "$1",
            "PYTHONARGS": "$@",
            "SLURM_ARRAY_JOB_ID": "$SLURM_ARRAY_JOB_ID",
            "SLURM_ARRAY_TASK_ID": "$SLURM_ARRAY_TASK_ID"
        }
    }
}