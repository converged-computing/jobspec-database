{
    "application": "AMREX",
    "software": [
        "MPI",
        "CUDA",
        "OpenMP",
        "JSRun",
        "NSys",
        "NV-Sight-CU-CLI"
    ],
    "resources": [
        "Summit",
        "GPU",
        "CPU",
        "HBM",
        "Nodes"
    ],
    "parameters": {
