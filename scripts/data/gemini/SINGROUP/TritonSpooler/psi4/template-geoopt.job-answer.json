{
    "application": "geoopt.py",
    "software": [
        "psi4env",
        "iomklc/triton-2017a",
        "cmake/3.12.1"
    ],
    "resources": [
        {
            "name": "QUEUE",
            "type": "queue"
        },
        {
            "name": "TIMELIM",
            "type": "time limit"
        },
        {
            "name": "CPUTYPE",
            "type": "CPU type"
        },
        {
            "name": "NTHR",
            "type": "number of threads"
        },
        {
            "name": "MEMLIM",
            "type": "memory limit per CPU"
        }
    ],
    "environment_variables": [
        {
            "name": "OMP_PROC_BIND",
            "value": "true"
        },
        {
            "name": "PSI_SCRATCH",
            "value": "/tmp/"
        }
    ]
}