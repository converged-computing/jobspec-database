        "ppn": "1",
        "gpus": "1",
        "email": "coreyker@gmail.com",
        "mail_notification": "abe",
        "modules": [
            "python/2.7.3",
            "cuda/6.5"
        ],
        "environment_variables": [
            "LD_LIBRARY_PATH=~/.local/lib:$LD_LIBRARY_PATH"
