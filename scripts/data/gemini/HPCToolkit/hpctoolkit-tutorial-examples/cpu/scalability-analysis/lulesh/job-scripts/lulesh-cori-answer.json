{
    "application": "lulesh",
    "software": [
        "hpcrun",
        "openmp",
        "hpctoolkit"
    ],
    "resource_requirements": {
        "queue": "debug",
        "time": "5",
        "nodes": "1",
        "tasks_per_node": "1",
        "cpus_per_task": "64",
        "constraint": "knl",
        "output_file": "log.run.out",
        "error_file": "log.run.stderr",
        "openmp_threads": [
            "16",
            "32"
        ],
        "cpu_binding": "cores"
    }
}