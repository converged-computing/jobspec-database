{
    "application": "toil-wdl-runner",
    "software": [
        "toil-wdl-runner",
        "miniwdl",
        "Singularity",
        "slurm",
        "awk"
    ],
    "resources": {
        "cpu": 4,
        "memory": "200gb",
        "time": "1-0:00",
        "storage": [
            "/data/tmp",
            "qc_logs",
            "qc_bigstore",
            "outputs/cache/.singularity/cache",
            "outputs/cache/.cache/miniwdl",
            "../initial_qc/qc_input_jsons",
            "analysis/qc"
        ],
        "environment_variables": [
            "TOIL_SLURM_ARGS",
            "TOIL_COORDINATION_DIR",
            "SINGULARITY_CACHEDIR",
            "MINIWDL__SINGULARITY__IMAGE_CACHE"
        ]
    }
}