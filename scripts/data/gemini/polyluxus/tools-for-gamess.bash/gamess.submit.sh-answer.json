{
    "application": "GAMESS",
    "software_details": {
        "name": "GAMESS",
        "version": "unknown",
        "description": "General Atomic and Molecular Electronic Structure System",
        "website": "https://www.msg.chem.iastate.edu/gamess/gamess.html"
    },
    "resource_requirements": {
        "memory": {
            "unit": "MB",
            "default": "512",
            "description": "Total memory requested, including overhead for GAMESS",
            "option": "-m"
        },
        "cpu": {
            "description": "Number of CPUs to be used",
            "option": "-p"
        },
        "disk": {
            "description": "Disk space requested",
            "option": "-d",
            "unit": "MB"
        },
        "walltime": {
            "description": "Maximum walltime for the job",
            "option": "-w",
            "format": "[[HH:]MM:]SS",
            "default": "unknown"
        },
        "queueing_system": {
            "description": "Type of job script to be produced",
            "option": "-Q",
            "types": [
                "pbs-gen",
                "slurm-gen",
                "slurm-rwth",
                "bsub-gen",
                "bsub-rwth"
            ],
            "default": "unknown"
        },
        "project": {
            "description": "Account (SLURM) or Project (BSUB) for statistics",
            "option": "-P",
            "default": "unknown"
        },
        "email": {
            "description": "User email address for notifications",
            "option": "-u",
            "default": "unknown"
        },
        "dependency": {
            "description": "Job ID of the job that the current job depends on",
            "option": "-j",
            "format": "integer"
        }
    }
}