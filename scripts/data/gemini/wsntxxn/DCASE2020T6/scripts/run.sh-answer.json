{
    "application": "SLURM",
    "details": {
        "job_name": "audio-caption",
        "partition": "gpu",
        "cpus_per_task": 2,
        "gres": "gpu:1",
        "exclude_nodes": "gqxx-01-016",
        "output_file": "logs/audio-caption-%j.log",
        "error_file": "logs/audio-caption-%j.err",
        "java_version": "1.8.0.162",
        "python_script": "${run_script}",
        "run_config_file": "${run_config}",
        "kaldi_stream_file": "${kaldi_stream}",
        "kaldi_scp_file": "${kaldi_scp}",
        "evaluation_caption_file": "${eval_caption_file}",
        "evaluation_embedding_file": "${eval_embedding_file}",
        "seed": 1,
        "experiment_path": "${experiment_path}"
    }
}