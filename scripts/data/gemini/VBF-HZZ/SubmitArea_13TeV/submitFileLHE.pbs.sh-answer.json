{
    "application": "cmsRun",
    "details": {
        "software": [
            "CMSSW_7_1_18",
            "scram",
            "cmsDriver",
            "curl",
            "sed"
        ],
        "resources": {
            "walltime": "01:00:00",
            "nodes": "1:ppn=1",
            "pmem": "8gb"
        },
        "environment": {
            "SCRAM_ARCH": "slc6_amd64_gcc491",
            "OSG_APP": "/osg/app",
            "VO_CMS_SW_DIR": "${OSG_APP}/cmssoft/cms",
            "CMS_PATH": "${VO_CMS_SW_DIR}",
            "X509_USER_PROXY": "$HOME/private/personal/voms_proxy.cert",
            "gridpack": "unknown (variable not defined in script)"
        }
    }
}