            "freesound_no_overlap_noesc50",
            "--datasetinfos",
            "train",
            "--seed",
            "3407",
            "--remotedata",
            "--logs",
            "/fsx/clap_logs",
            "--gather-with-grad",
            "--openai-model-cache-dir",
            "/fsx/yusong/transformers_cache",
