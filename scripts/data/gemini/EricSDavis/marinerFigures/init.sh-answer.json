{
    "application": "Snakemake",
    "details": {
        "software": [
            "Python 3.7.14",
            "Snakemake"
        ],
        "resources": {
            "slurm": {
                "job_name": "marinerFigures",
                "time": "10-00:00:00",
                "nodes": 1,
                "tasks_per_node": 1,
                "cores_per_task": 1,
                "partition": "general",
                "memory": "2gb",
                "output_file": "%x-%j.out",
                "logs_directory": "logs"
            },
            "snakemake": {
                "parallelism": 100,
                "rerun_incomplete": true
            }
        }
    }
}