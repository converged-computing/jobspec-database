{
    "application": "Slurm",
    "details": {
        "job_name": "fiona_error",
        "output_file": "fiona_error.out",
        "error_file": "fiona_error.err",
        "time_limit": "0:30:00",
        "partition": "broadwl",
        "nodes": 1,
        "tasks_per_node": 1,
        "memory_per_cpu": "2000",
        "email": "alexfeistritzer@uchicago.edu",
        "email_type": "ALL",
        "modules": [
            "python"
        ],
        "python_script": "$base_path/parallel/utils/block_summary.py",
        "arguments": [
            "--aoi_path",
            "$base_path/blocks/Africa/${aoi}/blocks_${full_name}.csv",
            "--landscan_path",
            "$base_path/LandScan_Global_2018/raw_tif/ls_2018.tif",
            "--buildings_dir",
            "$base_path/buildings/Africa/${aoi}/",
            "--blocks_dir",
            "$base_path/blocks/Africa/${aoi}",
            "--gadm_dir",
            "$base_path/GADM/${aoi}/",
            "--summary_out_path",
            "${full_name}_summary.geojson"
        ]
    }
}