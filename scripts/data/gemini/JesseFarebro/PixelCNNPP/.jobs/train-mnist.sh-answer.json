{
    "application": "TensorFlow",
    "details": {
        "resource_requirements": {
            "memory": "16GB",
            "cpus": "16",
            "gpus": "2",
            "time": "10 hours"
        },
        "environment": {
            "modules": "tf2",
            "virtual_environment": "~/.venv/tf2"
        },
        "dependencies": {
            "python": "3",
            "libraries": [
                "TensorFlow",
                "TensorBoard"
            ],
            "configuration_file": "experiments/mnist.gin"
        },
        "logging": {
            "output_file": "train-mnist-%j.out",
            "tensorboard_logdir": "${SLURM_SUBMIT_DIR}/logdir",
            "tensorboard_host": "0.0.0.0"
        },
        "script": "main.py"
    }
}