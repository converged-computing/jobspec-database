{
    "application": "SLURM",
    "details": {
        "job_name": "en_train",
        "output_file": "/private/home/%u/logs/%x.out",
        "error_file": "/private/home/%u/logs/%x.err",
        "nodes": 1,
        "tasks_per_node": 1,
        "gpus_per_task": 2,
        "cpus_per_task": 20,
        "memory_per_cpu": "5GB",
        "constraint": "volta32gb",
        "time_limit": "72:00:00",
        "signal": "USR1@300",
        "open_mode": "append",
        "partition": "dev",
        "conda_environment": "vlnce",
        "modules": [
            "cuda/10.1",
            "cudnn/v7.6.5.32-cuda.10.1",
            "NCCL/2.5.6-1-cuda.10.1"
        ],
        "python_script": "run.py",
        "config_file": "vlnce_baselines/config/rxr_baselines/rxr_cma_en.yaml",
        "run_type": "train"
    }
}