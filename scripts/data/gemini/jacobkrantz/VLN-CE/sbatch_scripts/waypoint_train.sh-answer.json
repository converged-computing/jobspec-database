{
    "application": "SLURM",
    "details": {
        "job_name": "waypoint_train",
        "output_file": "/private/home/%u/logs/%x.out",
        "error_file": "/private/home/%u/logs/%x.err",
        "nodes": 8,
        "tasks_per_node": 8,
        "gpus_per_task": 1,
        "cpus_per_task": 10,
        "memory_per_cpu": "5GB",
        "time_limit": "72:00:00",
        "signal": "USR1@600",
        "open_mode": "append",
        "partition": "dev",
        "conda_environment": "vlnce",
        "cuda_version": "10.1",
        "cudnn_version": "v7.6.5.32-cuda.10.1",
        "nccl_version": "2.5.6-1-cuda.10.1",
        "python_script": "run.py",
        "config_file": "vlnce_baselines/config/r2r_waypoint/2-wpn-dc.yaml",
        "run_type": "train"
    }
}