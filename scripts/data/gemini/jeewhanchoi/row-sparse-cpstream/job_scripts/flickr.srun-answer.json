{
    "application": "splatt",
    "details": {
        "partition": "fat",
        "job_name": "cc",
        "output_file": "rsp_flickr.out",
        "error_file": "rsp_flickr.err",
        "time_limit": "12 hours",
        "nodes": 1,
        "memory_per_cpu": "512GB",
        "tasks_per_node": 1,
        "threads_per_task": 56,
        "account": "hpctensor",
        "environment_variables": {
            "KMP_AFFINITY": "granularity=fine,compact,1",
            "OMP_NUM_THREADS": 56
        },
        "input_file": "../hpctensor/flickr-4d.tns",
        "arguments": [
            "cpd",
            "-v",
            "--stream=4",
            "-r 10",
            "-t 56",
            "--reg=frob,1e-2,4"
        ]
    }
}