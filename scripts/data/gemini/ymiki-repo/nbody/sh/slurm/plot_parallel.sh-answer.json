{
    "application": "SLURM",
    "details": {
        "job_name": "visualize",
        "time_limit": "00:15:00",
        "partition": "regular",
        "tasks": 8,
        "modules": [
            "anyenv",
            "miniforge3",
            "julia",
            "texlive",
            "openmpi"
        ],
        "executable": "julia jl/plot/dot.jl",
        "mpi_configuration": {
            "processes_per_node": "$SLURM_NTASKS_PER_NODE",
            "processes_per_socket": "$SLURM_NTASKS_PER_SOCKET"
        },
        "matplotlib_configuration": {
            "config_dir": "/tmp/matplotlib"
        },
        "output_format": "png"
    }
}