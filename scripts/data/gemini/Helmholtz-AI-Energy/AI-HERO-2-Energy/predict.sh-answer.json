{
    "application": "SLURM",
    "details": {
        "job_name": "AI-HERO_energy_baseline_prediction",
        "partition": "accelerated",
        "nodes": 1,
        "gpus": 1,
        "tasks": 1,
        "tasks_per_node": 1,
        "cpus_per_task": 76,
        "time": "20:00:00",
        "environment_variables": {
            "CUDA_CACHE_DISABLE": 1,
            "OMP_NUM_THREADS": 76,
            "data_workspace": "/hkfs/work/workspace/scratch/dz4120-energy-train-data",
            "group_workspace": "..."
