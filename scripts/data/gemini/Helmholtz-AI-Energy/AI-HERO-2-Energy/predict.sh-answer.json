        "modules": [
            "compiler/gnu/11",
            "mpi/openmpi/4.0",
            "lib/hdf5/1.12",
            "devel/cuda/11.8"
        ],
        "python_script": "${group_workspace}/predict.py",
        "arguments": [
            "${data_workspace}"
        ]
    }
}