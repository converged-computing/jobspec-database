{
    "application": "Snakemake",
    "software": [
        "Singularity",
        "hifiasm3",
        "gfatools",
        "Snakemake"
    ],
    "resources": {
        "cpu": 70,
        "memory": "670G",
        "nodes": 1
    },
    "environment": {
        "modules": "singularity/3.5.3",
        "conda_environment": "conda"
    },
    "input_data": {
        "genome": "cat_10metag.fasta",
        "config_file": "configs/Sample-Config.yaml"
    },
    "output_data": {
        "genome_assembly": "hifiasm3.p_ctg.gfa",
        "genome_assembly_fasta": "hifiasm3.p_ctg.gfa.fasta"
    }
}