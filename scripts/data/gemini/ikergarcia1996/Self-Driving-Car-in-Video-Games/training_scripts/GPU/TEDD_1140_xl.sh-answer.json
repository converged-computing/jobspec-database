{
    "application": "pytorch",
    "details": {
        "environment": "/ikerlariak/igarcia945/envs/pytorch-tximista",
        "resources": {
            "cpus": 40,
            "gpus": 2,
            "memory": "200G",
            "output_file": "TEDD_XL.out",
            "error_file": "TEDD_XL.err"
        },
        "software": {
            "python_version": "3",
            "framework": "pytorch",
            "model": "efficientnet_v2_l",
