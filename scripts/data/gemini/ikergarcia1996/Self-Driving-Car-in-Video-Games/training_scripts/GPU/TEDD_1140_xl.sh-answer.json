            "dropout_encoder": 0.1,
            "dropout_encoder_features": 0.3,
            "label_smoothing": 0.1,
            "control_mode": "keyboard",
            "val_check_interval": 0.25,
            "precision": "16",
            "devices": 2,
            "strategy": "ddp_find_unused_parameters_false"
        }
