{
    "application": "Slurm",
    "software": [
        "GNU Parallel",
        "Singularity"
    ],
    "resources": [
        "CPU",
        "GPU (TITAN)",
        "Memory (24GB)",
        "Storage (for output directory)",
        "Singularity image (code_img.sif)"
    ],
    "environment_variables": [
        "SLURM_JOB_ID",
        "SLURM_ARRAY_TASK_ID",
        "SCRIPT_PATH",
        "SCRIPT_DIR",
        "RUNS_PER_TASK",
        "OUTPUT_DIR",
        "EXP_NAME"
    ],
    "commands": [
        "scontrol show job",
        "awk",
        "dirname",
        "cd",
        "mkdir",
        "cp",
        "parallel",
        "singularity run",
        "SimpleSAC.conservative_sac_main"
    ],
    "libraries": [
        "SimpleSAC"
    ]
}