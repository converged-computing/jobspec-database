{
    "application": "Dask",
    "details": {
        "scheduler": "dask-scheduler",
        "worker": "dask-worker",
        "resource_manager": "LSF",
        "libraries": [
            "gcc/6.4.0",
            "cuda/10.1.168"
        ],
        "communication_protocol": "UCX",
        "interface": "ib0",
        "scheduler_file": "$MEMBERWORK/$PROJ_ID/my-scheduler.json",
        "local_directory": "$MEMBERWORK/$PROJ_ID",
        "nodes": 4,
        "cores_per_node": 42,
        "gpus_per_node": 6,
        "memory_limit": "512GB",
        "threads_per_worker": 42,
        "nanny_enabled": true,
        "death_timeout": 60
    }
}