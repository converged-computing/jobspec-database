{
    "application": "Dask",
    "details": {
        "scheduler": "dask-scheduler",
        "worker": "dask-cuda-worker",
        "resource_manager": "LSF (Load Sharing Facility)",
        "gpu_accelerator": "CUDA 10.1.168",
        "libraries": "nvrapids_0.11_gcc_6.4.0",
        "compiler": "gcc/6.4.0",
        "network_interface": "ib0 (Infiniband)",
        "communication_library": "MPI (with jsrun)",
        "nodes": 2,
        "cores_per_node": 12,
        "gpus_per_node": 6,
        "memory_limit": "85GB per worker",
        "device_memory_limit": "16GB per GPU",
        "scheduler_file": "$MEMBERWORK/$PROJ_ID/my-scheduler-gpu.json",
        "local_directory": "$MEMBERWORK/$PROJ_ID"
    }
}