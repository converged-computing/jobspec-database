{
    "application": "Dask",
    "details": {
        "scheduler": "dask-scheduler",
        "worker": "dask-cuda-worker",
        "modules": [
            "gcc/6.4.0",
            "cuda/10.1.168"
        ],
        "environment_variables": {
            "PATH": "$WORLDWORK/stf011/nvrapids_0.11_gcc_6.4.0/bin:$PATH",
            "CUPY_CACHE_DIR": "$MEMBERWORK/$PROJ_ID",
            "OMP_PROC_BIND": "FALSE"
        },
        "resources": {
            "nodes": 1,
            "cores": 1,
            "gpus": 6,
            "memory": "85GB",
            "device_memory": "16GB"
        },
        "scheduler_file": "$MEMBERWORK/$PROJ_ID/my-scheduler-gpu.json",
        "local_directory": "$MEMBERWORK/$PROJ_ID"
    }
}