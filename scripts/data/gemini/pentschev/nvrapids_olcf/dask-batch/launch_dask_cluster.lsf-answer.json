{
    "application": "Dask",
    "details": {
        "scheduler": "Dask scheduler",
        "workers": "Dask workers",
        "resource_manager": "LSF (Load Sharing Facility)",
        "gpu_accelerator": "Nvidia CUDA",
        "networking": "Infiniband (ib0)",
        "modules": {
            "gcc": "6.4.0",
            "cuda": "10.1.168"
