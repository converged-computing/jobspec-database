{
    "application": "slurm",
    "details": {
        "partition": "short",
        "qos": "short",
        "cpus_per_task": 12,
        "time": "00:30:00",
        "mem_per_cpu": "2000",
        "array_range": "10-31",
        "constraint": "xeon",
        "output": "tmp/slurm-%A_%a.out",
        "environment_variable": "OMP_PROC_BIND=true",
        "executable": "bin/run-test",
        "command": "triton build-omp",
        "argument": "$SLURM_ARRAY_TASK_ID"
    }
}