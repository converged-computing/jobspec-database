{
    "application": "IMPACT-Z",
    "details": {
        "software": [
            "Python",
            "OpenMPI",
            "GPTune",
            "SIMPLEX"
        ],
        "resources": {
            "nodes": 2,
            "walltime": "10:00:00",
            "partition": "regular",
            "cpu_type": "haswell"
        },
        "environment_variables": [
            "ModuleEnv",
            "GPTUNEROOT",
            "GPTUNE_LITE_MODE"
        ],
        "command_line_arguments": [
            "-ntask",
            "-nrun",
            "-optimization"
        ],
        "files": [
            "run_env.sh",
            "impact-z_single1D.py",
            "impact-z_single.py",
            "impact-z_single_simplex.py",
            ".gptune/meta.json",
            "gptune.db/*.json"
        ]
    }
}