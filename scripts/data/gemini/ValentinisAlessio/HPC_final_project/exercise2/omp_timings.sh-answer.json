{
    "application": "Slurm",
    "details": {
        "job_name": "omp_timings",
        "output": "omp_timings.out",
        "error": "omp_timings.err",
        "partition": "EPYC",
        "nodes": 1,
        "exclusive": true,
        "time": "02:00:00",
        "mail_type": "ALL",
        "mail_user": "ALESSIO.VALENTINIS@studenti.units.it",
        "modules": [
            "architecture/AMD",
            "openMPI/4.1.5/gnu/12.2.1"
        ],
        "environment_variables": [
            "OMP_PLACES=cores",
            "OMP_PROC_BIND=close",
            "OMP_NUM_THREADS=4"
        ],
        "executable": "./main",
        "data_file": "data/timings$N.csv",
        "data_size": "N=240000000",
        "mpi_implementation": "openMPI",
        "mpi_options": [
            "-np 1",
            "--map-by node",
            "--bind-to socket"
        ]
    }
}