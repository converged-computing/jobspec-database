{
    "application": "GROMACS",
    "other_software": [
        "hpcbench",
        "SLURM"
    ],
    "resource_requirements": {
        "nodes": 1,
        "time": "00:30:00",
        "gres": "gpu",
        "cpus_per_task": 4,
        "partition": "undefined"
    },
    "modules": [
        "gromacs"
    ],
    "environment_variables": [
        "PATH",
        "GMX_FORCE_UPDATE_DEFAULT_GPU",
        "GMX_GPU_DD_COMMS",
        "GMX_GPU_PME_PP_COMMS",
        "GMX_ENABLE_DIRECT_GPU_COMM"
    ],
    "commands": [
        "gmx mdrun",
        "hpcbench infolog",
        "hpcbench gpulog",
        "hpcbench cpulog",
        "kill",
        "hpcbench sacct",
        "hpcbench gmxlog",
        "hpcbench slurmlog",
        "hpcbench extra",
        "hpcbench gmxedr",
        "hpcbench collate",
        "rm"
    ],
    "input_files": [
        "benchmarkfile",
        "ener.edr"
    ],
    "output_files": [
        "sysinfo.json",
        "gpulog.json",
        "cpulog.json",
        "accounting.json",
        "run.json",
        "slurm.json",
        "meta.json",
        "thermo.json",
        "benchout"
    ]
}