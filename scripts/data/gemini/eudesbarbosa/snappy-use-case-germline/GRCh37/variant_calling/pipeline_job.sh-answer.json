{
    "application": "Snakemake",
    "details": {
        "environment": {
            "conda_path": "varies based on system"
        },
        "resources": {
            "slurm": {
                "partition": "medium",
                "time": "3-00",
                "memory": "6G",
                "job_name": "variant_calling_e2e_test",
                "logs": "slurm_log/%x-%J.log",
                "max_jobs": 500,
                "max_jobs_per_second": 10
            }
        },
        "other": {
            "restart_times": "0",
            "drmaa_snippet": "--partition=medium",
            "tmp_dir": "${HOME}/scratch/tmp",
            "log_dir": "slurm_log/${SLURM_JOB_ID}"
        }
    }
}