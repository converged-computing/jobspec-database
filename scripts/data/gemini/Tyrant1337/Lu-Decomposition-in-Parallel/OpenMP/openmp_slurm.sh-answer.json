{
    "application": "Slurm",
    "details": {
        "partition": "day-long-cpu",
        "job_name": "openmpcode",
        "output_file": "slurm_output.%j",
        "error_file": "slurm_error.%j",
        "time_limit": "0-00:01:00",
        "nodes": 1,
        "tasks_per_node": 1,
        "cpus_per_task": 48,
        "openmp_proc_bind": "close",
        "executable": "./lud_openMPpar",
        "environment_variables": {
            "OMP_PROC_BIND": "close",
            "SLURM_JOBID": "job id",
            "SLURM_CPUS_PER_TASK": 48
        }
    }
}