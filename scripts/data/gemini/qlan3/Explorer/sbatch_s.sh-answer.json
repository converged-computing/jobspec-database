{
    "application": "SLURM",
    "details": {
        "software": [
            "gcc/9.3.0",
            "arrow/2.0.0",
            "python/3.8",
            "scipy-stack",
            "tianshou"
        ],
        "resources": [
            "SLURM_ARRAY_JOB_ID",
            "SLURM_JOB_ID",
            "SLURM_ARRAY_TASK_ID",
            "SLURM_ARRAY_TASK_COUNT",
            "SLURM_TMPDIR",
            "SLURM_JOB_NODELIST",
            "OMP_NUM_THREADS",
            "SLURM_JOB_NAME",
            "config_file",
            "config_idx",
            "slurm_dir"
        ],
        "signals": [
            "USR1",
            "EXIT"
        ],
        "files": [
            "main.py",
            "./configs/${SLURM_JOB_NAME}.json",
            "./logs/$SLURM_JOB_NAME/"
        ],
        "environment": {
            "PYTHONPATH": "~/envs/tianshou/bin/activate"
        }
    }
}