{
    "application": "SLURM",
    "details": {
        "software": [
            "gcc/9.3.0",
            "arrow/2.0.0",
            "python/3.8",
            "scipy-stack",
            "parallel",
            "python"
        ],
        "environment": {
            "conda_env": "tianshou"
        },
        "resources": {
            "OMP_NUM_THREADS": "1",
            "SLURM_ARRAY_JOB_ID": "job array ID",
            "SLURM_JOB_ID": "job ID",
            "SLURM_ARRAY_TASK_ID": "task ID",
            "SLURM_ARRAY_TASK_COUNT": "total task count",
            "SLURM_TMPDIR": "temporary directory",
            "SLURM_JOB_NODELIST": "node list"
        },
        "files": [
            "configs/${SLURM_JOB_NAME}.json",
            "job_idx_${SLURM_JOB_NAME}_${SLURM_ARRAY_TASK_ID}.txt",
            "main.py",
            "logs/$SLURM_JOB_NAME/"
        ],
        "signals": {
            "USR1": "300 seconds before end of time limit"
        },
        "mail_type": "ALL"
    }
}