{
    "application": "Slurm",
    "details": {
        "job_name": "cpu02",
        "output_log": "stdout-cpu02.%j.log",
        "error_log": "stderr-cpu02.%j.log",
        "partition": "nssc",
        "nodes": 1,
        "tasks": 2,
        "cpus_per_task": 1,
        "time_limit": "00:10:00",
        "modules": [
            "pmi/pmix-x86_64",
            "mpi/openmpi-x86_64"
        ],
        "compiler": "mpic++",
        "compilation_flags": "-std=c++17 -O3 -pedantic -march=native -ffast-math",
        "executable": "impl_1d",
        "mpi_implementation": "openmpi",
        "mpi_mode": "1D",
        "filename_prefix": "cpu02",
        "resolutions": [
            125,
            250,
            1000,
            2000
        ],
        "iterations": 800,
        "arguments": "-100 +100"
    }
}