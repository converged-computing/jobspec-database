{
    "application": "hd-bet",
    "details": {
        "environment": "PBS",
        "resource_requirements": {
            "nodes": 1,
            "ppn": 16,
            "walltime": "02:00:00"
        },
        "software": {
            "singularity": "docker://anibalsolon/hd-bet:v0.0.1",
            "jq": "version not specified",
            "nvidia-smi": "optional for GPU processing",
            "cp": "for file copying"
        },
        "input_file": "config.json",
        "output_files": [
            "masked.nii.gz",
            "mask/t1.nii.gz",
            "output/t1.nii.gz"
        ],
        "additional_notes": "The script uses the hd-bet tool within a Singularity container and can be run on either CPU or GPU depending on the availability of nvidia-smi."
    }
}