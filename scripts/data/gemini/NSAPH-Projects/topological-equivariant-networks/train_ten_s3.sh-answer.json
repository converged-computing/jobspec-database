{
    "application": "EGNN",
    "software": [
        "ncf",
        "miniconda3",
        "cuda",
        "python",
        "EGNN"
    ],
    "resource_requirements": {
        "cores": 8,
        "runtime": "0-60:00",
        "partition": "gpu",
        "gpu": "nvidia_a100-sxm4-80gb:1",
        "memory": "32000"
    },
    "conda_environment": "ten",
    "script_arguments": {
        "lifters": "atom:0 bond:1 functional_group:2 ring:2 supercell:3",
        "target_name": "$TARGET_NAME",
        "connectivity": "self_and_neighbors",
        "visible_dims": "0 1 2 3",
        "neighbor_types": "+1 -1 max",
        "epochs": 1000,
        "batch_size": 96,
        "weight_decay": "1e-16",
        "lr": "$LR",
        "num_layers": 7,
        "num_hidden": 128,
        "model_name": "ten",
        "dim": 3,
        "splits": "egnn",
        "clip_gradient": "$CLIP_GRADIENT"
    }
}