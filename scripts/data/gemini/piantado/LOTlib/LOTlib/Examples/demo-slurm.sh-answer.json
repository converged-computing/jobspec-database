{
    "application": "slurm",
    "details": {
        "cluster": "colala",
        "queue": "colala",
        "job_name": "LOTlibSearch",
        "error_output": "output/err_%j",
        "output": "output/out_%j",
        "memory_per_cpu": "1000M",
        "time_limit": "100:15:00",
        "nodes": 4,
        "tasks_per_node": 24,
        "signal": "2@600",
        "modules": [
            "numpy",
            "python/2.7.6",
            "openmpi/1.6.5/b1"
        ],
        "python_script": "Search.py",
        "python_script_arguments": [
            "--model=Number",
            "--data=300",
            "--steps=10000"
        ]
    }
}