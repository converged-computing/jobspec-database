{
    "application": "Julia",
    "details": {
        "software": [
            "SLURM (Simple Linux Utility for Resource Management)"
        ],
        "resources": {
            "account": "def-simine",
            "time": "1 minute",
            "array": "1-300",
            "output": "slurm_%A-%a.out",
            "error": "slurm_%A-%a.err",
            "threads": "${SLURM_CPUS_PER_TASK:-1}",
            "scratch": "~/scratch/clean_bigMAC/10x10/relax/relaxed_structures"
        },
        "files": {
            "input": "run_files/*",
            "output": "sample-${nn}",
            "script": "runARPACK_MAC.jl",
            "data": "bigMAC-${nn}_relaxed.xsf"
        }
    }
}