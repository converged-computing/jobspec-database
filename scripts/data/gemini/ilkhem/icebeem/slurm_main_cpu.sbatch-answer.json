{
    "application": "SLURM",
    "details": {
        "job_name": "ice_cpu",
        "output": "slurm_log/ice_cpu.%A.out",
        "error": "slurm_log/ice_cpu.%A.err",
        "partition": "cpu",
        "nodes": 1,
        "cores": 12,
        "memory": "8G",
        "time": "0-12:00",
        "environment_variables": {
            "CUDA_DEVICE_ORDER": "PCI_BUS_ID",
            "CUDA_VISIBLE_DEVICES": "0,1,2"
        },
        "python_script": "main.py",
        "array_variable": "SLURM_ARRAY_TASK_ID"
    }
}