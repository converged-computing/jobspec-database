{
    "application": "Horovod",
    "details": {
        "framework": "TensorFlow",
        "library": "OpenSlide",
        "language": "Python",
        "virtualenv": "openslide-py38",
        "packages": [
            "tensorflow==2.3.0",
            "tensorboard==2.3.0",
            "scikit-learn",
            "Pillow",
            "tqdm",
            "six",
            "opencv-python",
            "openslide-python",
            "pandas",
            "numba"
        ],
        "modules": [
            "2019",
            "2020",
            "Python/3.8.2-GCCcore-9.3.0",
            "OpenMPI/4.0.3-GCC-9.3.0",
            "cuDNN/7.6.5.32-CUDA-10.1.243",
            "NCCL/2.5.6-CUDA-10.1.243",
            "CMake/3.11.4-GCCcore-8.3.0"
        ],
        "resources": [
            "GPU (Titan RTX)",
            "SLURM (3 nodes, 16 hours, gpu_titanrtx partition)"
        ],
        "environment_variables": [
            "HOROVOD_CUDA_HOME",
            "HOROVOD_CUDA_INCLUDE",
            "HOROVOD_CUDA_LIB",
            "HOROVOD_GPU_ALLREDUCE",
            "HOROVOD_WITHOUT_GLOO",
            "HOROVOD_GPU_BROADCAST",
            "HOROVOD_WITH_TENSORFLOW",
            "PATH",
            "LD_LIBRARY_PATH",
            "CPATH",
            "HOROVOD_NCCL_HOME",
            "MPICC",
            "MPICXX",
            "HOROVOD_MPICXX_SHOW",
            "TF_GPU_THREAD_MODE"
        ]
    }
