{
    "application": "Pose estimation",
    "details": {
        "environment": "SLURM",
        "resources": {
            "partition": "alvis",
            "nodes": 4,
            "gpus": "T4:1",
            "time": "20 hours",
            "modules": [
                "GCC/8.3.0",
                "CUDA/10.1.243",
                "OpenMPI/3.1.4",
                "PyTorch/1.6.0-Python-3.7.4",
                "SciPy-bundle/2019.10-Python-3.7.4",
                "OpenCV/4.2.0-Python-3.7.4",
                "Pillow"
            ],
            "virtual environment": "detect"
        },
        "libraries": [
            "numpy",
            "opencv",
            "pillow",
            "pytorch",
            "scipy"
        ],
        "script": "run-folder-cluster.sh",
        "data": {
            "input": "$HOME/vids/$vid",
            "output": "$HOME/poses/$vid"
        }
    }
}