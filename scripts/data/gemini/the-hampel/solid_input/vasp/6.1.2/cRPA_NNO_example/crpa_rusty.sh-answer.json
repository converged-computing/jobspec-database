{
    "application": "VASP",
    "details": {
        "software": [
            "VASP 6.1.2",
            "GNU OpenMPI",
            "SLURM"
        ],
        "resources": {
            "compute nodes": 1,
            "tasks per node": 64,
            "tasks per core": 1,
            "partition": "ccq",
            "constraint": "rome",
            "walltime": "24:00:00",
            "output file": "out.%j",
            "error file": "err.%j"
        },
        "environment variables": [
            "OMP_NUM_THREADS=1"
        ],
        "ulimit": {
            "stack size": "unlimited"
        }
    }
}