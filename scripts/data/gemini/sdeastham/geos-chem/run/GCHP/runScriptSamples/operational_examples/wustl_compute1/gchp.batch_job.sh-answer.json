{
    "application": "GCHP",
    "software": [
        "mpiexec",
        "gcchem"
    ],
    "resources": {
        "queue": "rvmartin",
        "nodes": 24,
        "walltime": "168:00",
        "memory": "300GB",
        "docker_image": "registry.gsc.wustl.edu/sleong/esm:intel-2021.1.2",
        "modules": [
            "setCommonRunSettings.sh",
            "setRestartLink.sh",
            "checkRunSettings.sh"
        ],
        "ulimits": {
            "coredumpsize": 0,
            "memorylocked": "unlimited",
            "maxproc": 50000,
            "vmemoryuse": "unlimited",
            "stacksize": "unlimited"
        }
    }
}