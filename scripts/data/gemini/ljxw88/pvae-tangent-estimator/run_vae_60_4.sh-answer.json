{
    "application": "PyTorch",
    "details": {
        "version": "1.4.0",
        "python_version": "3.6",
        "cuda_version": "9.0",
        "libraries": [
            "torchvision",
            "geoopt"
        ],
        "resources": {
            "nodes": 1,
            "tasks_per_node": 1,
            "gpus": 1,
            "time": "12:00:00",
            "memory": "20GB"
        },
        "script_file": "pvae/main.py",
        "arguments": {
            "model": "mnist",
            "manifold": "PoincareBall",
            "c": 1.4,
            "latent_dim": 60,
            "hidden_dim": 600,
            "prior": "WrappedNormal",
            "posterior": "WrappedNormal",
            "dec": "Geo",
            "enc": "Wrapped",
            "lr": 0.0005,
            "epochs": 80,
            "save_freq": 80,
            "batch_size": 128,
            "iwae_samples": 5000
        }
    }
}