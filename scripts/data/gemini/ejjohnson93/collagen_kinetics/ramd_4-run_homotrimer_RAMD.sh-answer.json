{
    "application": "GROMACS",
    "software": [
        "gmx pdb2gmx",
        "gmx editconf",
        "gmx solvate",
        "gmx grompp",
        "gmx genion",
        "gmx mdrun",
        "gmx trjconv",
        "gmx select",
        "gmx make_ndx",
        "gmx genrestr",
        "sed",
        "awk",
        "cat",
        "rm",
        "mkdir",
        "hostname",
        "pwd",
        "date",
        "echo"
    ],
    "resources": {
        "partition": "nodes",
        "nodes": 8,
        "tasks_per_node": 8,
        "cpus_per_task": 5,
        "walltime": "3-00:00:00",
        "memory_per_cpu": "9000M",
        "modules": [
            "apps/gromacs/2020.5_ramd-2.0"
        ],
        "environment_variables": [
            "OMP_NUM_THREADS",
            "SLURM_CPUS_PER_TASK",
            "SLURM_JOB_NAME",
            "SLURM_JOB_ID",
            "SLURM_JOB_USER",
            "SLURM_ARRAY_TASK_ID",
            "SLURM_SUBMIT_DIR",
            "TMPDIR",
            "SLURM_SUBMIT_HOST",
            "SLURM_JOB_PARTITION",
            "SLURM_JOB_NODELIST",
            "HOSTNAME",
            "SLURM_JOB_NUM_NODES",
            "SLURM_NNODES",
            "SLURM_NTASKS",
            "SLURM_TASKS_PER_NODE",
            "SLURM_NTASKS_PER_NODE",
            "SLURM_CPUS_PER_TASK",
            "SLURM_CPUS_ON_NODE",
            "SLURM_PRIO_PROCESS"
        ]
    }
}