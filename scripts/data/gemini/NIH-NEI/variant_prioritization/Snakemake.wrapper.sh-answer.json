{
    "application": "Snakemake",
    "details": {
        "version": "5.24.1",
        "dependencies": [
            "intervar/2.1.3"
        ],
        "resources": {
            "lscratch": "100",
            "cpus-per-task": "8",
            "memory": "32g",
            "cluster": {
                "mem": "cluster.mem",
                "time": "cluster.time",
                "partition": "cluster.partition",
                "output": "cluster.output",
                "error": "cluster.error",
                "extra": "cluster.extra"
            }
        },
        "configuration": {
            "genomeBuild": "GRCh38"
        },
        "execution": {
            "local-cores": "2",
            "jobs": "1999",
            "latency-wait": "120",
            "rerun-incomplete": true,
            "restart-times": "0",
            "resources": "res=1"
        }
    }
}