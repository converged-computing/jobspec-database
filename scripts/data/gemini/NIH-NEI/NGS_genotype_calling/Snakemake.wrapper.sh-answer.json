{
    "application": "Snakemake",
    "software_requirements": [
        "snakemake/6.0.5",
        "Statistics::Normality",
        "CoNVaDING",
        "sbatch",
        "zcat",
        "sort",
        "cut",
        "sed"
    ],
    "resource_requirements": [
        "lscratch:100",
        "cpus-per-task=8",
        "mem=32g",
        "parallel=4",
        "time=12:0:0"
    ],
    "dependencies": [
        "git",
        "Perl"
    ],
    "input_files": [
        "fastq/*.gz",
        "*metadata_file*.csv",
        "config_panel.yaml",
        "panel.Snakefile",
        "panel.cluster.json",
        "exome.Snakefile",
        "exome.cluster.json",
        "Snakefile",
        "cluster.json"
    ],
    "output_files": [
        "metadata_file.csv",
        "00log"
    ]
}