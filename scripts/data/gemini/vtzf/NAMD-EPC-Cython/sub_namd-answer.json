{
    "application": "NAMD",
    "details": {
        "scheduler": "SLURM",
        "partition": "sugon",
        "job_name": "namd-tyy",
        "time_limit": "30:00:00",
        "nodes": 4,
        "tasks_per_node": 16,
        "cpus_per_task": 1,
        "error_file": "namd_%j.err",
        "output_file": "namd_%j.log",
        "environment_variables": {
            "HDF5_USE_FILE_LOCKING": "FALSE",
            "OMP_NUM_THREADS": "1"
        },
        "mpi_implementation": "pmi2",
        "libraries": {
            "intel_mkl": "required"
        }
    }
}