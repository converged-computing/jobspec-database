{
    "application": "jsrun",
    "details": {
        "software": [
            "libtbx.python",
            "jsrun",
            "cProfiler"
        ],
        "resources": {
            "nodes": 300,
            "gpus": "gpumps",
            "mpi_ranks": 1800,
            "tasks_per_resource_set": 5,
            "cpu_cores_per_resource_set": 5,
            "resource_sets_per_node": 6,
            "gpu_devices_per_node": 1,
            "launch_distribution": "packed"
        },
        "environment_variables": {
            "BBPATH": "/mnt/bb/$USER",
            "PERSIST_DIR": "${BBPATH}",
            "PFS_DIR": "$PWD/spect/",
            "USE_EXASCALE_API": "True",
            "LOG_BY_RANK": 1,
            "RANK_PROFILE": 0,
            "N_SIM": 99000,
            "ADD_SPOTS_ALGORITHM": "cuda",
            "ADD_BACKGROUND_ALGORITHM": "cuda",
            "CACHE_FHKL_ON_GPU": "True",
            "DEVICES_PER_NODE": 1
        },
        "script_arguments": [
            "$(libtbx.find_in_repositories LS49)/adse13_196/step5_batch.py"
        ]
    }
}