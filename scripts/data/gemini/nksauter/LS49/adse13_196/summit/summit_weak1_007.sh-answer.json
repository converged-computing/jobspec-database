{
    "application": "jsrun",
    "details": {
        "batch_system": "LSF",
        "resources": {
            "nodes": 7,
            "cpus": 42,
            "gpus": 7,
            "memory": "not specified"
        },
        "environment_variables": [
            "BBPATH",
            "USE_EXASCALE_API",
            "LOG_BY_RANK",
            "RANK_PROFILE",
            "N_SIM",
            "ADD_SPOTS_ALGORITHM",
            "ADD_BACKGROUND_ALGORITHM",
            "CACHE_FHKL_ON_GPU",
            "DEVICES_PER_NODE"
        ],
        "python_script": "step5_batch.py",
        "python_path": "$(libtbx.find_in_repositories LS49)/adse13_196/",
        "output_directory": "${BBPATH}"
    },
    "commands": [
        "cd $WORK",
        "mkdir ${LSB_JOBID}",
        "cd ${LSB_JOBID}",
        "echo \"jobstart $(date)\";pwd;ls",
        "jsrun -n 7 -a 7 -c 7 -r 1 -g 1 libtbx.python $(libtbx.find_in_repositories LS49)/adse13_196/step5_batch.py logger.outdir=${BBPATH}",
        "jsrun -n 7 -r 1 find ${BBPATH} -type f -exec cp {} . \\;",
        "echo \"jobend $(date)\";pwd"
    ]
}