{
    "application": "Kubernetes",
    "details": {
        "other_software": [
            "podman",
            "kubectl",
            "kind",
            "ss",
            "awk",
            "cut",
            "grep",
            "stat",
            "uuidgen",
            "envsubst"
        ],
        "resource_requirements": {
            "cgroups": "cgroups v2 enabled",
            "dependencies": "podman, kubectl, kind",
            "os_release": "/etc/os-release"
        },
        "cluster_setup": [
            "kind create cluster",
            "kubectl get nodes",
            "kubectl cluster-info"
        ],
        "cluster_configuration": [
            "kind-config-template.yaml"
        ],
        "kubernetes_access": [
            "ServiceAccount",
            "ClusterRoleBinding"
        ],
        "kubernetes_api": [
            "kubectl config view",
            "kubectl create token"
        ],
        "workload_execution": [
            "bash $1"
        ]
    }
}