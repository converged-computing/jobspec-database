{
    "application": "LSF",
    "details": {
        "scheduler": "LSF",
        "resources": {
            "nodes": 1,
            "cores": 1,
            "threads": 1,
            "gpus": 1,
            "walltime": "10 minutes"
        },
        "modules": [
            "pgi",
            "cuda/9.1.85"
        ],
        "environment_variables": {
            "LD_LIBRARY_PATH": "$CRAY_LD_LIBRARY_PATH:$LD_LIBRARY_PATH",
            "OMP_NUM_THREADS": "1",
            "MPICH_RDMA_ENABLED_CUDA": "1"
        },
        "executable": "./Nyx3d.pgi.MPI.ex",
        "jsrun": "jsrun -n 1 -a 1 -g 1 -c 1 --bind=packed:1",
        "inputs": "inputs_short",
        "runtime_directory": "$SCRATCH/${LSB_JOBNAME}-${LSB_JOBID}"
    }
}