{
    "application": "LSF",
    "details": {
        "scheduler": "LSF",
        "job_name": "SStest",
        "resources": {
            "walltime": "10 minutes",
            "nodes": 1,
            "modules": [
                "pgi",
                "cuda/9.1.85"
            ],
            "libraries": [
                "CRAY_LD_LIBRARY_PATH",
                "/sw/summit/cuda/9.1.85/lib64"
            ],
            "environment_variables": {
                "MPICH_RDMA_ENABLED_CUDA": 1,
                "OMP_NUM_THREADS": 1
            }
        },
        "executable": "./Nyx3d.pgi.MPI.CUDA.ex",
        "input_files": [
            "inputs_short",
            "TREECOOL_middle",
            "probin"
        ],
        "output_files": [
            "out.${LSB_JOBID}",
            "memcheck.${LSB_JOBID}.txt",
            "nvprof.${LSB_JOBID}.txt",
            "nvprof-timeline-%p.nvvp",
            "nvprof-metrics-kernel-%p.nvvp",
            "nvprof-metrics-%p.nvvp"
        ]
    }
}