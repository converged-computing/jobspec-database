{
    "application": "MAESTRO",
    "software": [
        "aprun",
        "OpenMP",
        "MPI",
        "process.bw"
    ],
    "resources": [
        "Blue Waters supercomputer",
        "128 nodes",
        "32 cores per node",
        "8 threads per core",
        "4 MPI tasks per node"
