    "environment_variables": [
        "PSC_OMP_AFFINITY",
        "OMP_NUM_THREADS",
        "MPICH_PTL_OTHER_EVENTS",
        "MPICH_UNEX_BUFFER_SIZE",
        "MPICH_MAX_SHORT_MSG_SIZE"
    ]
}