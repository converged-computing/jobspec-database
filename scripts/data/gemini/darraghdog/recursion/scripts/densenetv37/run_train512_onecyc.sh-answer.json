{
    "application": "LSF",
    "details": {
        "queue": "lowpriority",
        "gpu_count": 2,
        "gpu_mode": "exclusive_process",
        "container_image": "darraghdog/kaggle:apex_build",
        "cores_per_node": 1,
        "memory_per_node": "span[ptile=4]",
        "script": "trainorig.py",
        "script_args": [
            "--accum 4",
            "--logmsg Recursion-addexpt-512",
            "--nbags 20",
            "--epochs 2",
            "--fold 5",
            "--lr 0.000025",
            "--lrmult 20",
            "--batchsize 32",
            "--workpath scripts/densenetv37",
            "--probsname probs_512",
            "--cutmix_prob 1.0",
            "--precision half",
            "--beta 1.0",
            "--imgpath data/mount/512X512X6/",
            "--weightsname weights/pytorch_cut_model_512_densenet5.bin"
        ]
    }
}