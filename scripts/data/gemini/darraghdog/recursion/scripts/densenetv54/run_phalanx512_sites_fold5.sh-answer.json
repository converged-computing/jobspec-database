{
    "application": "bsub",
    "details": {
        "queue": "lowpriority",
        "gpu_num": "2",
        "gpu_mode": "exclusive_process",
        "container_image": "darraghdog/kaggle:apex_build",
        "processors": "1",
        "processor_tiles": "4",
        "command": "python3 phalanx.py",
        "arguments": [
            "--logmsg Recursion-concat-$SIZE-fp16",
            "--nbags 30",
            "--epochs 100",
            "--fold $FOLD",
            "--lr 0.000025",
            "--lrmult 20",
            "--batchsize 16",
            "--workpath scripts/$WDIR",
            "--site 1",
            "--emb 0",
            "--probsname probs_$SIZE",
            "--cutmix_prob 1.0",
            "--precision half",
            "--beta 1.0",
            "--imgpath data/mount/512X512X6/",
            "--weightsname weights/pytorch_cut_model_512_densenet$FOLD.bin"
        ],
        "working_directory": "/share/dhanley2/recursion/scripts/$WDIR",
        "output_file": "log_train_%J"
    }
}