{
    "application": "LSF",
    "details": {
        "queue": "lowpriority",
        "gpu": {
            "num": 1,
            "mode": "exclusive_process"
        },
        "container_image": "darraghdog/kaggle:apex_build",
        "num_nodes": 1,
        "resources": {
            "span": "ptile=4"
        },
        "output": "log_train_%J",
        "script": {
            "command": "python3 trainorig.py",
            "arguments": [
                "--logmsg Recursion-addexpt-cut1-$SIZE",
                "--nbags 20",
                "--epochs 100",
                "--fold $FOLD",
                "--lr 0.000025",
                "--lrmult 20",
                "--batchsize 64",
                "--workpath scripts/$WDIR",
                "--probsname probs_$SIZE",
                "--cutmix_prob 1.0",
                "--precision half",
                "--beta 1.0",
                "--imgpath data/mount/256X256X6/",
                "--weightsname weights/pytorch_cut_model_256_densenet$FOLD.bin"
            ],
            "working_directory": "/share/dhanley2/recursion/scripts/$WDIR"
        }
    }
}