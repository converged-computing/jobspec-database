{
    "application": "GATK",
    "software": [
        "java",
        "gatk",
        "picard",
        "bgzip",
        "tabix"
    ],
    "resource_requirements": {
        "memory": "16G",
        "cpus": 8,
        "nodes": 1,
        "modules": [
            "java/8",
            "gatk/3.8",
            "picard"
        ]
    },
    "input_files": [
        "SRA_samples.csv",
        "genome/Af293.fasta",
        "bam/*.bam"
    ],
    "output_files": [
        "logs/GATK_HTC_sra.*.log",
        "gvcf/*.g.vcf",
        "gvcf/*.g.vcf.gz",
        "gvcf/*.g.vcf.gz.tbi"
    ]
}