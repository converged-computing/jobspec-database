{
    "application": "funannotate",
    "details": {
        "software": [
            "funannotate",
            "augustus",
            "genemark",
            "perl",
            "busco"
        ],
        "resources": {
            "cluster": "SLURM",
            "queue": "intel,batch",
            "time": "3-0:00:00",
            "nodes": 1,
            "tasks": 24,
            "memory": "96G",
            "output": "logs/predict.%a.log"
        },
        "parameters": {
            "busco_db": "fungi_odb10",
            "input_directory": "genomes",
            "output_directory": "annotate",
            "prediction_support_directory": "prediction_support",
            "samples_file": "samples.csv",
            "informant_proteins_file": "lib/informant_proteins.aa",
            "augustus_config_path": "lib/augustus/3.3/config",
            "funannotate_db": "/bigdata/stajichlab/shared/lib/funannotate_db",
            "genemark_key_path": "~/.gm_key",
            "seed_species": "anidulans"
        }
    }
}