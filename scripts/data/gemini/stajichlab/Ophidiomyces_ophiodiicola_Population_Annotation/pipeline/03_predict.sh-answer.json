{
    "application": "funannotate",
    "details": {
        "modules": [
            "funannotate",
            "workspace/scratch"
        ],
        "software": [
            "diamond",
            "augustus",
            "funannotate",
            "busco"
        ],
        "resources": {
            "slurm": {
                "partition": "intel,batch",
                "time": "3-0:00:00",
                "ntasks": 24,
                "nodes": 1,
                "mem": "96G",
                "output": "logs/predict.%a.log",
                "array": 1
            },
            "database": {
                "funannotate_db": "/bigdata/stajichlab/shared/lib/funannotate_db"
            },
            "input": {
                "genomes": "realpath genomes",
                "samples": "samples.csv",
                "informant_proteins": "realpath lib/informant_proteins.aa"
            },
            "output": {
                "annotate": "realpath annotate",
                "prediction_support": "realpath prediction_support"
            }
        }
    }
}