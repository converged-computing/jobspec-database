{
    "application": "BUSCO",
    "details": {
        "software": [
            "BUSCO",
            "Augustus"
        ],
        "resources": {
            "cluster_manager": "SLURM",
            "nodes": 1,
            "tasks": 8,
            "memory": "16G",
            "partition": "short",
            "output_directory": "logs",
            "output_file_pattern": "busco.%a.log",
            "job_name": "busco",
            "cpu_cores": 2,
            "module_environment": {
                "busco": "loaded",
                "miniconda3": "unloaded",
                "workspace/scratch": "loaded"
            },
            "augustus_config_path": "lib/augustus/3.3/config",
            "genome_folder": "genomes",
            "genome_file_extension": "fasta",
            "lineage": "eurotiomycetes_odb10",
            "output_folder": "BUSCO",
            "seed_species": "anidulans",
            "busco_lineages_path": "$BUSCO_LINEAGES"
        }
    }
}