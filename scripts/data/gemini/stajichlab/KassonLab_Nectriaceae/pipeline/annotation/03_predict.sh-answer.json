{
    "application": "Funannotate",
    "details": {
        "software": [
            "Funannotate",
            "AUGUSTUS",
            "GMHMM",
            "BUSCO"
        ],
        "resource_requirements": {
            "nodes": 1,
            "tasks": 16,
            "memory": "64G",
            "partition": "intel",
            "output_file": "logs/predict.%a.log",
            "array_task_ids": [
                1,
                3,
                5
            ],
            "queue": "batch",
            "databases": {
                "funannotate_db": "/bigdata/stajichlab/shared/lib/funannotate_db",
                "busco_db": "sordariomycetes_odb10"
            },
            "input_directory": "genomes",
            "output_directory": "annotation",
            "samples_file": "samples.csv",
            "busco_seed": "anidulans",
            "augustus_config_path": "${AUGUSTUS_CONFIG_PATH}"
        }
    }
}