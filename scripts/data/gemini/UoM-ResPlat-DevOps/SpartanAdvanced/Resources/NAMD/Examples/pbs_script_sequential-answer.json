{
    "application": "NAMD",
    "details": {
        "scheduler": "PBS",
        "nodes": 8,
        "walltime": "24:0:0",
        "modules": "namd/2.7b2-openmpi-gcc",
        "mpi_implementation": "OpenMPI",
        "compiler": "GCC",
        "input_files": [
            "example_optimization.conf",
            "example_sequential_production.conf"
        ],
        "output_files": [
            "OutputText/optimization.$date.out",
            "Errors/optimization.$date.err",
            "OutputFiles/$basename.dcd",
            "RestartFiles/$basename.restart.coor",
            "RestartFiles/$basename.restart.vel",
            "RestartFiles/$basename.restart.xsc",
            "RestartFiles/$basename.restart.xst",
            "OutputText/FFTW*",
            "JobLog/pbs_*.e*",
            "JobLog/pbs_*.o*"
        ],
        "intermediate_files": [
            "generic_optimization.restart.coor",
            "generic_optimization.restart.vel",
            "generic_optimization.restart.xsc",
            "generic_restartfile.dcd",
            "generic_restartfile.coor",
            "generic_restartfile.vel",
            "generic_restartfile.xsc",
            "generic_restartfile.xst",
            "*.BAK",
            "*.old",
            "*.coor",
            "*.vel",
            "*.xsc",
            "*.xst"
        ],
        "directories": [
            "OutputText",
            "Errors",
            "OutputFiles",
            "RestartFiles",
            "JobLog"
        ],
        "script_language": "Bash"
    }
}