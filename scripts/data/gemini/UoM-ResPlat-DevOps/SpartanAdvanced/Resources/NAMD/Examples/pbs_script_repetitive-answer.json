{
    "application": "NAMD",
    "details": {
        "scheduler": "PBS",
        "nodes": 8,
        "walltime": "24:0:0",
        "modules": "namd/2.7b2-openmpi-gcc",
        "output_files": [
            "OutputText/optimization.$date.out",
            "Errors/optimization.$date.err",
            "OutputFiles/$basename.dcd",
            "OutputText/$basename.out",
            "Errors/$basename.err",
            "OutputText/FFTW*",
            "JobLog/pbs_*.e*",
            "JobLog/pbs_*.o*"
        ],
        "restart_files": [
            "RestartFiles/generic_restartfile.restart.coor",
            "RestartFiles/generic_restartfile.restart.vel",
            "RestartFiles/generic_restartfile.restart.xsc",
            "RestartFiles/$basename.restart.coor",
            "RestartFiles/$basename.restart.vel",
            "RestartFiles/$basename.restart.xsc",
            "RestartFiles/$basename.xst"
        ],
        "input_files": [
            "example_optimization.conf",
            "example_repetitive_production.conf"
        ],
        "other_files": [
            "JobLog/$date2$jobname.qstat.txt"
        ]
    }
}