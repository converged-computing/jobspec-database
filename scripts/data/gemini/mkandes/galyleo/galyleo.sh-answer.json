{
    "application": "Jupyter",
    "software_requirements": [
        "jupyter",
        "jupyterlab",
        "satellite",
        "singularity",
        "conda",
        "spark"
    ],
    "resource_requirements": {
        "scheduler": "slurm",
        "account": "required",
        "partition": "shared",
        "nodes": 1,
        "cpus_per_task": 1,
        "memory_per_node": 1,
        "time_limit": "00:30:00",
        "gpus": "optional",
        "gres": "optional",
        "constraint": "optional",
        "scratch_dir": "/tmp",
        "reverse_proxy_fqdn": "expanse-user-content.sdsc.edu",
        "dns_domain": "eth.cluster"
    }
}