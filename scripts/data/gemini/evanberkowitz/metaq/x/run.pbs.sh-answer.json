{
    "application": "MetaQ",
    "software": "Bash",
    "resource_requirements": {
        "nodes": 10,
        "walltime": "15:00",
        "output_log": "/full/path/to/metaq/job/%J/log/%J.log",
        "job_name": "metaq_example_job",
        "metaq_path": "/full/path/to/metaq",
        "metaq_job_id": "${PBS_JOB_ID}",
        "metaq_nodes": "${PBS_NUM_NODES}",
        "metaq_run_time": 900,
        "metaq_machine": "machine",
        "metaq_task_folders": [
            "$METAQ/priority",
            "$METAQ/todo"
        ],
        "metaq_gpus": 0,
        "metaq_max_launches": 1048576,
        "metaq_loop_forever": "false",
        "metaq_sleep_after_launch": 0,
        "metaq_sleepy_time": 3,
        "metaq_verbosity": 2,
        "metaq_simultaneous_tasks": 1048576,
        "metaq_min_nodes": 0,
        "metaq_min_gpus": 0,
        "metaq_max_nodes": "${METAQ_NODES}",
        "metaq_max_gpus": "${METAQ_GPUS}",
        "metaq_sort_tasks": "sort",
        "metaq_skip_on_stolen": "false"
    }
}