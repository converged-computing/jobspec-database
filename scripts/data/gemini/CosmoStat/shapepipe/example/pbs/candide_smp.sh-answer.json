{
    "application": "ShapePipe",
    "software": [
        "ShapePipe",
        "conda",
        "intelpython"
    ],
    "resource_requirements": {
        "cores": 4,
        "walltime": "00:05:00",
        "email_notification": "ea",
        "job_name": "shapepipe_smp",
        "output_and_error_in_one_file": true
    },
    "environment_variables": [
        "SPENV",
        "SPDIR"
    ],
    "config_file": "$SPDIR/example/pbs/config_smp.ini"
}