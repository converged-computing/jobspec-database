{
    "application": "Singularity",
    "details": {
        "resource_requirements": {
            "partition": "unkillable-cpu",
            "job_name": "tgi-compile",
            "output_file": "%x.%j.out",
            "cpus_per_task": 2,
            "memory": "16G",
            "time": "10:00:00"
        },
        "software_dependencies": {
            "singularity": "3.7.1",
            "docker_image": "ghcr.io/huggingface/text-generation-inference:2.0.1"
        },
        "environment_variables": {
            "RELEASE_DIR": "$HOME/tgi-sif",
            "TGI_DIR": "$SCRATCH/tgi",
            "TGI_TMP": "$SLURM_TMPDIR/tgi"
        }
    }
}