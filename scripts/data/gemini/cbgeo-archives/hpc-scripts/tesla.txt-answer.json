{
    "application": "SLURM",
    "details": {
        "software": [
            "SLURM",
            "Intel MPI",
            "OpenMP",
            "Turbostream"
        ],
        "resources": {
            "nodes": "2",
            "tasks": "4",
            "wallclock_time": "02:00:00",
            "gpu_type": "Tesla",
            "work_directory": "$SLURM_SUBMIT_DIR"
        },
        "environment": {
            "modules": [
                "default-wilkes"
            ],
            "variables": [
                "OMP_NUM_THREADS=1",
                "I_MPI_PIN_DOMAIN=omp:compact",
                "I_MPI_PIN_ORDER=scatter"
            ]
        }
    }
}