{
    "application": "Singularity",
    "details": {
        "scheduler": "SLURM",
        "job_name": "runctonaineraigj",
        "partition": "32GB",
        "nodes": 2,
        "memory": 28672,
        "time_limit": "0-4:0:00",
        "output_file": "job_%j.out",
        "error_file": "job_%j.err",
        "email_type": "ALL",
        "singularity_version": "3.5.3",
        "container_path": "[your folder with the container]",
        "tool_parameters": {
            "imag_directory": "[file path to images]",
            "imag_savesegmented": "[file path to save segmentations]",
            "mode": "nuclei",
            "flow_threshold": 0,
            "cellprob_threshold": -1,
            "celldiameter": 19,
            "channel": 1
        },
        "container_image": "cellpose_container.sif"
    }
}