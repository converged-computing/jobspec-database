        "GPUs",
        "CPU cores",
        "Memory",
        "Storage"
    ],
    "script_details": {
        "SBATCH_JOB_NAME": "dct_${DATASET}_${MODEL}_${RATIO}S",
        "LOG_DIR": "logs",
