{
    "application": "SLURM",
    "details": {
        "partition": "test",
        "nodes": 1,
        "tasks_per_node": 1,
        "cpus_per_task": 1,
        "time": "0:05:00",
        "memory": "10G",
        "array_range": "1-2",
        "group": "MATH021322",
        "executable": "/bin/hostname",
        "working_directory": "${SLURM_SUBMIT_DIR}",
        "encoders": [
            "RN50_clip",
            "resnet18"
        ],
        "python_script": "dataset_encoder.py",
        "script_arguments": [
            "--pretrained_encoder",
            "1",
            "--regime",
            "latent_ER",
            "--dataset_name",
            "CIFAR100",
            "--dataset_encoder_name",
            "${LS[${SLURM_ARRAY_TASK_ID}]}"
        ],
        "virtual_environment": "~/tmp/mypyenvb/bin/activate"
    }
}