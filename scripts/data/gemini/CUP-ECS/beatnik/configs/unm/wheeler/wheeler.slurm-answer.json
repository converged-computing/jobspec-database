{
    "application": "rocketrig",
    "details": {
        "environment": {
            "SPACK_INSTALL": "${HOME}/spack",
            "BEATNIK_SCRATCH": "/carc/scratch/users/${USER}/beatnik-wheeler"
        },
        "dependencies": [
            "spack",
            "beatnik"
        ],
        "resources": {
            "nodes": 2,
            "tasks_per_node": 8,
            "cpus_per_task": 1,
            "time_limit": "1:00:00",
            "output_log": "BeatnikTest.%j.log"
        },
        "parameters": {
            "rocketrig_n": 512,
            "rocketrig_F": 0,
            "rocketrig_w": "${SLURM_NTASKS}"
        }
    }
}