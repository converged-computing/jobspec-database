{
    "application": "GEOS-Chem Inversion",
    "details": {
        "software": [
            "GEOS-Chem",
            "Python",
            "Bash",
            "SLURM"
        ],
        "dependencies": [
            "parse_yaml.sh",
            "postproc_diags.py",
            "calc_sensi.py",
            "setup_gc_cache.py",
            "jacobian.py",
            "invert.py",
            "make_gridded_posterior.py"
        ],
        "resources": [
            "CPU",
            "Memory",
            "Disk space",
            "Network access"
        ],
        "configuration": {
            "config_file": "config.yml",
            "inversion_path": "{INVERSION_PATH}",
            "output_path": "{OUTPUT_PATH}",
            "state_vector_file": "{STATE_VECTOR_PATH}",
            "lon_min": "{LON_MIN}",
            "lon_max": "{LON_MAX}",
            "lat_min": "{LAT_MIN}",
            "lat_max": "{LAT_MAX}",
            "state_vector_elements": "{STATE_VECTOR_ELEMENTS}",
