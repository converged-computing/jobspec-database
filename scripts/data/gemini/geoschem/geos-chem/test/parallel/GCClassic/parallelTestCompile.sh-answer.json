{
    "application": "GEOS-Chem",
    "details": {
        "scheduler": "SLURM or LSF",
        "cores": 8,
        "memory": "8GB",
        "time": "30 minutes",
        "partition": "REQUESTED_PARTITION",
        "docker_image": "registry.gsc.wustl.edu/sleong/esm:intel-2021.1.2",
        "environment": {
            "OMP_NUM_THREADS": 8,
            "OMP_STACKSIZE": "500m"
        },
        "resources": {
            "buildDir": "${ptRoot}/${BUILD_DIR}",
            "binDir": "${ptRoot}/${BIN_DIR}",
            "envDir": "${ptRoot}/${ENV_DIR}",
            "codeDir": "${ptRoot}/CodeDir",
            "logsDir": "${ptRoot}/${LOGS_DIR}",
            "scriptsDir": "${ptRoot}/${SCRIPTS_DIR}"
        },
        "build_options": "-DCMAKE_BUILD_TYPE=Debug -DRUNDIR='' -DINSTALLCOPY=${binDir}"
    }
}