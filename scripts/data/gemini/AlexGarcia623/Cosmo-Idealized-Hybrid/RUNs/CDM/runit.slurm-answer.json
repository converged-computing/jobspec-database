{
    "application": "Arepo",
    "details": {
        "software": {
            "intel": "2018.1.163",
            "openmpi": "3.1.2",
            "gsl": "2.4",
            "fftw": "3.3.7"
        },
        "resources": {
            "memory_per_cpu": "4000mb",
            "time": "4-00:00:00",
            "partition": "hpg2-compute",
            "tasks": 1024,
            "tasks_per_socket": 8,
            "cpus_per_task": 1,
            "account": "astronomy-dept",
            "qos": "astronomy-dept-b",
            "output": "./output-blue/output_%j.out",
            "error": "./output-blue/error_%j.err"
        },
        "environment_variables": {
            "OMPI_MCA_pml": "ucx",
            "OMPI_MCA_btl": "^vader,tcp,openib",
            "OMPI_MCA_oob_tcp_listen_mode": "listen_thread"
        }
    }
}