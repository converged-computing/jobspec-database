{
    "application": "Arepo",
    "software": [
        "intel/2018.1.163",
        "openmpi/3.1.2",
        "gsl/2.4",
        "fftw/3.3.7"
    ],
    "resource_requirements": {
        "memory": "3500mb per cpu",
        "time": "96 hours",
        "partition": "hpg2-compute",
        "tasks": 512,
        "cpus_per_task": 1,
        "qos": "paul.torrey-b",
        "output_file": "./output/output_%j.out",
        "error_file": "./output/error_%j.err"
    },
    "environment_variables": [
        "OMPI_MCA_pml=ucx",
        "OMPI_MCA_btl=^vader,tcp,openib",
        "OMPI_MCA_oob_tcp_listen_mode=listen_thread"
    ]
}