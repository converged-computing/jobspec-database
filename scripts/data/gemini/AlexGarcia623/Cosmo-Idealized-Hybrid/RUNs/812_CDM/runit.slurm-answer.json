{
    "application": "Arepo",
    "software": [
        "intel/2018.1.163",
        "openmpi/3.1.2",
        "gsl/2.4",
        "fftw/3.3.7"
    ],
    "resource_requirements": {
        "memory": "6000mb per cpu",
        "time": "2 days",
        "partition": "hpg2-compute",
        "tasks": 512,
        "tasks_per_socket": 8,
        "cpus_per_task": 1,
        "account": "astronomy-dept",
        "qos": "astronomy-dept-b",
        "output": "./output-blue/output_%j.out",
        "error": "./output-blue/error_%j.err"
    },
    "mpi_settings": {
        "pml": "ucx",
        "btl": "^vader,tcp,openib",
        "oob_tcp_listen_mode": "listen_thread"
    }
}