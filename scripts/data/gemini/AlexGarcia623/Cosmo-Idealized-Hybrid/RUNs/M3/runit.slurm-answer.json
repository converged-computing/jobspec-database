{
    "application": "Arepo",
    "software": [
        "intel/2018.1.163",
        "openmpi/3.1.2",
        "gsl/2.4",
        "fftw/3.3.7"
    ],
    "resources": {
        "memory": "6000mb per CPU",
        "time": "4 days",
        "partition": "hpg2-compute",
        "tasks": 1024,
        "tasks_per_socket": 8,
        "cpus_per_task": 1,
        "qos": "paul.torrey-b",
        "output_file": "./output-blue/output_%j.out",
        "error_file": "./output-blue/error_%j.err"
    },
    "dependencies": "singleton",
    "mpi_implementation": "pmix_v2",
    "communication_interface": "ucx",
    "communication_protocols": "vader,tcp,openib"
}