{
    "application": "dask-cuda-worker",
    "details": {
        "scheduler": "dask-cuda-worker",
        "scheduler_file": "$MEMBERWORK/$PROJ_ID/my-scheduler-gpu.json",
        "local_directory": "$MEMBERWORK/$PROJ_ID",
        "nthreads": 1,
        "memory_limit": "100GB",
        "device_memory_limit": "16GB",
        "death_timeout": 60,
        "interface": "ib0",
        "infiniband": "enabled",
        "nvlink": "enabled",
        "modules": [
            "gcc/6.4.0",
            "cuda/10.1.168"
        ],
        "environment_variable": "PATH=$WORLDWORK/stf011/nvrapids_0.11_gcc_6.4.0/bin:$PATH",
        "job_submission_system": "LSF",
        "job_submission_parameters": {
            "project": "ABC123",
            "walltime": "1:00",
            "alloc_flags": "gpumps",
            "nnodes": 2,
            "job_name": "dask_cuda_worker",
            "output_file": "dask_cuda_worker.o%J",
            "error_file": "dask_cuda_worker.e%J"
        },
        "jsrun_parameters": {
            "c": 42,
            "g": 6,
            "n": 2,
            "r": 1,
            "a": 1
        }
    }
}