{
    "application": "SLURM",
    "details": {
        "job_name": "docker",
        "output": "out_slurm/docker_%A.out",
        "error": "out_slurm/docker_%A.err",
        "array_size": 200,
        "conda_environment": "optimol_cpu",
        "python_script": "docker.py",
        "arguments": [
            "$SLURM_ARRAY_TASK_ID",
            "200",
            "--server",
            "$1",
            "--exhaustiveness",
            "$2",
            "--name",
            "$3",
            "--oracle",
            "$4",
            "--target",
            "$5"
        ]
    }
}