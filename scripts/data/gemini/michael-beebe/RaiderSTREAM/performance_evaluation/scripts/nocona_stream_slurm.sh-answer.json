{
    "application": "Slurm",
    "details": {
        "scheduler": "Slurm",
        "partition": "nocona",
        "nodes": 2,
        "sockets_per_node": 2,
        "tasks_per_node": 128,
        "walltime": "00:25:00",
        "compiler": "gcc/10.1.0",
        "mpi": "openmpi/4.0.4",
        "stream_array_size": 8700000,
        "omp_num_threads": 1,
        "np_value": 2,
        "stream_dir": "current directory",
        "output_dir": "outputs_mc_o3",
        "output_file": "multi_node_output_$(date +\"%d-%m-%y\")_$(date +\"%T\").txt",
        "make_commands": [
            "make stream_oshmem.exe CFLAGS=\"-fopenmp -O3\" PFLAGS=\"\""
        ],
        "benchmark_runs": 5,
        "executables": [
            "stream_oshmem.exe"
        ],
        "run_commands": [
            "oshrun -np $NP_VALUE stream_oshmem.exe"
        ]
    }
}