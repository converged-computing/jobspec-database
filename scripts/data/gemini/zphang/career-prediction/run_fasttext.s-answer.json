{
    "application": "fastText",
    "details": {
        "environment": {
            "pytorch": "0.2.0_3 (python3.6)",
            "conda environment": "jobembeddings"
        },
        "resources": {
            "nodes": 1,
            "tasks per node": 1,
            "cpus per task": 1,
            "time": "10:10:00",
            "memory": "50GB",
            "scratch directory": "$SCRATCH/data/jobembeddings"
        },
        "input": "$RUNDIR/json_sample10m_fasttext_train",
        "output": "$RUNDIR/fasttext_10m_model",
        "command": "fastText supervised"
    }
}