{
    "application": "PBS",
    "details": {
        "job_name": "runDataExport-<TMPL_VAR NAME=iSim>",
        "user": "<TMPL_VAR NAME=linuxUser>:<TMPL_VAR NAME=linuxUser>",
        "notification_email": "<TMPL_VAR NAME=emailAddress>",
        "notification_method": "abe",
        "walltime": "48:00:00",
        "nodes": "1:ppn=4",
        "memory": "20gb",
        "virtual_memory": "20gb",
        "log_output": "<TMPL_VAR NAME=outDir>/<TMPL_VAR NAME=conditionSetTimeStamp>/<TMPL_VAR NAME=iSim>/json/out.dataexport.log",
        "log_error": "<TMPL_VAR NAME=outDir>/<TMPL_VAR NAME=conditionSetTimeStamp>/<TMPL_VAR NAME=iSim>/json/err.dataexport.log",
        "umask": "002",
        "environment_variables": {
            "MATLAB_PREFDIR": "/tmp/emptydir",
            "MCR_CACHE_ROOT": "/tmp/mcr_cache_$PBS_JOBID",
            "XAPPLRESDIR": "/usr/local/bin/MATLAB/MATLAB_Compiler_Runtime/v716/X11/app-defaults",
            "LD_LIBRARY_PATH": "$LD_LIBRARY_PATH:/usr/local/bin/MATLAB/MATLAB_Compiler_Runtime/v716/runtime/glnxa64:/usr/local/bin/MATLAB/MATLAB_Compiler_Runtime/v716/sys/os/glnxa64:/usr/local/bin/MATLAB/MATLAB_Compiler_Runtime/v716/sys/java/jre/glnxa64/jre/lib/amd64/native_threads:/usr/local/bin/MATLAB/MATLAB_Compiler_Runtime/v716/sys/java/jre/glnxa64/jre/lib/amd64/server:/usr/local/bin/MATLAB/MATLAB_Compiler_Runtime/v716/sys/java/jre/glnxa64/jre/lib/amd64"
        },
        "scripts": {
            "runDataExport": "./run.sh runDataExport",
            "ftszring_draw": "python2.7 /home/projects/WholeCellViz/data/ftszring_draw.py"
        },
        "software": {
            "matlab": "MATLAB_Compiler_Runtime/v716",
            "python": "python2.7"
        },
        "resources": {
            "base_directory": "<TMPL_VAR NAME=baseDir>",
            "output_directory": "<TMPL_VAR NAME=outDir>",
            "condition_set_timestamp": "<TMPL_VAR NAME=conditionSetTimeStamp>",
            "iSim": "<TMPL_VAR NAME=iSim>",
            "runtime_path": "<TMPL_VAR NAME=pathToRunTime>"
        }
    }
}