{
    "application": "LAMMPS",
    "details": {
        "scheduler": "SLURM",
        "container_runtime": "Shifter",
        "container_image": "docker:deepmodeling/deepmd-kit:2.2.7_cuda11.6_gpu",
        "gpu_constraint": "gpu",
        "qos": "debug",
        "time_limit": "00:10:00",
        "nodes": 1,
        "tasks": 4,
        "tasks_per_node": 4,
        "gpus": 1,
        "mpi_implementation": "pmi2",
        "cpu_binding": "cores",
        "working_directory": "/workspace",
        "input_file": "in.lammps",
        "log_file": "log",
        "parallel_mode": "4x1"
    }
}