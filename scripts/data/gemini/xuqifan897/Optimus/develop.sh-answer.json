{
    "application": "SUMMA",
    "details": {
        "environment": {
            "cuda": "10.1",
            "anaconda": "anaconda3",
            "conda_env": "SUMMA"
        },
        "resources": {
            "scheduler": "SLURM",
            "partition": "rtx",
            "nodes": 2,
            "tasks_per_node": 8,
            "time_limit": "00:30:00"
        },
        "script": "pretrain_develop.py",
        "arguments": [
            "--checkpoint-activations",
            "--distribute-checkpointed-activations",
            "--master-port 2048",
            "--batch-size 96",
            "--hidden-size 2048",
            "--rank-rearrange"
        ]
    }
}