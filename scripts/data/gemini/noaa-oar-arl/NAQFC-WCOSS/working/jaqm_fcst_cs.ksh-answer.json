{
    "application": "CMAQ",
    "details": {
        "software": [
            "ksh",
            "PrgEnv-intel",
            "iobuf",
            "prod_util",
            "prod_envir"
        ],
        "resources": {
            "nodes": 24,
            "memory": "3000",
            "walltime": "02:30",
            "queue": "dev",
            "iobuf_params": "12M:verbose",
            "ulimit_stacksize": "unlimited"
        },
        "environment_variables": {
            "RUN_ENVIR": "notnco",
            "COMROOT": "/gpfs/hps3/ptmp/${USER}/com",
            "NWROOT": "/u/${USER}/noscrub/nwdev",
            "NWROOTp1": "/nwprod",
            "cmaq_ver": "v5.3.1",
            "envir": "para",
            "job": "aqm_cmaq_cs",
            "PARAFLAG": "YES",
            "SENDCOM": "YES",
            "SENDDBN": "NO",
            "SENDECF": "NO",
            "FCST": "YES",
            "HOMEaqm": "${HOMEaqm}"
        }
    }
}