{
    "application": "Slurm",
    "details": {
        "job_name": "run",
        "output_file": "scons.log.out",
        "error_file": "scons.log.err",
        "partition": "compute",
        "exclusive": true,
        "export_all": true,
        "tasks_per_node": 1,
        "nodes": 24,
        "node_list": "compute[136-159]",
        "dependency": "afterok:1470525",
        "command": "scons -f SConstruct",
        "environment_variable": "LOCALDATAPATH=/localscratch",
        "temporary_directory": "/local/tmpswfl",
        "srun_options": {
            "nodes": "$NODES",
            "ntasks": "$TASKS",
            "ntasks_per_node": "$TPN",
            "executable": "$EXE",
            "output_file": "output.$JOBID"
        }
    }
}