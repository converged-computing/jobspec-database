{
    "application": "MaSuRCA",
    "details": {
        "environment": {
            "modules": [
                "/beegfs/easybuild/CentOS/7.6.1810/Skylake/modules/all",
                "/beegfs/easybuild/common/modules/all"
            ],
            "conda_environment": "irenegmasurca"
        },
        "resources": {
            "memory": "200GB",
            "job_name": "MaSuRCA",
            "partition": "long",
            "time_limit": "1-59:00:20",
            "threads": 32
        },
        "input_files": [
            "{folder}_1.fastq.gz",
            "{folder}_2.fastq.gz"
        ],
        "output_files": [
            "contigs.fa"
        ]
    }
}