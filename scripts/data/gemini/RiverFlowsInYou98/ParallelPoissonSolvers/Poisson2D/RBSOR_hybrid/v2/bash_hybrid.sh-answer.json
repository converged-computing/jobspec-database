{
    "application": "Poisson2D_RBSOR_hybrid",
    "details": {
        "runtime": "1 hour",
        "cores": "32",
        "architecture": "intel cascade edr",
        "mpi tasks": "16",
        "threads per task": "2",
        "compiler": "gcc 10.2",
        "mpi": "openmpi 4.1.1",
        "openmp": "enabled",
        "libraries": [
            "cuda 11.7.1"
        ],
        "slurm": "used for job scheduling and resource management",
        "command": "srun --mpi=pmix ./Poisson2D_RBSOR_hybrid.out <Nx> <Ny>",
        "input": "Nx and Ny values to set the grid dimensions"
    }
}