{
    "application": "Poisson2D_RBSOR_hybrid",
    "details": {
        "runtime": "1 hour",
        "nodes": "1",
        "constraint": "32core|intel|cascade|edr",
        "tasks": "16",
        "cpus_per_task": "2",
        "tasks_per_core": "1",
        "modules": [
            "gcc/10.2",
            "cuda/11.7.1",
            "mpi/openmpi_4.1.1_gcc_10.2_slurm22"
        ],
        "compiler": "mpic++",
        "optimization_flags": "-O3",
        "openmp_flag": "-fopenmp",
        "mpi_library": "pmix",
        "output_files": [
            "Hybrid-%j.out",
            "Hybrid-%j.err"
        ]
    }
}