{
    "application": "PBS",
    "software": [
        "nosofs",
        "PrgEnv-intel",
        "craype",
        "intel",
        "cray-pals",
        "netcdf",
        "hdf5",
        "libjpeg",
        "subversion",
        "python",
        "prod_envir",
        "prod_util",
        "grib_util",
        "wgrib2",
        "cfp",
        "gsl",
        "udunits",
        "nco",
        "cdo"
    ],
    "resources": [
        "place=vscatter",
        "select=1:ncpus=8:mpiprocs=8",
        "walltime=1:30:00"
    ],
    "environment_variables": [
        "envir=dev",
        "OFS=cbofs",
        "cyc=06",
        "job=cbofs_prep_06",
        "platform=ptmp",
        "PDY=20210824",
        "KEEPDATA=YES",
        "SENDCOM=NO",
        "SENDDBN=NO",
        "SENDSMS=NO",
        "PACKAGEROOT=/lfs/h1/nos/nosofs/noscrub/$LOGNAME/packages",
        "COMPATH=/lfs/h1/ops/prod/com",
        "COMROOT=/lfs/h1/nos/ptmp/$LOGNAME/com",
        "DCOMROOT=/lfs/h1/ops/prod/dcom",
        "DATAROOT=/lfs/h1/nos/ptmp/$LOGNAME/work/${nosofs_ver}/${OFS}",
        "COMINnam=/lfs/h1/ops/prod/com/nam/${nam_ver}",
        "COMINhrrr=/lfs/h1/ops/prod/com/hrrr/${hrrr_ver}",
        "COMINrap=/lfs/h1/ops/prod/com/rap/${rap_ver}",
        "COMINgfs=/lfs/h1/ops/prod/com/gfs/${gfs_ver}",
        "COMINrtma=/lfs/h1/ops/prod/com/rtma/${rtma_ver}",
        "COMINetss=/lfs/h1/ops/prod/com/etss/${etss_ver}",
        "COMINrtofs_2d=/lfs/h1/ops/prod/com/rtofs/${rtofs_ver}",
        "COMINrtofs_3d=/lfs/h1/ops/prod/com/rtofs/${rtofs_ver}",
        "COMINnwm=/lfs/h1/ops/prod/com/nwm/${nwm_ver}"
    ],
    "executable": "/lfs/h1/nos/nosofs/noscrub/$LOGNAME/packages/nosofs.${nosofs_ver}/jobs/JNOS_OFS_PREP"
}