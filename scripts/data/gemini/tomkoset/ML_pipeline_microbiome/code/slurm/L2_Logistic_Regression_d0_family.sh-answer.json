{
    "application": "SLURM",
    "details": {
        "language": "Bash",
        "libraries": [
            "Rscript"
        ],
        "resources": {
            "nodes": 1,
            "tasks_per_node": 1,
            "cpus_per_task": 1,
            "memory_per_cpu": "4GB",
            "time_limit": "5:00:00",
            "account": "pschloss1",
            "partition": "standard",
            "mail_user": "tomkoset@umich.edu",
            "mail_type": [
                "BEGIN",
                "END",
                "FAIL"
            ],
            "output": "%x-%j.out",
            "environment": "ALL",
            "array_size": 100
        },
        "files": [
            "code/R/main.R",
            "test/data/classification_input_day0_data_family.csv",
            "test/data/hyperparams.csv"
        ],
        "commands": [
            "mkdir -p logs/slurm/",
            "Rscript code/R/main.R --seed $seed --model L2_Logistic_Regression --data  test/data/classification_input_day0_data_family.csv --hyperparams test/data/hyperparams.csv --outcome dx"
        ]
    }
}