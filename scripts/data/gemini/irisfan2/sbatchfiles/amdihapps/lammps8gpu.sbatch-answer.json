{
    "application": "LAMMPS",
    "details": {
        "version": "2022.5.04_130",
        "container": "singularity",
        "image": "/shared/apps/bin/lammps_2022.5.04_130.sif",
        "compute_nodes": 1,
        "tasks_per_node": 8,
        "cores_per_task": 8,
        "memory": "64GB",
        "gpus": 8,
        "gpu_architecture": "ROCm 5.2.3",
        "mpi": "OpenMPI",
        "parallelism": "MPI",
        "package_manager": "module",
        "potentials": [
            "LJ",
            "EAM",
            "TERSOFF",
            "REAXFF",
            "SNAP"
        ],
        "input_files": [
            "in.lj",
            "in.eam",
            "in.tersoff",
            "in.reaxc.hns",
            "in.snap.exaalt"
        ],
        "output_files": [
            "%x-%N-%j.out",
            "%x-%N-%j.err",
            "gpu8-$$-$SLURM_JOB_NODELIST-$SLURM_JOB_ID"
        ],
        "tmp_dir": "/tmp/$USER/gpu8-$$"
    }
}