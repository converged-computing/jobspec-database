{
    "application": "GROMACS",
    "details": {
        "environment": "ROCm 5.2.3",
        "singularity_image": "/shared/apps/bin/gromacs.sif",
        "benchmark": "STMV",
        "MPI": "mpirun -np 4",
        "compute_resources": {
            "nodes": 1,
            "tasks_per_node": 4,
            "cpus_per_task": 8,
            "memory": "64GB",
            "gpus": 2
        },
        "GROMACS_parameters": {
            "steps": 100000,
            "resetstep": 90000,
            "ntomp": 16,
            "nb": "gpu",
            "bonded": "cpu",
            "pme": "gpu",
            "npme": 1,
            "nstlist": 200,
            "gpu_id": "01"
        },
        "output_files": [
            "/tmp/$USER/gpu2-$$/stmv/md.log",
            "/tmp/$USER/gpu2-$$/stmv/ener.eder",
            "/tmp/$USER/gpu2-$$/stmv/state.cpt"
        ]
    }
}