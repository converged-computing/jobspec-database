{
    "application": "jupyter",
    "details": {
        "environment": {
            "modulepath": "/share/apps/compute/modulefiles/applications:$MODULEPATH",
            "anaconda": "loaded via module"
        },
        "resources": {
            "partition": "compute",
            "nodes": 1,
            "tasks_per_node": 24,
            "time": "4 hours",
            "output_file": "jupyter-notebook.%j.%N.out"
        },
        "jupyter_options": {
            "no_browser": true,
            "ip": "*"
        }
    }
}