{
    "application": "Dask",
    "details": {
        "resource_manager": "Slurm",
        "slurm_options": {
            "account": "ocp",
            "exclusive": true,
            "nodes": 1,
            "job_name": "worker",
            "time": "12:00:00",
            "array": "0-4"
        },
        "dask_options": {
            "memory_limit": "100e9",
            "nthreads": 4,
            "local_directory": "/local/dask-worker-dir-$$",
            "scheduler_file": "$HOME/.dask_schedule_file.json",
            "interface": "ib0"
        }
    }
}