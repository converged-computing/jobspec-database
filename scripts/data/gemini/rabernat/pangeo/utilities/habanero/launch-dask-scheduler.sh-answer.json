{
    "application": "Dask",
    "details": {
        "scheduler": "Dask-MPI",
        "resource_requirements": {
            "nodes": 1,
            "processes_per_node": 6,
            "threads_per_process": 4,
            "memory_limit": "0.15 (fraction of node memory)",
            "time_limit": "6 hours",
            "local_directory": "/local/$USER",
            "scheduler_file": "~/scheduler.json"
        },
        "environment": {
            "module": "anaconda",
            "conda_environment": "pangeo"
        },
        "communication": {
            "interface": "ib0 (commented out in script)"
        }
    }
}