{
    "application": "MPI",
    "details": {
        "resource_manager": "PBS",
        "walltime": "10 minutes",
        "queue": "workq",
        "filesystems": "home, tegu",
        "account": "datascience",
        "nodes": 2,
        "libraries": [
            "aws-ofi-nccl",
            "hwloc",
            "conda"
        ],
        "environment_variables": [
            "NCCL_NET_GDR_LEVEL",
            "NCCL_CROSS_NIC",
            "NCCL_COLLNET_ENABLE",
            "NCCL_NET",
            "LD_LIBRARY_PATH",
            "FI_CXI_DISABLE_HOST_REGISTER",
            "FI_MR_CACHE_MONITOR",
            "FI_CXI_DEFAULT_CQ_SIZE",
            "NCCL_DEBUG",
            "OUTPUT"
        ],
        "mpi_command": "mpiexec",
        "python_module": "torch_nccl.py",
        "launcher": "launcher.sh"
    }
}