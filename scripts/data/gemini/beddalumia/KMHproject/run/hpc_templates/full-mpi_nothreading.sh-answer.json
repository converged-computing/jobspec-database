{
    "application": "SLURM",
    "details": {
        "resource_manager": "SLURM",
        "modules": [
            "gnu8/8.3.0",
            "mkl/19.1.3.304",
            "openmpi3/3.1.4",
            "matlab"
        ],
        "cpu_resources": {
            "tasks_per_node": 20,
            "nodes": 3,
            "hint": "nomultithread"
        },
        "memory": {
            "per_node": 0
        },
        "partition": "regular1",
        "time_limit": "12:00:00",
        "output_log": "sLOG_%x_out%j.txt",
        "error_log": "sLOG_%x_err%j.txt",
        "job_commands": [
            "matlab -batch \"runDMFT.dry_line('ed_kane_mele',doMPI,Uold,Umin,Ustep,Umax,'t2',SOI)\"",
            "matlab -batch \"runDMFT.autostop_line('ed_kane_mele',doMPI,Uold,Umin,Ustep,Umax,'t2',SOI)\"",
            "matlab -batch \"runDMFT.autostep_line('ed_kane_mele',doMPI,Uold,Umin,Ustep,Umax,'t2',SOI)\""
        ]
    }
}