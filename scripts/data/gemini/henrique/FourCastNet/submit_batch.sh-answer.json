{
    "application": "Slurm",
    "details": {
        "runtime": "6 hours",
        "resources": {
            "nodes": 2,
            "partition": "nvgpu",
            "tasks_per_node": 4,
            "gpus_per_node": 4,
            "cpus_per_task": 32
        },
        "environment": {
            "conda_env": "cu113",
            "hdf5_locking": "disabled",
            "nccl_net_gdr_level": "PHB"
        },
        "script": {
            "name": "train.py",
            "arguments": "--enable_amp --yaml_config=$config_file --config=$config --run_num=$run_num",
            "configuration": {
                "config_file": "./config/AFNO.yaml",
                "config": "afno_backbone_finetune",
                "run_num": "0"
            }
        }
    }
}