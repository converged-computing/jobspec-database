{
    "application": "Snakemake",
    "details": {
        "scheduler": "SLURM",
        "time_limit": "200:00:00",
        "output_file": "${PWD}/snakemake.%j.out",
        "error_file": "${PWD}/snakemake.%j.err",
        "conda_environment": "snakemake",
        "temporary_directory": "/data/DCEG_Chernobyl/NP0436-HE5/GIAB_DATA/TMP",
        "execution_mode": "parallel",
        "parallel_jobs": 400,
        "container_type": "Singularity",
        "environment_modules": "enabled",
        "latency_wait": 120,
        "max_running_jobs": 0,
        "snakefile": "Snakefile_STR3",
        "workflow_profile": "workflow/profiles/biowulf",
        "verbosity": "verbose",
        "rerun_incomplete_jobs": "enabled",
        "keep_intermediate_files": "enabled"
    }
}