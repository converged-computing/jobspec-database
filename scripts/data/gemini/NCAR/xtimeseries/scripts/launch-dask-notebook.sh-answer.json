{
    "application": "Dask",
    "details": {
        "software": [
            "Dask",
            "MPI",
            "Conda",
            "Jupyter Lab",
            "qsub"
        ],
        "resources": {
            "compute": {
                "nodes": 1,
                "cpus": 36,
                "memory": "109GB"
            },
            "storage": {
                "scratch": "/glade/scratch/${USER}/dask"
            },
            "environment": {
                "conda_env": "${DEFAULT_CONDA_ENV}",
                "job_account": "${JOB_ACCOUNT}"
            },
            "queue": "${queue}",
            "walltime": "${walltime}"
        }
    }
}