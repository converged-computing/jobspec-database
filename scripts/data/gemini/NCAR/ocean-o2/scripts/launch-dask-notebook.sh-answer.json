{
    "application": "Dask",
    "software": [
        "Dask",
        "MPI",
        "Conda",
        "Jupyter Lab",
        "Setup-Jlab.py"
    ],
    "resource_requirements": {
        "walltime": "11:59:00",
        "queue": "economy",
        "workers": "4",
        "account": "${JOB_ACCOUNT}",
        "conda_env": "${DEFAULT_CONDA_ENV}",
        "memory": "109GB",
        "cores": "36",
        "mpiprocs": "9",
        "ompthreads": "4",
        "interface": "ib0",
        "local_directory": "${WORKDIR}",
        "scheduler_file": "${WORKDIR}/scheduler.json"
    },
    "environment_variables": {
        "DEFAULT_CONDA_ENV": "The default Conda environment to use",
        "JOB_ACCOUNT": "The account to use for the job",
        "WORKDIR": "The working directory to use"
    }
}