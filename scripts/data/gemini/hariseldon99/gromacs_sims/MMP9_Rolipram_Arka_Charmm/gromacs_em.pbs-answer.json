{
    "application": "GROMACS",
    "details": {
        "software": [
            "GROMACS",
            "PBS",
            "OpenMPI",
            "OpenMP",
            "curl"
        ],
        "resources": {
            "nodes": 1,
            "mpi_tasks_per_node": 24,
            "walltime": "06:00:00",
            "queue": "workq",
            "placement": "scatter"
        },
        "environment_variables": [
            "PBS_N",
            "PBS_Q",
            "PBS_O",
            "MPI_MCA_mca_base_component_show_load_errors",
            "PMIX_MCA_mca_base_component_show_load_errors",
            "USE_OPENMP",
            "GROFILE",
            "MDPFILE",
            "TOPOL_FILE",
            "EMFILE",
            "OMP_NUM_THREADS",
            "MPI_NUM_PROCS",
            "RUNTIME",
            "TOKEN",
            "CHATID",
            "LOGIN_NODE",
            "SSHBIN",
            "URL",
            "TEXT",
            "CMD"
        ],
        "files": [
            "mpi.out",
            "solv_ions.gro",
            "md.mdp",
            "topol.top",
            "em.tpr",
            "$PBS_NODEFILE",
            "${PBS_O_HOME}/.config/telegram/telegram.conf"
        ]
    }
}