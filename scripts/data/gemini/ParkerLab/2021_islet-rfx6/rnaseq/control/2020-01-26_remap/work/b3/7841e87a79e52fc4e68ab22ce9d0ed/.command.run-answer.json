{
    "application": "Nextflow",
    "software": [
        "bash",
        "ps",
        "date",
        "awk",
        "sed",
        "grep",
        "ln",
        "mkdir",
        "cp",
        "rm",
        "source",
        "tee",
        "wait",
        "kill"
    ],
    "resources": [
        "localscratch/vivekrai",
        "/dev/shm",
        "$TMPDIR",
        "/proc/meminfo",
        "/proc/cpuinfo",
        "/proc/$$/fd",
        "/proc/$pid/stat",
        "/proc/$pid/io",
        "$HOME/.bashrc"
    ],
    "environment_variables": [
        "NXF_DEBUG",
        "NXF_ENTRY",
        "NXF_CHDIR",
        "NXF_SCRATCH",
        "HOSTNAME"
    ]
}