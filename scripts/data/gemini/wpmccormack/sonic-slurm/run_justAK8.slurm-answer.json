{
    "application": "SLURM",
    "details": {
        "software": [
            "bash",
            "date",
            "sleep",
            "cmsset_default.sh",
            "scram",
            "ln",
            "tar",
            "cmsRun",
            "python3.6"
        ],
        "resources": {
            "nodes": 1,
            "tasks": 1,
            "cpus_per_task": 4,
            "memory": "1gb",
            "time_limit": "05:00:00",
            "output_directory": "/tmp/dir_${SLURM_JOB_ID}_${SLURM_PROCID}"
        },
        "libraries": [
            "CMSSW_11_3_0",
            "CMSSW_12_0_0_pre5",
            "sonic-workflows"
        ],
        "files": [
            "~/cms_path/SITECONF/local/JobConfig/site-local-config.xml",
            "~/CMSSW_12_0_0_pre5_ttbar_justAK8.tgz",
            "~/local_files/*.root",
            "~/file_of_files.txt",
            "~/throughputFinder.py"
        ],
        "environment_variables": [
            "SCRAM_ARCH",
            "CMS_PATH"
        ]
    }
}