{
    "application": "BatchControl",
    "details": {
        "software": [
            "Python",
            "psana",
            "PyDataSource",
            "mpirun"
        ],
        "resource_requirements": {
            "queue": [
                "psanaq",
                "psnehq"
            ],
            "jobs": {
                "max": 16,
                "default": 12
            },
            "environment_variables": [
                "PYTHONPATH",
                "LD_LIBRARY_PATH",
                "DISPLAY",
                "XAUTHORITY"
            ]
        }
    }
}