{
    "application": "Stable Diffusion",
    "details": {
        "framework": "PyTorch",
        "cluster": "SLURM",
        "resources": {
            "nodes": 44,
            "tasks_per_node": 1,
            "cpus_per_gpu": 4,
            "gpus": 8,
            "partition": "compute-od-gpu",
            "exclusive": true,
            "mpi": "Intel MPI",
            "libraries": [
                "aws-ofi-nccl",
                "amazon-efa",
                "cuda-11.0",
                "nccl"
            ],
            "environment_variables": [
                "LD_LIBRARY_PATH",
                "NCCL_PROTO",
                "PATH",
                "LD_PRELOAD",
                "FI_EFA_FORK_SAFE",
                "FI_LOG_LEVEL",
                "FI_EFA_USE_DEVICE_RDMA",
                "NCCL_DEBUG",
                "PYTHONFAULTHANDLER",
                "CUDA_LAUNCH_BLOCKING",
                "OMPI_MCA_mtl_base_verbose",
                "FI_EFA_ENABLE_SHM_TRANSFER",
                "FI_PROVIDER",
                "FI_EFA_TX_MIN_CREDITS",
                "NCCL_TREE_THRESHOLD",
                "HOSTNAMES",
                "MASTER_ADDR",
                "MASTER_PORT",
                "COUNT_NODE",
                "WORLD_SIZE"
            ]
        }
    }
}