{
    "application": "SLURM",
    "details": {
        "queue": "meta_gpu-ti",
        "array_size": 10,
        "environment": "pytorch",
        "python_script": "cifar10_master.py",
        "arguments": {
            "array_id": "$SLURM_ARRAY_TASK_ID",
            "total_num_workers": 10,
            "num_iterations": 32,
            "run_id": "$SLURM_ARRAY_JOB_ID",
            "working_directory": "./data/run_$SLURM_ARRAY_JOB_ID"
        },
        "output_files": [
            "./logs/%A-%a.o",
            "./logs/%A-%a.e"
        ],
        "execution_directory": "."
    }
}