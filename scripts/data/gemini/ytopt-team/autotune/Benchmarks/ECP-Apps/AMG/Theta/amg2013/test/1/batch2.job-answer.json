{
    "application": "amg2013",
    "details": {
        "scheduler": "COBALT",
        "modules": [
            "intel",
            "geopm"
        ],
        "environment_variables": [
            "OMP_NUM_THREADS=64"
        ],
        "execution_command": "aprun",
        "execution_arguments": "-n 1 -N 1 -cc depth -d 64 -j 1",
        "executable": "../amg2013",
        "arguments": [
            "-laplace",
            "-P 1 1 1",
            "-n 100 100 100"
        ],
        "output_files": [
            "out.txt",
            "out2.txt",
            "out3.txt",
            "out4.txt",
            "out5.txt"
        ]
    }
}