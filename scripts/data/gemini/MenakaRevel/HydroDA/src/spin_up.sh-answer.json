{
    "application": "CaMa-Flood",
    "details": {
        "software": "CaMa-Flood",
        "version": "v3.96a",
        "language": "Fortran",
        "input": {
            "forcing": {
                "type": "plain binary",
                "format": ".one",
                "frequency": "daily",
                "unit": "mm/day",
                "conversion_factor": "86400000 (mm/day->m/s)",
                "directory": "CaMa_in/$runname/Roff_TRUE",
                "prefix": "Roff__",
                "suffix": "T000.one",
                "interpolation": "interpolated with input matrix",
                "input matrix": "inpmat_test-1deg.bin"
            },
            "map": {
                "type": "plain binary",
                "format": ".bin",
                "directory": "map",
                "files": {
                    "nextxy": "nextxy.bin",
                    "ctmare": "ctmare.bin",
                    "elevtn": "elevtn.bin",
                    "nxtdst": "nxtdst.bin",
                    "rivlen": "rivlen.bin",
                    "fldhgt": "fldhgt.bin",
                    "rivwth": "rivwth_gwdlr.bin",
                    "rivhgt": "rivhgt_Xudong.bin",
                    "rivman": "rivmanTRUE.bin"
                },
                "dimension info": "diminfo_test-1deg.txt"
            },
            "restart": {
                "type": "binary",
                "format": ".bin",
                "frequency": "end of run",
                "directory": ".",
                "prefix": "restart",
                "suffix": ".bin"
            }
        },
        "output": {
            "type": "binary",
            "format": ".bin",
            "frequency": "daily",
            "directory": "CaMa_out/$EXP",
            "variables": [
                "rivout",
                "rivsto",
                "sfcelv",
                "outflw",
                "storge",
                "fldout",
                "fldsto"
            ]
        },
        "resources": {
            "cpu": "20",
            "memory": "10gb"
        }
    }
}