{
    "application": "SLURM",
    "details": {
        "job_name": "ggr",
        "output_file": "ggr.%j.out",
        "error_file": "ggr.%j.err",
        "time_limit": "02-00:00:00",
        "qos": "long",
        "partition": "akundaje,khavari",
        "nodes": 1,
        "exclusive": true,
        "tasks_per_node": 8,
        "memory_per_node": 8000,
        "cpus_per_node": 16,
        "mail_type": "END,FAIL",
        "mail_user": "dskim89@stanford.edu",
        "modules": [
            "R/3.4.0",
            "java/1.8.0_131",
            "python/3.6.1"
        ],
        "environment": "ggr_env",
        "tmp_dir": "$L_SCRATCH",
        "work_dir": "$PI_SCRATCH/users/dskim89/ggr/v1.1.0",
        "command": "ggr --cluster sherlock --threads $SLURM_JOB_CPUS_PER_NODE --out_dir $TMP_DIR"
    }
}