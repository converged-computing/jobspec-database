{
    "application": "WRF",
    "details": {
        "software": [
            "WRF",
            "libfabric-aws",
            "OpenMPI",
            "spack"
        ],
        "resources": {
            "nodes": 2,
            "tasks_per_node": 48,
            "cpus_per_task": 2,
            "threads_per_task": 2
        },
        "environment_variables": {
            "OMP_NUM_THREADS": 2,
            "FI_PROVIDER": "efa",
            "KMP_AFFINITY": "compact"
        },
        "execution_mode": "parallel",
        "binding": "core",
        "mapping": "slot:pe=${OMP_NUM_THREADS}"
    }
}