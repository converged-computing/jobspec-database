{
    "application": "Snakemake",
    "details": {
        "job_name": "somVars",
        "output_file": "slogs/%x-%j.log",
        "cores": 2,
        "nodes": 1,
        "memory_per_cpu": "500M",
        "partition": "long",
        "time": "20:00:00",
        "conda_environment": "cellranger-env",
        "cluster_config": "configs/cluster/cellranger-cluster.json",
        "cluster_options": {
            "partition": "{cluster.partition}",
            "time": "{cluster.t}",
            "mem": "{cluster.mem}",
            "name": "{cluster.name}",
            "nodes": "{cluster.nodes}",
            "threads": "{cluster.threads}"
        },
        "snakemake_options": {
            "unlock": true,
            "use_conda": true,
            "rerun_incomplete": true,
            "keep_going": true,
            "print_shell_commands": true,
            "job_priority": 3000
        }
    }
}