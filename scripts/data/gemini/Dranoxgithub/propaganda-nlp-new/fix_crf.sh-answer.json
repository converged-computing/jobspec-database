{
    "application": "slurm",
    "details": {
        "scheduler": "slurm",
        "partition": "bhuwan",
        "resources": {
            "gpu": 1
        },
        "job_name": "chunk",
        "output_file": "./slurm_logs/%j.out",
        "environment": "conda",
        "conda_environment": "base",
        "python_version": "3",
        "script": "run_chunk.py",
        "framework": "wandb",
        "project": "thesis",
        "watch": "all",
        "model_names": [
            "crf",
            "longformer"
        ],
        "context_length": 512,
        "learning_rate": 1e-05,
        "random_seed": 75,
        "dropout": 0,
        "max_len": 256,
        "num_epochs": 20,
        "weight_decay": 0.01,
        "batch_size": 8,
        "num_labels": 14,
        "data_location": "./split_data/processed_data"
    }
}