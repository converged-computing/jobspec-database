{
    "application": "Slurm",
    "details": {
        "job_name": "tl_rpc_lambd",
        "output_file": "./logs/tl_rpc_lambd_test_%J.out",
        "error_file": "./logs/tl_rpc_lambd_test_%J.err",
        "working_directory": "./",
        "environment_variables": "get-user-env",
        "clusters": "cm2_tiny",
        "partition": "cm2_tiny",
        "nodes": 2,
        "tasks_per_node": 1,
        "export": "NONE",
        "time_limit": "00:02:00",
        "modules": [
            "slurm_setup",
            "argobots-1.0-gcc-7.5.0-x75podl",
            "boost-1.75.0-gcc-7.5.0-xdru65d",
            "cereal-1.3.0-gcc-7.5.0-jwb3bux",
            "libfabric-1.11.1-gcc-7.5.0-p6j52ik",
            "mercury-2.0.0-gcc-7.5.0-z55j3mp",
            "mochi-abt-io-0.5.1-gcc-7.5.0-w7nm5r2",
            "mochi-margo-0.9.1-gcc-7.5.0-n2p7v3n",
            "mochi-thallium-0.7-gcc-7.5.0-nbeiina"
        ],
        "mpi": {
            "command": "mpirun",
            "processes": 1,
            "host": "${node_arr[0]}"
        },
        "server_executable": "./tl_rpc_lambd_server",
        "client_executable": "./tl_rpc_lambd_client",
        "server_address_file": "./f_server_addr.txt",
        "communication_protocol": "ofi+tcp"
    }
}