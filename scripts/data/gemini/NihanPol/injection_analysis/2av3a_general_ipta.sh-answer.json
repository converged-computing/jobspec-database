{
    "application": "PBS",
    "details": {
        "scheduler": "PBS",
        "nodes": 1,
        "cores_per_node": 2,
        "memory": "15GB",
        "modules": [
            "compilers/gcc/8.2.0",
            "compilers/python/2.7.13",
            "libraries/suitesparse/5.3_gcc82",
            "compilers/cuda/7.5",
            "libraries/openblas/0.3.2_gcc82",
            "mpi/openmpi/3.1.2_gcc82"
        ],
        "environment_variables": [
            "OPENBLAS_NUM_THREADS=1",
            "GOTO_NUM_THREADS=1",
            "OMP_NUM_THREADS=1",
            "TEMPO2=/dev/shm/tempo2"
        ],
        "script": "11yr_injection_2av3a_general_ipta.py",
        "arguments": [
            "-realiz 0",
            "-amp_index ${PBS_ARRAYID}",
            "-outdir /scratch/nspol/ipta_cut_results/2av3a/",
            "--psrlist /scratch/nspol/IPTA/partim_cut_IPTA/psrlist_cut_IPTA.txt",
            "--timpath /scratch/nspol/IPTA/",
            "--parpath /scratch/nspol/IPTA/partim_cut_IPTA/",
            "--noisepath /scratch/nspol/IPTA/partim_cut_IPTA/",
            "--amps_path ipta_amps.npy",
            "--dm_var",
            "--dm_gp",
            "--dm_annual"
        ],
        "input_files": [
            "/users/nspol/.local/tempo2/share/tempo2",
            "/scratch/nspol/IPTA/partim_cut_IPTA/psrlist_cut_IPTA.txt",
            "/scratch/nspol/IPTA/",
            "/scratch/nspol/IPTA/partim_cut_IPTA/",
            "ipta_amps.npy"
        ],
        "output_directory": "/scratch/nspol/ipta_cut_results/2av3a/"
    }
}