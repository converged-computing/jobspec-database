{
    "application": "Picard",
    "details": {
        "script_language": "Bash",
        "scheduler": "Slurm",
        "modules": "R/2.15.2",
        "java_options": "-Xmx15g -Djava.io.tmpdir=$TMPDIR",
        "picard_path": "${PICARDPATH}",
        "reference_genome": "${REF}",
        "reference_flat": "${REFFLAT}",
        "ribosomal_intervals": "${RIBINTS}",
        "bam_file": "${BAMFILE}",
        "output_dir": "${RUNDIR}/stats/",
        "runtime": "0-32:00:00",
        "memory": "4096 MB per CPU",
        "email_notification": "FAIL",
        "email_address": "jetstream@tgen.org"
    }
}