{
    "application": "SLURM",
    "details": {
        "job_name": "C-tr-pth1:V",
        "qos": "qos_gpu-t3",
        "tasks": 1,
        "gpus": 3,
        "array": "1-5",
        "cpus_per_task": 16,
        "mem_per_cpu": "8g",
        "output": "slurm_out/clevr/trunc-pth1:V-%j.out",
        "error": "slurm_out/clevr/trunc-pth1:V-%j.err",
        "time": "20:00:00",
        "modules": [
            "pytorch-gpu/py3/1.7.1",
            "conda"
        ],
        "conda_environment": "rl-nlp-2",
        "python_path": "src:${PYTHONPATH}",
        "script": "src/scripts/run.py",
        "environment_variables": {
            "DATA_PATH": "data",
            "LM_PATH": "output/lm_model/model.pt",
            "OUTPUT_PATH": "output/RL/CLEVR_lm/trunc_policy",
            "POLICY_PATH": "output/SL_LSTM_32_64/model.pt",
            "POLICY_PATH_VQA": "output/SL_LSTM_32_64_vqa/model.pt",
            "K_EPOCHS": 20,
            "MAX_LEN": 20,
            "UPDATE_EVERY": 128,
            "NUM_EPISODE_TRAIN": 50000,
            "ENV_": "clevr",
            "MODEL": "lstm",
            "AGENT": "PPO",
            "LR": 0.001,
            "WORD_EMB_SIZE": 32,
            "HIDDEN_SIZE": 64,
            "NUM_EPISODE_TEST": 5000,
            "EPS_CLIP": 0.02,
            "REWARD": "vqa",
            "CONDITION_ANSWER": "after_fusion",
            "DEBUG": "0,20000",
            "REWARD_PATH": "output/vqa_model_film/model.pt",
            "REWARD_VOCAB": "data/closure_vocab.json"
        },
        "script_arguments": [
            "-env",
            "clevr",
            "-max_len",
            "20",
            "-data_path",
            "data",
            "-out_path",
            "output/RL/CLEVR_lm/trunc_policy/${SLURM_ARRAY_TASK_ID}",
            "-model",
            "lstm",
            "-update_every",
            "128",
            "-agent",
            "PPO",
            "-K_epochs",
            "20",
            "-eps_clip",
            "0.02",
            "-lr",
            "0.001",
            "-word_emb_size",
            "32",
            "-hidden_size",
            "64",
            "-num_episodes_train",
            "50000",
            "-lm_path",
            "output/lm_model/model.pt",
            "-reward",
            "vqa",
            "-num_episodes_test",
            "5000",
            "-mask_answers",
            "1",
            "-grad_clip",
            "1",
            "-condition_answer",
            "after_fusion",
            "-truncate_mode",
            "proba_thr",
            "-debug",
            "0,20000",
            "-reward_vocab",
            "data/closure_vocab.json",
            "-reward_path",
            "output/vqa_model_film/model.pt",
            "-truncation_optim",
            "1"
        ]
    }
}