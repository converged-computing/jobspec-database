{
    "application": "Slurm",
    "details": {
        "job_name": "username-executable_name",
        "partition": "gecko",
        "cpus_per_task": 48,
        "mem_per_cpu": "2500M",
        "gres": "gpu:0",
        "time": "60:00",
        "output": "%j/cs310_output_%j.out",
        "error": "%j/cs310_error_%j.err",
        "environment": "/etc/profile.d/modules.sh",
        "cuda": "CUDA 12.3",
        "compiler": "GCC 12.2.0",
        "cpu": "lscpu",
        "gpu": "lshw -C display",
        "gpu_nvidia": "nvidia-smi",
        "makefile": "Makefile",
        "kokkos_devices": "OpenMP",
        "omp_num_threads": 48,
        "omp_proc_bind": "spread",
        "omp_places": "threads"
    }
}