{
    "application": "SLURM",
    "details": {
        "job_name": "mts-arr",
        "time_limit": "48:00:00",
        "output_log": "logs/%j.log",
        "exclusive_node_access": true,
        "cpus_per_task": 24,
        "array_task_range": "1-12",
        "python_version": "Required",
        "astronet_package": "Required",
        "tensorflow_package": "Required",
        "architecture_options": [
            "t2",
            "snx"
        ],
        "datasets": [
            "ArabicDigits",
            "AUSLAN",
            "CharacterTrajectories",
            "CMUsubject16",
            "ECG",
            "JapaneseVowels",
            "KickvsPunch",
            "Libras",
            "NetFlow",
            "UWave",
            "Wafer",
            "WalkvsRun"
        ],
        "hyperparameter_optimisation_script": "$ASNWD/astronet/$ARCH/opt/hypertrain.py",
        "training_script": "$ASNWD/astronet/$ARCH/train.py",
        "configuration_file": "$PWD/conf/astronet.conf"
    }
}