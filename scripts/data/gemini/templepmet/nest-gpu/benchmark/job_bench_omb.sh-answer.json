{
    "application": "PBS",
    "details": {
        "queue": "SQUID",
        "group": "${GROUP_NAME}",
        "runtime": "00:10:00",
        "output_file": "./log/job_%s_out.txt",
        "error_file": "./log/job_%s_err.txt",
        "nodes": 2,
        "cores_per_node": 2,
        "omp_threads": 1,
        "mpi_module": "BaseGPU/2022",
        "singularity_image": "../singularity/nestgpu_sandbox",
        "result_file": "./log/result.txt",
        "benchmark_executable": "./osu-micro-benchmarks/build/mpi/one-sided/osu_get_bw",
        "mpi_options": "$NQSV_MPIOPTS"
    }
}