{
    "application": "Slurm",
    "details": {
        "runtime": "10 hours",
        "account": "imi@v100",
        "job name": "PPO_GTL_a12_%a",
        "tasks per node": 1,
        "nodes": 1,
        "cpus per task": 20,
        "gpus": 1,
        "multithreading": "disabled",
        "output": "slurm_logs/PPO_GTL_a12_%a.out",
        "error": "slurm_logs/PPO_GTL_a12_%a.err",
        "array": "1-2",
        "qos": "qos_gpu-t3",
        "constraint": "v100-32g",
        "modules": [
            "python/3.8.2",
            "conda activate dlp"
        ],
        "script": "scripts/train_ppo_baseline.sh",
        "environment": "BabyAI-GoToLocal-v0 GTL 12 ${SLURM_ARRAY_TASK_ID}"
    }
}