{
    "application": "PoetGen",
    "details": {
        "framework": "Tensorflow",
        "language": "Python",
        "dependencies": [
            "torch",
            "torchvision",
            "torchaudio",
            "OpenBLAS",
            "requirements.txt"
        ],
        "resources": {
            "gpu": true,
            "cpu": 1,
            "memory": "24GB",
            "scratch_local": "16GB",
            "walltime": "24:00:00"
        },
        "modules": {
            "py-pip": "21.3.1-gcc-10.2.1-mjt74tn"
        },
        "environment": {
            "TMPDIR": "$SCRATCHDIR",
            "CMAKE_ARGS": "-DLLAMA_BLAS=ON -DLLAMA_BLAS_VENDOR=OpenBLAS",
            "PYTHONPATH": "${PYTHONPATH}:${TMPDIR}"
        }
    }
}