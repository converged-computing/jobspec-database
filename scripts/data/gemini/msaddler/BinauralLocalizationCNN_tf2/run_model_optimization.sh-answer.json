{
    "application": "SLURM",
    "details": {
        "job_name": "localization_model_train",
        "output_file": "slurm-%A_%a.out",
        "cpus_per_task": 10,
        "memory": "32G",
        "gpu": "tesla-v100",
        "gpu_count": 1,
        "array_size": 10,
        "partition": "normal",
        "time_limit": "2-0",
        "requeue": true,
        "python_environment": "tf",
        "python_script": "run_model.py",
        "config_file": "config.json",
        "architecture_file": "arch.json",
        "training_data": "/om2/scratch/*/msaddler/data_localize/dataset_localization/v01/train/*IHC3000Hz_dbspl030to090/*tfrecords",
        "validation_data": "/om2/scratch/*/msaddler/data_localize/dataset_localization/v01/valid/*IHC3000Hz_dbspl030to090/*tfrecords",
        "mixed_precision": false,
        "log_file": "$model_dir/log_optimize.out"
    }
}