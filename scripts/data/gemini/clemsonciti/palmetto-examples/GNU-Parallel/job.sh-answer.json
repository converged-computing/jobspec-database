{
    "application": "GNU Parallel",
    "details": {
        "resource_requirements": {
            "nodes": 1,
            "cores": 4,
            "memory": "1GB",
            "walltime": "00:05:00"
        },
        "modules": [
            "gnu-parallel",
            "anaconda3/4.2.0"
        ],
        "script": {
            "language": "bash",
            "commands": [
                "cd $PBS_O_WORKDIR",
                "ls ./inputs/* | parallel -j4 python transpose.py"
            ],
            "alternative_commands": [
                "parallel -j4 python transpose.py < inputs.txt"
            ]
        }
    }
}