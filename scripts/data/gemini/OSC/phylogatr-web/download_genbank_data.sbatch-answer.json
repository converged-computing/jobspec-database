{
    "application": "Phylogatr",
    "details": {
        "requirements": [
            "SLURM",
            "Ruby",
            "bin/bundle",
            "bin/db",
            "Phylogatr_GENBANK_DIR",
            "Phylogatr_RUBY_VERSION"
        ],
        "resource_usage": {
            "cpu_cores": 1,
            "memory": "not specified",
            "time": "72 hours"
        },
        "file_system": {
            "input": "env",
            "output": "log/%x-%j.out",
            "temp": "Phylogatr_GENBANK_DIR"
        },
        "modules": [
            "ruby/$PHYLOGATR_RUBY_VERSION"
        ],
        "description": "Downloads and updates GenBank database for Phylogatr"
    }
}