{
    "application": "Bash",
    "software_requirements": [
        "julia/0.6.2",
        "predvisits_bootstrap.jl"
    ],
    "resource_requirements": {
        "slurm_partition": "bigmem2",
        "nodes": 1,
        "tasks_per_node": 27,
        "memory_per_cpu": "5g",
        "walltime": "10:00:00"
    },
    "input_files": [
        "../input/estimates/${originmode}/estimates_${race}_${originmode}_bootstrap_clean.csv"
    ],
    "output_files": [
        "slurmlogs/predictvisits_bootstrap_${race}_${originmode}_job_%A_%a.out",
        "slurmlogs/predictvisits_bootstrap_${race}_${originmode}_job_%A_%a.err",
        "run_array_${race}_${originmode}_$count.sbatch"
    ]
}