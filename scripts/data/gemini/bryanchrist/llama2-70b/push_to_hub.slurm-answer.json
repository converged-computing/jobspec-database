{
    "application": "Slurm",
    "details": {
        "account": "bryan_research",
        "partition": "gpu",
        "gres": "gpu:a100:2",
        "constraint": "gpupod",
        "cpus_per_task": 10,
        "memory": "250G",
        "time": "0-12:00:00",
        "job_name": "push_to_hub",
        "output": "push_to_hub-%A.out",
        "error": "push_to_hub-%A.err",
        "modules": [
            "anaconda",
            "cuda/11.4.2",
            "falcon_40B"
        ],
        "python_environment": "falcon_40B",
        "python_packages": [
            "python-dotenv",
            "triton-nightly"
        ],
        "conda_packages": [
            "cudatoolkit"
        ],
        "environment_variables": {
            "LD_LIBRARY_PATH": "/opt/conda/lib/"
        },
        "script": "push_to_hub.py"
    }
}