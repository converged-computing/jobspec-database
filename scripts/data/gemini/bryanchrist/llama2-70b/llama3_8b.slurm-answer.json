{
    "application": "SLURM",
    "details": {
        "scheduler": "SLURM",
        "partition": "gpu",
        "gpu_type": "a100",
        "gpu_count": 1,
        "cpu_count": 10,
        "memory": "75G",
        "time_limit": "3-00:00:00",
        "job_name": "llama3-8b_generate",
        "output_file": "llama3-8b_generate-%A.out",
        "error_file": "llama3-8b_generate-%A.err",
        "modules": [
            "anaconda",
            "cuda/11.4.2",
            "falcon_40B"
        ],
        "python_environment": "falcon_40B",
        "packages": [
            "python-dotenv",
            "triton-nightly",
            "cudatoolkit"
        ],
        "environment_variables": {
            "LD_LIBRARY_PATH": "/opt/conda/lib/"
        },
        "script": "llama3-8b_generate.py"
    }
}