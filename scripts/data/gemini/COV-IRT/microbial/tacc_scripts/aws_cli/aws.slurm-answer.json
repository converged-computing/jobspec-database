{
    "application": "Singularity",
    "details": {
        "resource_requirements": {
            "queue": "normal",
            "nodes": 1,
            "mpi_tasks": 64,
            "runtime": "24:00:00",
            "allocation": "COVIRT19",
            "qos": "vip"
        },
        "software_requirements": {
            "modules": "tacc-singularity",
            "docker_hub": "amazon/aws-cli",
            "singularity_image": "$SCRATCH/COVIRT19/singularity_cache/amazon.sif"
        },
        "commands": [
            "s3 sync s3://bucket_youwant/file $PWD --exclude \"*human*\" --endpoint-url=the_aws_region --profile your_profile"
        ]
    }
}