{
    "application": "SLURM",
    "software": [
        "anaconda/2021b",
        "MoEBERT",
        "nvidia-smi",
        "rsync",
        "bash",
        "sh_scripts/experiments/launch_jobs_perm.sh"
    ],
    "resources": [
        "xeon-g6-volta partition",
        "xeon-g6 constraint",
        "gpu:volta:1",
        "nodes:1",
        "ntasks-per-node:1",
        "cpus-per-task:20"
    ],
    "environment_variables": [
        "TOTAL_GPUS",
        "GPUS_PER_NODE",
        "HF_USER_DIR",
        "HF_LOCAL_DIR",
        "HF_HOME",
        "TRANSFORMERS_OFFLINE",
        "HF_DATASETS_OFFLINE",
        "WANDB_DISABLED",
        "BACKEND",
        "HDF5_USE_FILE_LOCKING",
        "TASK_ID",
        "EXP_ID",
        "DATASET",
        "SEED",
        "output_dir"
    ]
}