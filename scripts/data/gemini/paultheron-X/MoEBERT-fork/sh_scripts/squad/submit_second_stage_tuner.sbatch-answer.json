{
    "application": "MoEBERT",
    "software": [
        "anaconda/2021b",
        "nvidia-smi",
        "rsync",
        "bash",
        "sh_scripts/experiments/launch_jobs.sh",
        "squad_v2"
    ],
    "resources": {
        "partition": "xeon-g6-volta",
        "constraint": "xeon-g6",
        "time": "0-48:0",
        "gpu": "volta:1",
        "nodes": 1,
        "tasks_per_node": 1,
        "cpus_per_task": 20,
        "array": "1-3"
    },
    "environment_variables": [
        "TOTAL_GPUS",
        "GPUS_PER_NODE",
        "HF_USER_DIR",
        "HF_LOCAL_DIR",
        "HF_HOME",
        "TRANSFORMERS_OFFLINE",
        "HF_DATASETS_OFFLINE",
        "WANDB_DISABLED",
        "BACKEND",
        "HDF5_USE_FILE_LOCKING",
        "TASK_ID",
        "EXP_ID",
        "DATASET",
        "SEED",
        "output_dir"
    ]
}