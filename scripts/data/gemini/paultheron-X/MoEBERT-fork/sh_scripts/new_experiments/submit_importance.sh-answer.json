{
    "application": "SLURM",
    "details": {
        "environment": {
            "modules": [
                "anaconda/2021b",
                "gurobi/gurobi-903"
            ],
            "conda_environment": "MoEBERT",
            "variables": {
                "TOTAL_GPUS": "${SLURM_NTASKS}",
                "HF_USER_DIR": "/home/gridsan/$(whoami)/.cache/huggingface",
                "HF_LOCAL_DIR": "/state/partition1/user/$(whoami)/cache/huggingface",
                "HF_HOME": "${HF_LOCAL_DIR}",
                "TRANSFORMERS_OFFLINE": "1",
                "HF_DATASETS_OFFLINE": "1",
                "WANDB_DISABLED": "true",
                "BACKEND": "pytorch",
                "HDF5_USE_FILE_LOCKING": "FALSE",
                "output_dir": "/home/gridsan/$(whoami)/MoEBERT-fork/results"
            }
        },
        "resources": {
            "nodes": 1,
            "gpus": "volta:1",
            "tasks_per_node": 1,
            "cpus_per_task": 4,
            "time": "0-24:0",
            "mail_type": "FAIL",
            "mail_user": "paulth@mit.edu",
            "output_file": "/home/gridsan/ptheron/MoEBERT-fork/logs/importance_mnli_new/experiments_importance_out%j.txt",
            "error_file": "/home/gridsan/ptheron/MoEBERT-fork/logs/importance_mnli_new/experiments_importance_err%j.txt"
        },
        "scripts": {
            "sh_scripts/new_experiments/importance_preprocess_new.sh": {
                "arguments": [
                    "$dset",
                    "$output_dir"
                ]
            },
            "merge_importance.py": {
                "arguments": {
                    "task": "$dset"
                }
            }
        }
    }
}