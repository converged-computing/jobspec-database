{
    "application": "Dask",
    "details": {
        "scheduler_file": "~/.dask_scheduler.json",
        "resource_manager": "Slurm",
        "slurm_options": {
            "job_name": "dask-workers",
            "output": "dask-workers.%j.%N.out",
            "partition": "compute",
            "nodes": 2,
            "ntasks_per_node": 24,
            "export": "ALL",
            "time_limit": "02:00:00"
        },
        "module": "anaconda",
        "python_path": "/.local/lib/python3.5/site-packages/:$PYTHONPATH"
    }
}