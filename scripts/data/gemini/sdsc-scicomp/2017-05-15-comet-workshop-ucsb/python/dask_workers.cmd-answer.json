{
    "application": "Dask",
    "details": {
        "scheduler": "Dask scheduler",
        "resource_manager": "SLURM",
        "python_version": "3.5",
        "modules": [
            "anaconda"
        ],
        "environment_variables": [
            "MODULEPATH",
            "PYTHONPATH"
        ],
        "nodes": 2,
        "tasks_per_node": 24,
        "time_limit": "02:00:00",
        "partition": "compute",
        "job_name": "dask-workers"
    }
}