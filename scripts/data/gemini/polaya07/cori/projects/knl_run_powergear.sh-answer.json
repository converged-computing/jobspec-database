{
    "application": "Slurm",
    "details": {
        "queue": "regular",
        "nodes": 1,
        "tasks": 64,
        "time": "20 minutes",
        "job_name": "gearshifft",
        "processor_filter": "knl,quad,cache",
        "output_file": "./outputs/%j.out",
        "modules": [
            {
                "name": "PrgEnv-intel",
                "swap": true
            },
            {
                "name": "PrgEnv-gnu",
                "swap": true
            },
            {
                "name": "cray-fftw",
                "version": "3.3.6.3"
            },
            {
                "name": "boost",
                "version": "1.65.1"
            }
        ],
        "command": "$HOME/projects/gearshifft-intel-knl/bin/gearshifft_fftwwrappers -f $FEXSMALL -v -o \"$HOME/projects/results/$today-knl_wc.csv\" --rigor estimate",
        "resources": {
            "powercap_plot": {
                "path": "$HOME/projects/powercap/powercap_plot",
                "arguments": []
            }
        }
    }
}