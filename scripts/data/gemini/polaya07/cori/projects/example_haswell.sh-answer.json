{
    "application": "Slurm",
    "details": {
        "queue": "regular",
        "nodes": 1,
        "tasks": 64,
        "processor_filter": "haswell",
        "time_limit": "00:10:00",
        "job_name": "gearshifft",
        "output_file": "./outputs/%j.out",
        "modules": [
            "PrgEnv-intel",
            "PrgEnv-gnu",
            "cray-fftw/$FFTW_VER",
            "boost/$BOOST_VER"
        ],
        "boost_version": "1.67.0",
        "fftw_version": "3.3.6.5",
        "data_files": [
            "$CURDIR/share/gearshifft/extents_uni.conf",
            "$CURDIR/share/gearshifft/extents_small.conf",
            "$CURDIR/share/gearshifft/extents_1d_fftw_large.conf",
            "$CURDIR/share/gearshifft/extents_1d_fftw_copy.conf",
            "$CURDIR/share/gearshifft/extents_1d_fftw.conf",
            "$CURDIR/share/gearshifft/extents_1d_publication.conf",
            "$CURDIR/share/gearshifft/extents_2d_publication.conf",
            "$CURDIR/share/gearshifft/extents_3d_publication.conf",
            "$CURDIR/share/gearshifft/extents_all_publication.conf"
        ],
        "executable": "$HOME/projects/gearshifft-gnu-wc/bin/gearshifft_fftw",
        "power_measurement_tool": "$HOME/projects/powercap/powercap_plot"
    }
}