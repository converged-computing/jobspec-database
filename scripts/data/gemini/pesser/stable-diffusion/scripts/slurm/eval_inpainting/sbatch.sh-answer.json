{
    "application": "Stable Diffusion",
    "details": {
        "cluster_manager": "Slurm",
        "partition": "gpu",
        "nodes": 1,
        "tasks_per_node": 8,
        "cpus_per_gpu": 4,
        "gpus_per_task": 1,
        "exclusive": true,
        "output_file": "%x_%j.out",
        "comment": "stablediffusion",
        "requeue": false,
        "modules": [
            "intelmpi"
        ],
        "environment_variables": [
            "LD_LIBRARY_PATH",
            "NCCL_PROTO",
            "PATH",
            "LD_PRELOAD",
            "FI_EFA_FORK_SAFE",
            "FI_LOG_LEVEL",
            "FI_EFA_USE_DEVICE_RDMA",
            "NCCL_DEBUG",
            "PYTHONFAULTHANDLER",
            "CUDA_LAUNCH_BLOCKING",
            "OMPI_MCA_mtl_base_verbose",
            "FI_EFA_ENABLE_SHM_TRANSFER",
            "FI_PROVIDER",
            "FI_EFA_TX_MIN_CREDITS",
            "NCCL_TREE_THRESHOLD",
            "HOSTNAMES",
            "MASTER_ADDR",
            "MASTER_PORT",
            "COUNT_NODE",
            "WORLD_SIZE"
        ],
        "mpi_implementation": "pmix_v3",
        "sub_script": "/fsx/stable-diffusion/stable-diffusion/scripts/slurm/eval_inpainting/launcher.sh"
    }
}