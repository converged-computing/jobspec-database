{
    "application": "SLURM",
    "other_software": [
        "crystfel",
        "indexamajig",
        "list_events",
        "split",
        "sed",
        "basename",
        "cat",
        "wc",
        "sbatch",
        "echo"
    ],
    "resource_requirements": {
        "modules": [
            "xray",
            "maxwell crystfel/0.10.2",
            "hdf5/1.10.5",
            "hdf5-openmpi/1.10.5"
        ],
        "partition": "upex,cfel,cfel-cdi",
        "time": "4:00:00",
        "nodes": "1",
        "nice": "100",
        "job_name": "RUN-NUMBER",
        "output": "$ERRORDIR/RUN-NUMBER-%N-%j.out",
        "error": "$ERRORDIR/RUN-NUMBER-%N-%j.err"
    }
}