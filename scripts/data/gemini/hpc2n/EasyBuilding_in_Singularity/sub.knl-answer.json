{
    "application": "EasyBuild",
    "details": {
        "software": "EasyBuild",
        "resource_requirements": {
            "partition": "knl",
            "account": "$SLURM_JOB_ACCOUNT",
            "nodes": 1,
            "tasks": 68,
            "cores": 4,
            "time": "8:00:00",
            "tmp_logdir": "$p/tmplogs/$SLURM_JOB_ID"
        },
        "optional_parameters": {
            "buildpath": "$p/$SLURM_JOB_ID",
            "tmpdir": "$p/tmpdir/$SLURM_JOB_ID",
            "eb_builder_branch": "develop",
            "ebuser": "$EBUSER",
            "eb_cmd": "real_eb",
            "eb_install_params": "--robot --rebuild",
            "eb_install_pkg": "pomkl-2018b.eb",
            "from_pr": "--from-pr",
            "job": "--job"
        },
        "environment_variables": {
            "EB_INSTALL_PARAMS": "$EB_INSTALL_PARAMS",
            "EB_INSTALL_PKG": "$EB_INSTALL_PKG",
            "EB_CMD": "$EB_CMD",
            "EB_BRANCH": "$EB_BRANCH",
            "EBUSER": "$EBUSER",
            "SBATCH_PARTITION": "knl",
            "SBATCH_ACCOUNT": "$SLURM_JOB_ACCOUNT",
            "EASYBUILD_TMPDIR": "$p/tmpdir/$SLURM_JOB_ID"
        }
    }
}