{
    "application": "Slurm",
    "details": {
        "job_name": "pspCityscapes",
        "output_file": "output/psp_%j.log",
        "error_file": "output/err/psp_%j.err",
        "num_tasks": 1,
        "cpus_per_task": 8,
        "task_distribution": "cyclic:cyclic",
        "time_limit": "0-02:00:00",
        "memory_per_cpu": "12000mb",
        "mail_events": "END,FAIL",
        "mail_user": "afcadiz@uc.cl",
        "partition": "ialab-high",
        "gpus": "4",
        "nodelist": "hydra",
        "python_script": "train.py",
        "data_directory": "${CS_PATH}",
        "model": "pspnet",
        "random_mirror": true,
        "random_scale": true,
        "learning_rate": "1e-2",
        "weight_decay": "5e-4",
        "batch_size": 8,
        "num_steps": 40000,
        "restore_from": "./dataset/MS_DeepLab_resnet_pretrained_init.pth",
        "gpu": 0
    }
}