{
    "application": "SLURM",
    "details": {
        "resource_requirements": {
            "nodes": 1,
            "tasks": 1,
            "cores_per_task": 10,
            "gpus_per_node": 1,
            "time": "10:00:00",
            "memory": "not specified"
        },
        "software": {
            "conda_environment": "pytorch",
            "python_package": "mmsrl",
            "other_dependencies": [
                "huggingface/transformers",
                "SLURM"
            ]
        },
        "data": {
            "data_path": "$WORK/data"
        },
        "output": {
            "output_files": [
                "/gpfswork/rech/lco/url46ht/outputs/generic_features_attention/generic_features_attention_%j_%x_%A_%a.out",
                "/gpfswork/rech/lco/url46ht/outputs/generic_features_attention/generic_features_attention_%j_%x_%A_%a.err"
            ],
            "model_output": [
                "$WORK/outputs/$DIRNAME/val_${name}_${SLURM_ARRAY_TASK_ID}.pkl",
                "$WORK/outputs/$DIRNAME/test_${name}_${SLURM_ARRAY_TASK_ID}.pkl"
            ]
        },
        "configuration": {
            "pooling": "attention",
            "patience": 26,
            "batch_per_epoch": 1000,
            "cyclic_subsample": "None",
            "subsample_labels": "'interpolate_micro_to_macro'"
        }
    }
}