{
    "application": "TorchRun",
    "details": {
        "resources": {
            "partition": "r8nv-gpu-hw",
            "time": "0-30:00:00",
            "nodes": 1,
            "gpus": 4,
            "gres_flags": "enforce-binding",
            "qos": "gpu-normal",
            "constraint": "80G"
        },
        "software": {
            "modules": [
                "cluster-tools/v1.0",
                "slurm-tools/v1.0",
                "cmake/3.21.7",
                "python3/3.8.16",
                "cuda-cudnn/11.6-8.4.1"
            ],
            "commands": [
                "torchrun"
            ],
            "libraries": [
                "codellama"
            ]
        },
        "environment": {
            "cuda_visible_devices": "specified by SLURM"
        }
    }
}