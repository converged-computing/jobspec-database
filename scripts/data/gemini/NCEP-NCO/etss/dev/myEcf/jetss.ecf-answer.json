{
    "application": "PBS",
    "details": {
        "job_name": "ETSS.${pid}",
        "queue": "${QUEUE:?}",
        "walltime": "1:50:00",
        "nodes": "1",
        "cpus_per_node": "14",
        "memory_per_node": "15GB",
        "modules": [
            "envvar/$envvar_ver",
            "PrgEnv-intel/${PrgEnv_intel_ver}",
            "intel/${intel_ver}",
            "craype/${craype_ver}",
            "prod_util/${prod_util_ver}",
            "cray-pals/${cray_pals_ver}",
            "cfp/${cfp_ver}"
        ],
        "executable": "${HOMEetss}/jobs/JETSS"
    }
}