{
    "application": "RepeatMasker",
    "details": {
        "modules": [
            "GCC/9.3.0",
            "OpenMPI/4.0.3",
            "RepeatMasker/4.1.2-p1-HMMER"
        ],
        "resources": {
            "time": "01:00:00",
            "cpus_per_task": 24,
            "memory": "180G",
            "output": "stdout.%x.%j",
            "error": "stderr.%x.%j"
        },
        "parameters": {
            "species": "Arabidopsis thaliana",
            "masking": "-xsmall"
        },
        "input": "upstream5000.fa.gz",
        "commands": [
            "RepeatMasker -s -species \"$species\" -pa $threads -e hmmer $masking $input_fasta"
        ],
        "citations": [
            "TAMU HPRC: https://hprc.tamu.edu/research/citations.html",
            "RepeatMasker: Smit, AFA, Hubley, R & Green, P. RepeatMasker Open-4.0. 2013-2015 <http://www.repeatmasker.org>."
        ]
    }
}