{
    "application": "SLURM",
    "details": {
        "scheduler": "SLURM",
        "partition": "prawnew",
        "memory": "20GB",
        "walltime": "1 hour",
        "program": "testing_cheevd_GPU.out",
        "output": "cheevd_GPU_(HOST)_time.txt",
        "info": "cheevd_GPU_(HOST)_info.txt",
        "data_directory": "$HOME/data/Diagonalization",
        "command": "$program $((500*$n))",
        "loop_range": "1 to 100 (inclusive)"
    }
}