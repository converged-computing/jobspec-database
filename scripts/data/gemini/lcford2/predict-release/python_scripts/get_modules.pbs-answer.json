{
    "application": "bwpy",
    "other_software": [
        "PrgEnv-cray",
        "bwpy",
        "python"
    ],
    "resource_requirements": {
        "nodes": 1,
        "cores_per_node": 1,
        "walltime": "01:00:00",
        "feature": "xe",
        "umask": "0027",
        "environment_variables": [
            "OMP_NUM_THREADS=2"
        ],
        "modules": [
            "craype-hugepages2M",
            "perftools",
            "PrgEnv-cray",
            "bwpy"
        ],
        "input_files": [
            "in"
        ],
        "output_files": [
            "./qsub_output/$PBS_JOBID.err",
            "./qsub_output/$PBS_JOBID.out"
        ],
        "working_directory": "$PBS_O_WORKDIR"
    }
}