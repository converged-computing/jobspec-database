{
    "application": "AblationAnalysis",
    "details": {
        "software": [
            "Python 3.9.6",
            "GCCcore 11.2.0",
            "measureDist-skylake (python virtual environment)"
        ],
        "resources": [
            "SLURM (job scheduler)",
            "3 CPUs per task",
            "short partition (queue)",
            "skl-compat constraint (nodes with specific hardware)",
            "requeue option (restart job on failure)"
        ],
        "input": "subjectsWithParietalPeak.txt (text file with subject IDs)",
        "output": "logs/AblationAnalysis-%j.out (output file for each job)"
    }
}