{
    "application": "SLURM",
    "details": {
        "resource_manager": "SLURM",
        "nodes": 8,
        "tasks": 8,
        "tasks_per_node": 1,
        "cpus_per_task": 12,
        "time": "00:05:00",
        "partition": "normal",
        "constraint": "gpu",
        "account": "g34",
        "output": "interleave6.txt",
        "modules": "daint-gpu",
        "conda_env": "py38_kfac",
        "model": "bert-large",
        "pipeline": "interleave",
        "stages": 8,
        "ngpus": 8,
        "microbs": 32,
        "acc": 1,
        "nsys_node_interval": 1,
        "nsys_output": "bert_prof/bert-large_interleave_8stages_8gpus_microbs32_acc1",
        "script": "scripts/nsys_wrap.sh",
        "python_script": "main_bert.py",
        "corpus_path": "./bert_data/wikipedia.segmented.nltk.txt",
        "vocab_path": "./bert_data/bert-large-uncased-vocab.txt",
        "corpus_lines": 10000,
        "do_lower_case": true,
        "bert_config_path": "./configs/bert_config_bert-large-uncased.json",
        "max_seq_length": 128,
        "micro_batch_size": 32,
        "num_optimization_steps": 8,
        "gradient_accumulation_steps": 1,
        "pipeline_method": "interleave",
        "p2p_backend": "gloo",
        "collective_backend": "nccl",
        "profile": true,
        "chunks": 2
    }
}