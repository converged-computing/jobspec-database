{
    "application": "Metassl",
    "details": {
        "environment": "conda",
        "environment_name": "metassl",
        "framework": "PyTorch",
        "gpu": "RTX 2080",
        "resources": {
            "partition": "alldlc_gpu-rtx2080",
            "queue": "dlc-wagnerd",
            "gpu_count": 1,
            "job_name": "C10_Combined",
            "time_limit": "23:59:59",
            "array_size": 200,
            "array_step": 10
        },
        "dataset": "CIFAR-10",
        "model": "SimSiam",
        "config": "metassl/default_metassl_config_cifar10.yaml",
        "training_parameters": {
            "epochs": 800,
            "finetuning_epochs": 100,
            "warmup_epochs": 0,
            "seed": 0,
            "save_model_frequency": 50,
            "data_usage_percentage": 100,
            "validation_size": 0.2
        },
        "experiment_details": {
            "experiment_name": "$EXPERIMENT_NAME",
            "non_gradient_based": true,
            "multiprocessing_distributed": true,
            "neps": {
                "is_neps_run": true,
                "config_space": "combined",
                "optimize_backbone_only": true,
                "is_user_prior": true
            }
        }
    }
}