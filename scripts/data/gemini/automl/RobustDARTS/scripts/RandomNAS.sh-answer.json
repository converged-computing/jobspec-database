{
    "application": "SLURM",
    "details": {
        "queue": "meta_gpu-ti",
        "gpu_request": "1",
        "array_tasks": "1-3",
        "job_name": "RandomNAS",
        "execution_directory": ".",
        "output_file": "./experiments/cluster_logs/%A_%a.o",
        "error_file": "./experiments/cluster_logs/%A_%a.e",
        "conda_environment": "pytorch-0.3.1-cu8-py36",
        "python_script": "src/search/randomNAS/random_weight_share.py",
        "arguments": [
            "--job_id",
            "$SLURM_ARRAY_JOB_ID",
            "--task_id",
            "$SLURM_ARRAY_TASK_ID",
            "--seed",
            "$SLURM_ARRAY_TASK_ID",
            "--epochs",
            "50",
            "--save",
            "experiments/search_logs_RandomNAS",
            "--space",
            "$1",
            "--dataset",
            "$2",
            "--drop_path_prob",
            "$3",
            "--weight_decay",
            "$4"
        ]
    }
}