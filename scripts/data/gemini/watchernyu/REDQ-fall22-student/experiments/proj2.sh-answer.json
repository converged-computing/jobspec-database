{
    "application": "SLURM",
    "details": {
        "queue": "aquila,parallel",
        "time": "168:00:00",
        "nodes": 1,
        "memory": "12GB",
        "mail_type": "ALL",
        "mail_user": "NETID@nyu.edu",
        "array_tasks": 8,
        "output": "logs/%A_%a.out",
        "error": "logs/%A_%a.err",
        "constraint": "cpu",
        "modules": [
            "singularity",
            "anaconda3",
            "cuda/9.0",
            "glfw/3.3",
            "gcc/7.3",
            "mesa/19.0.5",
            "llvm/7.0.1"
        ],
        "environment_variables": [
            "LD_LIBRARY_PATH",
            "MUJOCO_PY_MUJOCO_PATH",
            "MUJOCO_GL"
        ],
        "singularity_image": "/scratch/$USER/sing/mujoco-sandbox",
        "singularity_binds": [
            "/scratch/$USER/sing/REDQ-fall22-student:/workspace/REDQ",
            "/scratch/$USER/sing/mujoco-sandbox/opt/conda/lib/python3.8/site-packages/mujoco_py/:/opt/conda/lib/python3.8/site-packages/mujoco_py/"
        ],
        "script": "proj2.py",
        "script_arguments": "--setting ${SLURM_ARRAY_TASK_ID}"
    }
}