{
    "application": "Snakemake",
    "details": {
        "cluster_submission_tool": "qsub",
        "cluster_status_checker": "qsub-status.py",
        "resource_requirements": {
            "nodes": 1,
            "ppn": 4,
            "memory": "8gb",
            "walltime": "7:00:00:00",
            "group": "cpr_10006",
            "account": "cpr_10006"
        },
        "environment": {
            "envmodules": "enabled"
        },
        "execution_options": {
            "jobs": 59,
            "keep_going": "enabled",
            "parallel": "enabled",
            "latency_wait": 60,
            "rerun_incomplete": "enabled",
            "log_directory": "{params.logdir}"
        }
    }
}