{
    "application": "LULESH",
    "details": {
        "software": [
            "LULESH",
            "Catalyst",
            "ParaView"
        ],
        "resources": {
            "partition": "debug",
            "constraints": "gpu",
            "hints": "nomultithread",
            "nodes": 1,
            "tasks_per_core": 1,
            "tasks_per_node": 8,
            "cpus_per_task": 1,
            "time": "00:10:00"
        },
        "environment_variables": {
            "OMP_NUM_THREADS": "$SLURM_CPUS_PER_TASK",
            "VTK_SILENCE_GET_VOID_POINTER_WARNINGS": 1
        }
    }
}