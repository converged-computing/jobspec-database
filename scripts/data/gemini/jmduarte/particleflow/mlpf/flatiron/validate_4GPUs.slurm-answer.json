{
    "application": "slurm",
    "details": {
        "resource_requirements": {
            "walltime": "8 hours",
            "nodes": 1,
            "tasks_per_node": 1,
            "partition": "gpu",
            "gpus": 4,
            "gpu_type": "a100"
        },
        "software": {
            "modules": [
                "slurm",
                "gcc",
                "cuda/11.1.0_455.23.05",
                "cudnn/v8.0.4-cuda-11.1"
            ],
            "conda_environment": "tf2",
            "python_version": "3"
        },
        "other": {
            "script_name": "eval",
            "output_logs": "logs_slurm/log_%x_%j.out",
            "error_logs": "logs_slurm/log_%x_%j.err",
            "temporary_directory": "$TMPDIR/particleflow"
        }
    }
}