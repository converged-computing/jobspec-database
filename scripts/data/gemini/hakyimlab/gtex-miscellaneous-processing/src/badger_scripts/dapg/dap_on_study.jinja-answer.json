{
    "application": "PBS",
    "details": {
        "software": [
            "gcc/6.2.0",
            "gsl/2.3",
            "boost/1.61.0",
            "bzip2/1.0.6",
            "python/3.5.3"
        ],
        "resource_requirements": {
            "walltime": "{{job_walltime}}",
            "memory": "{{job_memory}}",
            "nodes": "1:ppn=1"
        },
        "logging": {
            "output": "{{logs_folder}}/{% raw %}${PBS_JOBNAME}.o${PBS_JOBID}.log{% endraw %}",
            "error": "{{logs_folder}}/{% raw %}${PBS_JOBNAME}.e${PBS_JOBID}.err{% endraw %}"
        },
        "command_arguments": [
            "{{ command }}",
            "{{ dap_command }}",
            "{{ frequency_filter }}",
            "{{ options }}",
            "{{ chromosome }}",
            "{{ sub_batches }}",
            "{{ sub_batch }}",
            "{{ grid }}",
            "{{ gene_annotation }}",
            "{{ genotype }}",
            "{{ genotype_metadata }}",
            "{{ phenotype }}",
            "{{ covariate }}",
            "{{ priors_folder }}",
            "{{ intermediate_folder }}",
            "{{ window }}",
            "{{ stats_name }}",
            "{{ parsimony }}",
            "{{ output }}"
        ]
    }
}