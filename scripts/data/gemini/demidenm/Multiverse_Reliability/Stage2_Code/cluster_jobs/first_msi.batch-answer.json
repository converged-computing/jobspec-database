{
    "application": "SLURM",
    "details": {
        "environment": {
            "conda": "fmri_env",
            "module": "fsl"
        },
        "resources": {
            "nodes": 1,
            "tasks_per_node": 1,
            "cpus_per_task": 6,
            "memory_per_cpu": "6G",
            "time": "04:00:00",
            "partition": "msismall,agsmall",
            "account": "faird"
        },
        "job_array": {
            "range": "0-137"
        },
        "output": {
            "stdout": "log_abcd/%x_%A_%a.out",
            "stderr": "log_abcd/%x_%A_%a.err"
        },
        "script": "./batch_jobs/first${ID}"
    }
}