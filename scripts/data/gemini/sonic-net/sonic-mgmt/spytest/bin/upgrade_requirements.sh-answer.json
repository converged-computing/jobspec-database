        "natsort",
        "redis",
        "requests",
        "jsonpatch",
        "rpyc",
        "Pyro4",
        "netaddr",
        "crc16",
        "autoflake",
        "pyang",
        "pyparsing",
        "pyangbind",
        "yapf",
        "pexpect",
        "pytz",
        "docker==2.7.0",
        "pdbpp",
        "pylint-pytest",
        "pylint-protobuf",
        "ruff",
        "pylint>=1.9.5",
        "paramiko==2.11.0",
        "netmiko==2.4.2",
