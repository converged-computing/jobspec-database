{
    "application": "COBALT",
    "details": {
        "resource_requirements": {
            "time": "10",
            "queue": "default",
            "account": "Performance"
        },
        "software": {
            "mpich": {
                "rank_reorder_display": "true"
            },
            "python": {
                "scripts": [
                    "parsenodes.py",
                    "parsejobnodes.py"
                ]
            },
            "multiroutes": {
                "executable": "./multiroutes"
            },
            "aprun": {
                "arguments": {
                    "n": "$(($COBALT_JOBSIZE*rpn))",
                    "N": "$rpn",
                    "d": "$depth",
                    "j": "1",
                    "cc": "depth"
                }
            }
        }
    }
}