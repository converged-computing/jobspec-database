{
    "application": "Singularity",
    "details": {
        "software": [
            "Singularity",
            "Python",
            "Nvidia-modprobe",
            "Ray"
        ],
        "resources": {
            "cpu": "1 core",
            "memory": "4GB",
            "partition": "orion",
            "gpu": "1",
            "constraint": "avx2",
            "output_file": "outputs/ensemble-%A.out",
            "error_file": "outputs/ensemble-%A.out",
            "working_directory": "$TMPDIR/$USER/hn_delin"
        },
        "script_parameters": {
            "$1": "Project path",
            "$2": "Experiment name",
            "ITER_PER_EPOCH": "200",
            "NUM_CPUS": "4",
            "RAY_ROOT": "$TMPDIR/ray",
            "mode": "concat"
        }
    }
}