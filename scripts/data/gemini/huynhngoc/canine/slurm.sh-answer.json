{
    "application": "Singularity",
    "details": {
        "environment": {
            "module": "singularity",
            "RAY_ROOT": "$TMPDIR/ray",
            "ITER_PER_EPOCH": 100,
            "NUM_CPUS": 4
        },
        "resource_requirements": {
            "cores": 16,
            "nodes": 1,
            "memory": "80G",
            "gpu": 1,
            "partition": "gpu",
            "job_name": "canine",
            "email": "aurogr@nmbu.no",
            "mail_type": "ALL",
            "output": "outputs/unet-%A.out",
            "error": "outputs/unet-%A.out"
        },
        "software": {
            "python_scripts": [
                "experiment_orion.py",
                "post_processing.py",
                "run_test.py"
            ],
            "singularity_image": "deoxys-new.sif"
        },
        "data_management": {
            "dataset_folder": "$TMPDIR/$USER/hn_delin",
            "dataset_source": "$HOME/datasets/headneck/",
            "scratch_folder": "$SCRATCH"
        }
    }
}