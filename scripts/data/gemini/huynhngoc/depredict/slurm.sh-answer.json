{
    "application": "Slurm",
    "details": {
        "resource_requirements": {
            "cores": 16,
            "nodes": 1,
            "memory": "200G",
            "partition": "gpu",
            "gpus": 1
        },
        "software_requirements": {
            "singularity": "deoxys-mar22-polished.sif",
            "python": "run_test.py",
            "nvidia-modprobe": ""
        },
        "environment_variables": {
            "RAY_ROOT": "$TMPDIR/ray",
            "NUM_CPUS": 4
        },
        "data_sources": {
            "training_data": "/net/fs-1/Ngoc/depredict/*",
            "hnperf_data": "/net/fs-1/Ngoc/hnperf/$2",
            "scratch_directory": {
                "hnperf": "$SCRATCH/hnperf/$2",
                "analysis": "$SCRATCH/analysis/$2"
            }
        }
    }
}