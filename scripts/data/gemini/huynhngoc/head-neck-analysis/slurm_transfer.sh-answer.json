{
    "application": "Singularity",
    "details": {
        "resource_requirements": {
            "cores": 1,
            "nodes": 1,
            "memory": "32G",
            "partition": "gpu",
            "gpus": 1,
            "cpus_per_task": 16
        },
        "software_requirements": {
            "singularity": "deoxys-new_dec.sif",
            "python": "transfer_learning.py",
            "ray": "ray"
        },
        "environment_variables": {
            "NUM_CPUS": 4,
            "RAY_ROOT": "$TMPDIR/ray"
        },
        "data_requirements": {
            "input_data": "$HOME/datasets/headneck/*",
            "output_data": "$SCRATCH/hnperf/$2",
            "analysis_data": "$SCRATCH/analysis/$2"
        }
    }
}