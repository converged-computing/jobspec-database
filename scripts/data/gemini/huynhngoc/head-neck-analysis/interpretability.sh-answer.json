{
    "application": "Singularity",
    "details": {
        "software": [
            "Singularity",
            "Python",
            "nvidia-modprobe",
            "ray"
        ],
        "resources": {
            "cpus": 1,
            "nodes": 1,
            "memory": "64G",
            "partition": "gpu",
            "gpu": 1,
            "scratch_space": "$SCRATCH_PROJECTS/ceheads/hnperf/$2",
            "analysis_folder": "$SCRATCH_PROJECTS/ceheads/hnperf_analysis/$2",
            "temp_folder": "$TMPDIR/ray"
        },
        "scripts": [
            "interpretability.py",
            "post_processing.py",
            "run_test.py"
        ],
        "environment_variables": [
            "ITER_PER_EPOCH",
            "NUM_CPUS",
            "RAY_ROOT",
            "MAX_SAVE_STEP_GB"
        ],
        "data_files": [
            "$PROJECTS/ngoc/datasets/headneck/*",
            "$PROJECTS/ngoc/outcome_model/$2"
        ]
    }
}