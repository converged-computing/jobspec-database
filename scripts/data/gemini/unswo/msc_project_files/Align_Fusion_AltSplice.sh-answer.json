{
    "application": "RNA-seq",
    "software": [
        "cutadapt",
        "STAR",
        "samtools",
        "stringtie",
        "STAR-Fusion",
        "FusionInspector",
        "MAJIQ",
        "voila"
    ],
    "resource_requirements": {
        "cores": "$CORES",
        "nodes": "1",
        "memory": "$MEM",
        "partition": "defq,long",
        "scratch_space": "$TMPDIR",
        "read_directory": "$READ_DIRECTORY",
        "star_index": "$STAR_INDEX",
        "star_output": "$STAR_OUTPUT",
        "gtf": "$GTF",
        "ctat_lib": "$CTAT_LIB",
        "fusion_output": "$FUSION_OUTPUT",
        "majiq_out": "$MAJIQ_OUT",
        "gff3": "$GFF3"
    },
    "conda_environments": [
        "rnaseqpipe",
        "majiq"
    ]
}