{
    "application": "PyTorch",
    "details": {
        "environment": "PyTorch-190",
        "resource_requirements": {
            "nodes": 1,
            "processors_per_node": 1,
            "gpu": true,
            "queue": "gpu"
        },
        "command": "python3 -m torch.distributed.run --nproc_per_node=$NPROC_PER_NODE --max_restarts=0 --nnodes=$PBS_NUM_NODES --rdzv_id=$PBS_JOBID --rdzv_backend=c10d --rdzv_endpoint=$MASTER:24455 $COMMAND > out",
        "output_file": "out",
        "conda_activation": "conda activate $conda_env"
    }
}