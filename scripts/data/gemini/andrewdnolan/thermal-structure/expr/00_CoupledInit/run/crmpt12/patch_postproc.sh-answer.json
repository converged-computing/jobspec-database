{
    "application": "Dask",
    "details": {
        "scheduler": "Dask scheduler",
        "workers": "Dask workers",
        "libraries": [
            "grid_data.py",
            "downsample.py"
        ],
        "resources": {
            "slurm": {
                "job_name": "dask_gridding",
                "time": "06:00:00",
                "ntasks": 1,
                "cpus_per_task": 16,
                "mem_per_cpu": "4000M",
                "mail_type": "END",
                "mail_user": "andrew.d.nolan@maine.edu",
                "output": "dask_%A_%a.out",
                "error": "dask_%A_%a.err"
            },
            "environment": {
                "CHUNK_SIZE": 48,
                "NUM_WORKERS": 16,
                "THREADS_PER_WORKER": 1,
                "SCHEDULER_FILE": "${SLURM_JOB_ID}_${SLURM_ARRAY_TASK_ID}-scheduler.json"
            },
            "storage": {
                "scratch": [
                    "result/crmpt12/nc",
                    "result/crmpt12/gridded",
                    "result/crmpt12/thinned"
                ],
                "local": "${SLURM_TMPDIR}"
            }
        }
    }
}