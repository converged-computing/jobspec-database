{
    "application": "PALM",
    "details": {
        "software": [
            "julia",
            "palmrun"
        ],
        "resource_requirements": {
            "nodes": 2,
            "tasks_per_node": 24,
            "account": "PAS0409"
        },
        "other": {
            "job_name_prefix": "palm_run",
            "output_log_path": "palm_logs/%j.log",
            "palm_path": "~/palm/current_version",
            "command_arguments": [
                "-r",
                "job_name",
                "-c",
                "intel",
                "-X48",
                "-T24",
                "-q",
                "parallel",
                "-t",
                "run_time",
                "-a",
                "io_config",
                "-v",
                "-m",
                4315
            ],
            "retry_attempts": 2,
            "sleep_interval": 30,
            "cleanup_pattern": "*$job_name*",
            "io_config_cleanup_path": "JOBS/$job_name",
            "output_file": "output.json"
        }
    }
}