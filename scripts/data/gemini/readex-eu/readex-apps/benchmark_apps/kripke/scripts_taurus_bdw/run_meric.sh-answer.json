{
    "application": "kripke",
    "details": {
        "scheduler": "Slurm",
        "resources": {
            "nodes": 1,
            "tasks_per_node": 28,
            "cpus_per_task": 1,
            "memory_per_cpu": "2200M",
            "partition": "broadwell",
            "reservation": "p_readex_56",
            "account": "p_readex",
            "exclusive": true,
            "comment": "no_monitoring",
            "job_name": "kripke"
        },
        "environment": {
            "MERIC_MODE": 1,
            "MERIC_COUNTERS": "perfevent",
            "MERIC_CONTINUAL": 1,
            "MERIC_DETAILED": 1,
            "MERIC_OUTPUT_DIR": "$SCRATCH/DELETE",
            "MERIC_FREQUENCY": 24,
            "MERIC_UNCORE_FREQUENCY": 25,
            "MERIC_NUM_THREADS": 0,
            "MERIC_OUTPUT_FILENAME": "24_25_CONFIG"
        },
        "execution": {
            "command": "./kripke $KRIPKE_COMMAND",
            "number_of_processes": 28
        }
    }
}