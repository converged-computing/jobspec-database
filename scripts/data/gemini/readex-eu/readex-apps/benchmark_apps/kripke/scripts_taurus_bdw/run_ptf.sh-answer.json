{
    "application": "psc_frontend",
    "details": {
        "resources": {
            "nodes": 2,
            "tasks_per_node": 28,
            "cpus_per_task": 1,
            "memory_per_cpu": "2200M",
            "partition": "broadwell",
            "reservation": "p_readex_56",
