{
    "application": "spack",
    "details": {
        "scheduler": "slurm",
        "spack_version": "0.17.3",
        "spack_instance_name": "gpu",
        "spack_instance_version": "b",
        "spack_instance_dir": "/cm/shared/apps/spack/0.17.3/gpu/b",
        "spack_package": "papi@6.0.0.1",
        "spack_compiler": "gcc@8.4.0",
        "spack_variants": "+cuda ~example ~infiniband ~lmsensors ~nvml ~powercap ~rapl ~rocm ~rocm_smi ~sde +shared ~static_tools",
        "spack_dependencies": "^cuda@10.2.89/$(spack find --format '{hash:7}' cuda@10.2.89 % ${SPACK_COMPILER})",
        "spack_spec": "${SPACK_PACKAGE} % ${SPACK_COMPILER} ${SPACK_VARIANTS} ${SPACK_DEPENDENCIES}",
        "slurm_job_script": "${SLURM_JOB_SCRIPT}",
        "slurm_job_id": "${SLURM_JOB_ID}",
        "slurm_job_md5sum": "${SLURM_JOB_MD5SUM}",
        "slurm_job_dependency": "${SLURM_JOB_DEPENDENCY}",
        "slurm_nodes": "1",
        "slurm_ntasks_per_node": "1",
        "slurm_cpus_per_task": "10",
        "slurm_mem": "93G",
        "slurm_gpus": "1",
        "slurm_time": "00:30:00",
        "local_scratch_dir": "/scratch/${USER}/job_${SLURM_JOB_ID}",
        "tmpdir": "${LOCAL_SCRATCH_DIR}",
        "system_name": "expanse"
    }
}