{
    "application": "spack",
    "details": {
        "scheduler": "slurm",
        "slurm_partition": "hotel-gpu",
        "slurm_nodes": 1,
        "slurm_tasks": 1,
        "slurm_cpus": 8,
        "slurm_time": "00:30:00",
        "spack_version": "0.17.3",
        "spack_instance": "gpu",
        "spack_package": "papi@6.0.0.1",
        "spack_compiler": "gcc@10.2.0",
        "spack_variants": "+cuda ~example ~infiniband ~lmsensors ~nvml ~powercap ~rapl ~rocm ~rocm_smi ~sde +shared ~static_tools",
        "spack_dependencies": "^cuda@11.2.2/$(spack find --format '{hash:7}' cuda@11.2.2 % ${SPACK_COMPILER})",
        "spack_spec": "${SPACK_PACKAGE} % ${SPACK_COMPILER} ${SPACK_VARIANTS} ${SPACK_DEPENDENCIES}",
        "spack_install_jobs": "${SLURM_CPUS_PER_TASK}",
        "valgrind_script": "valgrind@3.17.0.sh"
    }
}