{
    "application": "Spack",
    "details": {
        "scheduler": "Slurm",
        "scheduler_module": "slurm",
        "spack_version": "0.17.3",
        "spack_instance_name": "cpu",
        "spack_instance_version": "b",
        "spack_instance_dir": "/cm/shared/apps/spack/0.17.3/cpu/b",
        "spack_package": "valgrind@3.17.0",
        "spack_compiler": "gcc@10.2.0",
        "spack_variants": "+boost +mpi +only64bit +ubsan",
        "spack_dependencies": "^boost@1.77.0/$(spack find --format '{hash:7}' boost@1.77.0 % ${SPACK_COMPILER} +mpi ^mvapich2@2.3.7)",
        "spack_spec": "${SPACK_PACKAGE} % ${SPACK_COMPILER} ${SPACK_VARIANTS} ${SPACK_DEPENDENCIES}",
        "system_name": "expanse",
        "local_scratch_dir": "/scratch/${USER}/job_${SLURM_JOB_ID}",
        "tmpdir": "${LOCAL_SCRATCH_DIR}",
        "slurm_job_script": "$(scontrol show job ${SLURM_JOB_ID} | awk -F= '/Command=/{print $2}')",
        "slurm_job_md5sum": "$(md5sum ${SLURM_JOB_SCRIPT})",
        "slurm_job_id": "${SLURM_JOB_ID}",
        "slurm_job_dependency": "${SLURM_JOB_DEPENDENCY}",
        "slurm_nodes": 1,
        "slurm_ntasks_per_node": 1,
        "slurm_cpus_per_task": 16,
        "slurm_mem": "32G",
        "slurm_time": "00:30:00",
        "slurm_partition": "ind-shared",
        "slurm_account": "use300",
        "slurm_reservation": "rocky8u7_testing",
        "slurm_job_name": "valgrind@3.17.0"
    }
}