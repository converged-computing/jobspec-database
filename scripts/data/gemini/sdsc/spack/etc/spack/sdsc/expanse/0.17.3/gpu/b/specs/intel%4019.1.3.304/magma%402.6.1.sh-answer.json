{
    "application": "SLURM",
    "software": [
        "spack",
        "magma",
        "cuda",
        "intel-mkl",
        "intel",
        "gsl"
    ],
    "resources": [
        "CPU",
        "GPU",
        "memory",
        "disk"
    ],
    "details": {
        "SLURM": {
            "job_name": "magma@2.6.1",
            "account": "use300",
            "reservation": "rocky8u7_testing",
            "partition": "ind-gpu-shared",
            "nodes": 1,
            "tasks_per_node": 1,
            "cpus_per_task": 10,
            "memory": "93G",
            "gpus": 1,
            "time": "00:30:00",
            "output": "%x.o%j.%N"
        },
        "spack": {
            "version": "0.17.3",
            "instance_name": "gpu",
            "instance_version": "b",
            "instance_dir": "/cm/shared/apps/spack/${SPACK_VERSION}/${SPACK_INSTANCE_NAME}/${SPACK_INSTANCE_VERSION}",
            "package": "magma@2.6.1",
            "compiler": "intel@19.1.3.304",
            "variants": "+cuda cuda_arch=70,80 +fortran ~ipo ~rocm +shared",
            "dependencies": "^cuda@11.2.2/$(spack find --format '{hash:7}' cuda@11.2.2 % ${SPACK_COMPILER}) ^intel-mkl@2020.4.304/$(spack find --format '{hash:7}' intel-mkl@2020.4.304 % ${SPACK_COMPILER} ~ilp64 threads=none",
            "spec": "${SPACK_PACKAGE} % ${SPACK_COMPILER} ${SPACK_VARIANTS} ${SPACK_DEPENDENCIES}"
        },
        "magma": {
            "version": "2.6.1"
        },
        "cuda": {
            "version": "11.2.2"
        },
        "intel-mkl": {
            "version": "2020.4.304"
        },
        "intel": {
            "version": "19.1.3.304"
        },
        "gsl": {
            "version": "2.7"
        },
        "license_file": "40000@elprado.sdsc.edu:40200@elprado.sdsc.edu",
        "scheduler_module": "slurm",
        "scratch_dir": "/scratch/${USER}/job_${SLURM_JOB_ID}",
        "tmpdir": "${LOCAL_SCRATCH_DIR}",
        "system_name": "expanse"
    }
}