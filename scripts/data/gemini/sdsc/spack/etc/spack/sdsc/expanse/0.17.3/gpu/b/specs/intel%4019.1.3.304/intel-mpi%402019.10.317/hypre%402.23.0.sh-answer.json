{
    "application": "Spack",
    "details": {
        "scheduler": "Slurm",
        "environment": {
            "scratch_directory": "/scratch/${USER}/job_${SLURM_JOB_ID}",
            "system_name": "expanse",
            "spack_version": "0.17.3",
            "spack_instance_name": "gpu",
            "spack_instance_version": "b",
            "spack_instance_dir": "/cm/shared/apps/spack/${SPACK_VERSION}/${SPACK_INSTANCE_NAME}/${SPACK_INSTANCE_VERSION}",
            "intel_license_file": "40000@elprado.sdsc.edu:40200@elprado.sdsc.edu"
        },
        "resources": {
            "nodes": 1,
            "tasks_per_node": 1,
            "cpus_per_task": 10,
            "memory": "93G",
            "gpus": 1,
            "time": "00:30:00"
        },
        "package": {
            "name": "hypre",
            "version": "2.23.0",
            "compiler": "intel@19.1.3.304",
            "variants": "~complex +cuda cuda_arch=70,80 ~debug +fortran ~int64 ~internal-superlu ~mixedint +mpi ~openmp +shared +superlu-dist ~unified-memory",
            "dependencies": "^intel-mkl@2020.4.304/$(spack find --format '{hash:7}' intel-mkl@2020.4.304 % ${SPACK_COMPILER} ~ilp64 threads=none) ^superlu-dist@7.1.1/$(spack find --format '{hash:7}' superlu-dist@7.1.1 % ${SPACK_COMPILER} ^intel-mpi@2019.10.317)"
        }
    }
}