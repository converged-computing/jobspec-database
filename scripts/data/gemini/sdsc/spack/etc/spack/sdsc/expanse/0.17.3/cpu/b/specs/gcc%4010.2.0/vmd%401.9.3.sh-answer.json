{
    "application": "VMD",
    "software": [
        "SLURM",
        "SPACK",
        "VMD",
        "Meson",
        "GCC"
    ],
    "resource_requirements": {
        "scheduler": "SLURM",
        "partition": "ind-shared",
        "nodes": 1,
        "tasks_per_node": 1,
        "cpus_per_task": 16,
        "memory": "32G",
        "time": "02:00:00",
        "scratch_dir": "/scratch/${USER}/job_${SLURM_JOB_ID}",
        "spack_version": "0.17.3",
        "spack_instance_name": "cpu",
        "spack_instance_version": "b",
        "spack_instance_dir": "/cm/shared/apps/spack/${SPACK_VERSION}/${SPACK_INSTANCE_NAME}/${SPACK_INSTANCE_VERSION}",
        "spack_package": "vmd@1.9.3",
        "spack_compiler": "gcc@10.2.0",
        "spack_variants": "",
        "spack_dependencies": "^meson@0.60.0/$(spack find --format '{hash:7}' meson@0.60.0 % ${SPACK_COMPILER})"
    }
}