{
    "application": "Singularity",
    "details": {
        "container_image": "docker://$image",
        "container_image_cache": "${home}/cache/$sif",
        "scratch_directory": "$SCRATCHDIR",
        "environment_variables": "$ENVS",
        "command": "$CMD",
        "mount_points": "$mounts",
        "ssh_host": "${ssh_host}",
        "ssh_port": "${ssh_port}",
        "ssh_key": "$sshkey",
        "user_name": "${USER_NAME}",
        "cpu_cores": "$CPUL",
        "memory": "$MEML",
        "gpu_count": "$GPUR",
        "pbs_job_output": "zuphux.cerit-sc.cz:logs/$$.stdout",
        "pbs_job_error": "zuphux.cerit-sc.cz:logs/$$.stderr",
        "pbs_vnode": "^samson",
        "pbs_scratch_ssd": "20gb"
    }
}