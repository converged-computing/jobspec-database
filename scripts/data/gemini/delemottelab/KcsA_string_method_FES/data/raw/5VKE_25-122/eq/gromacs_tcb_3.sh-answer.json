{
    "application": "Gromacs",
    "details": {
        "version": "2020.5",
        "partition": "tcb",
        "nodes": 1,
        "mpi_processes_per_node": 8,
        "gpus": 2,
        "gpu_nodes": "gpu04",
        "wall_clock_time": "23:30:00",
        "input_files": [
            "../../../../models/mdp/NPTres${var}.mdp",
            "step${var0}.gro",
            "../topol.top",
            "../index.ndx",
            "topol_pp.top",
            "step${var0}.gro",
            "step${var}.cpt"
        ],
        "output_files": [
            "job-%j.err",
            "job-%j.out",
            "topol${var}.tpr",
            "step${var}.gro",
            "step${var}.cpt"
        ],
        "commands": [
            "gmx grompp -maxwarn 1 -f ../../../../models/mdp/NPTres${var}.mdp  -c step${var0}.gro  -p ../topol.top  -o topol${var}.tpr -n ../index.ndx -pp topol_pp.top -r step${var0}.gro",
            "gmx mdrun -v -maxh $time -s topol${var}.tpr  -pin on -deffnm step$var",
            "gmx mdrun -v -maxh $time -s topol${var}.tpr  -pin on -deffnm step$var -cpi step${var}.cpt"
        ]
    }
}