{
    "application": "Gromacs",
    "software_requirements": [
        "Gromacs/2020.1"
    ],
    "resource_requirements": [
        "SBATCH -p tcb",
        "SBATCH -J switch_FF",
        "SBATCH -t 23:30:00",
        "SBATCH -N 1",
        "SBATCH -C gpu --gres=gpu:2",
        "SBATCH -e job-%j.err -o job-%j.out",
