{
    "application": "nextflow",
    "details": {
        "version": "22.10.0",
        "pipeline": "KristinaGagalova/pante2-legacy",
        "profile": "pawsey_setonix,singularity",
        "singularity_version": "3.11.4-slurm",
        "singularity_cachedir": "./work",
        "resources": {
            "nodes": 1,
            "tasks": 1,
            "cpus_per_task": 2,
            "partition": "work",
