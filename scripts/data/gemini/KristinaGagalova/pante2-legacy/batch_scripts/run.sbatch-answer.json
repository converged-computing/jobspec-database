        "output_dir": "test/results",
        "optional_arguments": {
            "repbase": "--repbase /path/to/RepBaseRepeatMaskerEdition-20181026.tar.gz",
            "rm_meta": "--rm_meta /path/to/RepeatMaskerMetaData-20181026.tar.gz",
            "species": "--species \"Ascomycota\""
        }
    }
}