{
    "application": "iprscan",
    "details": {
        "environment": {
            "modules": [
                "miniconda2",
                "funannotate/1.8.0",
                "iprscan"
            ],
            "conda_env": "funannotate-1.8"
        },
        "resources": {
            "slurm": {
                "tasks": 24,
                "nodes": 1,
                "memory": "96G",
                "partition": "intel",
                "time": "72:00:00",
                "output": "logs/iprscan.%a.log"
            },
            "files": {
                "samples": "samples.csv",
                "output_dir": "annotate"
            }
        },
        "tools": {
            "funannotate": "1.8.0",
            "interproscan": "interproscan.sh"
        },
        "dependencies": {
            "perl": true,
            "python": true,
            "miniconda2": true,
            "anaconda3": true
        }
    }
}