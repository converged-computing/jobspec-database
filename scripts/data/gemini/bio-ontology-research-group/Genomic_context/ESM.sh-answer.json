{
    "application": "ESM",
    "details": {
        "environment": {
            "conda": "base",
            "cuda": "11.7.1"
        },
        "resources": {
            "nodes": 1,
            "partition": "batch",
            "job_name": "ESM",
            "output_file": "ESM.%J.out",
            "error_file": "ESM.%J.err",
            "mail_user": "daulet.toibazar@kaust.edu.sa",
            "mail_type": "ALL",
            "walltime": "2:00:00",
            "gpus": 1,
            "cpus_per_task": 4,
            "constraint": "v100",
            "memory_per_cpu": "20GB"
        },
        "script_arguments": {
            "model": "esm2_t36_3B_UR50D",
            "input_fasta": "ESM/few_proteins.fasta",
            "output_file": "ESM/some_proteins_emb_esm2",
            "repr_layers": 36,
            "include": "mean"
        }
    }
}