{
    "application": "buildstockbatch",
    "details": {
        "software": [
            "python",
            "apptainer",
            "dask",
            "buildstockbatch",
            "pdsh"
        ],
        "resources": {
            "file_systems": [
                "/kfs2/shared-projects/buildstock",
                "/tmp/scratch"
            ],
            "environment_variables": [
                "MY_PYTHON_ENV",
                "LOCAL_SCRATCH",
                "UPLOADONLY",
                "MEMORY",
                "NPROCS",
                "OUT_DIR",
                "PROJECTFILE"
            ],
            "slurm_parameters": [
                "--tmp=1000000",
                "SLURM_JOB_ID",
                "HOSTNAME",
                "SLURM_JOB_NODELIST_PACK_GROUP_0",
                "SLURM_JOB_NODELIST_PACK_GROUP_1"
            ]
        }
    }
}