{
    "application": "lammps",
    "details": {
        "environment": {
            "compiler": "gcc/6",
            "intel": "intel/18.0",
            "mpi": "intelmpi"
        },
        "resources": {
            "memory_per_cpu": "1024M",
            "job_name": "skx_potc_bench",
