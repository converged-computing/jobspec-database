{
    "application": "wandb",
    "details": {
        "software": [
            "python",
            "tensorflow",
            "nvidia-cudnn",
            "wandb"
        ],
        "resources": {
            "compute": {
                "cpu": "1 core",
                "gpu": "1 GPU"
            },
            "memory": "16GB",
            "time": "10 hours",
            "partition": "top6",
            "exclude_nodes": [
                "gpu23a",
                "gpu23b",
                "gpu23c",
                "gpu23d"
            ],
            "environment_variables": {
                "LD_LIBRARY_PATH": "$CONDA_PREFIX/lib/:$CUDNN_PATH/lib:$LD_LIBRARY_PATH",
                "PATH": "${PATH}:/usr/local/nvidia/bin:/usr/local/cuda/bin"
            }
        }
    }
}