{
    "application": "Slurm",
    "details": {
        "job_name": "BBPip",
        "partition": "htc",
        "nodes": 1,
        "tasks_per_node": 1,
        "cpus_per_task": 4,
        "memory": "20G",
        "time": "1-00:00:00",
        "output": "mulitple_jobs_%j.log",
        "array": "1-300",
        "environment_variable": "OMP_NUM_THREADS",
        "environment_variable_value": "${SLURM_CPUS_PER_TASK}",
        "mpi_command": "mpirun",
        "mpi_tasks": "$SLURM_NTASKS",
        "python_script": "main.py"
    }
}