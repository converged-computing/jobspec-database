{
    "application": "Quantum ESPRESSO",
    "details": {
        "version": "7.3.1",
        "compiler": "GCC-12.2.0",
        "slurm_partition": "scarf",
        "slurm_constraint": "amd",
        "slurm_nodes": 1,
        "slurm_tasks_per_node": 32,
        "slurm_time_limit": "10:00",
        "slurm_output_file": "job_%J.log",
        "slurm_error_file": "job_%J.err",
        "openmp_threads": 1,
        "input_file": "c60_scf.pwi",
        "output_file": "c60_scf.pwo"
    }
}