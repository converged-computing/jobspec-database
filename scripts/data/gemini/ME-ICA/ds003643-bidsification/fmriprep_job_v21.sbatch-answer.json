{
    "application": "fmriprep",
    "software": [
        "singularity-3.5.3",
        "fmriprep_21.0.0.sif",
        "freesurfer"
    ],
    "resources": [
        "nodes: 1",
        "ntasks: 1",
        "cpus-per-task: 8",
        "mem-per-cpu: 4gb",
        "time: 60:00:00",
        "partition: IB_40C_512G"
    ],
    "dependencies": [
        "templateflow",
        "bids"
    ],
    "environment": [
        "TEMPLATEFLOW_HOME",
        "FS_LICENSE"
    ],
    "commands": [
        "singularity run",
        "sed",
        "mkdir",
        "eval"
    ]
}