{
    "application": "Snakemake",
    "details": {
        "python_version": "3.4.0",
        "cluster_config": "cluster.json",
        "cluster_resources": {
            "gres": "{cluster.gres}",
            "threads": "{cluster.threads}",
            "partition": "{cluster.partition}",
            "time": "{cluster.time}",
            "mem": "{cluster.mem}"
        },
        "job_name": "{params.rname}",
        "max_jobs": 100,
        "output_stats": "Reports/initialqc.stats",
        "output_log": "Reports/snakemake.log",
        "scripts": [
            "Reports/sum-fastqc-lite.py",
            "Reports/ngser.pl",
            "Reports/stats2html.py"
        ]
    }
}