{
    "application": "python",
    "details": {
        "environment": {
            "os": "rhel8",
            "modules": [
                "rhel8/default-amp",
                "miniconda/3",
                "sim2real"
            ],
            "conda_environment": "sim2real"
        },
        "executable": "sim2real/train/tune.py",
        "workdir": "SLURM_SUBMIT_DIR",
        "openmp_threads": 1,
        "mpi_tasks_per_node": "SLURM_TASKS_PER_NODE",
        "logging": {
            "directory": "logs",
            "filename": "out.$SLURM_JOB_ID"
        },
        "resources": {
            "nodes": "SLURM_JOB_NUM_NODES",
            "tasks": "SLURM_NTASKS",
            "gpus": "SLURM_GRES_GPU",
            "walltime": "05:00:00"
        }
    }
}