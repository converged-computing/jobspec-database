{
    "application": "SLURM",
    "details": {
        "partition": "p100,t4v1,t4v2",
        "nodes": 1,
        "gpus": 1,
        "tasks_per_node": 1,
        "cpus": 6,
        "memory": "32G",
        "output_file": "logs_new/ep_2/%x_%A-%a_%n-%t.out",
        "job_name": "ep_2",
        "qos": "normal",
        "open_mode": "append",
        "environment": "combinact",
        "python_version": "3.9.13",
        "conda_version": "4.14.0",
        "pytorch_version": "1.13.1+cu117",
        "cuda_version": "11.7",
        "gpu_count": 1,
        "checkpoint_dir": "/checkpoint/${USER}/${SLURM_JOB_ID}",
        "output_dir": "/scratch/hdd001/home/$USER/checkpoints/$PROJECT_NAME",
        "script": "engine.py",
        "model": "resnet",
        "batch_size": 128,
        "optimizer": "onecycle",
        "epochs": "$EPOCHS",
        "dataset": "$DATASET",
        "augmentation": "True",
        "mixup_and_apex": "True",
        "batch_size_factor": 0.75,
        "learning_rate_factor": 0.95,
        "balanced_dataset": "True",
        "resnet_type": "1",
        "label": "_${RESNET_TYPE}_${ACTFUN_IDX}"
    }
}