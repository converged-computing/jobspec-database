{
    "application": "LSF",
    "other_software": [
        "craype-hugepages16M",
        "PrgEnv-intel",
        "ESMF-intel-haswell",
        "cfp-intel-sandybridge",
        "iobuf",
        "craype-haswell",
        "prod_envir",
        "prod_util",
