{
    "application": "SLURM",
    "details": {
        "job_name": "hddqn_agent",
        "tasks": 1,
        "memory": "24G",
        "cores": 32,
        "output_file": "hddqn_agent.log",
        "gpus": 1,
        "modules": [
            "Python3.10",
            "Xvfb",
            "freeglut",
            "glew",
            "MuJoCo"
        ],
        "python_environment": "rl",
        "script": "run_hdddqn.py",
        "arguments": [
            "--cuda",
            "--gym-id",
            "$1",
            "--total-timesteps",
            "1000000",
            "--capture-video",
            "--track",
            "--update-freq",
            "5",
            "--pre-train-steps",
            "50000"
        ]
    }
}