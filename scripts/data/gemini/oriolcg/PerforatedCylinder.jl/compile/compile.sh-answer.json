{
    "application": "julia",
    "details": {
        "software": [
            "mpiexecjl",
            "julia"
        ],
        "resources": {
            "partition": "compute",
            "time": "04:00:00",
            "nodes": 1,
            "tasks_per_node": 1,
            "cpus_per_task": 1,
            "memory_per_cpu": "20G"
        },
        "environment": {
            "module_file": "modules.sh"
        },
        "project": "../",
        "julia_version": "1.7.2",
        "optimization_flags": "-O3",
        "bounds_checking": "--check-bounds=no",
        "color": "--color=yes",
        "script": "compile.jl"
    }
}