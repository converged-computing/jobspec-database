{
    "application": "Slurm",
    "details": {
        "queue": "bosch_cpu-cascadelake",
        "time": "4 hours",
        "nodes": 1,
        "cores": 2,
        "array_size": 3,
        "output_file": "logs/cluster/%x.%N.%j.out",
        "error_file": "logs/cluster/%x.%N.%j.err",
        "job_name": "apt_test",
        "python_version": "3",
        "virtual_environment": "env",
        "script": "examples/ensemble/test_ensemble.py",
        "arguments": {
            "run_id": "$1",
            "task_id": "$SLURM_ARRAY_TASK_ID",
            "num_workers": 3,
            "dataset_id": "$2",
            "seed": "$3",
            "ensemble_setting": "ensemble",
            "portfolio_type": "greedy",
            "num_threads": 2,
            "test": "false"
        }
    }
}