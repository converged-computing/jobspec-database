{
    "application": "SLURM",
    "details": {
        "job_name": "tf-wavenet-singleton-fast-generate",
        "output": "generate.log",
        "open_mode": "append",
        "dependency": "singleton",
        "partition": "gpu",
        "time": "1:00:00",
        "cpus_per_task": 1,
        "cores_per_socket": 1,
        "gres": "gpu:1",
        "gres_flags": "enforce-binding",
        "mail_type": "ALL",
        "mail_user": "brinton@cs.stanford.edu",
        "modules": [
            "cudnn"
        ],
        "virtualenv": "../bin/activate",
        "environment_variables": [
            "LD_LIBRARY_PATH=/farmshare/software/free/cudnn/6.0/lib64/:$LD_LIBRARY_PATH",
            "LD_LIBRARY_PATH=..:$LD_LIBRARY_PATH"
        ],
        "execution_script": "generate.sh"
    }
}