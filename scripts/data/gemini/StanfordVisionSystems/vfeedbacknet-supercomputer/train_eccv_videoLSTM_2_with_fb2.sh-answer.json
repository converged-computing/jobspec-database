{
    "application": "Slurm",
    "details": {
        "job_name": "vlstm_f2_jester",
        "output_file": "eccv_model_videolstm_with_fb2_%j.log",
        "error_file": "eccv_model_videolstm_with_fb2_%j.log",
        "runtime": "12:00:00",
        "tasks": 1,
        "cpus_per_task": 5,
        "memory": "64000M",
        "tmp_space": "64000M",
        "gpus": 4,
        "modules": [
            "GCC/4.9.2-binutils-2.25",
            "OpenMPI/1.8.5",
            "Python/3.6.0",
            "tensorflow/1.5.0-cp36"
        ],
        "virtualenv": "$HOME/.local/venv/bin/activate",
        "data_dir": "$TMPFS/20bn-jester",
        "training_script": "$HOME/projects/vfeedbacknet/scripts/jemmons_train_20bn-jester.xs.sh",
        "training_parameters": {
            "model_name": "vfeedbacknet_eccv_videoLSTM_2_with_fb2",
            "result_dir": "$WORK/vfeedbacknet-results/20bn/vfeedbacknet_eccv_videoLSTM_2_with_fb2.xs",
            "video_length": 20,
            "video_height": 112,
            "video_width": 112,
            "video_downsample_ratio": 2,
            "learning_rate_init": 0.1,
            "learning_rate_decay": 0.998,
            "learning_rate_min": 0.001,
            "global_step_init": 0,
            "train_batch_size": 32,
            "prefetch_batch_size": 256,
            "validation_interval": 16,
            "last_loss_multipler": 1,
            "num_gpus": 4,
            "num_cpus": 10,
            "pretrain_root_prefix": "$WORK/pretrained-models"
        }
    }
}