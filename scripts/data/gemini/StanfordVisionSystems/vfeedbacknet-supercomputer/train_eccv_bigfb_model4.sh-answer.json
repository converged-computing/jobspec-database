{
    "application": "TensorFlow",
    "details": {
        "environment": {
            "operating_system": "Linux",
            "compiler": "GCC/4.9.2-binutils-2.25",
            "mpi": "OpenMPI/1.8.5",
            "python": "Python/3.6.0",
            "tensorflow": "tensorflow/1.5.0-cp36",
            "virtualenv": "$HOME/.local/venv"
        },
        "resources": {
            "cpus": 5,
            "memory": "64000M",
            "tmp_storage": "64000M",
            "gpus": 4
        },
        "data": {
            "dataset": "20bn-jester.xs",
            "data_root": "$TMPFS/20bn-jester",
            "source_path": "$WORK/datasets/20bn-jester.xs.tar"
        },
        "training_script": "$HOME/projects/vfeedbacknet/scripts/jemmons_train_20bn-jester.xs.sh",
        "training_parameters": {
            "model_name": "vfeedbacknet_eccv_bigfb_model4",
            "video_length": 20,
            "video_height": 112,
            "video_width": 112,
            "video_downsample_ratio": 2,
            "learning_rate_init": 0.1,
            "learning_rate_decay": 0.998,
            "learning_rate_min": 0.001,
            "global_step_init": 0,
            "train_batch_size": 32,
            "prefetch_batch_size": 256,
            "validation_interval": 16,
            "last_loss_multipler": 4,
            "num_gpus": 4,
            "num_cpus": 5,
            "pretrain_root_prefix": "$WORK/pretrained-models"
        },
        "results_path": "$WORK/vfeedbacknet-results/20bn/vfeedbacknet_eccv_bigfb_model4.xs"
    }
}