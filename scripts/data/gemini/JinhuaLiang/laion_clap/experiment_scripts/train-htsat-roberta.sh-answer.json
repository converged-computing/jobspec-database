{
    "application": "SLURM",
    "details": {
        "software": [
            "openmpi",
            "cuda",
            "python",
            "transformers",
            "wandb"
        ],
        "resources": {
            "nodes": 3,
            "tasks_per_node": 8,
            "cpus_per_gpu": 6,
            "partition": "g40423",
            "filesystem": "/fsx",
            "cache_dir": "/fsx/yusong/transformers_cache",
            "logs_dir": "/fsx/clap_logs",
            "pretrained_audio_dir": "/fsx/yusong/audio_pretrained_model"
        },
        "environment_variables": [
            "NCCL_PROTO",
            "FI_EFA_FORK_SAFE",
            "FI_LOG_LEVEL",
            "FI_EFA_USE_DEVICE_RDMA",
            "NCCL_DEBUG",
            "OMPI_MCA_mtl_base_verbose",
            "FI_EFA_ENABLE_SHM_TRANSFER",
            "FI_PROVIDER",
            "FI_EFA_TX_MIN_CREDITS",
            "NCCL_TREE_THRESHOLD",
            "HOSTNAMES",
            "MASTER_ADDR",
            "MASTER_PORT",
            "COUNT_NODE",
            "TRANSFORMERS_CACHE"
        ],
        "script_arguments": [
            "--save-frequency 5",
            "--save-top-performance 3",
            "--save-most-recent",
            "--dataset-type=\"webdataset\"",
            "--precision=\"fp32\"",
            "--batch-size=96",
            "--lr=1e-4",
            "--wd=0.0",
            "--epochs=45",
            "--workers=6",
            "--use-bn-sync",
            "--amodel HTSAT-tiny",
            "--tmodel roberta",
            "--warmup 3200",
            "--report-to \"wandb\"",
            "--wandb-notes \"10.16-clap-dataset-1#-htsat-roberta\"",
            "--datasetnames \"Clotho\" \"audiocaps\"",
            "--datasetinfos \"train\" \"unbalanced_train\"",
            "--top-k-checkpoint-select-dataset=\"Clotho-test\"",
            "--top-k-checkpoint-select-metric=\"mAP@10\"",
            "--openai-model-cache-dir /fsx/yusong/transformers_cache",
            "--logs /fsx/clap_logs",
            "--seed 3407",
            "--remotedata",
            "--gather-with-grad",
            "--optimizer \"adam\"",
            "--data-filling \"repeatpad\"",
            "--data-truncating \"rand_trunc\"",
            "--pretrained-audio /fsx/yusong/audio_pretrained_model/HTSAT-fullset-imagenet-map=0.467.ckpt"
        ]
    }
}