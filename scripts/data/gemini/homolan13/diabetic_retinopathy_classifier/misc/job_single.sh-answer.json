        "dependencies": [
            "pytorch/pytorch:1.13.1-cuda11.6-cudnn8-runtime",
            "tensorboard",
            "scikit-learn"
        ],
        "script": "training.py",
        "script_arguments": {
            "-t": "type",
            "-b": "backbone",
            "-v": "fvs",
            "-m": "image_modality",
            "-i": "image_size",
            "-s": "batch_size",
            "-n": "suffix"
        }
    }
}