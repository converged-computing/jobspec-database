{
    "application": "SLURM",
    "details": {
        "script_name": "run_ubelix_job.sh",
        "python_script": "filter_mc4.py",
        "resources": {
            "nodes": 1,
            "tasks_per_node": 1,
            "time": "15-00:00:00",
            "memory": "256GB",
            "cpus_per_task": 1,
            "qos": "job_epyc2_long",
            "partition": "epyc2"
        },
        "commands": {
            "submit": "sbatch run_ubelix_job.sh",
            "check": "squeue --user=jn20t930 --jobs={job_id}",
            "monitor": "scontrol show --detail jobid {job_id}",
            "cancel": "scancel {job_id}",
            "gpu_monitor": "ssh gnode14 && nvidia-smi",
            "interactive_job": "srun --partition=gpu-invest --gres=gpu:rtx3090:1 --mem=64G --time=02:00:00 --pty /bin/bash"
        }
    }
}