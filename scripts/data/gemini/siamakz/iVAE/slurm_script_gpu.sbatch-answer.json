{
    "application": "SLURM",
    "details": {
        "job_name": "ivae-gpu",
        "output": "slurm_log/ivae-gpu.%A_%a.out",
        "error": "slurm_log/ivae-gpu.%A_%a.err",
        "partition": "gpu",
        "nodes": 1,
        "memory": "8G",
        "time": "0-6:00",
        "gres": "gpu:1",
        "modules": "nvidia/9.0",
        "conda_environment": "deep",
        "python_script": "main.py",
        "arguments_file": "args_gpu_seeded.txt",
        "argument_selection": "sed -n ${SLURM_ARRAY_TASK_ID}p"
    }
}