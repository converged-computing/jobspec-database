{
    "application": "Slurm",
    "details": {
        "job_name": "2D TGV",
        "nodes": 1,
        "gpus": 8,
        "cpus_per_gpu": 32,
        "partition": "dgxa100_80g_2tb",
        "time": "0-04:00:00",
        "output": "logs/slurm-%A_%a.out",
        "array": "1-8%1",
        "singularity_image": "${HOME}/images/modulus-22.03.sif",
        "python_script": "${ROOT}/main.py",
        "log_file": "${ROOT}/logs/run-${TIME}.log"
    }
}