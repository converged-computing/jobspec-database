{
    "application": "slurm",
    "software": [
        "pytorch",
        "nsight-systems",
        "run_with_profiling.sh",
        "run.sh"
    ],
    "modules": [
        "cgpu",
        "pytorch/v1.6.0-gpu",
        "nsight-systems"
    ],
    "environment_variables": {
        "BATCHSIZE": "2",
        "DO_PROFILING": "false",
        "DO_NCCL_DEBUG": "false",
        "NODES": "${SLURM_NNODES}"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": "10",
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": "1",
        "exclusive": true,
        "cpus_per_gpu": null,
        "gpu_type": "cgpu",
        "time": "2:00:00",
        "ntasks_per_node": "8",
        "nodes": "1",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "pytorch": "v1.6.0-gpu"
    }
}