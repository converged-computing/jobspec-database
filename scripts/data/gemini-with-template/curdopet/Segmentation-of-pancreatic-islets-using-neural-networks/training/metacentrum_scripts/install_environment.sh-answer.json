{
    "application": "pytorch",
    "software": [
        "conda",
        "pytorch",
        "torchvision",
        "torchaudio",
        "pytorch-cuda",
        "progress",
        "wandb",
        "openmim",
        "mmengine",
        "mmcv",
        "mmdetection"
    ],
    "modules": [
        "<name-of-the-conda-module-file>"
    ],
    "environment_variables": {
        "MODULEPATH": "<your-modules-directory-on-frontend-node>"
    },
    "resources": {
        "gres": "gpu",
        "cpus_per_task": "1",
        "tasks": "1",
        "ntasks_per_code": "1",
        "gpus": "1",
        "gpus_per_node": "1",
        "cores_per_socket": null,
        "gpus_per_task": "1",
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "4:00:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": "8gb",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": "scratch_local=10gb:scratch_ssd=10gb:cluster=galdor"
    },
    "versions": {
        "python": "3.8",
        "pytorch-cuda": "12.1",
        "mmcv": "2.0.0",
        "mmdetection": "v3.2.0"
    }
}