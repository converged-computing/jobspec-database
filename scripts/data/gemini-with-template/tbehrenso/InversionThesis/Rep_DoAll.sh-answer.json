{
    "application": "slim",
    "software": [
        "slim",
        "R",
        "Rscript"
    ],
    "modules": [
        "R/latest",
        "Development/gcc/9.2.1"
    ],
    "environment_variables": {
        "sim_types": [
            "neutral_2pop",
            "locallyAdapted_2pop",
            "adaptiveInversion_2pop",
            "inversionLAA_2pop"
        ],
        "s": 0.1,
        "m": 0.001,
        "mu": 1e-05,
        "nrep": 100,
        "rec": 1e-06,
        "sim_type": "${sim_types[$SLURM_ARRAY_TASK_ID]}",
        "dir_name": "${sim_type}_s${s}_m${m}_mu${mu}_r${rec}",
        "tempdir": "$SCRATCH"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": "1",
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "24:00:00",
        "ntasks_per_node": null,
        "nodes": null,
        "memory": "20G",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": "20G",
        "gres_flags": null
    },
    "versions": {}
}