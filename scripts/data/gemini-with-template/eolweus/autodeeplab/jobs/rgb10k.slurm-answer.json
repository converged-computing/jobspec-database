        "cpus_per_gpu": "8",
        "gpu_type": "a100",
        "time": "24:00:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": "80G",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": "gpu80g|gpu40g"
    },
    "versions": {
        "CUDA": "11.3.1",
        "Anaconda3": "2022.05"
    }
}