        "python",
        "nvidia-smi",
        "free",
        "truncate",
        "conda",
        "autodeeplab",
        "CUDA",
        "Anaconda3"
    ],
    "modules": [
        "CUDA/11.3.1",
        "Anaconda3/2022.05"
