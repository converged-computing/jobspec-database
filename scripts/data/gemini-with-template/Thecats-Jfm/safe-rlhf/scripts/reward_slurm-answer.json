{
    "application": "deepspeed",
    "software": [
        "deepspeed",
        "python",
        "torch.distributed.run",
        "wandb"
    ],
    "modules": [
        "anaconda/2023.03",
        "cuda/11.8",
        "safe-rlhf"
    ],
    "environment_variables": {
        "NCCL_IB_HCA": "mlx5_0:1,mlx5_1:1,mlx5_4:1,mlx5_5:1",
        "NCCL_IB_DISABLE": "0",
        "NCCL_SOCKET_IFNAME": "bond0",
        "NCCL_DEBUG": "INFO",
        "WANDB_MODE": "offline",
        "PYTHONPATH": "${ROOT_DIR}${PYTHONPATH:+:${PYTHONPATH}}",
        "LOGLEVEL": "${LOGLEVEL:-WARNING}",
        "MODEL_NAME_OR_PATH": "${MODEL_NAME_OR_PATH}",
        "OUTPUT_DIR": "${OUTPUT_DIR}",
        "ZERO_STAGE": "${ZERO_STAGE}",
        "WANDB_API_KEY": "${WANDB_API_KEY}",
        "MASTER_PORT": "${MASTER_PORT}"
    },
    "resources": {
        "gres": "gpu:8",
        "cpus_per_task": "1",
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": "8",
        "gpus_per_node": "8",
        "cores_per_socket": null,
        "gpus_per_task": "1",
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "40:00:00",
        "ntasks_per_node": "1",
        "nodes": "2",
        "memory": "500GB",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "anaconda": "2023.03",
        "cuda": "11.8"
    }
}