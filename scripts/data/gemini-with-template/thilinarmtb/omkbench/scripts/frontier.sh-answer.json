    "resources": {
        "gres": "nvme",
        "cpus_per_task": "1",
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": "1",
        "gpus_per_node": "1",
        "cores_per_socket": null,
        "gpus_per_task": "1",
        "exclusive": true,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "$time",
        "ntasks_per_node": "1",
        "nodes": "$nodes",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
