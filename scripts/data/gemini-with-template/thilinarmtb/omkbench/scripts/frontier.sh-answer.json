{
    "application": "sbatch",
    "software": [
        "driver"
    ],
    "modules": [
        "PrgEnv-gnu",
        "craype-accel-amd-gfx90a",
        "cray-mpich",
        "rocm"
    ],
    "environment_variables": {
        "PROJ_ID": "CSC262",
        "OMK_INSTALL_DIR": "${OMK_INSTALL_DIR}",
        "QUEUE": "batch",
        "OCCA_VERBOSE": "1",
