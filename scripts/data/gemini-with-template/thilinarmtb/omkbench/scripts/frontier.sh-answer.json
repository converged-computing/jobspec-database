        "PE_MPICH_GTL_DIR_amd_gfx90a": "-L${CRAY_MPICH_ROOTDIR}/gtl/lib",
        "PE_MPICH_GTL_LIBS_amd_gfx90a": "-lmpi_gtl_hsa"
