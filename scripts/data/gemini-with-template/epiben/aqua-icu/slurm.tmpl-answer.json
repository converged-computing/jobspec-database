{
    "application": "slurm",
    "software": [
        "R",
        "Rscript"
    ],
    "modules": [
        "gcc/11.2.0",
        "R/4.3.1"
    ],
    "environment_variables": {
        "DEBUGME": "Sys.getenv(\"DEBUGME\")",
        "OMP_NUM_THREADS": "resources$omp.threads",
        "OPENBLAS_NUM_THREADS": "resources$blas.threads",
        "MKL_NUM_THREADS": "resources$blas.threads"
    },
    "resources": {
        "time": "ceiling(max(1, resources$walltime))",
        "memory": "max(1024, resources$memory)",
        "cpus_per_task": "max(1, resources$ncpus)",
        "tasks": "1",
        "partition": "resources$partition",
        "gres": "",
        "gres_flags": ""
    },
    "versions": {
        "gcc": "11.2.0",
        "R": "4.3.1"
    }
}