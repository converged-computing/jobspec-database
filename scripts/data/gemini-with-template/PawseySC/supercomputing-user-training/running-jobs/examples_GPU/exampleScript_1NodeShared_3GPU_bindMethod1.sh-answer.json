        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "00:05:00",
        "ntasks_per_node": "3",
        "nodes": "1",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": "gpu-bind=closest"
    },
    "versions": {}
