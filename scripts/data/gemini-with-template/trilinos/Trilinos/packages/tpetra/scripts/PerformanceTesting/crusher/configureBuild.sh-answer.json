{
    "application": "Trilinos",
    "software": [
        "cmake",
        "ninja",
        "git",
        "numactl",
        "hipcc",
        "crayclang",
        "craymath",
        "libmpi_gtl_hsa",
        "craympich",
        "boost",
        "hdf5",
        "metis",
        "parmetis",
        "superlu",
        "zlib"
    ],
    "modules": [
        "cray-netcdf",
        "cray-mpich",
        "cray-binutils",
        "cray-libci"
    ],
    "environment_variables": {
        "all_proxy": "socks://proxy.ccs.ornl.gov:3128/",
        "ftp_proxy": "ftp://proxy.ccs.ornl.gov:3128/",
        "http_proxy": "http://proxy.ccs.ornl.gov:3128/",
        "https_proxy": "http://proxy.ccs.ornl.gov:3128/",
        "no_proxy": "'localhost,127.0.0.0/8,*.ccs.ornl.gov,*.ncrc.gov',*.olcf.ornl.gov",
        "TRILINOS_SRC": "$HOME/crusher/sources/trilinos/Trilinos",
        "TRILINOS_BUILD": "$HOME/crusher/builds/performance-new",
        "BUILD_TYPE": "RELEASE",
        "HIPFLAGS": "--amdgpu-target=gfx90a",
        "PE_MPICH_GTL_DIR_amd_gfx90a": "-L${CRAY_MPICH_ROOTDIR}/gtl/lib",
        "PE_MPICH_GTL_LIBS_amd_gfx90a": "-lmpi_gtl_hsa",
        "THE_REAL_NETCDF_C_ROOT": "${CRAY_NETCDF_DIR}/crayclang/10.0",
        "CMAKE_OPTIONS": "--debug-trycompile",
        "NUMTHREADS": "31"
    },
    "resources": {
        "gres": "",
