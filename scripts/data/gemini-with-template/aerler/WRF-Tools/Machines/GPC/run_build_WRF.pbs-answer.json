        "WPSINTAR": "${WPSOUTTAR}",
        "WRFROOT": "${HOME}/WRFV3.6",
        "WPSSRC": "${WRFROOT}/WPS",
        "WRFSRC": "${WRFROOT}/WRFV3",
        "TASKS": "16",
        "RAMDISK": "/dev/shm/${USER}",
        "WRFDIR": "${RAMDISK}/WRFV3",
        "WPSDIR": "${RAMDISK}/WPS",
        "J": "-j ${TASKS}"
