        "netcdf/4.1.3_hdf5_serial-intel",
        "gcc/4.8.1",
        "python/2.7.5",
        "gdal/1.9.2",
        "ncl/6.2.0",
        "gsl/1.13-intel",
        "udunits/2.1.11",
        "nco/4.3.2-intel",
        "extras/64_6.4"
