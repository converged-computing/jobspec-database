{
    "application": "gaussian",
    "software": [
        "gaussian"
    ],
    "modules": [
        "gaussian",
        "nbo6"
    ],
    "environment_variables": {
        "GAUSS_SCRDIR": "$g16_subscratch",
        "g16root": "$g16_installpath",
        "PATH": "$nbo6_installpath/bin:$PATH",
        "MODULEPATH": "$MODULEPATH"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": "$requested_numCPU",
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "$requested_walltime",
        "ntasks_per_node": null,
        "nodes": "1",
        "memory": "$overhead_memory",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "gaussian": "$version",
        "nbo6": null
    }
}