{
    "application": "ensemble.sh",
    "software": [
        "meld_classifier",
        "miniconda3",
        "python"
    ],
    "modules": [
        "rhel7/default-gpu",
        "miniconda/3"
    ],
    "environment_variables": {
        "CONDA_PATH": "$HOME/.conda/envs/meld",
        "MELD_PATH": "$HOME/software/meld_classifier",
        "OMP_NUM_THREADS": "1"
    },
    "resources": {
        "gres": "gpu:1",
        "cpus_per_task": "1",
        "tasks": "1",
        "ntasks_per_code": "1",
        "gpus": "1",
        "gpus_per_node": "1",
        "cores_per_socket": null,
        "gpus_per_task": "1",
        "exclusive": null,
        "cpus_per_gpu": "3",
        "gpu_type": "P100",
        "time": "04:00:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "miniconda3": "3"
    }
}