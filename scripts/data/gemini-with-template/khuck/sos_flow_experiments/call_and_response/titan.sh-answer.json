{
    "application": "sos_flow",
    "software": [
        "sosd",
        "showdb",
        "main",
        "spawn_aggregators.sh",
        "cleanup.sh"
    ],
    "modules": [
        "PrgEnv-gnu",
        "cudatoolkit",
        "papi",
        "cmake",
        "flexpath/1.12",
        "adios/1.12.0",
        "python/2.7.9"
    ],
    "environment_variables": {
        "SOS_WORK": "/dev/shm",
        "sos_cmd_part": "${sosbin}/sosd -l ${num_listeners} -a ${aggregators} ",
        "sos_cmd": "${sos_cmd_part} -w ${SOS_WORK}",
        "SOS_FORK_COMMAND": "${sos_cmd} -k @LISTENER_RANK@ -r listener",
        "SOS_LISTENER_RANK_OFFSET": "2",
        "SOS_APP_RANKS_PER_NODE": "${app_ranks_per_node}",
        "SOS_FORK_SHUTDOWN": "${sosbin}/sosd_stop",
        "SOS_CMD_PORT": "22500",
        "SOS_EVPATH_MEETUP": "${cwd}"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": null,
        "tasks": "512",
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "0:10:00",
        "ntasks_per_node": "2",
        "nodes": "257",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}