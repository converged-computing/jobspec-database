{
    "application": "openfoam",
    "software": [
        "renumberMesh",
        "foamListTimes",
        "foamDictionary"
    ],
    "modules": [],
    "environment_variables": {
        "cores": "$SLURM_NTASKS",
        "startTime": "0",
        "writeIntervalBeforeAvg": "5000",
        "endTimeBeforeAvg": "40000",
        "startTimeAvg": "$endTimeBeforeAvg",
        "writeIntervalAvg": "10000",
        "endTimeAvg": "60000",
        "solver": "superDeliciousVanilla",
        "interp": "linearupwind",
        "lastBdData": "$(ls $(ls -d constant/boundaryData/* | head -1) | tail -2 | head -1)",
        "latestTime": "$(foamListTimes -processor -latestTime -withZero -noFunctionObjects | tail -1)",
        "continueTime": "(( $latestTime > $endTimeBeforeAvg ? $latestTime : $startTimeAvg ))"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "1",
        "tasks": "128",
        "ntasks_per_code": "128",
        "gpus": "0",
        "gpus_per_node": "0",
        "cores_per_socket": "",
        "gpus_per_task": "0",
        "exclusive": "",
        "cpus_per_gpu": "0",
        "gpu_type": "",
        "time": "10-00",
        "ntasks_per_node": "",
        "nodes": "",
        "memory": "",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {
        "openfoam": "OpenFOAM-6-gcc-dev"
    }
}