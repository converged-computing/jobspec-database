{
    "application": "strumpack",
    "software": [
        "spack"
    ],
    "modules": [
        "slurm"
    ],
    "environment_variables": {
        "LOCAL_TIME": "date +'%Y%m%dT%H%M%S%z'",
        "UNIX_TIME": "date +'%s'",
        "SYSTEM_NAME": "'expanse'",
        "SPACK_VERSION": "'0.17.2'",
        "SPACK_INSTANCE_NAME": "'cpu'",
        "SPACK_INSTANCE_DIR": "\"/cm/shared/apps/spack/${SPACK_VERSION}/${SPACK_INSTANCE_NAME}\"",
        "SLURM_JOB_SCRIPT": "scontrol show job ${SLURM_JOB_ID} | awk -F= '/Command=/{print $2}'",
        "SLURM_JOB_MD5SUM": "md5sum ${SLURM_JOB_SCRIPT}",
        "SCHEDULER_MODULE": "'slurm'",
        "SPACK_PACKAGE": "'strumpack@6.1.0'",
        "SPACK_COMPILER": "'gcc@10.2.0'",
        "SPACK_VARIANTS": "'+butterflypack +c_interface ~count_flops ~cuda ~ipo +mpi ~openmp +parmetis ~rocm ~scotch +shared +slate ~task_timers +zfp'",
        "SPACK_DEPENDENCIES": "\"^butterflypack@2.0.0/$(spack find --format '{hash:7}' butterflypack@2.0.0 % ${SPACK_COMPILER} ^openmpi@4.1.3) ^slate@2021.05.02/$(spack find --format '{hash:7}' slate@2021.05.02 % ${SPACK_COMPILER} ^openmpi@4.1.3)\"",
        "SPACK_SPEC": "${SPACK_PACKAGE} % ${SPACK_COMPILER} ${SPACK_VARIANTS} $butterflypackNCIES"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": "16",
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "01:00:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": "32G",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "spack": "0.17.2",
        "slurm": "20.02.7",
        "gcc": "10.2.0",
        "strumpack": "6.1.0",
        "butterflypack": "2.0.0",
        "slate": "2021.05.02",
        "openmpi": "4.1.3",
        "arpack-ng": "3.7.0",
        "openblas": "0.3.18",
        "hwloc": "2.6.0",
        "libpciaccess": "0.16",
        "libxml2": "2.9.12",
        "libiconv": "1.16",
        "xz": "5.2.5",
        "zlib": "1.2.11",
        "ncurses": "6.2",
        "libevent": "2.1.8",
        "openssl": "1.1.1k",
        "lustre": "2.12.8",
        "numactl": "2.0.14",
        "openssh": "8.7p1",
        "libedit": "3.1-20210216",
        "pmix": "3.2.1",
        "ucx": "1.10.1",
        "rdma-core": "28.0",
        "blaspp": "2021.04.01",
        "lapackpp": "2021.04.00",
        "netlib-scalapack": "2.1.0"
    }
}