{
    "application": "spack",
    "software": [
        "py-matplotlib",
        "py-numpy",
        "openblas",
        "imagemagick",
        "ffmpeg"
    ],
    "modules": [
        "slurm"
    ],
    "environment_variables": {
        "LOCAL_TIME": "date +'%Y%m%dT%H%M%S%z'",
        "UNIX_TIME": "date +'%s'",
        "SYSTEM_NAME": "'expanse'",
        "SPACK_VERSION": "'0.17.3'",
        "SPACK_INSTANCE_NAME": "'cpu'",
        "SPACK_INSTANCE_DIR": "$(HOME)/cm/shared/apps/spack/${SPACK_VERSION}/${SPACK_INSTANCE_NAME}",
        "SLURM_JOB_SCRIPT": "$(scontrol show job ${SLURM_JOB_ID} | awk -F= '/Command=/{print $2}')",
        "SLURM_JOB_MD5SUM": "$(md5sum ${SLURM_JOB_SCRIPT})",
        "SCHEDULER_MODULE": "'slurm'",
        "SPACK_PACKAGE": "'py-matplotlib@3.4.3'",
        "SPACK_COMPILER": "'gcc@10.2.0'",
        "SPACK_VARIANTS": "'+animation +fonts +image ~latex +movies'",
        "SPACK_DEPENDENCIES": "^py-numpy@1.20.3/$(spack find --format '{hash:7}' py-numpy@1.20.3 % ${SPACK_COMPILER}) ^openblas@0.3.17/$(spack find --format '{hash:7}' openblas@0.3.17 % ${SPACK_COMPILER} ~ilp64 threads=none) ^imagemagick@7.0.8-7/$(spack find --format '{hash:7}' imagemagick@7.0.8-7 % ${SPACK_COMPILER}) ^ffmpeg@4.3.2/$(spack find --format '{hash:7}' ffmpeg@4.3.2 % ${SPACK_COMPILER})",
        "SPACK_SPEC": "${SPACK_PACKAGE} % ${SPACK_COMPILER} ${SPACK_VARIANTS} ${SPACK_DEPENDENCIES}"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": "16",
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "04:00:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": "32G",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "py-matplotlib": "3.4.3",
        "py-numpy": "1.20.3",
        "openblas": "0.3.17",
        "imagemagick": "7.0.8-7",
        "ffmpeg": "4.3.2",
        "gcc": "10.2.0",
        "spack": "0.17.3"
    }
}