{
    "application": "spack",
    "software": [
        "spack@0.17.3",
        "boost@1.77.0",
        "gcc@10.2.0",
        "py-numpy@1.20.3",
        "openblas@0.3.18",
        "py-pycuda@2020.1"
    ],
    "modules": [
        "slurm"
    ],
    "environment_variables": {
        "LOCAL_TIME": "date +'%Y%m%dT%H%M%S%z'",
        "UNIX_TIME": "date +'%s'",
        "SYSTEM_NAME": "expanse",
        "SPACK_VERSION": "0.17.3",
        "SPACK_INSTANCE_NAME": "gpu",
        "SPACK_INSTANCE_DIR": "${HOME}/cm/shared/apps/spack/${SPACK_VERSION}/${SPACK_INSTANCE_NAME}",
        "SLURM_JOB_SCRIPT": "scontrol show job ${SLURM_JOB_ID} | awk -F= '/Command=/{print $2}'",
        "SLURM_JOB_MD5SUM": "md5sum ${SLURM_JOB_SCRIPT}",
        "SCHEDULER_MODULE": "slurm",
        "SPACK_PACKAGE": "boost@1.77.0",
        "SPACK_COMPILER": "gcc@10.2.0",
        "SPACK_VARIANTS": "+atomic +chrono ~clanglibcpp ~container ~context ~coroutine +date_time ~debug +exception ~fiber +filesystem +graph ~icu +iostreams +locale +log +math ~mpi +multithreaded +numpy +pic +program_options +python +random +regex +serialization +shared +signals ~singlethreaded +system ~taggedlayout +test +thread +timer ~versionedlayout +wave",
        "SPACK_DEPENDENCIES": "^py-numpy@1.20.3/$(spack find --format '{hash:7}' py-numpy@1.20.3 % ${SPACK_COMPILER}) ^openblas@0.3.18/$(spack find --format '{hash:7}' openblas@0.3.18 % ${SPACK_COMPILER} ~ilp64 threads=none)",
        "SPACK_SPEC": "${SPACK_PACKAGE} % ${SPACK_COMPILER} ${SPACK_VARIANTS} ${SPACK_DEPENDENCIES}"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": "10",
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": "1",
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "00:30:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": "93G",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "spack": "0.17.3",
        "boost": "1.77.0",
        "gcc": "10.2.0",
        "py-numpy": "1.20.3",
        "openblas": "0.3.18",
        "py-pycuda": "2020.1"
    }
}