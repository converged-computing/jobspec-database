{
    "application": "spack",
    "software": [
        "hypre@2.23.0",
        "gcc@10.2.0",
        "openblas@0.3.17",
        "superlu-dist@7.1.1",
        "openmpi@4.1.3",
        "valgrind@3.17.0"
    ],
    "modules": [
        "slurm"
    ],
    "environment_variables": {
        "LOCAL_TIME": null,
        "UNIX_TIME": null,
        "SYSTEM_NAME": "expanse",
        "SPACK_VERSION": "0.17.3",
        "SPACK_INSTANCE_NAME": "gpu",
        "SPACK_INSTANCE_DIR": "${HOME}/cm/shared/apps/spack/${SPACK_VERSION}/${SPACK_INSTANCE_NAME}",
        "SLURM_JOB_SCRIPT": "$(scontrol show job ${SLURM_JOB_ID} | awk -F= '/Command=/{print $2})",
        "SLURM_JOB_MD5SUM": "$(md5sum ${SLURM_JOB_SCRIPT})",
        "SCHEDULER_MODULE": "slurm",
        "SPACK_PACKAGE": "hypre@2.23.0",
        "SPACK_COMPILER": "gcc@10.2.0",
        "SPACK_VARIANTS": "~complex +cuda cuda_arch=70,80 ~debug +fortran ~int64 ~internal-superlu ~mixedint +mpi ~openmp +shared +superlu-dist ~unified-memory",
        "SPACK_DEPENDENCIES": "^openblas@0.3.17/$(spack find --format '{hash:7}' openblas@0.3.17 % ${SPACK_COMPILER} ~ilp64 threads=none) ^superlu-dist@7.1.1/$(spack find --format '{hash:7}' superlu-dist@7.1.1 % ${SPACK_COMPILER} ^openmpi@4.1.3)",
        "SPACK_SPEC": "${SPACK_PACKAGE} % ${SPACK_COMPILER} ${SPACK_VARIANTS} ${SPACK_DEPENDENCIES}"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": "10",
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": "1",
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "00:30:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": "93G",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "hypre": "2.23.0",
        "gcc": "10.2.0",
        "openblas": "0.3.17",
        "superlu-dist": "7.1.1",
        "openmpi": "4.1.3",
        "valgrind": "3.17.0"
    }
}