{
    "application": "spack",
    "software": [
        "gcc@10.2.0",
        "gcc@8.5.0"
    ],
    "modules": [
        "slurm",
        "cuda/10.2.89"
    ],
    "environment_variables": {
        "LOCAL_TIME": "date +'%Y%m%dT%H%M%S%z'",
        "UNIX_TIME": "date +'%s'",
        "SYSTEM_NAME": "'expanse'",
        "SPACK_VERSION": "'0.17.3'",
        "SPACK_INSTANCE_NAME": "'gpu'",
        "SPACK_INSTANCE_DIR": "${HOME}/cm/shared/apps/spack/${SPACK_VERSION}/${SPACK_INSTANCE_NAME}",
        "SLURM_JOB_SCRIPT": "scontrol show job ${SLURM_JOB_ID} | awk -F= '/Command=/{print $2}'",
        "SLURM_JOB_MD5SUM": "md5sum ${SLURM_JOB_SCRIPT}",
        "SCHEDULER_MODULE": "'slurm'",
        "SPACK_PACKAGE": "'gcc@10.2.0'",
        "SPACK_COMPILER": "'gcc@8.5.0'",
        "SPACK_VARIANTS": "'~binutils ~bootstrap ~graphite ~nvptx ~piclibs ~strip'",
        "SPACK_DEPENDENCIES": "'^cuda@10.2.89/$(spack find --format '{hash:7}' cuda@10.2.89 % gcc@8.5.0)'",
        "SPACK_SPEC": "${SPACK_PACKAGE} % ${SPACK_COMPILER} ${SPACK_VARIANTS} ${SPACK_DEPENDENCIES}"
    },
    "resources": {
        "gres": "gpu:1",
        "cpus_per_task": "16",
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": "1",
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "01:00:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": "93G",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "spack": "0.17.3",
        "gcc": "10.2.0"
    }
}