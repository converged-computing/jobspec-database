{
    "application": "gdb",
    "software": [
        "spack",
        "ninja"
    ],
    "modules": [
        "slurm",
        "lmod"
    ],
    "environment_variables": {
        "LOCAL_TIME": null,
        "UNIX_TIME": null,
        "SYSTEM_NAME": "expanse",
        "SPACK_VERSION": "0.17.3",
        "SPACK_INSTANCE_NAME": "gpu",
        "SPACK_INSTANCE_DIR": "${HOME}/cm/shared/apps/spack/${SPACK_VERSION}/${SPACK_INSTANCE_NAME}",
        "SLURM_JOB_SCRIPT": "$(scontrol show job ${SLURM_JOB_ID} | awk -F= '/Command=/{print $2})",
        "SLURM_JOB_MD5SUM": "$(md5sum ${SLURM_JOB_SCRIPT})",
        "SCHEDULER_MODULE": "slurm",
        "SPACK_PACKAGE": "gdb@11.1",
        "SPACK_COMPILER": "gcc@10.2.0",
        "SPACK_VARIANTS": "~gold ~ld ~lto ~python ~quad ~source-highlight ~tui +xz",
        "SPACK_DEPENDENCIES": "",
        "SPACK_SPEC": "${SPACK_PACKAGE} % ${SPACK_COMPILER} ${SPACK_VARIANTS} ${SPACK_DEPENDENCIES}"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": "10",
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": "1",
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "00:30:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": "93G",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "gdb": "11.1",
        "gcc": "10.2.0",
        "ninja": "1.10.2",
        "spack": "0.17.3"
    }
}