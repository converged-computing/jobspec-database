{
    "application": "spack",
    "software": [
        "intel-mkl@2020.4.304",
        "gcc@10.2.0",
        "magma@2.6.1"
    ],
    "modules": [
        "slurm"
    ],
    "environment_variables": {
        "LOCAL_TIME": "$(date +'%Y%m%dT%H%M%S%z')",
        "UNIX_TIME": "$(date +'%s')",
        "SYSTEM_NAME": "expanse",
        "SPACK_VERSION": "0.17.3",
