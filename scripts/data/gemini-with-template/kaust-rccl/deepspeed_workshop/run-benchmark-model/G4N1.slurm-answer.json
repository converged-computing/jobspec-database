{
    "application": "slurm",
    "software": [
        "nvidia-smi",
        "ibv_devinfo",
        "run-big-model.sh"
    ],
    "modules": [
        "dl",
        "deepspeed/0.8.3"
    ],
    "environment_variables": {
        "NCCL_DEBUG": "INFO",
        "NCCL_TREE_THRESHOLD": "0",
        "NCCL_SOCKET_IFNAME": "ib0",
        "NCCL_NET_GDR_LEVEL": "4",
        "NCCL_TOPO_DUMP_FILE": "./nccl_dump.log.${SLURM_JOB_NAME}",
        "MAX_JOBS": "${SLURM_CPUS_PER_TASK}",
        "master_ip": "$(srun -n 1 -N 1 --gpus=1 -w ${nodes_array[0]} /bin/hostname -I | cut -d \" \" -f 2)"
    },
    "resources": {
        "gres": "gpu:a100",
        "cpus_per_task": "4",
        "tasks": "4",
        "ntasks_per_code": "4",
        "gpus": "4",
        "gpus_per_node": "4",
        "cores_per_socket": null,
        "gpus_per_task": "1",
        "exclusive": null,
        "cpus_per_gpu": "1",
        "gpu_type": "a100",
        "time": "00:15:00",
        "ntasks_per_node": "4",
        "nodes": "1",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}