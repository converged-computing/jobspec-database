{
    "application": "xhpl",
    "software": [
        "openmpi",
        "intel"
    ],
    "modules": [
        "intel",
        "openmpi"
    ],
    "environment_variables": {
        "PATH": "~tin/gsHPCS_toolkit/benchmark/hpl/hpl-2.2/bin/intel64_nehalem:$PATH",
        "HPL_HOST_ARCH": "9",
        "JobBaseDir": "/global/scratch/tin/benchmark_hpl/",
        "CorePerNode": "24",
        "NNode": "${SLURM_JOB_NUM_NODES}",
        "Ncore": "(( $NNode * $CorePerNode ))",
        "HplDatNum": "(( printf %04d $NNode ))",
        "FECHA": "$(date \"+%m%d_%H%M\")",
        "RUNDIR": "./STAGING_OUT/J${SLURM_JOB_ID}_${SLURM_JOB_PARTITION}_${NNode}x${SLURM_CPUS_ON_NODE}_${FECHA}",
        "BIOSOUT": "/tmp/bios.settings.out",
        "BIOSHIGHLIGHT": "/tmp/bios.settings.highlight"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "1",
        "tasks": "64",
        "ntasks_per_code": "",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "true",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "11:55:00",
        "ntasks_per_node": "${SLURM_TASKS_PER_NODE}",
        "nodes": "${SLURM_JOB_NUM_NODES}",
        "memory": "",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {
        "openmpi": "2.0.2",
        "intel": "2018.1.163"
    }
}