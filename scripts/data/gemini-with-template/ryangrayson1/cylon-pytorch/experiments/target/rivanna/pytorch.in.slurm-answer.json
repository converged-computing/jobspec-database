{
    "application": "slurm",
    "software": [
        "singularity",
        "python",
        "mpirun"
    ],
    "modules": [
        "gcc/11.2.0",
        "openmpi/4.1.4",
        "python/3.11.1"
    ],
    "environment_variables": {
        "USER_PROJECT": "/project/bii_dsc_community/$USER",
        "PYTORCH_SIF": "{data.pytorch_sif}"
    },
    "resources": {
        "gres": "gpu:{experiment.card_name}:{experiment.ngpus}",
        "cpus_per_task": "{experiment.ncpus}",
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": "{experiment.ngpus}",
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": "{experiment.card_name}",
        "time": "{ee.time}",
        "ntasks_per_node": null,
        "nodes": "{ee.nodes}",
        "memory": "{ee.mem}",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "gcc": "11.2.0",
        "openmpi": "4.1.4",
        "python": "3.11.1"
    }
}