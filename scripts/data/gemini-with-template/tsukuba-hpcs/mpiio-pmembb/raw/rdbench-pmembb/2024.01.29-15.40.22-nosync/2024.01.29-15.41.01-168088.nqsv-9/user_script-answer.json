{
    "application": "rdbench",
    "software": [
        "rdbench",
        "openmpi",
        "rpmbb",
        "lfs",
        "dropcaches",
        "tree",
        "time_json"
    ],
    "modules": [
        "use.own",
        "openmpi/5.0.0rc12-pmembb-eval-gcc-11.4.0-r5rhxc6"
    ],
    "environment_variables": {
        "NQSV_MPI_VER": "5.0.0rc12-pmembb-eval-gcc-11.4.0-r5rhxc6",
        "USE_DEVDAX": "pmemkv",
        "NUM_DEVDAX": "1",
        "SPACK_ENV_NAME": "",
        "SCRIPT_DIR": "",
        "OUTPUT_DIR": "",
        "BACKEND_DIR": "",
        "RDBENCH_STEPS": "640",
        "RDBENCH_INTERVAL": "64",
        "NQSII_MPIOPTS": "",
        "LD_PRELOAD": "",
        "PMEM_PATH": "/dev/dax0.0",
        "PMEM_SIZE": "0",
        "ROMIO_HINTS": "",
        "ppn": "48",
        "rdbench_length_per_node": "48*2**10",
        "rdbench_length": "rdbench_length_per_node * $(sqrt \"$NNODES\")",
        "max_stripe_count": "2000",
        "stripe_size": "2*2**20",
        "stripe_count": "1",
        "runid": "0",
        "workflow_id": "0",
        "test_dir": "$JOB_BACKEND_DIR"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": null,
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "0:30:00",
        "ntasks_per_node": null,
        "nodes": null,
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "lustre_version": "$(lfs --version | awk '{print $2}')"
    }
}