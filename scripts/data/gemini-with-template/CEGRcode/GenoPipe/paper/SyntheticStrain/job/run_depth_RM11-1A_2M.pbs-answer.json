{
    "application": "pbs",
    "software": [
        "bedtools",
        "bwa",
        "samtools",
        "anaconda3",
        "genopipe"
    ],
    "modules": [
        "gcc/8.3.1",
        "bedtools/2.27.1",
        "bwa/0.7.15",
        "samtools/1.5",
        "anaconda3"
    ],
    "environment_variables": {
        "WRK": "/path/to/GenoPipe/paper/SyntheticStrain",
        "INFO": "sed \"12q;d\" depth_simulations.txt",
        "STRAIN": "awk '{print $1}'  <(echo $INFO)",
        "DEPTH": "awk '{print $2}'  <(echo $INFO)",
        "BASE": "awk '{print $3}'  <(echo $INFO)",
        "REF": "awk -F_ '{print $1}' <(echo $STRAIN)",
        "GENOME": "synthetic_genome/$STRAIN.fa",
        "OUTPUT": "results/$STRAIN_$DEPTH",
        "SEED": "(($BASE+$PBS_ARRAYID))"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": "4",
        "tasks": "1000",
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "00:05:00",
        "ntasks_per_node": null,
        "nodes": "1",
        "memory": "16gb",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}