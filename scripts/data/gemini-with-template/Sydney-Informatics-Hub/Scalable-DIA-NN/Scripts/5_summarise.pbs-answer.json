{
    "application": "diann",
    "software": [
        "singularity",
        "wine",
        "diann"
    ],
    "modules": [
        "singularity"
    ],
    "environment_variables": {
        "WINEPREFIX": "${PBS_JOBFS}/dot_wine",
        "WINEDEBUG": "-all"
    },
    "resources": {
        "cpus_per_task": "<NCPUS>",
        "memory": "<MEM>GB",
        "time": "02:00:00",
        "jobfs": "10GB",
        "storage": "<lstorage>"
    },
    "versions": {
        "diann": "1.8.1"
    }
}