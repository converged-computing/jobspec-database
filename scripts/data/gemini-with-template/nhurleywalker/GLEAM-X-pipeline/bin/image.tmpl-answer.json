{
    "application": "wsclean",
    "software": [
        "wsclean",
        "track_task.py",
        "pyhead.py",
        "chgcentre",
        "python",
        "calc_pointing.py",
        "lookup_jones.py",
        "wsclean2gleam.py",
        "singularity",
        "bash",
        "grep",
        "wc",
        "ls",
        "head",
        "sed",
        "awk",
        "bc",
        "date"
    ],
    "modules": [
        "singularity"
    ],
    "environment_variables": {
        "pipeuser": "PIPEUSER",
        "version": "3.0",
        "cores": "NCPUS",
        "obsnum": "OBSNUM",
        "base": "BASEDIR",
        "debug": "DEBUG",
        "datacolumn": "DATA",
        "subchans": "MFS 0000 0001 0002 0003",
        "minuv": "75",
        "msigma": "3",
        "tsigma": "1",
        "datadir": "${base}/${obsnum}",
        "metafits": "ls -t ${obsnum}*metafits* | head -1",
        "telescope": "MWA128T",
        "basescale": "1.1",
        "imsize": "4000",
        "robust": "-1.0",
        "chan": "pyhead.py -p CENTCHAN ${metafits} | awk '{print $3}'",
        "bandwidth": "pyhead.py -p BANDWDTH ${metafits} | awk '{print $3}'",
        "centfreq": "pyhead.py -p FREQCENT ${metafits} | awk '{print $3}'",
        "chans": "pyhead.py -p CHANNELS ${metafits} | awk '{print $3}' | sed \"s/,/ /g\"",
        "scale": "echo \"$basescale / $chan\" | bc -l",
        "lowfreq": "echo \"$centfreq $bandwidth\" | awk '{printf(\"%00d\\n\",$1-($2/2.)+0.5)}'",
        "highfreq": "echo \"$centfreq $bandwidth\" | awk '{printf(\"%00d\\n\",$1+($2/2.)+0.5)}'",
        "freqrange": "${lowfreq}-${highfreq}",
        "minuvm": "echo \"234 * $minuv / $chan\" | bc -l",
        "multiscale": "-multiscale -mgain 0.85 -multiscale-gain 0.15",
        "current": "chgcentre ${obsnum}.ms",
        "coords": "python /group/mwasci/$pipeuser/GLEAM-X-pipeline/bin/calc_pointing.py --metafits=${metafits}",
        "pols": "XX XXi XY XYi YX YXi YY YYi"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": null,
        "tasks": "NCPUS",
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "12:00:00",
        "ntasks_per_node": null,
        "nodes": "1",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "wsclean": "2.9.2"
    }
}