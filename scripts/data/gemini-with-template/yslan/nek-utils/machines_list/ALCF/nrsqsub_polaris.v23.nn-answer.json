{
    "application": "nekrs",
    "software": [
        "nekrs",
        "mpich",
        "nvidia-smi",
        "libfabric",
        "cmake"
    ],
    "modules": [
        "libfabric",
        "PrgEnv-gnu",
        "nvhpc-mixed",
        "craype-x86-milan",
        "craype-accel-nvidia80",
        "spack-pe-base",
        "cmake"
    ],
    "environment_variables": {
        "PROJ_ID": "",
        "QUEUE": "prod",
        "NEKRS_HOME": "$HOME/.local/nekrs",
        "NEKRS_CACHE_BCAST": 1,
        "NEKRS_BUILD_ONLY": 0,
        "NEKRS_SKIP_BUILD_ONLY": 0,
        "NEKRS_DFLOAT_FP32": 0,
        "NEKRS_CIMODE": "",
        "bin": "${NEKRS_HOME}/bin/nekrs",
        "case": "case name",
        "nodes": "nodes",
        "gpu_per_node": 4,
        "cores_per_numa": 8,
        "nn": "nodes * gpu_per_node",
        "ntasks": "nn",
        "time": "hh:mm:ss",
        "backend": "CUDA",
        "NEKRS_GPU_MPI": 1,
        "cimode": "cimode",
        "time_fmt": "3",
        "isneknek": 0,
        "casename_nn": "case name",
        "qnodes": "nodes",
        "jobname": "nekRS_case name",
        "striping_unit": 16777216,
        "max_striping_factor": 128,
        "striping_factor": "nodes / 2",
        "MPICH_MPIIO_HINTS": "*:striping_unit=16777216:striping_factor=striping_factor:romio_cb_write=enable:romio_ds_write=disable:romio_no_indep_rw=true",
        "SFILE": "s.bin",
        "CMD": ".lhelper",
        "gpu_id": "(gpu_per_node - 1 - PMI_LOCAL_RANK % gpu_per_node)",
        "nsess_nn": "count of lines in case file",
        "cname_nn": "case name",
        "nrank_nn": "number of ranks",
        "fldr_nn": "case folder name",
        "pp": "case name",
        "ntasks_nn": "number of ranks in case file",
        "LD_PRELOAD": "/opt/cray/pe/gcc/12.2.0/snos/lib64/libstdc++.so.6",
        "APP": "$*",
        "FI_OFI_RXM_RX_SIZE": 32768,
        "NEKRS_LOCAL_TMP_DIR": "/local/scratch",
        "MPICH_GPU_SUPPORT_ENABLED": 1,
        "MPICH_OFI_NIC_POLICY": "NUMA"
    },
    "resources": {
        "gres": "gpu:4",
        "cpus_per_task": "8",
        "tasks": "nodes * gpu_per_node",
        "ntasks_per_code": null,
        "gpus": "nodes * 4",
        "gpus_per_node": "4",
        "cores_per_socket": null,
        "gpus_per_task": "1",
        "exclusive": null,
        "cpus_per_gpu": "8",
        "gpu_type": "nvidia80",
        "time": "hh:mm:ss",
        "ntasks_per_node": "4",
        "nodes": "nodes",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}