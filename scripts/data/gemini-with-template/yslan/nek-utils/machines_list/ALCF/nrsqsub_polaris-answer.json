{
    "application": "nekrs",
    "software": [
        "mpiexec",
        "nvidia-smi",
        "ldd",
        "cmake",
        "nvhpc-mixed",
        "PrgEnv-gnu",
        "cpe-cuda",
        "libfabric"
    ],
    "modules": [
        "libfabric",
        "cpe-cuda",
        "PrgEnv-gnu",
        "nvhpc-mixed",
        "cmake"
    ],
    "environment_variables": {
        "NEKRS_HOME": "$HOME/.local/nekrs",
        "NEKRS_CACHE_BCAST": 1,
        "NEKRS_BUILD_ONLY": 0,
        "NEKRS_SKIP_BUILD_ONLY": 0,
        "NEKRS_DFLOAT_FP32": 0,
        "backend": "CUDA",
        "NEKRS_GPU_MPI": 1,
        "MPICH_MPIIO_HINTS": "*:striping_unit=16777216:striping_factor=64:romio_cb_write=enable:romio_ds_write=disable:romio_no_indep_rw=true",
        "MPICH_MPIIO_STATS": 1,
        "NEKRS_LOCAL_TMP_DIR": "/local/scratch",
        "MPICH_GPU_SUPPORT_ENABLED": 1,
        "MPICH_OFI_NIC_POLICY": "NUMA",
        "FI_OFI_RXM_RX_SIZE": 32768,
        "FI_CXI_RX_MATCH_MODE": "hybrid",
        "LD_PRELOAD": "/opt/cray/pe/gcc/11.2.0/snos/lib64/libstdc++.so.6",
        "CUDA_VISIBLE_DEVICES": "${gpu_id}"
    },
    "resources": {
        "gres": "gpu:4",
        "cpus_per_task": 8,
        "tasks": "$nn",
        "ntasks_per_code": "$gpu_per_node",
        "gpus": "$nn",
        "gpus_per_node": 4,
        "cores_per_socket": 8,
        "gpus_per_task": 1,
        "exclusive": null,
        "cpus_per_gpu": 2,
        "gpu_type": null,
        "time": "$time",
        "ntasks_per_node": 4,
        "nodes": "$nodes",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}