{
    "application": "vncserver",
    "software": [
        "vncserver",
        "ssh",
        "vglclient",
        "xterm"
    ],
    "modules": [],
    "environment_variables": {
        "TAP_FUNCTIONS": "/share/doc/slurm/tap_functions",
        "CONDA": "/usr/bin/conda",
        "NODE_HOSTNAME": "your_node_hostname",
        "VNCSERVER_BIN": "/usr/bin/vncserver",
        "CONDA_ENV": "your_conda_environment",
        "VNC_ARGS": "-localhost",
        "VNC_DISPLAY": ":1",
        "LOCAL_VNC_PORT": "5901",
        "LOGIN_PORT": "your_login_port",
        "TUNNEL_CMD": "ssh -q -g -f -N -o StrictHostKeyChecking=no -L localhost:your_login_port:localhost:5901 your_node_hostname",
        "VGL_PID": "your_vglclient_pid",
        "TAP_LOCKFILE": "/home/your_username/.tap/your_job_id.lock",
        "DISPLAY": ":1"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "1",
        "tasks": "2",
        "ntasks_per_code": "2",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "02:00:00",
        "ntasks_per_node": "1",
        "nodes": "2",
        "memory": "",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
