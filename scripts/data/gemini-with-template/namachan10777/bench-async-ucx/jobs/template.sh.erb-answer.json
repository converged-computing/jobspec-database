{
    "application": "mpirun",
    "software": [
        "openmpi",
        "ucx",
        "rust"
    ],
    "modules": [
        "use.own",
        "openmpi/${NQSV_MPI_VER}",
        "ucx/1.13.1/cuda11.8.0"
    ],
    "environment_variables": {
        "RUST_LOG": "Info",
        "SERVER_THREAD_COUNT": "<%= server_thread_count %>",
        "CLIENT_THREAD_COUNT": "<%= client_thread_count %>",
        "CLIENT_TASK_COUNT": "<%= client_task_count %>",
        "RUST_BACKTRACE": "1"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": "48",
        "tasks": "2",
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "00:10:00",
        "ntasks_per_node": null,
        "nodes": "<%= nodes %>",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "openmpi": "${NQSV_MPI_VER}",
        "ucx": "1.13.1"
    }
}