{
    "application": "tensorflow",
    "software": [
        "python",
        "srun",
        "nccl",
        "openmpi",
        "cuda",
        "gcc",
        "anaconda",
        "hdf5",
        "pmi2",
        "horovod"
    ],
    "modules": [
        "cuda",
        "nccl",
        "gcc/7.3.0",
        "openmpi/3.1.0-ucx",
        "python3/3.6-anaconda-4.4"
    ],
    "environment_variables": {
        "OMP_NUM_THREADS": "40 / ${rankspernode}",
        "OMP_PLACES": "threads",
        "OMP_PROC_BIND": "spread",
        "HDF5_USE_FILE_LOCKING": "FALSE",
        "sruncmd": "srun -u --mpi=pmi2 -N ${SLURM_NNODES} -n $(( ${SLURM_NNODES} * ${rankspernode} )) -c $(( 80 / ${rankspernode} )) --cpu_bind=cores"
    },
    "resources": {
        "gres": "gpu:8",
        "cpus_per_task": "80 / ${rankspernode}",
        "tasks": "${SLURM_NNODES} * ${rankspernode}",
        "ntasks_per_code": "${SLURM_NNODES}",
        "gpus": "8",
        "gpus_per_node": "8",
        "cores_per_socket": "40",
        "gpus_per_task": "1",
        "exclusive": "true",
        "cpus_per_gpu": "10",
        "gpu_type": null,
        "time": "02:00:00",
        "ntasks_per_node": "${rankspernode}",
        "nodes": "${SLURM_NNODES}",
        "memory": null,
        "sockets_per_node": "1",
        "ntasks_per_socket": "8",
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": "enforce-binding"
    },
    "versions": {
        "cuda": null,
        "nccl": null,
        "gcc": "7.3.0",
        "openmpi": "3.1.0-ucx",
        "python3": "3.6",
        "anaconda": "4.4",
        "tensorflow": null
    }
}