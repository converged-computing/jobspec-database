{
    "application": "slurm",
    "software": [
        "python",
        "ffcv",
        "wandb",
        "anaconda",
        "conda",
        "bash"
    ],
    "modules": [
        "anaconda/3"
    ],
    "environment_variables": {
        "dataset": "cifar10",
        "batch_size": "128",
        "lambd_arr": "[0.0001 0.0002 0.0004 0.0008 0.001 0.002 0.004 0.006 0.01 0.02]",
        "pdim_arr": "[256 8192]",
        "augs_arr": "[2 4 8 16]",
        "len1": "10",
        "len2": "2",
        "len12": "20",
        "aidx": "SLURM_ARRAY_TASK_ID/20",
        "lpidx": "SLURM_ARRAY_TASK_ID%20",
        "pidx": "lpidx/2",
        "lidx": "lpidx%10",
        "lambd": "lambd_arr[lidx]",
        "pdim": "pdim_arr[pidx]",
        "augs": "augs_arr[aidx]",
        "wandb_group": "blake-richards",
        "wandb_projname": "multiPatch-Barlow-sweep-ep100",
        "model": "resnet50proj",
        "checkpt_dir": "$SCRATCH/fastssl/checkpoints_mp_v3",
        "train_dpath": "$SCRATCH/ffcv/ffcv_datasets/{dataset}/train.beton",
        "val_dpath": "$SCRATCH/ffcv/ffcv_datasets/{dataset}/test.beton"
    },
    "resources": {
        "gres": "gpu:rtx8000:1",
        "cpus_per_task": "4",
        "tasks": "80",
        "ntasks_per_code": "1",
        "gpus": "1",
        "gpus_per_node": "1",
        "cores_per_socket": null,
        "gpus_per_task": "1",
        "exclusive": null,
        "cpus_per_gpu": "4",
        "gpu_type": "rtx8000",
        "time": "15:00:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": "16GB",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}