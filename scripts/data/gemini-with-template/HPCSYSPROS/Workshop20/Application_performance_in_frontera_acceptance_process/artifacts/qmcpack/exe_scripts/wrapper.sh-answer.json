{
    "application": "qmcpack",
    "software": [
        "qmcpack",
        "perf",
        "sed",
        "cat",
        "hostname",
        "aprun",
        "ibrun",
        "mkdir",
        "unbuffer"
    ],
    "modules": [
        "boost",
        "fftw3",
        "hdf5"
    ],
    "environment_variables": {
        "MKL_DEBUG_CPU_TYPE": "7",
        "HOST": "hostname -s",
        "MYRANK": "$PMI_RANK",
        "qmcexe": "qmcpack",
        "NP": "1200",
        "NPPNODE": "1",
        "OMP_NUM_THREADS": null,
        "DMCBLOCKS": "30",
        "DMC_SAMPLES_PER_THREAD": "16",
        "VMCBLOCKS": "$DMC_SAMPLES_PER_THREAD",
        "VMCSTEPS": "10",
        "VMCWALKERS": "$OMP_NUM_THREADS",
        "SAMPLES": "2560000",
        "WFSXML": "gr4x4x1.wfs.xml",
        "title": "gr4x4x1.p${NP}x${OMP_NUM_THREADS}",
        "HUGETLB_MORECORE": "yes",
        "COUNTER1": "GET_TIME_OF_DAY",
        "executable": "qmcpack",
        "frequency": "22",
        "I_MPI_STATS": "0",
        "data_dir": "${executable}_${frequency}_${SLURM_JOBID}",
        "I_MPI_STATS_FILE": "$data_dir/mpi_stats.txt",
        "MY_TASKS_PER_NODE": "($PMI_SIZE$MPIRUN_SIZE / $SLURM_NNODES)",
        "MY_LOCAL_RANK": "($PMI_RANK$MPIRUN_RANK % $MY_TASKS_PER_NODE)"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": null,
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": null,
        "ntasks_per_node": null,
        "nodes": null,
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}