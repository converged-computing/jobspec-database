{
    "application": "icon",
    "software": [
        "mpirun",
        "icon"
    ],
    "modules": [
        "necmpivars.sh"
    ],
    "environment_variables": {
        "NE": "1",
        "CPE": "8",
        "NMPI_ROOT": "/opt/nec/ve/mpi/2.3.1",
        "NMPI_MALLOC_HEAP_EXPANSION_SIZE": "512",
        "NMPI_MALLOC_MMAP_THRESHOLD": "512",
        "MPI_IB_VBUF_TOTAL_SIZE": "131072",
        "NMPI_DAEMON_PATH": "${NMPI_ROOT}/libexec/mpid",
        "OMP_SCHEDULE": "static",
        "OMP_DYNAMIC": "false",
        "OMP_NUM_THREADS": "1",
        "PPN": "${CPE}*${NE}/${OMP_NUM_THREADS}",
        "NE1": "${NE}-1",
        "VE_ERRCTL_ALLOCATE": "MSG",
        "NMPI_PROGINF": "YES",
        "VE_TRACEBACK": "VERBOSE",
        "NMPI_SEPSELECT": "3",
        "GMON_OUT_PREFIX": "scal_prof",
        "VE_FPE_ENABLE": "DIV,FOF,INV",
        "GFORTRAN_UNBUFFERED_PRECONNECTED": "y",
        "NMPI_EXPORT": "GFORTRAN_UNBUFFERED_PRECONNECTED"
    },
