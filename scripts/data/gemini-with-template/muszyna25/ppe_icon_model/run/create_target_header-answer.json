        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "$s_cpu_time",
        "ntasks_per_node": "${tasks_per_node}",
        "nodes": "${use_nodes}",
        "memory": "0",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {}
