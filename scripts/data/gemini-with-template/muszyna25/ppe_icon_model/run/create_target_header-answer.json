{
    "application": "icon",
    "software": [
        "cdo",
        "diffn",
        "ksh"
    ],
    "modules": [],
    "environment_variables": {
        "OMP_NUM_THREADS": "${use_openmp_threads}",
        "ICON_THREADS": "${use_openmp_threads}",
        "OMP_SCHEDULE": "${use_OMP_SCHEDULE}",
        "OMP_DYNAMIC": "false",
        "OMP_STACKSIZE": "${use_omp_stacksize}",
        "no_of_nodes": "${use_nodes}",
        "mpi_procs_pernode": "$use_mpi_procs_pernode",
        "mpi_total_procs": "no_of_nodes * mpi_procs_pernode",
        "nproma": "$use_nproma",
        "nblocks_c": "$use_nblocks_c",
        "thisdir": "$(pwd)",
        "basedir": "${thisdir%/*}",
        "icon_data_rootFolder": "${icon_data_rootFolder}",
        "START": "$start",
        "MODEL": "${basedir}/bin/icon",
        "submit": "$use_submit",
        "job_name": "$job_name",
        "cdo": "${cdo}",
        "cdo_diff": "${cdo} ${cdo_diff}",
        "MALLOC_TRIM_THRESHOLD_": "-1",
        "I_MPI_PMI_LIBRARY": "/usr/lib64/libpmi.so",
        "CRAY_CUDA_MPS": "1",
        "PGI_ACC_NOTIFY": "1",
        "PGI_ACC_SYNCHRONOUS": "1",
        "ECCODES_DEFINITION_PATH": "${icon_data_rootFolder}/mch/eccodes_definitions",
        "MPICH_RDMA_ENABLED_CUDA": "1"
    },
    "resources": {
        "gres": "gpu:${tasks_per_node}",
        "cpus_per_task": "$(($smt_adjust * OMP_NUM_THREADS))",
        "tasks": "$((no_of_nodes * mpi_procs_pernode))",
        "ntasks_per_code": "",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
