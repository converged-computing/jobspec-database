{
    "application": "python",
    "software": [
        "mhcflurry",
        "tensorflow",
        "cuda",
        "cudnn"
    ],
    "modules": [
        "cuda/11.8.0",
        "cudnn/8.9.5-11"
    ],
    "environment_variables": {
        "TMPDIR": "/local/JOBS/mhcflurry-{work_item_num}",
        "PATH": "$HOME/mhcflurry-conda-environment/bin/:$PATH",
        "PYTHONUNBUFFERED": "1",
        "KMP_SETTINGS": "1",
        "XLA_FLAGS": "--xla_gpu_cuda_data_dir=$CUDA_PATH",
        "TF_GPU_ALLOCATOR": "cuda_malloc_async"
    },
    "resources": {
        "gres": "gpu",
        "cpus_per_task": "1",
        "tasks": "1",
        "ntasks_per_code": "1",
        "gpus": "1",
        "gpus_per_node": "1",
        "cores_per_socket": null,
        "gpus_per_task": "1",
        "exclusive": "yes",
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "46:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": "30000",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": "mode=exclusive_process:mps=no:j_exclusive=yes"
    },
    "versions": {
        "cuda": "11.8.0",
        "cudnn": "8.9.5-11"
    }
}