{
    "application": "slurm",
    "software": [
        "tensorflow",
        "nv-nsight-cu-cli",
        "python"
    ],
    "modules": [
        "tensorflow/gpu-1.15.0-py37"
    ],
    "environment_variables": {
        "PROFILER": "cupy",
        "enable_xla": "noxla",
        "XLA_FLAGS": "--xla_dump_to=$run_dir",
        "CUDA_DIR": "${CUDA_HOME}",
        "OMP_NUM_THREADS": "(( 40 / ${rankspernode} ))",
        "OMP_PLACES": "threads",
        "OMP_PROC_BIND": "spread",
        "TF_XLA_FLAGS": "--tf_xla_auto_jit=2"
    },
    "resources": {
        "gres": "gpu:1",
        "cpus_per_task": null,
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": 1,
        "gpus_per_node": 1,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": true,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "4:00:00",
        "ntasks_per_node": null,
        "nodes": null,
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "tensorflow": "1.15.0",
        "cuda": "11.0.167"
    }
}