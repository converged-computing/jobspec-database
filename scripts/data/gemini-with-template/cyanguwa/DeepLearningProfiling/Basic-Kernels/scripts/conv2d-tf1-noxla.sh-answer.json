{
    "application": "slurm",
    "software": [
        "tensorflow/gpu-1.15.0-py37",
        "nv-nsight-cu-cli",
        "python"
    ],
    "modules": [
        "tensorflow/gpu-1.15.0-py37"
    ],
    "environment_variables": {
        "PROFILER": "cupy",
        "enable_xla": "noxla",
        "XLA_FLAGS": "--xla_gpu_cuda_data_dir=${CUDA_HOME}",
        "CUDA_DIR": "${CUDA_HOME}",
        "rankspernode": "1",
        "OMP_NUM_THREADS": "(( 40 / ${rankspernode} ))",
        "OMP_PLACES": "threads",
        "OMP_PROC_BIND": "spread",
        "sruncmd": "srun -N ${SLURM_NNODES} -n $(( ${SLURM_NNODES} * ${rankspernode} )) --cpu_bind=cores",
        "run_dir": "$PWD/tf_cnn_kernels_nsight/conv2d-tf1-noxla/$SLURM_JOBID/",
        "script_dir": "../python",
        "script": "conv2d_tf1.py",
        "TF_XLA_FLAGS": "''",
        "data_formats": "NHWC ",
        "precs": "16 32",
        "num_warmup": "5",
        "num_iter": "1"
    },
    "resources": {
        "gres": "gpu:1",
        "cpus_per_task": null,
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": "1",
        "gpus_per_node": "1",
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": true,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "2:00:00",
        "ntasks_per_node": null,
        "nodes": null,
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": ""
    },
    "versions": {
        "tensorflow": "1.15.0",
        "cuda": "11.0.167"
    }
}