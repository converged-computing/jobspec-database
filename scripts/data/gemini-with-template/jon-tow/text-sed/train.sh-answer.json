{
    "application": "pytorch",
    "software": [
        "python3.8",
        "train.py",
        "srun"
    ],
    "modules": [
        "cuda/11.6",
        "openmpi"
    ],
    "environment_variables": {
        "LD_LIBRARY_PATH": "/opt/nccl/build/lib:/opt/aws-ofi-nccl-install/lib",
        "NCCL_PROTO": "simple",
        "PATH": "/opt/amazon/efa/bin:/opt/amazon/openmpi/bin:/opt/amazon/efa/bin:$PATH",
        "LD_PRELOAD": "/opt/nccl/build/lib/libnccl.so",
        "TORCH_CPP_LOG_LEVEL": "INFO",
        "TORCH_DISTRIBUTED_DEBUG": "INFO",
        "NCCL_DEBUG": "info",
        "NCCL_TREE_THRESHOLD": "0",
        "PYTHONFAULTHANDLER": "1",
        "CUDA_LAUNCH_BLOCKING": "1",
        "FI_EFA_FORK_SAFE": "1",
        "FI_LOG_LEVEL": "1",
        "FI_EFA_ENABLE_SHM_TRANSFER": "0",
        "FI_PROVIDER": "efa",
        "FI_EFA_TX_MIN_CREDITS": "64",
        "OMPI_MCA_mtl_base_verbose": "1",
        "OMPI_MCA_pml": "^cm",
        "OMPI_MCA_btl": "tcp,self",
        "OMPI_MCA_btl_tcp_if_exclude": "lo,docker1",
        "OMPI_MCA_btl_base_verbose": "30",
        "OMPI_MCA_plm_rsh_no_tree_spawn": "1",
        "HOSTNAMES": "scontrol show hostnames \"$SLURM_JOB_NODELIST\"",
        "MASTER_ADDR": "scontrol show hostnames \"$SLURM_JOB_NODELIST\" | head -n 1",
        "MASTER_PORT": "12802",
        "COUNT_NODE": "scontrol show hostnames \"$SLURM_JOB_NODELIST\" | wc -l",
        "NAME": "$SLURM_JOB_ID-text-sed",
        "CONFIG_PATH": "./configs/default.yaml"
    },
    "resources": {
        "gres": "gpu:8",
        "cpus_per_task": "6",
        "tasks": "8",
        "ntasks_per_code": "8",
        "gpus": "8",
        "gpus_per_node": "8",
        "cores_per_socket": null,
        "gpus_per_task": "1",
        "exclusive": true,
        "cpus_per_gpu": "0.75",
        "gpu_type": null,
        "time": "72:00:00",
        "ntasks_per_node": "8",
        "nodes": "1",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "cuda": "11.6",
        "openmpi": null
    }
}