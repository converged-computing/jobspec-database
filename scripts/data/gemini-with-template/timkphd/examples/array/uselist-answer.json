{
    "application": "slurm",
    "software": [
        "python",
        "invertp.py",
        "invertc",
        "phostone",
        "tymer"
    ],
    "modules": [
        "slurm",
        "python/3.11.1",
        "gcc/12.1.0",
        "comp-intel/2018.0.3",
        "intel-mpi/2018.0.3",
        "mkl/2018.3.222"
    ],
    "environment_variables": {
        "OMP_NUM_THREADS": "2",
        "EXE": "invertp.py",
        "JOB_ID": "$SLURM_ARRAY_JOB_ID",
        "SUB_ID": "$SLURM_ARRAY_TASK_ID",
        "LIST": "list",
        "input": "head -n $SUB_ID $SLURM_SUBMIT_DIR/$LIST | tail -1"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "1",
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": "user",
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "00:05:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": "20G",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": ""
    },
    "versions": {
        "python": "3.11.1",
        "gcc": "12.1.0",
        "comp-intel": "2018.0.3",
        "intel-mpi": "2018.0.3",
        "mkl": "2018.3.222"
    }
