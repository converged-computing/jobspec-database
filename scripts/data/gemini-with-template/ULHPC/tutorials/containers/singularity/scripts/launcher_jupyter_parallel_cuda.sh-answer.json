{
    "application": "jupyter",
    "software": [
        "Singularity",
        "virtualenv",
        "pip",
        "ipykernel",
        "ipyparallel"
    ],
    "modules": [
        "tools/Singularity"
    ],
    "environment_variables": {
        "VENV": "$HOME/.envs/venv_cuda_${ULHPC_CLUSTER}",
        "JUPYTER_CONFIG_DIR": "$HOME/jupyter_sing/$SLURM_JOBID/",
        "JUPYTER_PATH": "$VENV/share/jupyter\":\"$HOME/jupyter_sing/$SLURM_JOBID/jupyter_path",
        "JUPYTER_DATA_DIR": "$HOME/jupyter_sing/$SLURM_JOBID/jupyter_data",
        "JUPYTER_RUNTIME_DIR": "$HOME/jupyter_sing/$SLURM_JOBID/jupyter_runtime",
        "IPYTHONDIR": "$HOME/ipython_sing/$SLURM_JOBID",
        "IP_ADDRESS": "$(hostname -I | awk '{print $1}')",
        "XDG_RUNTIME_DIR": ""
    },
    "resources": {
        "gres": "gpu:1",
        "cpus_per_task": "4",
        "tasks": "1",
        "ntasks_per_code": "1",
        "gpus": "1",
        "gpus_per_node": "1",
        "cores_per_socket": null,
        "gpus_per_task": "1",
        "exclusive": null,
        "cpus_per_gpu": "4",
        "gpu_type": null,
        "time": "0-01:00:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}