{
    "application": "habitat_baselines",
    "software": [
        "python",
        "habitat_baselines"
    ],
    "modules": [
        "cuda/10.1",
        "cudnn/v7.6.5.32-cuda.10.1",
        "anaconda3/5.0.1",
        "gcc/7.1.0",
        "cmake/3.10.1/gcc.5.4.0"
    ],
    "environment_variables": {
        "GLOG_minloglevel": "2",
        "MAGNUM_LOG": "quiet",
        "MASTER_ADDR": "$(srun --ntasks=1 hostname 2>&1 | tail -n1)",
        "CUDA_HOME": "/public/apps/cuda/10.1",
        "CUDA_NVCC_EXECUTABLE": "/public/apps/cuda/10.1/bin/nvcc",
        "CUDNN_INCLUDE_PATH": "/public/apps/cuda/10.1/include/",
        "CUDNN_LIBRARY_PATH": "/public/apps/cuda/10.1/lib64/",
        "LIBRARY_PATH": "/public/apps/cuda/10.1/lib64",
        "CMAKE_PREFIX_PATH": "${CONDA_PREFIX:-\"$(dirname $(which conda))/../\"}",
        "USE_CUDA": "1",
        "USE_CUDNN": "1",
        "USE_MKLDNN": "1",
        "CURRENT_DATETIME": "`date +%Y_%m_%d_%H_%M_%S`"
    },
    "resources": {
        "gres": "gpu:1",
        "cpus_per_task": "10",
        "tasks": "8",
        "ntasks_per_code": "8",
        "gpus": "1",
        "gpus_per_node": "1",
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": "volta32gb",
        "time": "72:00:00",
        "ntasks_per_node": "8",
        "nodes": "1",
        "memory": "450GB",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "cuda": "10.1",
        "cudnn": "v7.6.5.32-cuda.10.1",
        "anaconda3": "5.0.1",
        "gcc": "7.1.0",
        "cmake": "3.10.1"
    }
}