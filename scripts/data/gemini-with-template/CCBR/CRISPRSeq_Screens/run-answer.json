{
    "application": "snakemake",
    "software": [
        "snakemake",
        "singularity",
        "python",
        "sbatch"
    ],
    "modules": [
        "python/3.7",
        "snakemake/5.24.1",
        "singularity"
    ],
    "environment_variables": {
        "PYTHON_VERSION": "python/3.7",
        "SNAKEMAKE_VERSION": "snakemake/5.24.1",
        "EXTRA_SINGULARITY_BINDS": "-B /data/CCBR_Pipeliner/:/data/CCBR_Pipeliner/",
        "SINGULARITY_BINDS": "-B /data/CCBR_Pipeliner/:/data/CCBR_Pipeliner/ -B ${PIPELINE_HOME}:${PIPELINE_HOME} -B ${WORKDIR}:${WORKDIR}"
    },
    "resources": {
        "gres": "{cluster.gres}",
        "cpus_per_task": "{cluster.threads}",
        "tasks": "500",
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "{cluster.time}",
        "ntasks_per_node": null,
        "nodes": null,
        "memory": "{cluster.mem}",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": ""
    },
    "versions": {
        "python": "3.7",
        "snakemake": "5.24.1"
    }
}