        "xhpcg",
        "singularity",
        "sed",
        "awk",
        "grep",
        "head",
        "tail",
        "mktemp",
        "mkdir",
        "cp"
    ],
    "modules": [
        "singularity",
        "gcc/11.2.0-655h",
        "hpl",
