{
    "application": "snakemake",
    "software": [
        "singularity",
        "snakemake"
    ],
    "modules": [],
    "environment_variables": {
        "SINGULARITY_CACHEDIR": "/path/to/output/directory/.singularity",
        "SLURM_DIR": "/path/to/output/directory/logfiles/slurmfiles",
        "CLUSTER_OPTS": "sbatch --gres {cluster.gres} --cpus-per-task {cluster.threads} -p {cluster.partition} -t {cluster.time} --mem {cluster.mem} --job-name={params.rname} -e $SLURM_DIR/slurm-%j_{params.rname}.out -o $SLURM_DIR/slurm-%j_{params.rname}.out"
    },
    "resources": {
        "gres": "{cluster.gres}",
        "cpus_per_task": "{cluster.threads}",
        "tasks": "500",
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "{cluster.time}",
        "ntasks_per_node": null,
        "nodes": null,
        "memory": "{cluster.mem}",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": ""
    },
    "versions": {}
}