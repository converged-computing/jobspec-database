{
    "application": "pytorch",
    "software": [
        "enroot",
        "nvidia-smi",
        "ibv_devinfo",
        "all_reduce_perf_mpi",
        "run_and_time.sh",
        "python3",
        "mlperf_logging.compliance_checker"
    ],
    "modules": [],
    "environment_variables": {
        "LOGDIR": "./results",
        "UNITTESTDIR": "${curDir}/unittest/${SLURM_JOB_ID}",
        "TGT_DIR": "/${FS}/bruno/mlperf",
        "DATADIR": "${TGT_DIR}/hdf5/training-4320/hdf5_4320_shards_varlength",
        "DATADIR_PHASE2": "${TGT_DIR}/hdf5/training-4320/hdf5_4320_shards_varlength",
        "EVALDIR": "${TGT_DIR}/hdf5/eval_varlength",
        "CHECKPOINTDIR_PHASE1": "${TGT_DIR}/phase1/",
        "MLPERF_RULESET": "2.1.0",
        "API_LOG_DIR": "./api_logs",
        "API_LOGGING": "0",
        "CLEAR_CACHES": "1",
        "CONT_FILE": "/cstor/SHARED/containers/enroot/mlperfv21.bert.sqsh",
        "CONTAINER_PRELOAD_LUSTRE": "0",
        "DATESTAMP": "$(date +'%y%m%d%H%M%S%N')",
        "NSYSCMD": "",
        "NVTX_FLAG": "0",
        "TIME_TAGS": "0",
        "NCCL_TEST": "0",
        "SYNTH_DATA": "0",
        "EPOCH_PROF": "0",
        "DISABLE_CG": "0",
        "WORK_DIR": "/workspace/bert",
        "WORLD_SIZE": "${SLURM_NPROCS}",
        "RANK": "$((${DGXNGPU}*${SLURM_NODEID}+${SLURM_LOCALID}))",
        "MASTER_ADDR": "${SLURMD_NODENAME}",
        "MASTER_PORT": "5678",
        "LOG_BASE": "${DATESTAMP}",
        "SPREFIX": "language_model_pytorch_${DGXNNODES}x${DGXNGPU}x${BATCHSIZE}_${DATESTAMP}",
        "LOG_FILE_BASE": "${LOGDIR}/${LOG_BASE}",
        "CONT": "${CONT:?CONT not set}",
        "DGXSYSTEM": "${DGXSYSTEM:?DGXSYSTEM not set}",
        "NEXP": "${NEXP:?NEXP not set}",
        "CONT_MOUNTS": "${CONT_MOUNTS},${LOGDIR}:/results",
        "MELLANOX_VISIBLE_DEVICES": "${MELLANOX_VISIBLE_DEVICES:-}"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "8",
        "tasks": "${SLURM_NPROCS}",
        "ntasks_per_code": "1",
        "gpus": "${DGXNGPU}",
        "gpus_per_node": "${DGXNGPU}",
        "cores_per_socket": "",
        "gpus_per_task": "1",
        "exclusive": "True",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "",
        "ntasks_per_node": "${DGXNGPU}",
        "nodes": "${SLURM_JOB_NUM_NODES}",
        "memory": "",
        "sockets_per_node": "",
        "ntasks_per_socket": "4",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {}
}