{
    "application": "rnnt",
    "software": [
        "pytorch",
        "pmix",
        "nccl"
    ],
    "modules": [],
    "environment_variables": {
        "LOGDIR": "${curDir}/logs/${SLURM_JOB_ID}",
        "DATADIR": "/$FS/mlperf/LibriSpeech",
        "METADATA_DIR": "/$FS/mlperf/LibriSpeech/tokenized",
        "SENTENCEPIECES_DIR": "/$FS/mlperf/LibriSpeech/sentencepieces",
        "WORLD_SIZE": "${SLURM_NPROCS}",
        "RANK": "$((${DGXNGPU}*${SLURM_NODEID}+${SLURM_LOCALID}))",
        "MASTER_ADDR": "${SLURMD_NODENAME}",
        "MASTER_PORT": "5678",
        "DGXSYSTEM": null,
        "CONT": null,
        "NEXP": "10",
        "DATESTAMP": "$(date +'%y%m%d%H%M%S%N')",
        "CLEAR_CACHES": "1",
        "API_LOG_DIR": "./api_logs",
        "SEED": "$RANDOM",
        "WALLTIME_DRYRUN": "$WALLTIME",
        "LOGBASE": "${DATESTAMP}",
        "SPREFIX": "rnn_speech_recognition_pytorch_${DGXNNODES}x${DGXNGPU}x${BATCHSIZE}_${DATESTAMP}",
        "TIME_TAGS": "0",
        "NVTX_FLAG": "0",
        "NCCL_TEST": "0",
        "SYNTH_DATA": "0",
        "EPOCH_PROF": "0",
        "_logfile_base": "${LOGDIR}/${LOGBASE}",
        "_cont_name": "rnn_speech_recognition",
        "_cont_mounts": "/apps:/apps,/nfs:/nfs,${SCRIPTDIR}/pytorch:/workspace/rnnt,${DATADIR}:/datasets/,${LOGDIR}:/results,${METADATA_DIR}:/metadata,${SENTENCEPIECES_DIR}:/sentencepieces",
        "MLPERF_HOST_OS": null
    },
    "resources": {
        "gres": null,
        "cpus_per_task": "8",
        "tasks": "${SLURM_JOB_NUM_NODES * DGXNGPU}",
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": "${DGXNGPU}",
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": "true",
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": null,
        "ntasks_per_node": "${DGXNGPU}",
        "nodes": "${SLURM_JOB_NUM_NODES}",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": "4",
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}