        "NSYSCMD": "\"\"",
        "NVTX_FLAG": "0",
        "TIME_TAGS": "0",
        "WORK_DIR": "/workspace/bert",
        "NCCL_DEBUG": "INFO",
        "NCCL_TOPO_FILE": "/workspace/bert/dgxa100_nic_affinity.xml"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "",
        "tasks": "${SLURM_JOB_NUM_NODES * DGXNGPU}",
        "ntasks_per_code": "",
        "gpus": "${DGXNGPU}",
        "gpus_per_node": "${DGXNGPU}",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": true,
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "",
        "ntasks_per_node": "${DGXNGPU}",
        "nodes": "${SLURM_JOB_NUM_NODES}",
        "memory": "0",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {}
