{
    "application": "bert",
    "software": [
        "python",
        "mlperf_logger",
        "nsys",
        "enroot"
    ],
    "modules": [],
    "environment_variables": {
        "API_LOG_DIR": "./api_logs",
        "API_LOGGING": "0",
        "CLEAR_CACHES": "1",
        "CONT_FILE": "/lustre/fsw/containers/${SLURM_JOBID}_$(basename ${CONT}).squashfs",
        "CONT_NAME": "language_model",
        "CONTAINER_PRELOAD_LUSTRE": "0",
        "DATESTAMP": "$(date +'%y%m%d%H%M%S%N')",
