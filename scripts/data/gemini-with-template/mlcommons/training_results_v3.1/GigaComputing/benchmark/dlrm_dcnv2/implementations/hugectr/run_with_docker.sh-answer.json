{
    "application": "hugectr",
    "software": [
        "docker",
        "python3",
        "mlperf_logging"
    ],
    "modules": [],
    "environment_variables": {
        "DGXSYSTEM": "not set",
        "CONT": "not set",
        "DATADIR": "not set",
        "NEXP": "20",
        "DATESTAMP": "date +'%y%m%d%H%M%S%N'",
        "CLEAR_CACHES": "1",
        "CHECK_COMPLIANCE": "1",
        "MLPERF_RULESET": "3.0.0",
        "MOUNTS": "${DATADIR}:/data",
        "LOGDIR": "./results",
        "DLRM_BIND": "",
        "_config_file": "./config_${DGXSYSTEM}.sh",
        "_logfile_base": "${LOGDIR}/${DATESTAMP}",
        "_cont_name": "dlrm_hugectr",
        "_cont_mounts": [
            "--volume=${DATADIR}:/data",
            "--volume=${DATADIR}:/data_val",
            "--volume=${LOGDIR}:/resutls",
            "--volume=$(pwd):/workspace/dlrm"
        ],
        "MLPERF_SUBMISSION_ORG": "GigaComputing",
        "MLPERF_SUBMISSION_PLATFORM": "G593-ZD2"
    },
    "resources": {
        "gres": "all",
        "cpus_per_task": null,
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": "all",
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "00:30:00",
        "ntasks_per_node": null,
        "nodes": null,
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}