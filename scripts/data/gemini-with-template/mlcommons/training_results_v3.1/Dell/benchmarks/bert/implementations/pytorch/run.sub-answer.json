{
    "application": "mlperf",
    "software": [
        "slurm2pytorch",
        "all_reduce_perf_mpi",
        "mlperf_logging.compliance_checker",
        "nvidia-smi",
        "ibv_devinfo",
        "enroot",
        "python3",
        "jq",
        "date",
        "bash"
    ],
    "modules": [],
    "environment_variables": {
        "CONT": "",
        "DGXSYSTEM": "",
        "NEXP": "",
        "MLPERF_RULESET": "3.1.0",
        "MLPERF_CLUSTER_NAME": "unknown",
        "API_LOG_DIR": "./api_logs",
        "API_LOGGING": "0",
        "CLEAR_CACHES": "1",
        "CONT_FILE": "/lustre/fsw/containers/${SLURM_JOBID}_$(basename ${CONT}).squashfs",
        "CONTAINER_PRELOAD_LUSTRE": "0",
        "DATESTAMP": "$(date +'%y%m%d%H%M%S%N')",
        "CHECK_COMPLIANCE": "1",
        "DROPCACHE_CMD": "sudo /sbin/sysctl vm.drop_caches=3",
        "LOGDIR": "./results",
        "ABSLOGDIR": "",
        "POWERCMDDIR": "",
        "NSYSCMD": "",
        "NVTX_FLAG": "0",
        "TIME_TAGS": "0",
        "NCCL_TEST": "1",
        "SYNTH_DATA": "0",
        "EPOCH_PROF": "0",
        "DISABLE_CG": "0",
        "WORK_DIR": "/workspace/bert",
        "SET_MAXQ_CLK": "0",
        "SET_MINEDP_CLK": "0",
        "MLPERF_SLURM_FIRSTNODE": "$(scontrol show hostnames \"${SLURM_JOB_NODELIST-}\" | head -n1)",
        "MODEL_NAME": "language_model",
        "MODEL_FRAMEWORK": "pytorch",
        "_cont_name": "${MODEL_NAME}_${SLURM_JOB_ID}",
        "LOG_BASE": "${DATESTAMP}",
        "SPREFIX": "${MODEL_NAME}_${MODEL_FRAMEWORK}_${DGXNNODES}x${DGXNGPU}x${BATCHSIZE}_${DATESTAMP}",
        "MAX_STEPS": "100",
        "LOG_FILE_BASE": "${LOGDIR}/${LOG_BASE}",
        "MELLANOX_VISIBLE_DEVICES": "${MELLANOX_VISIBLE_DEVICES:-}",
        "JSON_MODEL_NAME": "MLPERF_${MODEL_NAME}_${APILOG_MODEL_NAME}_${MODEL_FRAMEWORK}_train",
        "JSON_README_LINK": "${README_PREFIX}/${MODEL_NAME}/${MODEL_FRAMEWORK}/README.md",
        "JSON_FMT": "{model_name: $mn, readme_link: $rl, configs: {($dt): [$bs]}, sweep: {($dt): [$bs]}}",
        "JSON_OUTPUT": "${JSON_MODEL_NAME}.cudnn.json",
        "JET": "0",
        "GPCCLK": "",
        "NODENAME": "",
        "SLURM_JOB_NODELIST": "",
        "SLURM_JOB_ID": "",
        "JET_CREATE": "${JET_CREATE:-} --data workload.spec.nodes=${DGXNNODES} --data workload.spec.name=${MODEL_NAME}_${MODEL_FRAMEWORK}_${DGXSYSTEM} --data workload.key=${MODEL_NAME}_${MODEL_FRAMEWORK}_${DGXSYSTEM} --mllogger ",
        "JET_UPLOAD": ""
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "",
        "tasks": "${SLURM_JOB_NUM_NODES * DGXNGPU}",
        "ntasks_per_code": "",
        "gpus": "${SLURM_JOB_NUM_NODES * DGXNGPU}",
        "gpus_per_node": "${DGXNGPU}",
        "cores_per_socket": "",
        "gpus_per_task": "1",
        "exclusive": "1",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "",
        "ntasks_per_node": "${DGXNGPU}",
        "nodes": "${SLURM_JOB_NUM_NODES}",
        "memory": "0",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {}
}