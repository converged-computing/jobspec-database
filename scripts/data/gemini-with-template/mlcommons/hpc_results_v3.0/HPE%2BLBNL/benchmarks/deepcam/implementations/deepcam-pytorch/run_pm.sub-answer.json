{
    "application": "shifter",
    "software": [
        "shifter",
        "bash",
        "nccl-2.18",
        "python3",
        "mlperf_logging.compliance_checker",
        "nvidia-smi",
        "ibv_devinfo",
        "conda",
        "pip",
        "all_reduce_perf_mpi",
        "gpucommtest",
        "power_monitor.sh"
    ],
    "modules": [
        "gpu",
        "nccl-2.18"
    ],
    "environment_variables": {
        "DGXSYSTEM": null,
        "CONT": null,
        "MLPERF_RULESET": "2.0.0",
        "MLPERF_CLUSTER_NAME": "unknown",
        "CHECK_COMPLIANCE": "1",
        "DGXRUNNODES": "${SLURM_JOB_NUM_NODES}",
        "NEXP": "1",
        "NUM_INSTANCES": "1",
        "DATESTAMP": "$(date +'%y%m%d%H%M%S%N')",
        "CLEAR_CACHES": "1",
        "LOGDIR": "./results",
        "ABSLOGDIR": "${PWD}/results",
        "POWERCMDDIR": " ",
        "API_LOG_DIR": "./api_logs",
        "CUDNN_V8_API_ENABLED": "1",
        "NCCL_ASYNC_ERROR_HANDLING": "0",
        "NCCL_TEST": "1",
        "NCCL_BISECT": "0",
        "WIREUP_METHOD": "nccl-slurm",
        "ADDITIONAL_SRUN_ARGS": "",
        "COMP_CLOCK": "${BASE_COMP_CLOCK}",
        "MEM_CLOCK": "${BASE_MEM_CLOCK}",
        "DROPCACHE_CMD": "sudo /sbin/sysctl vm.drop_caches=3",
        "MODEL_NAME": "deepcam",
        "MODEL_FRAMEWORK": "pytorch",
        "_seed_override": "${SEED:-}",
        "_logfile_base": "${LOGDIR}/slurm_${DATESTAMP}",
        "_cont_name": "${MODEL_NAME}_${SLURM_JOB_ID}",
        "_cont_mounts": "${DATADIR}:/data:ro;${LOGDIR}:/results",
        "MLPERF_HOST_OS": null,
        "SEED": null,
        "EXP_ID": null
    },
    "resources": {
        "gres": "gpu",
        "cpus_per_task": "32",
        "tasks": "${TOTALGPU}",
        "ntasks_per_code": null,
        "gpus": "${DGXNGPU}",
        "gpus_per_node": "${DGXNGPU}",
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": null,
        "ntasks_per_node": "${DGXNGPU}",
        "nodes": "${DGXRUNNODES}",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "nccl": "2.18"
    }
}