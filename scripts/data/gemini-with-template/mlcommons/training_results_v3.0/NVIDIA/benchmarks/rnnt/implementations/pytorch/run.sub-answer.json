{
    "application": "pytorch",
    "software": [
        "slurm2pytorch",
        "mlperf_logging.compliance_checker",
        "nvidia-smi",
        "ibv_devinfo",
        "all_reduce_perf_mpi",
        "python",
        "bash"
    ],
    "modules": [],
    "environment_variables": {
        "DGXSYSTEM": "",
        "CONT": "",
        "DATADIR": "",
        "METADATA_DIR": "",
        "SENTENCEPIECES_DIR": "",
        "MLPERF_RULESET": "2.1.0",
        "NEXP": "1",
        "DATESTAMP": "",
        "CLEAR_CACHES": "1",
        "CHECK_COMPLIANCE": "1",
        "LOGDIR": "./results",
        "ABSLOGDIR": "${PWD}/results",
