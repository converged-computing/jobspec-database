{
    "application": "docker",
    "software": [
        "nvidia-docker",
        "docker",
        "bash",
        "python",
        "mpirun",
        "orte",
        "slurm",
        "sync",
        "sysctl",
        "chown",
        "chmod",
        "cp",
        "rm",
        "mkdir",
        "grep",
        "tar",
        "cat",
        "hostname",
        "timeout"
    ],
    "modules": [],
    "environment_variables": {
        "DATESTAMP": "date +'%y%m%d%H%M%S%N'",
        "BENCHMARK": "image_classification",
        "BENCHMARK_NAME": "RESNET",
        "CONT": "mlperf-fujitsu:$BENCHMARK",
        "DATADIR": "/raid/data/imagenet/train-val-recordio-passthrough",
        "LOGDIR": "/raid/results/$BENCHMARK",
        "SEED": "od -A n -t d -N 3 /dev/urandom",
        "CONTAINER_UID": "0",
        "CONTAINER_GID": "0",
        "NEXP": "5",
        "CLEAR_CACHES": "1",
        "SYSLOGGING": "1",
        "LOG_CLEAR_CACHES": "'from mlperf_logging.mllog import constants as mlperf_constants; from mlperf_log_utils import mx_resnet_print; mx_resnet_print(mlperf_constants.CACHE_CLEAR, val=True, stack_offset=0)'",
        "SYS_LOG_GET": "'from mlperf_logging.mllog import constants as mlperf_constants; from mlperf_log_utils import mlperf_submission_log; mlperf_submission_log(mlperf_constants.$BENCHMARK_NAME)'",
        "PGSYSTEM": "PG",
        "IBDEVICES": "$PGIBDEVICES",
        "INSLURM": "1",
        "SLURM_JOB_ID": "${DATESTAMP}",
        "SLURM_JOB_NUM_NODES": "1",
        "OMPI_COMM_WORLD_LOCAL_RANK": "0",
        "SLURM_NTASKS_PER_NODE": "${PGNGPU}",
        "LOGFILE_BASE": "${LOGDIR}/${DATESTAMP}",
        "VOLS": "-v $DATADIR:/data -v $LOGDIR:/results",
        "MLPERF_HOST_OS": "Ubuntu 18.04",
        "CONTNAME": "mpi_${SLURM_JOB_ID}",
        "PULL": "0",
        "KVSTORE": "horovod",
        "DEBUG": "0",
        "HOSTS": "scontrol show hostname |tr \"\\n\" \" \""
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "",
        "tasks": "",
        "ntasks_per_code": "",
        "gpus": "",
        "gpus_per_node": "${PGNGPU}",
        "cores_per_socket": "24",
        "gpus_per_task": "",
        "exclusive": "1",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "12:00:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": "0",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {}
}