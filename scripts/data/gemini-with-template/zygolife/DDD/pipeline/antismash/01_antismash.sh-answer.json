{
    "application": "antismash",
    "software": [
        "prodigal"
    ],
    "modules": [
        "antismash/6.0.0"
    ],
    "environment_variables": {
        "CPU": "1",
        "N": "$SLURM_ARRAY_TASK_ID",
        "TOP": "results/antismash",
        "INFILE": "$(ls data/*.fasta | sed -n ${N}p)",
        "OUTDIR": "$(basename $INFILE .fasta)"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "$SLURM_CPUS_ON_NODE",
        "tasks": "$SLURM_ARRAY_TASK_ID",
        "ntasks_per_code": "16",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "",
        "ntasks_per_node": "16",
        "nodes": "1",
        "memory": "64gb",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {}
}