{
    "application": "mpiexec",
    "software": [
        "conda",
        "mpiexec",
        "python",
        "nvidia-smi",
        "wc",
        "hostname"
    ],
    "modules": [
        "conda",
        "3DCNN"
    ],
    "environment_variables": {
        "MPICH_GPU_SUPPORT_ENABLED": "1",
        "NCCL_COLLNET_ENABLE": "1",
        "NCCL_NET_GDR_LEVEL": "PHB",
        "MASTER_ADDR": "`/bin/hostname -s`",
        "NNODES": "`wc -l < $PBS_NODEFILE`",
        "NRANKS_PER_NODE": "`nvidia-smi -L | wc -l`",
        "NTOTRANKS": "`( $NNODES * $NRANKS_PER_NODE )`",
        "NDEPTH": "16",
        "set_affinity_cmd": "./scripts/polaris/set_affinity_gpu_polaris.sh",
        "Command": "python main.py --step 3 --batch_size_phase3 4 --exp_name from_scratch_split_3  --image_path /lus/grand/projects/STlearn/HCP_MNI_to_TRs  --dataset_split_num 3"
    },
    "resources": {
        "gres": "gpu",
        "cpus_per_task": "1",
        "tasks": "$NTOTRANKS",
        "ntasks_per_code": "$NTOTRANKS",
        "gpus": "$NTOTRANKS",
        "gpus_per_node": "$NRANKS_PER_NODE",
        "cores_per_socket": null,
        "gpus_per_task": "1",
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "1:00:00",
        "ntasks_per_node": "$NRANKS_PER_NODE",
        "nodes": "10",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}