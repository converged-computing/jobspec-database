{
    "application": "concept",
    "software": [
        "openmpi",
        "mpich",
        "mvapich",
        "python",
        "mpi4py",
        "matplotlib",
        "blessings",
        "hdf5"
    ],
    "modules": [],
    "environment_variables": {
        "PYTHONPATH": "${src_dir}:${PYTHONPATH}",
        "PYTHONHASHSEED": "0",
        "LD_LIBRARY_PATH": "${mpi_libdir}:${LD_LIBRARY_PATH}",
        "HDF5_USE_FILE_LOCKING": "FALSE",
        "PATH": "${mpi_bindir}:${PATH}",
        "OMPI_MCA_orte_base_help_aggregate": "0",
        "OMPI_MCA_mpi_warn_on_fork": "0",
        "OMPI_ALLOW_RUN_AS_ROOT": "1",
        "OMPI_ALLOW_RUN_AS_ROOT_CONFIRM": "1",
        "OMPI_MCA_rmaps_base_oversubscribe": "1",
        "OMPI_MCA_btl_openib_allow_ib": "1",
        "OMPI_MCA_hwloc_base_binding_policy": "none",
        "MV2_ENABLE_AFFINITY": "0",
        "SLURM_UNBUFFEREDIO": "1",
        "PYTHONWARNINGS": "default"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "1",
        "tasks": "${nprocs}",
        "ntasks_per_code": "",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "${walltime}",
        "ntasks_per_node": "${nprocs_per_node}",
        "nodes": "${nnodes}",
        "memory": "${memory}",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "${mb_per_process}M",
        "gres_flags": ""
    },
    "versions": {
        "openmpi": "${mpi_version}",
        "mpich": "${mpi_version}",
        "mvapich": "${mpi_version}"
    }
}