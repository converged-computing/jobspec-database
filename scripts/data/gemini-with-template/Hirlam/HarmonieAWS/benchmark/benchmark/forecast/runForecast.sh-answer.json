{
    "application": "mpi",
    "software": [
        "MASTERODB"
    ],
    "modules": [
        "intelmpi",
        "libfabric-aws/1.11.1amzn1.0",
        "openmpi/4.1.0",
        "libfabric-aws/1.11.1amzn1.0",
        "intel oneAPI MPI",
        "intel MPI"
    ],
    "environment_variables": {
        "LOADHUGEPAGES": "yes",
        "MPPEXEC": "/opt/intel/impi/2019.8.254/intel64/bin/mpirun -np ${NPROC}",
        "MPPEXECP": ": -n $NPROCM1 $BINDIR/MASTERODB",
        "OMP_DYNAMIC": "false",
        "OMP_NUM_THREADS": "${THREADS}",
        "MPICH_MAX_THREAD_SAFETY": "multiple",
        "MPICH_VERSION_DISPLAY": "1",
        "DR_HOOK": "0",
        "DR_HOOK_IGNORE_SIGNALS": "8",
        "DR_HOOK_SHOW_PROCESS_OPTIONS": "0",
        "I_MPI_OFI_LIBRARY_INTERNAL": "0",
        "I_MPI_DEBUG": "5",
        "LD_LIBRARY_PATH": "/opt/intel/oneapi/tbb/2021.2.0/lib/intel64/gcc4.8:$LD_LIBRARY_PATH",
        "NODES": "${NODES}",
        "REPEAT": "${REPEAT}",
        "OMP_STACKSIZE": "4G",
        "DR_HOOK_SILENT": "1",
        "DR_HOOK_OPT": "prof",
        "DR_HOOK_PROFILE": "drhook.prof.%d",
        "NPROCX": "${NPROCX}",
        "NPROCY": "${NPROCY}",
        "NPROCXY": "${NPROCXY}",
        "NPROC_IO": "${NPROC_IO}",
        "NPROC": "${NPROC}",
        "DOMAIN": "${DOMAIN}",
        "FCLEN": "${FCLEN}",
        "BINDIR": "${BINDIR}",
        "INPUT_DATA": "${INPDIR}",
        "d_FORECAST": "${d_FORECAST}",
        "BMDIR": "${BMDIR}",
        "TBB_MALLOC_USE_HUGE_PAGES": "1"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": "${THREADS}",
        "tasks": "${NPROC}",
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "00:30:00",
        "ntasks_per_node": "${NTPN}",
        "nodes": "${MAXNODES}",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}