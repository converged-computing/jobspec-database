{
    "application": "openfoam",
    "software": [
        "openfoam",
        "gcc",
        "hpcx",
        "python3",
        "spack"
    ],
    "modules": [
        "gcc-13.1.0",
        "mpi/hpcx-v2.15.0"
    ],
    "environment_variables": {
        "compiler": "gcc-13.1.0",
        "mpi_library": "hpcx-v2.15.0",
        "FOAM_SPEC": "openfoam@2006",
        "MESH_DIM": "120x88x88",
        "NODE_LIST": "(16 32)",
        "PPN_LIST": "(176 120 96)",
        "basedir": "/path/to/directory",
        "VMINFO": "output_of_curl_command",
        "clustertype": "output_of_sed_command",
        "PATH": "/apps/spack/bin/:$PATH",
        "az_FOAMROOT": "$WM_PROJECT_DIR",
        "az_FOAM_VERSION": "",
        "az_FOAM_SIZE": "120x88x88",
        "LOGFILE": "log_${clustertype}_decompose_${compiler}_${mpi_library}.txt",
        "JOBID": "INITIALIZE",
        "OMP_NUM_THREADS": "1",
        "OUTPUTDIR": "/path/to/directory/run_${clustertype}/${compiler}/${mpi_library}/motorbike_${az_FOAM_SIZE}",
        "NODES_N": "${NODES}",
        "PPN_N": "${PPN}",
        "SPACK_VERSION": "version_of_spack"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "1",
        "tasks": "${NTASKS}",
        "ntasks_per_code": "1",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "exclhost",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "08:30:00",
        "ntasks_per_node": "${PPN}",
        "nodes": "1",
        "memory": "",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {
        "openfoam": "7"
    }
}