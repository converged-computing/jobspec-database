{
    "application": "gromacs",
    "software": [
        "gromacs",
        "gcc",
        "hpcx",
        "mpirun"
    ],
    "modules": [
        "gcc",
        "mpi"
    ],
    "environment_variables": {
        "NODE_LIST": "(1 2 4 8)",
        "PPN_LIST": "(96 120 144 176)",
        "THRD_LIST": "(1)",
        "compiler": "gcc-13.1.0",
        "mpi_library": "hpcx-v2.15",
        "BENCHMARK": "PEP",
        "REPS": 3,
        "SKU_TYPE": "hbv4",
        "INPUTDIR": "/share/home/hpcuser/gromacs/inputs",
        "GMX_BIN": "/apps/gromacs/bin/",
        "clustertype": null,
        "JOBID": "INITIALIZE",
        "LOGFILE": "log_${clustertype}_bench${BENCHMARK}_${compiler}_${mpi_library}.txt",
        "OUTPUTDIR": "${basedir}/run_${clustertype}/${compiler}/${mpi_library}/$BENCHMARK/",
        "NTASKS": "((NNODES * NPPNS))",
        "NCPUS": "((NPPNS * NTHRDS))",
        "GMX_MPI": "$(command -v gmx_mpi)",
        "IB_PKEY": null,
        "UCX_IB_PKEY": null,
        "mppflags": null,
        "NUMA_OPT": null,
        "NNUMA": null,
        "PPS": null,
        "mpi_options": null
    },
    "resources": {
        "gres": null,
        "cpus_per_task": null,
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "02:08:00",
        "ntasks_per_node": null,
        "nodes": null,
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "gcc": "gcc-13.1.0",
        "mpi": "hpcx-v2.15"
    }
}