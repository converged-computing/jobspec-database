{
    "application": "jupyter",
    "software": [
        "Miniconda3",
        "mamba",
        "papermill"
    ],
    "modules": [
        "slurm",
        "gpu"
    ],
    "environment_variables": {
        "CONDA_ENV": "df-parallel-gpu",
        "REPO_DIR": "${HOME}/df-parallel",
        "CONDA_YML": "${REPO_DIR}/environment-gpu.yml",
        "NOTEBOOK_DIR": "${REPO_DIR}/notebooks",
        "RESULT_DIR": "${REPO_DIR}/results",
        "LOCAL_SCRATCH_DIR": "/scratch/${USER}/job_${SLURM_JOB_ID}",
        "CONDA_INSTALL_PATH": "${LOCAL_SCRATCH_DIR}/miniconda3",
        "CONDA_ENVS_PATH": "${CONDA_INSTALL_PATH}/envs",
        "CONDA_PKGS_DIRS": "${CONDA_INSTALL_PATH}/pkgs"
    },
    "resources": {
        "gres": "gpu:1",
        "cpus_per_task": "1",
        "tasks": "16",
        "ntasks_per_code": "16",
        "gpus": "1",
        "gpus_per_node": "1",
        "cores_per_socket": "null",
        "gpus_per_task": "1",
        "exclusive": "null",
        "cpus_per_gpu": "null",
        "gpu_type": "null",
        "time": "08:00:00",
        "ntasks_per_node": "16",
        "nodes": "1",
        "memory": "92G",
        "sockets_per_node": "null",
        "ntasks_per_socket": "null",
        "mem_per_gpu": "null",
        "mem_per_cpu": "null",
        "gres_flags": "gpu:1"
    },
    "versions": {}
}