{
    "application": "slurm",
    "software": [
        "OpenMP",
        "OpenMPI",
        "MVAPICH2",
        "Intel MPI"
    ],
    "modules": [],
    "environment_variables": {
        "MYCLUSTER_QUEUE": "{{queue_name}}",
        "MYCLUSTER_JOB_NAME": "{{my_name}}",
        "NUM_TASKS": "{{num_tasks}}",
        "TASKS_PER_NODE": "{{tpn}}",
        "THREADS_PER_TASK": "{{num_threads_per_task}}",
        "CPUS_PER_NODE": "${SLURM_JOB_CPUS_PER_NODE}",
        "NUM_NODES": "{{num_nodes}}",
        "JOBID": "$SLURM_JOB_ID",
        "OMP_NUM_THREADS": "$THREADS_PER_TASK",
        "OMP_PROC_BIND": "true",
        "OMP_PLACES": "sockets",
        "OMPI_CMD": "mpiexec -n $NUM_TASKS -npernode $TASKS_PER_NODE {{openmpi_args}}",
        "OMPI_NEW_CMD": "mpiexec -n $NUM_TASKS --map-by ppr:1:numa --bind-to numa",
        "MV2_CPU_BINDING_LEVEL": "SOCKET",
        "MV2_CPU_BINDING_POLICY": "scatter",
        "MVAPICH_CMD": "mpiexec -n $NUM_TASKS -ppn $TASKS_PER_NODE -bind-to-socket",
        "I_MPI_PIN_DOMAIN": "omp:compact",
        "I_MPI_PIN_ORDER": "scatter",
        "IMPI_CMD": "mpiexec -n $NUM_TASKS -ppn $TASKS_PER_NODE"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "${THREADS_PER_TASK}",
        "tasks": "{{num_tasks}}",
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": "true",
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "{{wall_clock}}",
        "ntasks_per_node": "{{tpn}}",
        "nodes": "{{num_nodes}}",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": ""
    },
    "versions": {}
}