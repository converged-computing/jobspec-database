{
    "application": "deepvariant",
    "software": [
        "singularity",
        "conda",
        "deepvariant",
        "glnexus",
        "bcftools",
        "bgzip"
    ],
    "modules": [
        "singularity"
    ],
    "environment_variables": {
        "ANNOTATEVARIANTS_INSTALL": "annotate_variants_dir",
        "BIN_VERSION": "1.1.0",
        "NSLOTS": "$SLURM_CPUS_PER_TASK",
        "WORKING_DIR": "working_dir",
        "GENOME": "genome_build",
        "FASTA_DIR": "fasta_dir",
        "FASTA_FILE": "fasta_file",
        "SEQ_TYPE": "seq_type",
        "BAM_DIR": "$WORKING_DIR",
        "FAMILY_ID": "family_id",
        "PROBAND_ID": "proband_id",
        "MOTHER_ID": "mother_id",
        "FATHER_ID": "father_id",
        "PED": "$FAMILY_ID.ped",
        "PROBAND_BAM": "${PROBAND_ID}_${GENOME}.dupremoved.sorted.bam",
        "FATHER_BAM": "${FATHER_ID}_${GENOME}.dupremoved.sorted.bam",
        "MOTHER_BAM": "${MOTHER_ID}_${GENOME}.dupremoved.sorted.bam",
        "PROBAND_VCF": "${PROBAND_ID}.vcf.gz",
        "FATHER_VCF": "${FATHER_ID}.vcf.gz",
        "MOTHER_VCF": "${MOTHER_ID}.vcf.gz",
        "PROBAND_GVCF": "${PROBAND_ID}.gvcf.gz",
        "FATHER_GVCF": "${FATHER_ID}.gvcf.gz",
        "MOTHER_GVCF": "${MOTHER_ID}.gvcf.gz"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "40",
        "tasks": "",
        "ntasks_per_code": "",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "48:00:00",
        "ntasks_per_node": "",
        "nodes": "1",
        "memory": "350G",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {
        "deepvariant": "deeptrio-1.1.0"
    }
}