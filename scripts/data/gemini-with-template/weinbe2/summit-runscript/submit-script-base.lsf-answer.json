{
    "application": "jsrun",
    "software": [
        "staggered_invert_test",
        "jsrun",
        "bind-4gpu.sh",
        "bind-6gpu.sh"
    ],
    "modules": [],
    "environment_variables": {
        "EXE": "./staggered_invert_test",
        "ARGS": "--gridsize ${GRIDSIZE} --dim ${LOCALSIZE} --dslash-type staggered --recon 12 --recon-sloppy 12 --niter 10000 --tol 1e-5  --pipeline 1 ",
        "APP": "$EXE $ARGS",
        "QUDA_RESOURCE_PATH": "${LS_SUBCWD}/tunecache-${DIMX}x${DIMY}x${DIMZ}x${DIMT}-n${NNODES}-${GRIDX}x${GRIDY}x${GRIDZ}x${GRIDT}/",
        "CUDA_VISIBLE_DEVICES": "0,1,3,4",
        "OMP_NUM_THREADS": "7",
        "COMMAND": "jsrun --nrs ${NNODES} -a6 -g6 -c42 -dpacked -b packed:7 --latency_priority gpu-cpu --smpiargs=\"-gpu\" ./bind-6gpu.sh"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": null,
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": ":NGPUS:",
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "30",
        "ntasks_per_node": null,
        "nodes": ":NNODES:",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": "gpumps"
    },
    "versions": {}
}