{
    "application": "grads",
    "software": [
        "ndate",
        "gribmap",
        "grads"
    ],
    "modules": [
        "GrADS/2.2.0",
        "prod_envir/1.0.3"
    ],
    "environment_variables": {
        "hl": "hostname | cut -c1",
        "phase12_id": "g or t",
        "flag_test": "yes or no",
        "flag_bsub": "yes or no",
        "TODAY": "date +%Y%m%d",
        "aqm": "aqm",
        "capaqm": "aqm in uppercase",
        "smlaqm": "aqm in lowercase",
        "sel_cyc": "all|06|12",
        "FIRSTDAY": "YYYYMMDD",
        "LASTDAY": "YYYYMMDD",
        "opt1": "exp1",
        "opt2": "exp2",
        "capopt": "opt in uppercase",
        "smlopt": "opt in lowercase",
        "project": "naqfc",
        "project2": "meso",
        "capexp1": "capopt",
        "exp1": "smlopt",
        "comdir": "/gpfs/hps/nco/ops/com/${smlaqm}/${exp1}",
        "comdir2": "/gpfs/${phase12_id}d1/emc/meso/noscrub/${USER}/com/${smlaqm}/${exp1}",
        "mydir": "/gpfs/hps3/emc/${project}/noscrub/${USER}/com/${smlaqm}/${exp1}",
        "mydir2": "/gpfs/hps3/ptmp/${USER}/com/${smlaqm}/${exp1}",
        "comdir1_s1": "comdir",
        "comdir2_s1": "comdir2",
        "mydir1_s1": "mydir",
        "mydir2_s1": "mydir2",
        "capexp2": "capopt",
        "exp2": "smlopt",
        "comdir1_s2": "comdir",
        "comdir2_s2": "comdir2",
        "mydir1_s2": "mydir",
        "mydir2_s2": "mydir2",
        "capexp": "${capexp2}-${capexp1}",
        "exp": "${exp2}-${exp1}",
        "ftype": "_${exp2}m${exp1}.gif",
        "flag_update": "yes or no",
        "gs_dir": "pwd",
        "grid_id2": "148",
        "grid_id": "227",
        "NOW": "FIRSTDAY",
        "F1": "/nwprod/util/exec/ndate +1 ${cdate}| cut -c9-10",
        "TOMORROW": "/nwprod/util/exec/ndate +24 ${cdate}| cut -c1-8",
        "THIRDDAY": "/nwprod/util/exec/ndate +48 ${cdate}| cut -c1-8",
        "Y1": "echo ${NOW} | cut -c1-4",
        "MX": "echo ${NOW} | cut -c5-5",
        "M1": "echo ${NOW} | cut -c6-6",
        "D1": "echo ${NOW} | cut -c7-8",
        "Y2": "echo ${TOMORROW} | cut -c1-4",
        "M2": "echo ${TOMORROW} | cut -c6-6",
        "D2": "echo ${TOMORROW} | cut -c7-8",
        "Y3": "echo ${THIRDDAY} | cut -c1-4",
        "M3": "echo ${THIRDDAY} | cut -c6-6",
        "D3": "echo ${THIRDDAY} | cut -c7-8",
        "range1": "05Z${D1}${mchr[$M1-1]}${Y1}-04Z${D2}${mchr[$M2-1]}${Y2}",
        "range2": "05Z${D2}${mchr[$M2-1]}${Y2}-04Z${D3}${mchr[$M3-1]}${Y3}",
        "tmpdir": "/gpfs/dell2/stmp/${USER}/com2_aqm_${exp}_pm_max.${NOW}${cych}",
        "numcyc": "${cych}",
        "cychr": "t${cych}z",
        "outdir": "/gpfs/dell2/stmp/${USER}/daily_plot_pm25_max/aqm_${exp}_pm_max.${NOW}${cych}",
        "n0": "0",
        "n1": "${ntyp}-1",
        "ptyp": "n0",
        "var": "PDMAX1sig1000 or PMTFsig1000",
        "remote_dir": "/home/people/emc/www/htdocs/mmb/hchuang/web/fig",
        "remote_host": "emcrzdm.ncep.noaa.gov",
        "remote_user": "hchuang",
        "iconus": "1",
        "ieast": "2",
        "iwest": "3",
        "ine10": "4",
        "inw10": "5",
        "ise10": "6",
        "iswse": "7",
        "iak": "8",
        "ihi": "9",
        "ntyp": "length of typ array",
        "ndate": "/nwprod/util/exec/ndate",
        "cdate": "${NOW}${cych}",
        "idir1": "${comdir1_s1}/aqm.${NOW}",
        "idir2": "${comdir1_s2}/aqm.${NOW}",
        "working_dir": "/gpfs/dell2/stmp/${USER}/job_submit",
        "task_cpu": "05:00",
        "job_name": "cmaq_pmmax_${exp}${sel_cyc}",
        "batch_script": "trans_cmaqpmmax_${exp}.sh",
        "logdir": "/gpfs/dell2/ptmp/${USER}/batch.logs",
        "logfile": "${logdir}/${job_name}_${FIRSTDAY}_${LASTDAY}.out",
        "file_hd": "aqm",
        "file_type": "gif"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "1",
        "tasks": "1",
        "ntasks_per_code": "1",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "05:00",
        "ntasks_per_node": "",
        "nodes": "1",
        "memory": "100",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {}
}