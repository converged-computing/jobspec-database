{
    "application": "trakem2",
    "software": [
        "Fiji",
        "HappyNeuron",
        "python"
    ],
    "modules": [
        "darshan"
    ],
    "environment_variables": {
        "HDF5_USE_FILE_LOCKING": "FALSE",
        "FIJI": "/lus/theta-fs0/projects/connectomics_aesp/software/Fiji.app/ImageJ-linux64",
        "HPN": "/lus/theta-fs0/projects/connectomics_aesp/software/HappyNeuron/happyneuron",
        "PRE_TILES": "/lus/theta-fs0/projects/connectomics_aesp/software/HappyNeuron/happyneuron/trakem2/preprocess_tiles.py",
        "MONTAGE": "/lus/theta-fs0/projects/connectomics_aesp/software/HappyNeuron/happyneuron/trakem2/mpi_montage.py",
        "PRE_STACK": "/lus/theta-fs0/projects/connectomics_aesp/software/HappyNeuron/happyneuron/trakem2/preprocess_stack.py",
        "ALIGN": "/lus/theta-fs0/projects/connectomics_aesp/software/HappyNeuron/happyneuron/trakem2/align.py",
        "EXPORT": "/lus/theta-fs0/projects/connectomics_aesp/software/HappyNeuron/happyneuron/trakem2/mpi_export.py",
        "NRANK_PER_NODE": "1",
        "NTHREAD_PER_CORE": "4",
        "NNODE": "$COBALT_JOBSIZE",
        "NRANK": "$((NRANK_PER_NODE*NNODE))",
        "SUBMIT_THETA": "aprun -n $NRANK -N $NRANK_PER_NODE -e KMP_BLOCKTIME=0 -e KMP_AFFINITY=granularity=fine,compact,1,0 -e OMP_NUM_THREADS=$NTHREAD -cc depth -d $NTHREAD -j $NTHREAD_PER_CORE",
        "RAW_INPUT": "./trakem2_HL00732",
        "PROCESS_FOLDER": "./trakem2_HL00732_process_1",
        "MIN": "1024",
        "MAX": "2048",
        "RANGE": "0, 100"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "$NTHREAD",
        "tasks": "$NRANK",
        "ntasks_per_code": "1",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "",
        "ntasks_per_node": "$NRANK_PER_NODE",
        "nodes": "$COBALT_JOBSIZE",
        "memory": "",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {}
}