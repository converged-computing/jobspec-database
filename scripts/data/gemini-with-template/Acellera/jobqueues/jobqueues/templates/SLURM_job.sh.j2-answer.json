{
    "application": "slurm",
    "software": [
        "nvidia-cuda-mps-control",
        "nvidia-cuda-mps-server"
    ],
    "modules": [],
    "environment_variables": {
        "CUDA_MPS_PIPE_DIRECTORY": "~/.nvidia-mps_$(hostname)_gpu_${GPU}",
        "CUDA_MPS_LOG_DIRECTORY": "~/.nvidia-mps-logs_$(hostname)_gpu_${GPU}"
    },
    "resources": {
        "gres": "{{ gpustring }}",
        "cpus_per_task": "{{ ncpu }}",
        "tasks": "{{ ntasks }}",
        "ntasks_per_code": "{{ ntasks_per_core }}",
        "gpus": "{{ gpustring }}",
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "{{ time }}",
        "ntasks_per_node": "{{ ntasks_per_node }}",
        "nodes": "{{ nodes }}",
        "memory": "{{ memory }}",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}