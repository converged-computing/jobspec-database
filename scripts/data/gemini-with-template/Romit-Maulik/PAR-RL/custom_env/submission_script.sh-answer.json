{
    "application": "ray",
    "software": [
        "aprun",
        "python",
        "start_ray.py"
    ],
    "modules": [
        "trackdeps",
        "darshan",
        "xalt",
        "rllib_env"
    ],
    "environment_variables": {
        "MPICH_GNI_FORK_MODE": "FULLCOPY",
        "PMI_NO_FORK": "1",
        "KMP_AFFINITY": "disabled",
        "LC_ALL": "C.UTF-8",
        "LANG": "C.UTF-8",
        "PATH": "/soft/datascience/anaconda3/bin:/soft/libraries/mpi/mvapich2/gcc/bin/:$PATH"
    },
    "resources": {
        "gres": "ssds=required:ssd_size=128",
        "cpus_per_task": "1",
        "tasks": "$COBALT_JOBSIZE",
        "ntasks_per_code": "1",
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "60",
        "ntasks_per_node": null,
        "nodes": "128",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": "enable_ssh=1"
    },
    "versions": {}
}