{
    "application": "ray",
    "software": [
        "OpenFOAM",
        "mpi4py",
        "Click_",
        "intelpython36",
        "mvapich2",
        "rllib_env1_fenics"
    ],
    "modules": [
        "intelpython36",
        "trackdeps",
        "darshan",
        "xalt"
    ],
    "environment_variables": {
        "LD_LIBRARY_PATH": "${FOAM_LIBBIN}:${FOAM_LIBBIN}/mpi:${LD_LIBRARY_PATH}",
        "KMP_BLOCKTIME": "0",
        "KMP_AFFINITY": "disabled",
        "OMP_PROC_BIND": "'spread,close'",
        "OMP_NESTED": "'TRUE'",
        "MPICH_GNI_FORK_MODE": "FULLCOPY",
        "PMI_NO_FORK": "1",
        "LC_ALL": "C.UTF-8",
        "LANG": "C.UTF-8",
        "PATH": "/soft/libraries/mpi/mvapich2/gcc/bin/:$PATH"
    },
    "resources": {
        "gres": "ssds=required:ssd_size=128",
        "cpus_per_task": null,
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "180",
        "ntasks_per_node": "1",
        "nodes": "128",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": "enable_ssh=1"
    },
    "versions": {
        "OpenFOAM": "5.x"
    }
}