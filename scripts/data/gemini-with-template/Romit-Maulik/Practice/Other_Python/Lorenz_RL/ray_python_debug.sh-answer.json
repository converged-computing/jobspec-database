{
    "application": "ray",
    "software": [
        "aprun",
        "python",
        "start_ray.py"
    ],
    "modules": [
        "intelpython36",
        "mvapich2"
    ],
    "environment_variables": {
        "MPICH_GNI_FORK_MODE": "FULLCOPY",
        "PMI_NO_FORK": "1",
        "KMP_AFFINITY": "disabled",
        "LC_ALL": "C.UTF-8",
        "LANG": "C.UTF-8",
        "PATH": "/soft/libraries/mpi/mvapich2/gcc/bin/:$PATH"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "",
        "tasks": "",
        "ntasks_per_code": "",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "60",
        "ntasks_per_node": "1",
        "nodes": "4",
        "memory": "",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": "enable_ssh=1:ssds=required:ssd_size=128"
    },
    "versions": {}
}