{
    "application": "CLM",
    "software": [
        "OASIS",
        "ESMF"
    ],
    "modules": [
        "daint-gpu",
        "PrgEnv-xxx",
        "cray-mpich",
        "cray-python",
        "cray-netcdf-hdf5parallel",
        "cray-hdf5-parallel",
        "cray-parallel-netcdf"
    ],
    "environment_variables": {
        "CLMROOT": "$PWD",
        "CCLM2ROOT": "$CLMROOT/..",
        "CASEDIR": "$SCRATCH/CCLM2_cases/$CASENAME",
        "CESMDATAROOT": "$SCRATCH/CCLM2_inputdata",
        "CESMOUTPUTROOT": "$SCRATCH/CCLM2_output/$CASENAME",
        "OASIS_PATH": "$(spack location -i oasis%${COMPILERNAME}+fix_mct_conflict)",
        "ESMF_PATH": "$(spack location -i esmf@8.2.0%$COMPILERNAME)"
    },
    "resources": {
        "gres": "gpu",
        "cpus_per_task": "1",
        "tasks": "$NCORES",
        "ntasks_per_code": "$NCORES",
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "$WALLTIME",
        "ntasks_per_node": "12",
        "nodes": "$NNODES",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "ESMF": "8.2.0"
    }
}