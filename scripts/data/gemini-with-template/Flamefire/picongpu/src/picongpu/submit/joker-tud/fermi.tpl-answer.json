{
    "application": "picongpu",
    "software": [
        "picongpu",
        "cuda_memtest.sh",
        "tbg",
        "openmpi",
        "boost",
        "cupti",
        "papi",
        "cuda",
        "gdb",
        "pngwriter",
        "cmake",
        "hdf5"
    ],
    "modules": [
        "shared",
        "software",
        "boost",
        "cupti",
        "papi/4.2.0",
        "cuda/5.0.35",
        "gdb",
        "pngwriter",
        "cmake",
        "hdf5/1.8.5-threadsafe",
        "gcc/4.6.2",
        "openmpi/1.6.2-gnu"
    ],
    "environment_variables": {
        "TBG_gpu_arch": "fermi",
        "TBG_queue": "workq",
        "TBG_mailSettings": "${MY_MAILNOTIFY:-\"n\"}",
        "TBG_mailAddress": "${MY_MAIL:-\"someone@example.com\"}",
        "TBG_author": "${MY_NAME:+--author \"${MY_NAME}\"}",
        "TBG_gpusPerNode": "`if [ $TBG_tasks -gt 4 ] ; then echo 4; else echo $TBG_tasks; fi`",
        "TBG_coresPerNode": "$TBG_gpusPerNode",
        "TBG_nodes": "$(( ( TBG_tasks + TBG_gpusPerNode -1 ) / TBG_gpusPerNode))",
        "MODULES_NO_OUTPUT": 1
    },
    "resources": {
        "gres": null,
        "cpus_per_task": null,
        "tasks": "$TBG_tasks",
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": "$TBG_gpusPerNode",
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": "excl",
        "cpus_per_gpu": null,
        "gpu_type": "$TBG_gpu_arch",
        "time": "$TBG_wallTime",
        "ntasks_per_node": "$TBG_gpusPerNode",
        "nodes": "$TBG_nodes",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "openmpi": "1.6.2-gnu",
        "hdf5": "1.8.5-threadsafe",
        "gcc": "4.6.2",
        "cuda": "5.0.35",
        "papi": "4.2.0"
    }
}