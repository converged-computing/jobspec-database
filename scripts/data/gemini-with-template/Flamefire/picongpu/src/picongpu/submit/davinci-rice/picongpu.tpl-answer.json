{
    "application": "picongpu",
    "software": [
        "picongpu",
        "cmake",
        "openmpi",
        "cuda"
    ],
    "modules": [
        "cuda/4.2.9",
        "cmake",
        "openmpi/1.4.4-gcc"
    ],
    "environment_variables": {
        "LD_LIBRARY_PATH": "TBG_outDir/build_libsplash:TBG_outDir/build_simlib:$LD_LIBRARY_PATH",
        "MODULES_NO_OUTPUT": 1,
        "TBG_mailSettings": "${MY_MAILNOTIFY:-\"n\"}",
        "TBG_mailAddress": "${MY_MAIL:-\"someone@example.com\"}",
        "TBG_author": "${MY_NAME:+--author \"${MY_NAME}\"}"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": null,
        "tasks": "TBG_tasks",
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": "TBG_GpusPerNode",
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "TBG_wallTime",
        "ntasks_per_node": null,
        "nodes": "TBG_nodes",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "cuda": "4.2.9",
        "openmpi": "1.4.4-gcc"
    }
}