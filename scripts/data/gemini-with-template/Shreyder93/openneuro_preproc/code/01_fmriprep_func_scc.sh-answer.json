        "exclusive": "",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "24:00:00",
        "ntasks_per_node": "",
        "nodes": "1",
        "memory": "24000",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {
        "fmriprep": "20.2.7",
        "Singularity": "3.8.5"
    }
