{
    "application": "fmriprep",
    "software": [
        "Singularity"
    ],
    "modules": [
        "tools/Singularity/3.8.5"
    ],
    "environment_variables": {
        "OPENNEURO_DSID": "basename of SLURM_SUBMIT_DIR",
        "BASEDIR": "dirname of SLURM_SUBMIT_DIR",
        "BIDS_DIR": "${BASEDIR}/${OPENNEURO_DSID}/bids",
        "FMRIPREP_HOME": "${BASEDIR}/fmriprep_home",
        "SING_CONTAINER": "${BASEDIR}/containers/fmriprep-20.2.7.simg",
        "OUTPUT_DIR": "${BASEDIR}/${OPENNEURO_DSID}/derived/",
        "WORK_DIR": "${BASEDIR}/${OPENNEURO_DSID}/work/",
        "LOGS_DIR": "${BASEDIR}/${OPENNEURO_DSID}/logs",
        "SINGULARITYENV_TEMPLATEFLOW_HOME": "/home/fmriprep/.cache/templateflow",
        "SINGULARITYENV_FS_LICENSE": "/home/fmriprep/.freesurfer.txt"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "8",
