{
    "application": "slurm",
    "software": [
        "python",
        "mpi4py"
    ],
    "modules": [
        "psconda",
        "smalldata_tools"
    ],
    "environment_variables": {
        "EXPERIMENT": "EXP",
        "RUN_NUM": "RUN",
        "MYDIR": "ABS_PATH",
        "SIT_PSDM_DATA": "DATAPATH",
        "SIT_ENV_DIR": "/cds/group/psdm",
        "ARP_LOCATION": "LOCAL",
        "HOSTNAME": "HOSTNAME",
        "LCLS2_HUTCHES": "rix, tmo, ued",
        "PYTHONEXE": "smd_producer.py",
        "FORCE_S3DF": "FORCE_S3DF",
        "INTEG": "INTEG",
        "QUEUE": "DEFQUEUE",
        "ACCOUNT": "lcls:$EXP",
        "CORES": "CORES",
        "MAX_NODES": "MAX_NODES",
        "HUTCH": "EXP:0:3",
        "NEVENTS": "NEVENTS",
        "LOGDIR": "LOGDIR",
        "RESERVATION": "RESERVATION",
        "LOGFILE": "smd_'${EXPERIMENT}'_Run'${RUN_NUM}'_%J.log",
        "SBATCH_ARGS": "-p $QUEUE --nodes 0-$MAX_NODES --ntasks $CORES -o $LOGFILE",
        "MPI_CMD": "mpirun -np $CORES python -u -m mpi4py.run ${ABS_PATH}/${PYTHONEXE} $*",
        "DATAPATH": "DATAPATH"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "",
        "tasks": "CORES",
        "ntasks_per_code": "",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "",
        "ntasks_per_node": "",
        "nodes": "0-$MAX_NODES",
        "memory": "",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {}
}