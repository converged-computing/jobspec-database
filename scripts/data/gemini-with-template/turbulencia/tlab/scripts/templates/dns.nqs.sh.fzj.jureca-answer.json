{
    "application": "lammps",
    "software": [
        "dns.pre",
        "preprocess",
        "simulation",
        "postprocess"
    ],
    "modules": [
        "Intel ParaStationMPI",
        "FFTW",
        "GCCcore/.8.3.0",
        "CMake",
        "imkl"
    ],
    "environment_variables": {
        "TOOLS_HOME": "$HOME/bin",
        "PATH": "$PATH:$TOOLS_HOME",
        "CONFIG_FILE": "$TOOLS_HOME/CONFIG.default",
        "ABORT": "no",
        "LOCALDIR": "$WORK/$JOBDIR",
        "TRASHDIR": "$WORK/$JOBDIR-transfered",
        "OMP_NUM_THREADS": "${SLURM_CPUS_PER_TASK}",
        "OMP_MAX_THREADS": "${SLURM_CPUS_PER_TASK}",
        "KMP_AFFINITY": "compact",
        "PSP_OPENIB": "0",
        "PSP_UCP": "1",
        "PSP_ONDEMAND": "0",
        "PSP_GUARD": "0"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "${SLURM_CPUS_PER_TASK}",
        "tasks": "",
        "ntasks_per_code": "",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "$TIME",
        "ntasks_per_node": "",
        "nodes": "",
        "memory": "$MEM",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {}
