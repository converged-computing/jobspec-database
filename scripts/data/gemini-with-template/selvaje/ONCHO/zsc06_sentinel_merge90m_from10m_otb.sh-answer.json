{
    "application": "otbcli_Mosaic",
    "software": [
        "gdal_translate",
        "gdalinfo"
    ],
    "modules": [
        "~/bin/gdal3",
        "orfeotoolbox"
    ],
    "environment_variables": {
        "GDAL_CACHEMAX": "99999",
        "OTB_MAX_RAM_HINT": "10000",
        "file": "ls $IN/*.vrt   | head -$SLURM_ARRAY_TASK_ID | tail -1 ",
        "filename": "basename $file .vrt  "
    },
    "resources": {
        "gres": null,
        "cpus_per_task": "1",
        "tasks": "4",
        "ntasks_per_code": "1",
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "24:00:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": "120G",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}