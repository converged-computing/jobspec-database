{
    "application": "tspg",
    "software": [
        "python",
        "tensorflow",
        "advgan",
        "conda",
        "tsne",
        "heatmap"
    ],
    "modules": [
        "anaconda3/5.1.0-gcc"
    ],
    "environment_variables": {
        "TF_CPP_MIN_LOG_LEVEL": "3",
        "TSPG_DIR": "${HOME}/TSPG",
