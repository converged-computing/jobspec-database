        "cores_per_socket": null,
        "gpus_per_task": "1",
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": "p100",
        "time": "72:00:00",
        "ntasks_per_node": null,
        "nodes": "1",
        "memory": "64gb",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "anaconda": "5.1.0"
    }
