{
    "application": "slurm",
    "software": [
        "python",
        "kerchunk-dask-byhand.py"
    ],
    "modules": [
        "py39",
        "eso"
    ],
    "environment_variables": {
        "NARRAY": "10",
        "NCHUNK": "365 / $NARRAY",
        "dstart": "1 + ($SLURM_ARRAY_TASK_ID - 1) * ($NCHUNK+1)",
        "dend": "$dstart + $NCHUNK",
        "doys": "seq $dstart $dend"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": "8",
        "tasks": "10",
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "00:59:00",
        "ntasks_per_node": null,
        "nodes": null,
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}