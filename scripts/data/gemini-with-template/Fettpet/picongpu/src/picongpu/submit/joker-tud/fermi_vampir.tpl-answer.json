{
    "application": "picongpu",
    "software": [
        "boost",
        "cupti",
        "papi",
        "cuda",
        "gdb",
        "pngwriter",
        "cmake",
        "hdf5",
        "gcc",
        "openmpi",
        "vampirtrace"
    ],
    "modules": [
        "shared",
        "software",
        "boost",
        "cupti",
        "papi/4.2.0",
        "cuda/5.0.35",
        "gdb",
        "pngwriter",
        "cmake",
        "gdb",
        "hdf5/1.8.5-threadsafe",
        "gcc/4.6.2",
        "openmpi/1.6.2-gnu",
        "vampirtrace/gpu-gnu-cuda5.0"
    ],
    "environment_variables": {
        "MODULES_NO_OUTPUT": "1",
        "VT_MPI_IGNORE_FILTER": "yes",
        "VT_PFORM_GDIR": "traces",
        "VT_FILE_PREFIX": "trace",
        "VT_BUFFER_SIZE": "3G",
        "VT_MAX_FLUSHES": "3",
        "VT_GNU_DEMANGLE": "yes",
        "VT_PTHREAD_REUSE": "yes",
        "VT_FILTER_SPEC": "!TBG_dstPath/tbg/cuda.filter",
        "VT_UNIFY": "yes",
        "VT_GPUTRACE": "yes,cupti,idle,memusage,concurrent",
        "VT_VERBOSE": "1",
        "VT_CUPTI_METRICS": "",
        "VT_CUDATRACE_BUFFER_SIZE": "200M"
    },
    "resources": {
        "gres": "gpu: !TBG_gpu_arch",
        "cpus_per_task": "!TBG_coresPerNode",
        "tasks": "!TBG_tasks",
        "ntasks_per_code": null,
        "gpus": "!TBG_gpusPerNode",
        "gpus_per_node": "!TBG_gpusPerNode",
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": true,
        "cpus_per_gpu": "1",
        "gpu_type": "!TBG_gpu_arch",
        "time": "!TBG_wallTime",
        "ntasks_per_node": null,
        "nodes": "!TBG_nodes",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": ""
    },
    "versions": {
        "papi": "4.2.0",
        "cuda": "5.0.35",
        "hdf5": "1.8.5-threadsafe",
        "gcc": "4.6.2",
        "openmpi": "1.6.2-gnu",
        "vampirtrace": "gpu-gnu-cuda5.0"
    }
}