{
    "application": "pytorch",
    "software": [
        "singularity",
        "python",
        "nsys",
        "dlprof",
        "easybuild",
        "anaconda3",
        "cudnn",
        "cuda"
    ],
    "modules": [
        "site/infer-skylake_v100/easybuild/arc.arcadm",
        "EasyBuild/4.6.1",
        "cuDNN/8.4.1.50-CUDA-11.7.0",
        "CUDA/11.7.0",
        "Anaconda3/2022.05",
        "containers/singularity/3.8.5"
    ],
    "environment_variables": {
        "MASTER_PORT": "dynamically allocated based on available ports",
        "MASTER_ADDR": "scontrol show hostnames \"$SLURM_JOB_NODELIST\" | head -n 1",
        "WORLD_SIZE": "$SLURM_NNODES * $SLURM_NTASKS_PER_NODE",
        "batch_size": "4",
        "dest_dir": "$TMPDIR/tmpfs",
        "imagefile": "/projects/synergy_lab/ayush/containers/pytorch_2.0.sif or /projects/synergy_lab/ayush/containers/pytorch_22.04.sif (based on 'enable_gr' flag)",
        "infer_command": "python ddnet_inference.py --filepath ${SLURM_JOBID} --batch ${batch_size} --epochs ${epochs} --out_dir ${SLURM_JOBID} --model ${model} --retrain ${retrain}",
        "file": "trainers.py",
        "CMD": "python ${file} --batch ${batch_size} --epochs ${epochs} --retrain ${retrain} --out_dir ${SLURM_JOBID} --amp ${mp} --num_w $num_data_w  --new_load ${new_load} --prune_amt $prune_amt --prune_t $prune_t  --wan $wandb --lr ${lr} --dr ${dr} --distback ${distback} --enable_profile ${enable_profile} --gr_mode ${gr_mode} --gr_backend ${gr_back} --enable_gr=${enable_gr} --schedtype ${schedtype} --model ${model} --port ${MASTER_PORT}",
        "BASE": "singularity exec --nv --writable-tmpfs --bind=/projects/synergy_lab/garvit217,/cm/shared,${TMPFS} ${imagefile} ",
        "filepath": "user specified path",
        "NEXP": "1 (default, can be modified)",
        "enable_gr": "boolean flag",
        "enable_profile": "boolean flag",
        "inferonly": "boolean flag",
        "model": "user specified model",
        "epochs": "user specified epochs",
        "retrain": "user specified retrain flag",
        "mp": "user specified mixed precision flag",
        "num_data_w": "user specified number of data workers",
        "new_load": "user specified new load flag",
        "prune_amt": "user specified prune amount",
        "prune_t": "user specified prune threshold",
        "wandb": "user specified wandb flag",
        "lr": "user specified learning rate",
        "dr": "user specified dropout rate",
        "distback": "user specified distributed backpropagation flag",
        "gr_mode": "user specified gradient reduction mode",
        "gr_back": "user specified gradient reduction backend",
        "schedtype": "user specified scheduler type"
    },
    "resources": {
        "gres": "gpu",
        "cpus_per_task": "8",
        "tasks": "1",
        "ntasks_per_code": "1",
        "gpus": "1",
        "gpus_per_node": "1",
        "cores_per_socket": null,
        "gpus_per_task": "1",
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": "v100",
        "time": "24:30:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": "8384",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": "8384",
        "gres_flags": null
    },
    "versions": {}
}