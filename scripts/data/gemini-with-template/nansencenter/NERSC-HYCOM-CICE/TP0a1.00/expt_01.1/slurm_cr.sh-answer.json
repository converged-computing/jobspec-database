{
    "application": "nextsim",
    "software": [
        "mpirun",
        "hycom_oasis",
        "nextsim.exec",
        "blkdat_get",
        "atmo_synoptic.sh",
        "expt_preprocess.sh"
    ],
    "modules": [
        "NCL/6.4.0-intel-2017a",
        "FFTW/3.3.6-intel-2017a",
        "system"
    ],
    "environment_variables": {
        "STARTD": "grep \"time_init=\" $CONFIG | awk '{printf(\"%1s\", $1)}' | tail -c11 | head -c10",
        "DURATION": "grep \"^duration=\" $CONFIG | sed s/duration=// | awk '{printf(\"%d\", $1)}'",
        "START": "date '+%Y-%m-%dT%H:%M:%S' -d \"$STARTD\"",
        "ENDD": "date '+%Y-%m-%dT%H:%M:%S' -d \"$STARTD+$DURATION days\"",
        "END": "$ENDD",
        "INITFLG": "--init",
        "hycom_dir": "/cluster/work/users/annettes/TP0a1.00",
        "hycom_exp": "$hycom_dir/expt_01.3",
        "SCRATCH": "SCRATCH",
        "NEXTSIM_MESH_DIR": "mesh",
        "NEXTSIM_DATA_DIR": "data",
        "MUMPS_MEM": "1000",
        "ENV_FILE": "$HOME/HYCOM-CICE/NERSC-HYCOM-CICE/hycom/RELO/config/nextsim_configuration.src",
        "DEBUG": "true",
        "TEST": "false",
        "npseaice": "4",
        "npocean": "4",
        "CONFIG": "$CONFIG",
        "config": "$config",
        "log": "$(basename $config .cfg).log",
        "OID": "OASIS_input",
        "progdir": "$SLURM_SUBMIT_DIR/bin",
        "DURSEC": "$DURATION * 86400",
        "BACLIN": "blkdat_get $SCRATCH/blkdat.input baclin"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": "1",
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "0:30:00",
        "ntasks_per_node": "8",
        "nodes": "1",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}