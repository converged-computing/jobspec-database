{
    "application": "PBS",
    "software": [
        "cmake-3.6.1",
        "git",
        "argon2-gpu",
        "OpenCL-Headers"
    ],
    "modules": [
        "cmake-3.6.1"
    ],
    "environment_variables": {
        "C_INCLUDE_PATH": "$C_INCLUDE_PATH:$(readlink -f include)",
        "CPLUS_INCLUDE_PATH": "$CPLUS_INCLUDE_PATH:$(readlink -f include)"
    },
    "resources": {
        "gres": "gpu=1",
        "cpus_per_task": "1",
        "tasks": "1",
        "ntasks_per_code": "1",
        "gpus": "1",
        "gpus_per_node": "1",
        "cores_per_socket": null,
        "gpus_per_task": "1",
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "$duration",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": "16gb",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": "cl_$machine"
    },
    "versions": {
        "cmake": "3.6.1"
    }
}