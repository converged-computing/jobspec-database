{
    "application": "python",
    "software": [
        "cartopy",
        "matplotlib"
    ],
    "modules": [],
    "environment_variables": {
        "TOTAL_TASKS": "${TOTAL_TASKS:-${SLURM_NTASKS:-240}}",
        "NCTSK": "${NCTSK:-60}",
        "NCNODE": "${NCNODE:-4}",
        "OMP_NUM_THREADS": "${OMP_NUM_THREADS:-1}",
        "MPLBACKEND": "agg",
        "plotATCF": "${plotATCF:-yes}",
        "plotAtmos": "${plotAtmos:-yes}",
        "plotOcean": "${plotOcean:-yes}",
        "YMDH": "${YMDH:-2022092000}",
        "STORM": "${STORM:-FIONA}",
        "STORMID": "`echo ${stormid} | tr '[a-z]' '[A-Z]' `",
        "stormModel": "${stormModel:-HFSA}",
        "modelLabels": "['BEST','OFCL','${stormModel}','HWRF','HMON','AVNO']",
        "modelColors": "['black','red','cyan','purple','green','blue']",
        "modelMarkers": "['hr','.','.','.','.','.']",
        "modelMarkerSizes": "[18,15,15,15,15,15]",
        "nset": "",
        "ymdh": "${YMDH}",
        "stormname": "`echo ${stormname} | tr '[A-Z]' '[a-z]' `",
        "stormid": "`echo ${stormid} | tr '[A-Z]' '[a-z]' `",
        "STORMNAME": "`echo ${stormname} | tr '[a-z]' '[A-Z]' `",
        "stormnmid": "`echo ${stormname}${stormid} | tr '[A-Z]' '[a-z]' `",
        "STORMNMID": "`echo ${stormnmid} | tr '[a-z]' '[A-Z]' `",
        "STORMNM": "${STORMNMID:0:-3}",
        "stormnm": "${STORMNM,,}",
        "STID": "${STORMNMID: -3}",
        "stid": "${STID,,}",
        "STORMNUM": "${STID:0:2}",
        "BASIN1C": "${STID: -1}",
        "basin1c": "${BASIN1C,,}",
        "yyyy": "`echo ${ymdh} | cut -c1-4`",
        "ymd": "`echo ${ymdh} | cut -c1-8`",
        "hh": "`echo ${ymdh} | cut -c9-10`",
        "plotWave": "${plotWave:-no}",
        "HOMEgraph": "${HOMEgraph:-$(pwd)/../}",
        "USHgraph": "${USHgraph:-${HOMEgraph}/ush}",
        "DRIVERATMOS": "${USHgraph}/driverAtmos.sh",
        "DRIVEROCEAN": "${USHgraph}/driverOcean.sh",
        "COMhafs": "$(compath.py ${envir}/${NET}/${hafs_ver})/${RUN}.${ymd}/${hh}",
        "WORKgraph": "${WORKgraph:-/lfs/h2/emc/ptmp/${USER}/workgraph_${RUN,,}}",
        "COMgraph": "${COMgraph:-/lfs/h2/emc/ptmp/${USER}/comgraph_${RUN,,}}",
        "atcfFile": "${COMhafs}/${stormid}.${ymdh}.${stormModel,,}.trak.atcfunix",
        "machine": "${WHERE_AM_I:-wcoss2}",
        "ADECKgraph": "${ADECKgraph:-/lfs/h2/emc/hur/noscrub/input/abdeck/aid}",
        "BDECKgraph": "${BDECKgraph:-/lfs/h2/emc/hur/noscrub/input/abdeck/btk}",
        "cartopyDataDir": "${cartopyDataDir:-/lfs/h2/emc/hur/noscrub/local/share/cartopy}",
        "work_dir": "${WORKgraph}",
        "archbase": "${COMgraph}/figures",
        "archdir": "${archbase}/RT${yyyy}_${BASIN}/${STORMNM}${STID}/${STORMNM}${STID}.${ymdh}",
        "basin2c": "al",
        "BASIN2C": "AL",
        "BASIN": "NATL",
        "is6Hr": "${is6Hr:-False}",
        "trackOn": "${trackOn:-True}",
        "figTimeLevels": "$(seq 0 42)"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "1",
        "tasks": "240",
        "ntasks_per_code": null,
        "gpus": "",
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "01:00:00",
        "ntasks_per_node": "60",
        "nodes": "4",
        "memory": "500G",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": ""
    },
    "versions": {}
}