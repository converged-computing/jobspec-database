        "ESMF",
        "MAGICS",
        "CDO",
        "NCO",
        "INTEL",
        "IMPI",
        "EMOS",
        "libemos"
    ],
    "environment_variables": {
        "OUTDIR": "DOUT_S_ROOT",
        "AUXDIR": "DOUT_S_ROOT/grid",
        "EXPID": "CASE",
        "CONF": "ORCA1",
        "restart": "RESTART",
        "CDO": "cdo",
        "REBUILD": "./rebuild_nemo",
        "NCPU": "LSB_HOSTS | wc -w",
        "y1": "YYYYMMDD1",
        "y2": "YYYYMMDD2",
        "nproc": "ps -f -u ${USER} | grep ${REBUILD} | grep ${y1} | wc -l",
        "flist": "ls ${EXPID}_*_${y1}_${y2}_*grid*0000.nc 2> /dev/null | wc -w",
        "thisfile": "ff | sed -e \"s/_0000.nc//\"",
        "isscal": "0",
        "npes": "ls ${thisfile}_[0-9][0-9][0-9][0-9].nc | wc -w",
        "fname": "${OUTDIR}/${EXPID}_1m_${y1}_${y2}_grid_T.nc",
        "nlon": "CDO griddes ${fname} | grep -i xsize | cut -d'=' -f2 | tr -d '[:blank:]'",
        "meshmask": "${EXPID}_mesh_mask.nc",
        "v": "tmaskutil umaskutil vmaskutil",
        "grid": "v | cut -c1 | tr '[:lower:]' '[:upper:]'",
        "g": "v | tr '[:lower:]' '[:upper:]'",
        "m": "e1 e2 e3"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": "REBPROC",
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "01:30",
        "ntasks_per_node": null,
        "nodes": null,
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": "span[ptile=16]"
    },
    "versions": {
        "COMPILERVER": "COMPILERVER"
    }
}