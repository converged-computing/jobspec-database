{
    "application": "mpiexec",
    "software": [
        "python",
        "mpi4py",
        "anaconda3",
        "openmpi"
    ],
    "modules": [
        "apps/anaconda3/5.2.0",
        "mpi/openmpi/1.10.7/gcc-5.5.0"
    ],
    "environment_variables": {
        "SLURM_JOB_NAME": "SIR_new",
        "SLURM_JOB_ID": "$SLURM_JOB_ID",
        "SLURM_JOB_USER": "$SLURM_JOB_USER",
        "SLURM_ARRAY_TASK_ID": "$SLURM_ARRAY_TASK_ID",
        "SLURM_SUBMIT_DIR": "./",
        "TMPDIR": "$TMPDIR",
        "SLURM_SUBMIT_HOST": "$SLURM_SUBMIT_HOST",
        "SLURM_JOB_PARTITION": "bighyp",
        "SLURM_JOB_NODELIST": "$SLURM_JOB_NODELIST",
        "HOSTNAME": "$HOSTNAME",
        "SLURM_JOB_NUM_NODES": "$SLURM_JOB_NUM_NODES",
        "SLURM_NNODES": "$SLURM_NNODES",
        "SLURM_NTASKS": "$SLURM_NTASKS",
        "SLURM_TASKS_PER_NODE": "$SLURM_TASKS_PER_NODE",
        "SLURM_NTASKS_PER_NODE": "32",
        "SLURM_CPUS_PER_TASK": "1",
        "SLURM_PRIO_PROCESS": "$SLURM_PRIO_PROCESS"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "1",
        "tasks": "$SLURM_NTASKS",
        "ntasks_per_code": null,
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": true,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "72:00:00",
        "ntasks_per_node": "32",
        "nodes": "$SLURM_JOB_NUM_NODES",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": ""
    },
    "versions": {
        "anaconda3": "5.2.0",
        "openmpi": "1.10.7"
    }
}