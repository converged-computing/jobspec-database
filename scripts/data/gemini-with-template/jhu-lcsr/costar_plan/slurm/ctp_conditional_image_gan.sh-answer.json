{
    "application": "slurm",
    "software": [
        "tensorflow",
        "costar_plan",
        "ctp_model_tool"
    ],
    "modules": [
        "tensorflow/cuda-8.0/r1.3"
    ],
    "environment_variables": {
        "DATASET": "ctp_dec",
        "train_image_encoder": "false",
        "train_image_encoder_gan": "false",
        "train_conditional_gan": "true",
        "learning_rate": "$1",
        "dropout": "$2",
        "optimizer": "$3",
        "noise_dim": "$4",
        "loss": "$5",
        "MODELDIR": "$HOME/.costar/stack_$learning_rate$optimizer$dropout$noise_dim$loss"
    },
    "resources": {
        "gres": "gpu:1",
        "cpus_per_task": "1",
        "tasks": "1",
        "ntasks_per_code": "1",
        "gpus": "1",
        "gpus_per_node": "1",
        "cores_per_socket": "1",
        "gpus_per_task": "1",
        "exclusive": "false",
        "cpus_per_gpu": "1",
        "gpu_type": null,
        "time": "0-48:0:0",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": "8G",
        "sockets_per_node": "1",
        "ntasks_per_socket": "1",
        "mem_per_gpu": "8G",
        "mem_per_cpu": "8G",
        "gres_flags": "gpu:1"
    },
    "versions": {
        "tensorflow": "r1.3"
    }
}