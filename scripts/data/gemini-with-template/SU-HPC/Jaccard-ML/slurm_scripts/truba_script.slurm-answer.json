{
    "application": "jaccard",
    "software": [
        "cmake",
        "gcc",
        "cuda",
        "anaconda"
    ],
    "modules": [
        "centos7.3/comp/cmake/3.18.0",
        "centos7.3/comp/gcc/7",
        "centos7.3/lib/cuda/10.1"
    ],
    "environment_variables": {
        "OMP_NUM_THREADS": "CPU_THREADS",
        "JOB_ID": "${SLURM_JOB_ID}",
        "JACCARD_PATH": "/truba/home/aalabsialjundi/Jaccard-ML/",
        "DATA_PATH": "/truba/home/aalabsialjundi/graphs/",
        "EXPERIMENT_PARAMS": "${JACCARD_PATH}/parameters/experiment.json",
        "AVG": "10",
        "OUTPUT_FILE": "model",
        "CPU_THREADS": "20",
        "NODE": "$(echo -e \"${SLURM_NODELIST}\" | tr -d '[:space:]')",
        "RES_PATH": "${NODE}/",
        "BUILD_FOLDER": "${JACCARD_PATH}/build"
    },
    "resources": {
        "gres": "gpu:1",
        "cpus_per_task": "20",
        "tasks": "1",
        "ntasks_per_code": "1",
        "gpus": "1",
        "gpus_per_node": "1",
        "cores_per_socket": null,
        "gpus_per_task": "1",
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "0-10:00:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "cmake": "3.18.0",
        "gcc": "7",
        "cuda": "10.1"
    }
}