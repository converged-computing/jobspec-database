{
    "application": "synergia",
    "software": [
        "openmpi",
        "hdf5"
    ],
    "modules": [
        "@@setupsh@@"
    ],
    "environment_variables": {
        "OMP_NUM_THREADS": "1",
        "OMP_PLACES": "threads",
        "OMP_PROC_BIND": "spread",
        "SYNERGIA2DIR": "@@synergia2dir@@",
        "MPI_HOME": "/usr/local/openmpi-3.0.1",
        "HDF5_DISABLE_VERSION_CHECK": "2",
        "OMPI_MCA_mpi_warn_on_fork": "0"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "",
        "tasks": "@@numproc@@",
        "ntasks_per_code": "",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "@@walltime@@",
        "ntasks_per_node": "@@procspernode@@",
        "nodes": "@@numnode@@",
        "memory": "",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {}
}