{
    "application": "deepspeed",
    "software": [
        "python",
        "torch",
        "deepspeed",
        "torch.distributed.run",
        "pretrain_gpt.py"
    ],
    "modules": [
        "torch",
        "deepspeed",
        "torch.distributed"
    ],
    "environment_variables": {
        "TRANSFORMERS_CACHE": "$ajs_ALL_CCFRWORK/models",
        "HF_DATASETS_CACHE": "$ajs_ALL_CCFRWORK/datasets",
        "HF_MODULES_CACHE": "$ajs_ALL_CCFRWORK/modules",
        "HF_METRICS_CACHE": "$ajs_ALL_CCFRWORK/metrics",
        "HF_DATASETS_OFFLINE": "1",
        "TRANSFORMERS_OFFLINE": "1",
        "MASTER_ADDR": "$(scontrol show hostnames $SLURM_JOB_NODELIST | head -n 1)",
        "MASTER_PORT": "6000",
        "GPUS_PER_NODE": "8",
        "NNODES": "$SLURM_NNODES",
        "PP_SIZE": "1",
        "TP_SIZE": "2",
        "MICRO_BATCH_SIZE": "16",
        "GLOBAL_BATCH_SIZE": "512",
        "NLAYERS": "32",
        "NHIDDEN": "2560",
        "NHEADS": "32",
        "SEQ_LEN": "2048",
        "SAVE_INTERVAL": "250",
        "TRAIN_SAMPLES": "1_953_125",
        "LR_DECAY_SAMPLES": "1_953_125",
        "LR_WARMUP_SAMPLES": "183_105",
        "LAUNCHER": "python -u -m torch.distributed.run \\n    --nproc_per_node $GPUS_PER_NODE \\n    --nnodes $NNODES \\n    --rdzv_endpoint $MASTER_ADDR:$MASTER_PORT \\n    --rdzv_backend c10d \\n    --max_restarts 0 \\n    --tee 3 \\n    ",
        "CMD": " \\n    `pwd`/pretrain_gpt.py \\n    --tensor-model-parallel-size $TP_SIZE \\n    --pipeline-model-parallel-size $PP_SIZE \\n    $GPT_ARGS \\n    $OUTPUT_ARGS \\n    $MUP_ARGS \\n    --save $CHECKPOINT_PATH \\n    --load $CHECKPOINT_PATH \\n    --data-path $DATA_PATH \\n    --data-impl mmap \\n    --distributed-backend nccl \\n     $DEEPSPEED_ARGS \\n    ",
        "CUDA_LAUNCH_BLOCKING": "1",
        "TORCHELASTIC_ERROR_FILE": "/tmp/torch-elastic-error.json"
    },
    "resources": {
        "gres": "gpu:8",
        "cpus_per_task": "16",
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": "8",
        "gpus_per_node": "8",
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": "a100",
        "time": "05:00:00",
        "ntasks_per_node": "1",
        "nodes": "8",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": ""
    },
    "versions": {}
}