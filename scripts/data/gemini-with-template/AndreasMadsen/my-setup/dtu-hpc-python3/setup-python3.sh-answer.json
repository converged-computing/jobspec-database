{
    "application": "pbs",
    "software": [
        "python3",
        "gcc",
        "cuda",
        "cudnn",
        "qt",
        "boost",
        "numpy",
        "scipy",
        "matplotlib",
        "pyqt5",
        "basemap",
        "scikit-learn",
        "pandas",
        "mako",
        "ctags",
        "pyopencl",
        "pydot",
        "pyparsing",
        "cmake",
        "libgpuarray",
        "theano",
        "lasagne",
        "bazel",
        "wheel",
        "tensorflow"
    ],
    "modules": [
        "python3",
        "gcc/4.9.2",
        "cuda/$CUDA_VERSION",
        "cudnn/v$CUDNN_VERSION-prod",
        "qt",
        "boost"
    ],
    "environment_variables": {
        "HOME": "$PBS_O_WORKDIR",
        "CC": "gcc -w",
        "CXX": "g++ -w",
        "CUDA_VERSION": "8.0",
        "CUDNN_VERSION": "5.1",
        "CUDA_PATH": "/appl/cuda/${CUDA_VERSION}",
        "CUDNN_PATH": "/appl/cudnn/v${CUDNN_VERSION}-prod",
        "PATH": "$HOME/bin:$PATH",
        "LD_LIBRARY_PATH": "$HOME/lib:$LD_LIBRARY_PATH",
        "PYTHONPATH": "",
        "PYTHON_BIN_PATH": "`which python3`",
        "CC_OPT_FLAGS": "-march=sandybridge -w",
        "TF_NEED_GCP": "0",
        "TF_NEED_HDFS": "0",
        "TF_NEED_CUDA": "1",
        "TF_NEED_JEMALLOC": "0",
        "TF_ENABLE_XLA": "0",
        "TF_NEED_OPENCL": "0",
        "GCC_HOST_COMPILER_PATH": "`which gcc`",
        "TF_CUDA_VERSION": "$CUDA_VERSION",
        "CUDA_TOOLKIT_PATH": "$CUDA_PATH",
        "TF_CUDNN_VERSION": "`echo $CUDNN_VERSION | head -c 1`",
        "CUDNN_INSTALL_PATH": "$CUDNN_PATH",
        "TF_CUDA_COMPUTE_CAPABILITIES": "3.5,3.7,6.1"
    },
    "resources": {
        "gres": "gpus=1",
        "cpus_per_task": "4",
        "tasks": "1",
        "ntasks_per_code": "1",
        "gpus": "1",
        "gpus_per_node": "1",
        "cores_per_socket": null,
        "gpus_per_task": "1",
        "exclusive": null,
        "cpus_per_gpu": "4",
        "gpu_type": "k40",
        "time": "01:30:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}