{
    "application": "slurm",
    "software": [
        "shifter",
        "python",
        "mpi",
        "mpich-cle6"
    ],
    "modules": [
        "mpich-cle6"
    ],
    "environment_variables": {
        "KMP_AFFINITY": "disabled",
        "MPICH_GNI_FORK_MODE": "FULLCOPY",
        "MKL_NUM_THREADS": "1",
        "OMP_NUM_THREADS": "1",
        "XDG_CONFIG_HOME": "/dev/shm",
        "PYTHONPATH": "./mpi:$PYTHONPATH",
        "rowstart": "100",
        "name_for_run": "cosmos_new_seed",
        "dataset": "cosmos",
        "nobj": "50",
        "threads": "16",
        "cosmos_section": "$1",
        "rsdir": "rs${rowstart}_cosmos${cosmos_section}",
        "topdir": "$CSCRATCH/Obiwan/dr9_LRG/obiwan_out/",
        "outdir": "$CSCRATCH/Obiwan/dr9_LRG/obiwan_out/$name_for_run/output/${rsdir}"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "16",
        "tasks": "20",
        "ntasks_per_code": "20",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "00:30:00",
        "ntasks_per_node": "20",
        "nodes": "5",
        "memory": "",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {
        "legacypipe": "DR9.7.1"
    }
}