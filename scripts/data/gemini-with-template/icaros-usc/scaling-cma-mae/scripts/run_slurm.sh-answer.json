{
    "application": "slurm",
    "software": [
        "dask",
        "singularity",
        "python"
    ],
    "modules": [],
    "environment_variables": {
        "PROJECT_DIR": "/project",
        "SINGULARITY_OPTS": "--cleanenv --no-home --bind $PWD --bind ${PROJECT_DIR}:/project",
        "CONFIG": "$1",
        "SEED": "$2",
        "HPC_CONFIG": "$3",
        "DRY_RUN": "",
        "RELOAD_ARG": "",
        "HPC_SLURM_ACCOUNT": "account_123",
        "HPC_SLURM_TIME": "20:00:00",
