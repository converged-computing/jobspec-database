{
    "application": "cuquantum",
    "software": [
        "apptainer",
        "python"
    ],
    "modules": [
        "containers/apptainer"
    ],
    "environment_variables": {
        "USER": "whoami",
        "CTNR": "/global/arcsingularity/cuquantum-appliance_22.07-cirq.sif",
