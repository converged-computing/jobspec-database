{
    "application": "scalbo",
    "software": [
        "python",
        "mpiexec",
        "postgres",
        "optuna",
        "deephyper"
    ],
    "modules": [
        "../../../../build/activate-dhenv.sh",
        "set_affinity_gpu_polaris.sh",
        "profile_gpu_polaris.sh"
    ],
    "environment_variables": {
        "problem": "dhb_combo",
        "OPTUNA_N_OBJECTIVES": "3",
        "search": "OPT-TPE",
        "pruning_strategy": "NONE",
        "timeout": "10800",
        "random_state": "42",
        "lower_bounds": "0.85,None,None",
        "DEEPHYPER_BENCHMARK_MOO": "1",
        "NDEPTH": "16",
        "NRANKS_PER_NODE": "4",
        "NNODES": "`,wc -l < $PBS_NODEFILE`",
        "NTOTRANKS": "`,(( $NNODES * $NRANKS_PER_NODE ))`",
        "OMP_NUM_THREADS": "`,$NDEPTH`",
        "log_dir": "output/$problem-$search-$NNODES-$timeout-$random_state-MOO",
        "OPTUNA_DB_DIR": "$log_dir/optunadb",
        "OPTUNA_DB_HOST": "$HOST",
        "GPUSTAT_LOG_DIR": "$PBS_O_WORKDIR/$log_dir"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "",
        "tasks": "`,$NTOTRANKS`",
        "ntasks_per_code": "`,$NRANKS_PER_NODE`",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "03:10:00",
        "ntasks_per_node": "`,$NRANKS_PER_NODE`",
        "nodes": "`,$NNODES`",
        "memory": "",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {}
}