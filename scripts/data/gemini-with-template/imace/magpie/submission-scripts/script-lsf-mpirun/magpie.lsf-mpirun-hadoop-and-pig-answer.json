{
    "application": "magpie",
    "software": [
        "hadoop",
        "hbase",
        "phoenix",
        "pig",
        "mahout",
        "spark",
        "storm",
        "tachyon",
        "zookeeper"
    ],
    "modules": [],
    "environment_variables": {
        "MAGPIE_SUBMISSION_TYPE": "lsfmpirun",
        "MAGPIE_SCRIPTS_HOME": "${HOME}/magpie",
        "MAGPIE_LOCAL_DIR": "/tmp/${USER}/magpie",
        "MAGPIE_JOB_TYPE": "pig",
        "MAGPIE_SCRIPT_PATH": "${HOME}/my-job-script",
        "MAGPIE_STARTUP_TIME": "30",
        "MAGPIE_SHUTDOWN_TIME": "30",
        "MAGPIE_PRE_JOB_RUN": "${HOME}/magpie-my-pre-job-script",
        "MAGPIE_POST_JOB_RUN": "${HOME}/magpie-my-post-job-script",
        "MAGPIE_ENVIRONMENT_VARIABLE_SCRIPT": "${HOME}/my-job-env",
        "MAGPIE_ENVIRONMENT_VARIABLE_SCRIPT_SHELL": "/bin/bash",
        "MAGPIE_REMOTE_CMD": "ssh",
        "MAGPIE_REMOTE_CMD_OPTS": "",
        "JAVA_HOME": "/usr/lib/jvm/jre-1.7.0-oracle.x86_64/",
        "HADOOP_SETUP": "yes",
        "HADOOP_SETUP_TYPE": "MR2",
        "HADOOP_VERSION": "2.7.1",
        "HADOOP_HOME": "${HOME}/hadoop-${HADOOP_VERSION}",
        "HADOOP_LOCAL_DIR": "/tmp/${USER}/hadoop",
        "HADOOP_CONF_FILES": "${HOME}/myconf",
        "HADOOP_DAEMON_HEAP_MAX": "2000",
        "HADOOP_NAMENODE_DAEMON_HEAP_MAX": "2000",
        "HADOOP_ENVIRONMENT_EXTRA_PATH": "${HOME}/hadoop-my-environment",
        "HADOOP_MODE": "launch",
        "HADOOP_MAX_TASKS_PER_NODE": "8",
        "HADOOP_DEFAULT_MAP_TASKS": "8",
        "HADOOP_DEFAULT_REDUCE_TASKS": "8",
        "HADOOP_MAX_MAP_TASKS": "8",
        "HADOOP_MAX_REDUCE_TASKS": "8",
        "HADOOP_CHILD_HEAPSIZE": "2048",
        "HADOOP_CHILD_MAP_HEAPSIZE": "2048",
        "HADOOP_CHILD_REDUCE_HEAPSIZE": "4096",
        "HADOOP_CHILD_MAP_CONTAINER_BUFFER": "256",
        "HADOOP_CHILD_REDUCE_CONTAINER_BUFFER": "512",
        "HADOOP_MAPREDUCE_SLOWSTART": "0.05",
        "YARN_RESOURCE_MEMORY": "32768",
        "HADOOP_COMPRESSION": "yes",
        "HADOOP_IO_SORT_FACTOR": "10",
        "HADOOP_IO_SORT_MB": "100",
        "HADOOP_PARALLEL_COPIES": "10",
        "HADOOP_FILESYSTEM_MODE": "hdfsoverlustre",
        "HADOOP_HDFS_PATH": "/ssd/${USER}/hdfs",
        "HADOOP_HDFS_PATH_CLEAR": "yes",
        "HADOOP_HDFSOVERLUSTRE_PATH": "/lustre/${USER}/hdfsoverlustre/",
        "HADOOP_HDFSOVERLUSTRE_REMOVE_LOCKS": "yes",
        "HADOOP_HDFSOVERNETWORKFS_PATH": "/networkfs/${USER}/hdfsovernetworkfs/",
        "HADOOP_HDFSOVERNETWORKFS_REMOVE_LOCKS": "yes",
        "HADOOP_RAWNETWORKFS_PATH": "/lustre/${USER}/rawnetworkfs/",
        "HADOOP_LOCALSTORE": "/ssd/${USER}/localstore/",
        "HADOOP_PER_JOB_HDFS_PATH": "yes",
        "HADOOP_HDFS_BLOCKSIZE": "134217728",
        "HADOOP_HDFS_REPLICATION": "3",
        "HADOOP_RAWNETWORKFS_BLOCKSIZE": "33554432",
        "PIG_SETUP": "yes",
        "PIG_VERSION": "0.15.0",
        "PIG_HOME": "${HOME}/pig-${PIG_VERSION}",
        "PIG_CONF_FILES": "${HOME}/myconf",
        "PIG_LOCAL_DIR": "/tmp/${USER}/pig",
        "PIG_MODE": "testpig",
        "PIG_SCRIPT_PATH": "${HOME}/my-pig-script",
        "PIG_SCRIPT_OPTS": "-l ${HOME}/mypig.log",
        "PIG_CLASSPATH": "${HOME}/myjarfiles.jar",
        "PIG_OPTS": "-Djava.io.tmpdir=${PIG_LOCAL_JOB_DIR}/tmp",
        "MPIRUN_OPTIONS": "-genvall -genv MV2_USE_APM 0"
    },
    "resources": {
        "gres": "",
