{
    "application": "pbs",
    "software": [
        "JGODAS2_ANALYSIS"
    ],
    "modules": [
        "gcc",
        "libjpeg",
        "envvar",
        "PrgEnv-intel",
        "craype",
        "intel",
        "cray-mpich",
        "cray-pals",
        "w3emc",
        "prod_envir",
        "prod_util",
        "w3nco",
        "bufr",
        "bacio",
        "netcdf",
        "bufr_dump",
        "grib_util"
    ],
    "environment_variables": {
        "HOMEgodas": "/lfs/h2/emc/da/noscrub/${USER}/GODAS/godas.v1.1.0",
        "envir": "prod",
        "NET": "godas",
        "model": "godas2",
        "cyc": "00",
        "job": "godas2_analysis",
        "dsk": "gd2",
        "LOUD": "on",
        "cycle": "t${cyc}z",
        "WPTMP": "/lfs/h2/emc/ptmp",
        "DATA": "${WPTMP}/${USER}/${job}.$$",
        "COMDIR": "${WPTMP}/$USER/com/${NET}/$envir",
        "COMROOT": "/lfs/h2/emc/da/noscrub/${USER}/${envir}/com",
        "COMPATH_ROOT": "/lfs/h1/ops/canned/com",
        "TANK_ROOT": "/lfs/h1/ops/canned/dcom/",
        "COMPATH": "${COMPATH_ROOT}/cdas2",
        "COMINflux": "${COMINflux:-`compath.py ${envir}/com/cdas2/${cdas2_ver}/cdas2`}",
        "COMBASE": "${COMDIR}/${model}",
        "EXECgodas": "${HOMEgodas}/exec",
        "FIXgodas": "${HOMEgodas}/fix",
        "PARMgodas": "${HOMEgodas}/parm",
        "USHgodas": "${HOMEgodas}/ush",
        "UTILP": "${UTILROOT}",
        "UGRIB": "${grib_util_ROOT}",
        "UDUMP": "${HOMEbufr_dump}",
        "utilscript": "${UTILROOT}/ush",
        "utilparm": "${UTILP}/nwprod/util/parm",
        "utilexec": "${grib_util_ROOT}/exec",
        "EXECoverdategrb": "${HOMEgodas}/ush/overdate.grib",
        "EXECcopygb": "${grib_util_ROOT}/bin/copygb",
        "EXECwgrib": "${grib_util_ROOT}/bin/wgrib",
        "EXECcombfr": "${HOMEbufr_dump}/exec/bufr_combfr",
        "HOMEbufr": "${HOMEbufr_dump}",
        "USHbufr": "${HOMEbufr}/ush",
        "SENDECF": "NO",
        "KEEPDATA": "YES"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "32",
        "tasks": "1",
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "00:30:00",
        "ntasks_per_node": null,
        "nodes": "1",
        "memory": "8gb",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": ""
    },
    "versions": {
        "gcc": "${gcc_ver}",
        "libjpeg": "${libjpeg_ver}",
        "envvar": "${envvar_ver}",
        "PrgEnv-intel": "${PrgEnv_intel_ver}",
        "craype": "${craype_ver}",
        "intel": "${intel_ver}",
        "cray-mpich": "${cray_mpich_ver}",
        "cray-pals": "${cray_pals_ver}",
        "w3emc": "${w3emc_ver}",
        "prod_envir": "${prod_envir_ver}",
        "prod_util": "${prod_util_ver}",
        "w3nco": "${w3nco_ver}",
        "bufr": "${bufr_ver}",
        "bacio": "${bacio_ver}",
        "netcdf": "${netcdf_ver}",
        "bufr_dump": "${bufr_dump_ver}",
        "grib_util": "${grib_util_ver}",
        "cdas2": "${cdas2_ver}"
    }
}