{
    "application": "fv3gfs",
    "software": [
        "fv3gfs_driver_grid.sh"
    ],
    "modules": [
        "PrgEnv-intel",
        "cfp-intel-sandybridge/1.1.0"
    ],
    "environment_variables": {
        "res": "96",
        "gtype": "regional",
        "stretch_fac": "1.5",
        "target_lon": "-97.5",
        "target_lat": "35.5",
        "refine_ratio": "3",
        "istart_nest": "27",
        "jstart_nest": "37",
        "iend_nest": "166",
        "jend_nest": "164",
        "halo": "3",
        "home_dir": "$LS_SUBCWD/..",
        "TMPDIR": "/gpfs/hps3/stmp/$LOGNAME/fv3_grid.$gtype",
        "out_dir": "/gpfs/hps3/stmp/$LOGNAME/C${res}",
        "NODES": "1",
        "APRUN": "aprun -n 1 -N 1 -j 1 -d 1 -cc depth",
        "APRUN_SFC": "aprun -j 1 -n 24 -N 24",
        "OMP_NUM_THREADS": "6",
        "OMP_STACKSIZE": "2048m",
        "KMP_AFFINITY": "disabled",
        "machine": "WCOSS_C"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "1",
        "tasks": "1",
        "ntasks_per_code": "1",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "00:30",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": "2400",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {}
}