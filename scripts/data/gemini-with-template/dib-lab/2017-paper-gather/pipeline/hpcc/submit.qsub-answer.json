{
    "application": "snakemake",
    "software": [
        "snakemake"
    ],
    "modules": [
        "anaconda"
    ],
    "environment_variables": {
        "logdir": "hpcc/log",
        "QSUB": "qsub -l nodes=1:ppn={threads} -l walltime={cluster.time} -l mem={cluster.mem} -o $logdir -e $logdir -A ged"
    },
    "resources": {
        "cpus_per_task": "4",
        "time": "4:00:00",
        "memory": "1G",
        "nodes": "1",
        "tasks": "3000"
    },
    "versions": {}
}