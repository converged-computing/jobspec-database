{
    "application": "slurm",
    "software": [
        "python",
        "nrnivmodl"
    ],
    "modules": [
        "SCRATCH",
        "project",
        "haswell"
    ],
    "environment_variables": {
        "MODELNAME": "hh_ball_stick_9param",
        "VERSION": "2",
        "RUNDIR": "runs/${SLURM_JOB_ID}",
        "DSET_NAME": "${MODELNAME}_v$VERSION",
        "NSAMPLES": "500000",
        "stimname": "chirp16a",
        "stimfile": "stims/${stimname}.csv",
        "paramfile": "$RUNDIR/${DSET_NAME}.csv",
        "OUTFILE": "$RUNDIR/${DSET_NAME}_${SLURM_ARRAY_TASK_ID}_${stimname}.h5",
        "args": "--outfile $OUTFILE --stim-file ${stimfile} --param-file ${paramfile}       --model $MODELNAME --num $NSAMPLES --print-every 1000"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": "1",
        "tasks": "64",
        "ntasks_per_code": "1",
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "00:30:00",
        "ntasks_per_node": null,
        "nodes": "1",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}