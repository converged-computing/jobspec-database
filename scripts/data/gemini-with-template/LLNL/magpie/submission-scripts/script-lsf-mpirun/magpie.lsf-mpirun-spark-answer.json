{
    "application": "magpie",
    "software": [
        "Spark",
        "Hadoop",
        "Zookeeper"
    ],
    "modules": [],
    "environment_variables": {
        "JAVA_HOME": "/usr/lib/jvm/jre-1.8.0/",
        "MAGPIE_PYTHON": "/usr/bin/python3",
        "SPARK_SETUP": "yes",
        "SPARK_SETUP_TYPE": "STANDALONE",
        "SPARK_VERSION": "3.5.0-bin-hadoop3",
        "SPARK_HOME": "${HOME}/spark-${SPARK_VERSION}",
        "SPARK_LOCAL_DIR": "/tmp/${USER}/spark",
        "SPARK_CONF_FILES": "${HOME}/myconf",
        "SPARK_WORKER_CORES_PER_NODE": "8",
        "SPARK_WORKER_MEMORY_PER_NODE": "16000",
        "SPARK_WORKER_DIRECTORY": "/tmp/${USER}/spark/work",
        "SPARK_JOB_MEMORY": "2048",
        "SPARK_DRIVER_MEMORY": "2048",
        "SPARK_DAEMON_HEAP_MAX": "2000",
        "SPARK_ENVIRONMENT_EXTRA_PATH": "${HOME}/spark-my-environment",
        "SPARK_JOB": "sparkpi",
        "SPARK_DEFAULT_PARALLELISM": "8",
        "SPARK_MEMORY_FRACTION": "0.6",
        "SPARK_MEMORY_STORAGE_FRACTION": "0.5",
        "SPARK_STORAGE_MEMORY_FRACTION": "0.6",
        "SPARK_SHUFFLE_MEMORY_FRACTION": "0.2",
        "SPARK_RDD_COMPRESS": "true",
        "SPARK_IO_COMPRESSION_CODEC": "lz4",
        "SPARK_DEPLOY_SPREADOUT": "true",
        "SPARK_LOCAL_SCRATCH_DIR": "/lustre/${USER}/sparkscratch/",
        "SPARK_LOCAL_SCRATCH_DIR_CLEAR": "yes",
        "SPARK_NETWORK_TIMEOUT": "120",
        "SPARK_YARN_STAGING_DIR": "file:///lustre/${USER}/sparkStaging/",
        "SPARK_SPARKPI_SLICES": "4",
        "SPARK_SPARKWORDCOUNT_FILE": "file:///path/mywordcountfile",
        "SPARK_SPARKWORDCOUNT_COPY_IN_FILE": "file:///mywordcountinfile",
        "MAGPIE_SCRIPTS_HOME": "${HOME}/magpie",
        "MAGPIE_LOCAL_DIR": "/tmp/${USER}/magpie",
        "MAGPIE_JOB_TYPE": "spark",
        "MAGPIE_SUBMISSION_TYPE": "lsfmpirun",
        "MAGPIE_ONE_TIME_RUN": "yes",
        "MAGPIE_PRE_JOB_RUN": "${MAGPIE_SCRIPTS_HOME}/scripts/pre-job-run-scripts/my-pre-job-script",
        "MAGPIE_POST_JOB_RUN": "${MAGPIE_SCRIPTS_HOME}/scripts/post-job-run-scripts/my-post-job-script",
        "MAGPIE_PRE_EXECUTE_RUN": "${MAGPIE_SCRIPTS_HOME}/scripts/pre-job-run-scripts/my-pre-job-script",
        "MAGPIE_POST_EXECUTE_RUN": "${MAGPIE_SCRIPTS_HOME}/scripts/post-job-run-scripts/my-post-job-script",
        "MAGPIE_ENVIRONMENT_VARIABLE_SCRIPT": "${HOME}/my-job-env",
        "MAGPIE_ENVIRONMENT_VARIABLE_SCRIPT_SHELL": "/bin/bash",
        "MAGPIE_HOSTNAME_CMD": "hostname",
        "MAGPIE_REMOTE_CMD": "ssh",
        "MAGPIE_REMOTE_CMD_OPTS": ""
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "",
        "tasks": "",
        "ntasks_per_code": "",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "",
        "ntasks_per_node": "",
        "nodes": "",
        "memory": "",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {}
}