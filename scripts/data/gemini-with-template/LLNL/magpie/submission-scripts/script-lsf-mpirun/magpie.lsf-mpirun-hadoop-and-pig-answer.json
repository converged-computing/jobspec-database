{
    "application": "magpie",
    "software": [
        "hadoop",
        "pig",
        "zookeeper",
        "lsf"
    ],
    "modules": [],
    "environment_variables": {
        "MAGPIE_SUBMISSION_TYPE": "lsfmpirun",
