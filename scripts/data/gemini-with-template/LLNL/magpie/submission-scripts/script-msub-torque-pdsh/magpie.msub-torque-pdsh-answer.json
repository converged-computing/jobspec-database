{
    "application": "magpie",
    "software": [
        "hadoop",
        "pig",
        "hbase",
        "phoenix",
        "spark",
        "kafka",
        "zeppelin",
        "storm",
        "zookeeper",
        "alluxio",
        "hive",
        "postgres"
    ],
    "modules": [],
    "environment_variables": {
        "MAGPIE_SUBMISSION_TYPE": "msubtorquepdsh",
        "MAGPIE_SCRIPTS_HOME": "${HOME}/magpie",
        "MAGPIE_LOCAL_DIR": "/tmp/${USER}/magpie",
        "MAGPIE_JOB_TYPE": "script",
        "JAVA_HOME": "/usr/lib/jvm/jre-1.8.0/",
        "MAGPIE_PYTHON": "/usr/bin/python3",
        "HADOOP_SETUP": "no",
        "HADOOP_SETUP_TYPE": "MR",
        "HADOOP_VERSION": "3.3.6",
        "HADOOP_HOME": "${HOME}/hadoop-${HADOOP_VERSION}",
        "HADOOP_LOCAL_DIR": "/tmp/${USER}/hadoop",
        "HADOOP_JOB": "terasort",
        "PIG_SETUP": "no",
        "PIG_VERSION": "0.17.0",
        "PIG_HOME": "${HOME}/pig-${PIG_VERSION}",
        "PIG_LOCAL_DIR": "/tmp/${USER}/pig",
        "PIG_JOB": "testpig",
        "HBASE_SETUP": "no",
        "HBASE_VERSION": "1.6.0",
        "HBASE_HOME": "${HOME}/hbase-${HBASE_VERSION}",
        "HBASE_LOCAL_DIR": "/tmp/${USER}/hbase",
        "HBASE_JOB": "performanceeval",
        "HIVE_SETUP": "no",
        "HIVE_VERSION": "2.3.0",
        "HIVE_PORT": "10000",
        "HIVE_HOME": "${HOME}/apache-hive-${HIVE_VERSION}-bin",
        "HIVE_LOCAL_DIR": "/tmp/${USER}/hive",
        "HIVE_METASTORE_DIR": "/lustre/${USER}/hive/database/postgres/data",
        "POSTGRES_HOME": "${HOME}/pgsql",
        "PGPORT": "11115",
        "PGDATA": "/lustre/${USER}/hive/database/postgres/data",
        "HIVE_DB_NAME": "hive_default_db",
        "HIVE_USE_TEZ": "no",
        "TEZ_VERSION": "0.9.1",
        "TEZ_HOME": "${HOME}/apache-tez-${TEZ_VERSION}-bin",
        "HIVE_JOB": "testbench",
        "HIVE_TESTBENCH_DIR": "${HOME}/hive-testbench",
        "HIVE_TESTBENCH_TYPE": "tpch",
        "HIVE_CLI_VERSION": "beeline",
        "HIVE_TESTBENCH_DATA_SIZE": "2",
        "HIVE_TESTBENCH_QUERY_COUNT": "4",
        "PHOENIX_SETUP": "no",
        "PHOENIX_VERSION": "4.14.0-HBase-1.4",
        "PHOENIX_HOME": "${HOME}/apache-phoenix-${PHOENIX_VERSION}-bin",
        "PHOENIX_LOCAL_DIR": "/tmp/${USER}/phoenix",
        "PHOENIX_JOB": "performanceeval",
        "SPARK_SETUP": "no",
        "SPARK_SETUP_TYPE": "STANDALONE",
        "SPARK_VERSION": "3.5.0-bin-hadoop3",
        "SPARK_HOME": "${HOME}/spark-${SPARK_VERSION}",
        "SPARK_LOCAL_DIR": "/tmp/${USER}/spark",
        "SPARK_JOB": "sparkpi",
        "STORM_SETUP": "no",
        "STORM_VERSION": "1.2.3",
        "STORM_HOME": "${HOME}/apache-storm-${STORM_VERSION}",
        "STORM_LOCAL_DIR": "/tmp/${USER}/storm",
        "STORM_JOB": "stormwordcount",
        "ZOOKEEPER_SETUP": "no",
        "ZOOKEEPER_REPLICATION_COUNT": "3",
        "ZOOKEEPER_VERSION": "3.4.14",
        "ZOOKEEPER_HOME": "${HOME}/zookeeper-${ZOOKEEPER_VERSION}",
        "ZOOKEEPER_DATA_DIR": "/lustre/${USER}/zookeeper",
        "ZOOKEEPER_LOCAL_DIR": "/tmp/${USER}/zookeeper",
        "ZOOKEEPER_JOB": "zookeeperruok",
        "ALLUXIO_SETUP": "no",
        "ALLUXIO_VERSION": "2.3.0",
        "ALLUXIO_HOME": "${HOME}/alluxio-${ALLUXIO_VERSION}",
        "ALLUXIO_WORKER_ON_MASTER": "yes",
        "ALLUXIO_LOCAL_DIR": "/tmp/${USER}/alluxio",
        "ALLUXIO_UNDER_FS_DIR": "/lustre/${USER}/alluxio",
        "ALLUXIO_DATA_CLEAR": "yes",
        "ALLUXIO_WORKER_MEMORY_SIZE": "16GB",
        "ALLUXIO_WORKER_TIER0_PATH": "/mnt/ramdisk",
        "ALLUXIO_RAMFS_MOUNT_OPTION": "SudoMount",
        "ALLUXIO_READ_TYPE": "CACHE",
        "ALLUXIO_WRITE_TYPE": "CACHE_THROUGH",
        "ALLUXIO_JOB": "testalluxio"
    },
    "resources": {
        "gres": "",
