{
    "application": "mpiexec",
    "software": [
        "conda",
        "nvidia-cuda-mps-control",
        "mpiexec",
        "python",
        "smiles_regress_transformer_run.py",
        "set_affinity_gpu_polaris.sh",
        "enable_mps_polaris.sh"
    ],
    "modules": [
        "conda/2023-01-10-unstable"
    ],
    "environment_variables": {
        "TF_GPU_ALLOCATOR": "cuda_malloc_async",
        "TF_FORCE_GPU_ALLOW_GROWTH": "true",
        "OMP_NUM_THREADS": "$NTHREADS"
    },
    "resources": {
        "gres": "grand",
        "cpus_per_task": "1",
        "tasks": "$NTOTRANKS",
        "ntasks_per_code": "$NTOTRANKS",
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": "polaris",
        "time": "06:00:00",
        "ntasks_per_node": "$NRANKS_PER_NODE",
        "nodes": "$NNODES",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}