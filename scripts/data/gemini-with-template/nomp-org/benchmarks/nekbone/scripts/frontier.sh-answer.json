{
    "application": "nekbone",
    "software": [
        "nekbone-driver",
        "sbatch",
        "squeue",
        "rocm-smi",
        "date",
        "ulimit",
        "module load PrgEnv-gnu",
        "module load craype-accel-amd-gfx90a",
        "module load cray-mpich",
        "module load rocm",
        "module unload cray-libsci",
        "sleep"
    ],
    "modules": [
        "PrgEnv-gnu",
        "craype-accel-amd-gfx90a",
        "cray-mpich",
        "rocm",
        "cray-libsci"
    ],
    "environment_variables": {
        "MPICH_GPU_SUPPORT_ENABLED": "0",
        "MPICH_OFI_NIC_POLICY": "NUMA"
    },
    "resources": {
        "gres": "gpu:1",
        "cpus_per_task": "8",
        "tasks": "1",
        "ntasks_per_code": "1",
        "gpus": "1",
        "gpus_per_node": "1",
        "cores_per_socket": null,
        "gpus_per_task": "1",
        "exclusive": "true",
        "cpus_per_gpu": "8",
        "gpu_type": "nvme",
        "time": "$time",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": "gpu:1"
    },
    "versions": {}
}