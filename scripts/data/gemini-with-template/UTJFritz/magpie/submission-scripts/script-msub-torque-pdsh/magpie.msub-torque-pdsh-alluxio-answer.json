{
    "application": "magpie",
    "software": [
        "Alluxio",
        "Java",
        "Python",
        "Hadoop",
        "HBase",
        "Pig",
        "Zookeeper",
        "Kafka",
        "Zeppelin",
        "Phoenix",
        "Spark",
        "Storm"
    ],
    "modules": [],
    "environment_variables": {
        "JAVA_HOME": "/usr/lib/jvm/jre-1.8.0/",
        "MAGPIE_PYTHON": "/usr/bin/python",
        "ALLUXIO_SETUP": "yes",
        "ALLUXIO_VERSION": "2.3.0",
        "ALLUXIO_HOME": "${HOME}/alluxio-${ALLUXIO_VERSION}",
        "ALLUXIO_WORKER_ON_MASTER": "yes",
        "ALLUXIO_LOCAL_DIR": "/tmp/${USER}/alluxio",
        "ALLUXIO_UNDER_FS_DIR": "/lustre/${USER}/alluxio",
        "ALLUXIO_DATA_CLEAR": "yes",
        "ALLUXIO_WORKER_MEMORY_SIZE": "16GB",
        "ALLUXIO_WORKER_TIER0_PATH": "/mnt/ramdisk",
        "ALLUXIO_RAMFS_MOUNT_OPTION": "SudoMount",
        "ALLUXIO_READ_TYPE": "CACHE",
        "ALLUXIO_WRITE_TYPE": "CACHE_THROUGH",
        "ALLUXIO_JOB": "testalluxio",
        "MAGPIE_SCRIPTS_HOME": "${HOME}/magpie",
        "MAGPIE_LOCAL_DIR": "/tmp/${USER}/magpie",
        "MAGPIE_JOB_TYPE": "alluxio",
        "MAGPIE_SUBMISSION_TYPE": "msubtorquepdsh",
        "MAGPIE_STARTUP_TIME": "30",
        "MAGPIE_SHUTDOWN_TIME": "30",
        "MAGPIE_ONE_TIME_RUN": "yes",
        "MAGPIE_PRE_JOB_RUN": "${MAGPIE_SCRIPTS_HOME}/scripts/pre-job-run-scripts/my-pre-job-script",
        "MAGPIE_POST_JOB_RUN": "${MAGPIE_SCRIPTS_HOME}/scripts/post-job-run-scripts/my-post-job-script",
        "MAGPIE_PRE_EXECUTE_RUN": "${MAGPIE_SCRIPTS_HOME}/scripts/pre-job-run-scripts/my-pre-job-script",
        "MAGPIE_POST_EXECUTE_RUN": "${MAGPIE_SCRIPTS_HOME}/scripts/post-job-run-scripts/my-post-job-script",
        "MAGPIE_ENVIRONMENT_VARIABLE_SCRIPT": "${HOME}/my-job-env",
        "MAGPIE_ENVIRONMENT_VARIABLE_SCRIPT_SHELL": "/bin/bash",
        "MAGPIE_REMOTE_CMD": "ssh",
        "MAGPIE_REMOTE_CMD_OPTS": ""
    },
    "resources": {
        "gres": null,
        "cpus_per_task": null,
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "<my_time_in_seconds_or_HH:MM:SS>",
        "ntasks_per_node": null,
        "nodes": "<my_node_count>",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "Alluxio": "2.3.0",
        "Java": "1.8.0"
    }
}