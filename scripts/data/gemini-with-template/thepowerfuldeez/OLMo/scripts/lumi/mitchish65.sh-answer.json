{
    "application": "slurm",
    "software": [
        "singularity",
        "python",
        "scripts/run_with_environment.sh",
        "scripts/train.py",
        "configs/mitchish65.yaml"
    ],
    "modules": [],
    "environment_variables": {
        "OLMO_CONTAINER": "llm-lumi-torch21_latest.sif",
