{
    "application": "pytorch",
    "software": [
        "python",
        "torch.distributed.launch",
        "train.py"
    ],
    "modules": [
        "cgpu",
        "pytorch/1.7.0-gpu"
    ],
    "environment_variables": {
        "nproc_per_node": "8",
        "config": "bs2048-warmup-opt",
        "nodes": "scontrol show hostnames $SLURM_JOB_NODELIST",
        "worker_num": "$SLURM_JOB_NUM_NODES",
        "master_node": "${nodes_array[0]}",
        "master_addr": "srun --nodes=1 --ntasks=1 -w $master_node hostname --ip-address"
    },
    "resources": {
        "gres": "gpu",
        "cpus_per_task": "80",
        "tasks": "1",
        "ntasks_per_code": "1",
        "gpus": "8",
        "gpus_per_node": "8",
        "cores_per_socket": null,
        "gpus_per_task": "8",
        "exclusive": null,
        "cpus_per_gpu": "10",
        "gpu_type": null,
        "time": "00:30:00",
        "ntasks_per_node": "1",
        "nodes": "2",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "pytorch": "1.7.0-gpu"
    }
}