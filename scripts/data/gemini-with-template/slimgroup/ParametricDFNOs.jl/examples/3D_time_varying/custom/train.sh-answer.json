{
    "application": "slurm",
    "software": [
        "julia"
    ],
    "modules": [
        "cudnn/8.9.3_cuda12",
        "julia/1.9"
    ],
    "environment_variables": {
        "SLURM_CPU_BIND": "cores",
        "PATH": "$PATH:$HOME/.julia/bin",
        "DFNO_3D_GPU": "1",
        "LD_PRELOAD": "/opt/cray/pe/lib64/libmpi_gtl_cuda.so.0"
    },
    "resources": {
        "gres": "gpu",
        "cpus_per_task": null,
        "tasks": "$2",
        "ntasks_per_code": null,
        "gpus": "$2",
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": "1",
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "15:00:00",
        "ntasks_per_node": null,
        "nodes": "$1",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": "gpu-bind=none"
    },
    "versions": {}
}