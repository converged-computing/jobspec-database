{
    "application": "slurm",
    "software": [
        "torchrun",
        "conda",
        "python",
        "rsync"
    ],
    "modules": [],
    "environment_variables": {
        "PROJECT_NAME": "template-experiment",
        "PROJECT_DIRN": "template_experiment",
        "SCRIPT_PATH": "template_experiment/train.py",
        "ENVNAME": "template-experiment",
        "JOB_ID": "${SLURM_ARRAY_JOB_ID}_${SLURM_ARRAY_TASK_ID}",
        "JOB_LABEL": "${SLURM_JOB_NAME}__${JOB_ID}",
        "CKPT_DIR": "/checkpoint/${SLURM_JOB_USER}/${SLURM_JOB_ID}",
        "CKPT_PTH": "$CKPT_DIR/checkpoint_latest.pt",
        "OUTPUT_DIR": "/scratch/hdd001/home/$SLURM_JOB_USER/checkpoints/template-experiment",
        "JOB_OUTPUT_DIR": "$OUTPUT_DIR/$JOB_LABEL",
        "MASTER_ADDR": "$(scontrol show hostnames \"$SLURM_JOB_NODELIST\" | head -n 1)",
        "MASTER_PORT": "$(( $SLURM_JOB_ID % 16384 + 49152 ))",
        "WORLD_SIZE": "$(($SLURM_NNODES * $SLURM_GPUS_ON_NODE))",
        "NCCL_DEBUG": "INFO",
        "NCCL_IB_DISABLE": "1",
        "NCCL_SOCKET_IFNAME": "bond0",
        "TORCH_NCCL_BLOCKING_WAIT": "1",
        "SEED": "$SLURM_ARRAY_TASK_ID"
    },
    "resources": {
        "gres": "gpu:1",
        "cpus_per_task": "4",
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": "1",
        "gpus_per_node": "1",
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": "4",
        "gpu_type": null,
        "time": "08:00:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": "10G",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": "10G",
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}