{
    "application": "slurm",
    "software": [
        "python",
        "pip",
        "torch",
        "torchvision",
        "horovod",
        "torchgpipe",
        "pipedream",
        "mpirun",
        "mpicopy",
        "cmake",
        "pillow",
        "pyyaml"
    ],
    "modules": [
        "2019",
        "Python/3.6.6-foss-2019b",
        "Python/3.7.5-foss-2019b",
        "NCCL/2.5.6-CUDA-10.1.243",
        "cuDNN/7.6.5.32-CUDA-10.1.243",
        "OpenMPI/3.1.4-GCC-8.3.0",
        "libyaml/0.2.1-GCCcore-8.3.0",
        "CMake/3.12.1-GCCcore-8.3.0",
        "mpicopy"
    ],
    "environment_variables": {
        "CORES_GPU": "$5",
        "EPOCHS": "3",
        "LOGINTER": "$10",
        "DATADIR": "${TMPDIR}",
        "HOROVOD_CUDA_HOME": "$CUDA_HOME",
        "HOROVOD_CUDA_INCLUDE": "$CUDA_HOME/include",
        "HOROVOD_CUDA_LIB": "$CUDA_HOME/lib64",
        "HOROVOD_NCCL_HOME": "$EBROOTNCCL",
        "HOROVOD_GPU_ALLREDUCE": "NCCL",
        "HOROVOD_GPU_BROADCAST": "NCCL",
        "HOROVOD_WITH_PYTORCH": "1",
        "MPICC": "mpicc",
        "MPICXX": "mpicxx",
        "BATCH_SIZE": "128",
        "MICROBATCHES": "24",
        "NETWORK_BANDWIDTH": "5000000000",
        "PCIE_BANDWIDTH": "32000000000",
        "MEMORY_SIZE": "11000000000",
        "synth": "0",
        "workers": "$5"
    },
    "resources": {
        "gres": "gpu:$3",
        "cpus_per_task": "$5",
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": "$3",
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": "$5",
        "gpu_type": null,
        "time": "$7",
        "ntasks_per_node": "$3",
        "nodes": "$4",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}