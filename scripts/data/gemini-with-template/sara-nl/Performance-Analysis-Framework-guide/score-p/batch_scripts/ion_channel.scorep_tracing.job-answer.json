{
    "application": "gromacs",
    "software": [
        "scorep",
        "gmx_mpi"
    ],
    "modules": [
        "2019",
        "foss/2018b"
    ],
    "environment_variables": {
        "GROMACSINSTALLDIR": "/home/$USER/gromacs-2019.3/install_scorep",
        "GROMACSTESTCASEDIR": "/home/$USER/gromacs_testcase",
        "NNODES": "$SLURM_JOB_NUM_NODES",
        "NPROC": "$SLURM_NTASKS",
        "NTHREADS": "$SLURM_CPUS_PER_TASK",
        "TESTCASE": "ion_channel",
        "INPUTFILENAME": "ion_channel.tpr",
        "RESULTDIR": "${SLURM_SUBMIT_DIR}/${SLURM_JOB_ID}_gromacs_$TESTCASE_${NNODES}N_${NPROC}P_${NTHREADS}T_scorep_tracing",
        "WORKDIR": "/scratch-shared/${USER}/${SLURM_JOB_ID}",
        "FILTERFILENAME": "scorep-gromacs.filt",
        "SCOREP_PROFILING": "true",
        "SCOREP_TRACING": "true",
        "SCOREP_MEM": "1GB",
        "PAPI_METRICS": "PAPI_TOT_INS,PAPI_TOT_CYC,PAPI_REF_CYC,PAPI_SP_OPS,PAPI_DP_OPS,PAPI_VEC_SP,PAPI_VEC_DP"
