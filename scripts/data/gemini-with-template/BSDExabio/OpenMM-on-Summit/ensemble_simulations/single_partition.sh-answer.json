{
    "application": "dask",
    "software": [
        "dask",
        "python3",
        "openmm",
        "conda",
        "jsrun",
        "CUDA",
        "gcc"
    ],
    "modules": [
        "cuda/11.0.3",
        "gcc/11.1.0"
    ],
    "environment_variables": {
        "SRC_DIR": "/path/to/source/dir",
        "RUN_DIR": "/path/to/working/dir/$LSB_JOBID",
        "SCHEDULER_FILE": "${RUN_DIR}/scheduler_file.json",
        "N_NODES": "$N_NODES",
        "N_WORKERS": "$N_WORKERS",
        "N_TASKS": "$N_TASKS"
    },
    "resources": {
        "gres": "gpudefault nvme",
        "cpus_per_task": "1",
        "tasks": "$N_TASKS",
        "ntasks_per_code": "1",
        "gpus": "1",
        "gpus_per_node": "6",
        "cores_per_socket": "36",
        "gpus_per_task": "1",
        "exclusive": null,
        "cpus_per_gpu": "1",
        "gpu_type": "nvme",
        "time": "0:40",
        "ntasks_per_node": "6",
        "nodes": "1500",
        "memory": null,
        "sockets_per_node": "1",
        "ntasks_per_socket": "1",
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "cuda": "11.0.3",
        "gcc": "11.1.0"
    }
}