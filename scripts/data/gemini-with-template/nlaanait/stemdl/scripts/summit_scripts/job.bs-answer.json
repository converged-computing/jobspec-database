{
    "application": "horovod",
    "software": [
        "python",
        "stemdl",
        "nvme_stage",
        "jsrun",
        "horovod",
        "cuda",
        "spectrum-mpi",
        "nccl",
        "pami",
        "anaconda3",
        "tf"
    ],
    "modules": [
        "cuda/9.2.148",
        "spectrum-mpi",
        "darshan-runtime",
        "xalt"
    ],
    "environment_variables": {
        "TF_CPP_MIN_LOG_LEVEL": "2",
        "TF_CUDNN_USE_AUTOTUNE": "1",
        "TF_AUTOTUNE_THRESHOLD": "2",
        "TF_ENABLE_WINOGRAD_NONFUSED": "1",
        "TF_ENABLE_XLA": "0",
        "TF_USE_CUDNN_BATCHNORM_SPATIAL_PERSISTENT": "1",
        "LD_LIBRARY_PATH": "$BUILDS/nccl/lib:$BUILDS/cudnn/lib:${CUDA_DIR}/extras/CUPTI/lib64:$LD_LIBRARY_PATH",
        "PATH": "$BUILDS/anaconda3/bin:$PATH",
        "PYTHONIOENCODING": "utf8",
        "HOROVOD_HIERARCHICAL_ALLREDUCE": "0",
        "HOROVOD_HIERARCHICAL_ALLGATHER": "0",
        "HOROVOD_GROUPED_ALLREDUCES": "1",
        "HOROVOD_CYCLE_TIME": "2",
        "CKPT_DIR": "checkpoints",
        "NROOT": "FCDenseNet_custom",
        "HYPER": "hyper_params.json",
        "INPUT": "input_flags.json",
        "NETWORK": "network_${NROOT}.json",
        "CKPT": "/mnt/bb/${USER}/${NROOT}_${NODES}_${LSB_JOBID}_checkpoint",
        "cpus": "1",
        "ILR": "1.e-3",
        "BN_DECAY": "0.1",
        "EPOCH_PER_DECAY": "5.0",
        "EPOCH_PER_SAVE": "200.0",
        "EPOCH_PER_VALIDATE": "200.0",
        "SCALING": "1.0",
        "FP": "fp16",
        "MODE": "train",
        "MAX_STEPS": "10000",
        "BATCH": "1",
        "DATA": "${PROJWORK}/lrn001/nl/sims/data/lmdb_test",
        "FILETYPE": "lmdb",
        "LOG": "output_logs/printout_${NODES}_${LSB_JOBID}_group_perlayer.log",
        "NVME_PURGE": "0",
        "CMD": "python -u nvme_stage.py $DATA $FILETYPE",
        "SCRIPT": "stemdl_run.py"
    },
    "resources": {
        "gres": "nvme",
        "cpus_per_task": "42",
        "tasks": "256",
        "ntasks_per_code": "1",
        "gpus": "256",
        "gpus_per_node": "1",
        "cores_per_socket": "6",
        "gpus_per_task": "1",
        "exclusive": null,
        "cpus_per_gpu": "42",
        "gpu_type": null,
        "time": "00:15",
        "ntasks_per_node": "1",
        "nodes": "256",
        "memory": null,
        "sockets_per_node": "1",
        "ntasks_per_socket": "1",
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": "smt4 nvme maximizegpfs"
    },
    "versions": {}
}