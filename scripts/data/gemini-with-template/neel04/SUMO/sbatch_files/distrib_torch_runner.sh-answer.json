{
    "application": "slurm",
    "software": [
        "mpirun",
        "srun",
        "torch_runner.sh"
    ],
    "modules": [
        "intelmpi"
    ],
    "environment_variables": {
        "LD_LIBRARY_PATH": "/opt/aws-ofi-nccl/lib:/opt/amazon/efa/lib64:/usr/local/cuda-11.0/efa/lib:/usr/local/cuda-11.0/lib:/usr/local/cuda-11.0/lib64:/usr/local/cuda-11.0:/opt/nccl/build/lib:/opt/aws-ofi-nccl-install/lib:/opt/aws-ofi-nccl/lib:$LD_LIBRARY_PATH",
        "NCCL_PROTO": "simple",
        "PATH": "/opt/amazon/efa/bin:$PATH",
        "LD_PRELOAD": "/opt/nccl/build/lib/libnccl.so",
        "FI_EFA_FORK_SAFE": "1",
        "FI_LOG_LEVEL": "1",
        "FI_EFA_USE_DEVICE_RDMA": "1",
        "NCCL_ALGO": "ring",
        "NCCL_DEBUG": "INFO",
        "NCCL_DEBUG_SUBSYS": "INIT,ENV,GRAPH,COLL",
        "PYTHONFAULTHANDLER": "1",
        "CUDA_LAUNCH_BLOCKING": "0",
        "OMP_NUM_THREADS": "6",
        "OMPI_MCA_mtl_base_verbose": "1",
        "FI_EFA_ENABLE_SHM_TRANSFER": "0",
        "FI_PROVIDER": "efa",
        "FI_EFA_TX_MIN_CREDITS": "64",
        "NCCL_TREE_THRESHOLD": "0",
        "SINGULARITY_OMPI_DIR": "/opt/amazon/openmpi",
        "SINGULARITYENV_APPEND_PATH": "/opt/amazon/openmpi/bin",
        "SINGULAIRTYENV_APPEND_LD_LIBRARY_PATH": "/opt/amazon/openmpi/lib",
        "HOSTNAMES": "scontrol show hostnames \"$SLURM_JOB_NODELIST\"",
        "MASTER_ADDR": "scontrol show hostnames \"$SLURM_JOB_NODELIST\" | head -n 1",
        "MASTER_IP": "hostname -I | cut -d' ' -f1",
        "MASTER_PORT": "8888",
        "COUNT_NODE": "scontrol show hostnames \"$SLURM_JOB_NODELIST\" | wc -l",
        "HOSTNAMES_LIST": "echo $HOSTNAMES | sed 's/ /,/g'"
    },
    "resources": {
        "gres": "gpu",
        "cpus_per_task": null,
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": true,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "7-72:00:00",
        "ntasks_per_node": "1",
        "nodes": null,
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}