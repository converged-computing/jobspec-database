{
    "application": "dask",
    "software": [
        "dask-scheduler",
        "dask-worker",
        "jsrun"
    ],
    "modules": [
        "gcc/7.4.0",
        "cuda/10.1.243",
        "python/3.7.0-anaconda3-5.3.0"
    ],
    "environment_variables": {
        "PROJ_ID": "stf011",
        "DASK_DIR": "$MEMBERWORK/$PROJ_ID/dask",
        "SCHEDULER_FILE": "$DASK_DIR/my-scheduler.json",
        "NUM_WORKERS": "$num_workers",
        "PORT_SCHED": "$PORT_SCHED",
        "PORT_DASH": "$PORT_DASH"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": "1",
        "tasks": "$NUM_WORKERS",
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "1:00",
        "ntasks_per_node": null,
        "nodes": "1",
        "memory": "$MEM_PER_WORKER GB",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": "gpumps"
    },
    "versions": {}
}