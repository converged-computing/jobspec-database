{
    "application": "slurm",
    "software": [
        "python",
        "run_intermediate_expansion.py",
        "conda",
        "wandb"
    ],
    "modules": [],
    "environment_variables": {
        "PYTHONPATH": ".:${PYTHONPATH}",
        "discovery_factors": "(0.1 0.3 1.0)",
        "widen_policy_values": "(0.1 0.2 0.5)",
        "run_number": "$[$SLURM_ARRAY_TASK_ID/9]",
        "hyperparam_number": "$[$SLURM_ARRAY_TASK_ID%9]",
        "discovery_here_idx": "$[$hyperparam_number%3]",
        "widen_here_idx": "$[$hyperparam_number/3]",
        "discovery_here": "${discovery_factors[$discovery_here_idx]}",
        "widen_here": "${widen_policy_values[$widen_here_idx]}",
        "WANDB_USERNAME": "seas",
        "WANDB_PROJECT": "vmcts",
        "WANDB_GROUP": "exploration-sweep",
        "WANDB_NAME": "$run_number/$discovery_here/$widen_here",
        "SEED": "$RANDOM"
    },
    "resources": {
        "gres": "gpu:h100",
        "cpus_per_task": "4",
        "tasks": "190",
        "ntasks_per_code": "1",
        "gpus": "1",
        "gpus_per_node": "1",
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": "h100",
        "time": "24:00:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": "250GB",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
