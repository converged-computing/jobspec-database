{
    "application": "deepspeed",
    "software": [
        "deepspeed",
        "pytorch",
        "torchvision",
        "nccl",
        "python"
    ],
    "modules": [
        "lib/NCCL/2.12.12-GCCcore-11.3.0-CUDA-11.7.0",
        "ai/PyTorch/1.12.0-foss-2022a-CUDA-11.7.0",
        "vis/torchvision/0.13.1-foss-2022a-CUDA-11.7.0"
    ],
    "environment_variables": {
        "CUDA_LAUNCH_BLOCKING": "1",
        "TORCHELASTIC_ERROR_FILE": "${OUTPUT_BASEPATH}/torch-elastic-error.json",
        "NCCL_ASYNC_ERROR_HANDLING": "1",
        "LAUNCHER": "python -u -m torch.distributed.run     --nproc_per_node $GPUS_PER_NODE     --nnodes $NNODES     --rdzv_id=$RANDOM     --rdzv_endpoint $MASTER_ADDR:$MASTER_PORT     --rdzv_backend c10d     --max_restarts 0     --tee 3     ",
        "CMD": "${LIB_DIR}/pretrain_gpt.py ${megatron_options} ${data_options} ${deepspeed_options}"
    },
    "resources": {
        "gres": "gpu:a100:1",
        "cpus_per_task": null,
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": "$SLURM_GPUS_ON_NODE",
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": "a100",
        "time": "00:30:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": ""
    },
    "versions": {
        "deepspeed": null,
        "pytorch": "1.12.0",
        "torchvision": "0.13.1",
        "nccl": "2.12.12"
    }
}