{
    "application": "slurm",
    "software": [
        "python3.6",
        "train.py"
    ],
    "modules": [
        "python-env/intelpython3.6-2018.3",
        "gcc/5.4.0",
        "cuda/9.0",
        "cudnn/7.1-cuda9"
    ],
    "environment_variables": {
        "MODEL_TEMPLATE": "$1",
        "K": "$2",
        "BETAS": "$3",
        "TF_MODE": "$4",
        "BETAS_ARRAY": "($BETAS)",
        "BETA1": "${BETAS_ARRAY[$(($SLURM_ARRAY_TASK_ID * 2))]}",
        "BETA2": "${BETAS_ARRAY[$(($SLURM_ARRAY_TASK_ID * 2 + 1))]}",
        "MODEL": "$(eval echo $MODEL_TEMPLATE)"
    },
    "resources": {
        "gres": "gpu:p100:1",
        "cpus_per_task": "2",
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": "1",
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": "1",
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": "p100",
        "time": "0-3",
        "ntasks_per_node": null,
        "nodes": null,
        "memory": "16GB",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": "gpu:p100:1"
    },
    "versions": {}
}