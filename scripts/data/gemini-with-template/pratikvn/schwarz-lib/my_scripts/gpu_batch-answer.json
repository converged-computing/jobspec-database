{
    "application": "bsub",
    "software": [
        "jsrun",
        "nvprof"
    ],
    "modules": [],
    "environment_variables": {
        "OMP_NUM_THREADS": "1",
        "PAMI_DISABLE_IPC": "1",
        "run_exec": "$EXEC_DIR/benchmarking/bench_ras",
        "run_flags": "--executor=${EXEC} --num_iters=${NUM_ITERS} --timings_file=${TIME_FILE} --partition=${PARTITION} --set_tol=1e-6 --explicit_laplacian --set_1d_laplacian_size=${PROB_SIZE} ${COMM_TYPE_FLAG} --local_solver=${LOCAL_SOLVE} --factor_ordering_natural --local_tol=1e-12 --enable_global_tree_check --enable_put_all_local_residual_norms --enable_global_check"
    },
    "resources": {
        "gres": "gpu",
        "cpus_per_task": "${NUM_PROCS}",
        "tasks": "${NUM_RES_SETS}",
        "ntasks_per_code": "${NUM_PROCS}",
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "02:00",
        "ntasks_per_node": "${NUM_PROCS}",
        "nodes": "$NUM_NODES",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": "-gpu -async"
    },
    "versions": {}
}