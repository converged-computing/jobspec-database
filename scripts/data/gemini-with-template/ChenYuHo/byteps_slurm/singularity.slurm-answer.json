        "cpus_per_task": "1",
        "tasks": "4",
        "ntasks_per_code": "1",
        "gpus": "4",
        "gpus_per_node": "1",
        "cores_per_socket": null,
        "gpus_per_task": "1",
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": "v100",
        "time": "0:4:59",
        "ntasks_per_node": "4",
        "nodes": null,
        "memory": "64g",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": "gpu_ai"
    },
    "versions": {}
