{
    "application": "byteps",
    "software": [
        "singularity",
        "openmpi",
        "python",
        "bpslaunch",
        "bpsrun_singularity",
        "bpsrun_singularity2",
        "mpirun"
    ],
    "modules": [
        "singularity/3.6",
        "openmpi/4.0.3-cuda10.1"
    ],
    "environment_variables": {
        "INTERFACE": "ib0",
        "SINGULARITYENV_DMLC_ENABLE_RDMA": "ibverbs",
        "SINGULARITYENV_DMLC_INTERFACE": "ib0",
        "SINGULARITYENV_DMLC_NUM_WORKER": "NW",
        "SINGULARITYENV_DMLC_NUM_SERVER": "NS",
        "SINGULARITYENV_DMLC_PS_ROOT_URI": "${interface_addr}",
        "SINGULARITYENV_DMLC_PS_ROOT_PORT": "$PORT",
        "SINGULARITYENV_BYTEPS_ENABLE_IPC": "0",
        "SINGULARITYENV_DMLC_ROLE": "scheduler"
    },
    "resources": {
