{
    "application": "mlperf",
    "software": [
        "nvidia-docker",
        "docker",
        "mpirun",
        "bash",
        "python",
        "sync",
        "sudo",
        "sysctl",
        "tar",
        "rm",
        "mkdir",
        "chmod",
        "hostname",
        "sleep",
        "timeout",
        "grep",
        "cat",
        "cp",
        "chown"
    ],
    "modules": [],
    "environment_variables": {
        "DATESTAMP": "date +'%y%m%d%H%M%S%N'",
        "BENCHMARK": "image_classification",
        "BENCHMARK_NAME": "RESNET",
        "CONT": "mlperf-nvidia:$BENCHMARK",
        "DATADIR": "/raid/data/imagenet/train-val-recordio-passthrough",
        "LOGDIR": "/raid/results/$BENCHMARK",
        "SEED": "od -A n -t d -N 3 /dev/urandom",
        "CONTAINER_UID": "0",
        "CONTAINER_GID": "0",
        "NEXP": "1",
        "CLEAR_CACHES": "1",
        "SYSLOGGING": "1",
        "LOG_CLEAR_CACHES": "'from mlperf_compliance import constants, mlperf_log as log; import os; log.mlperf_print(key=constants.CACHE_CLEAR, value=True, benchmark=constants.${BENCHMARK_NAME}, stack_offset=0, root_dir=\"`pwd`\", extra_print=False)'",
        "SYS_LOG_GET": "'import mlperf_compliance; from mlperf_log_utils import mlperf_submission_log; mlperf_submission_log(mlperf_compliance.constants.$BENCHMARK_NAME)'",
        "DGXSYSTEM": "DGX1",
        "IBDEVICES": "$DGXIBDEVICES",
        "INSLURM": "1",
        "SLURM_JOB_ID": "${DATESTAMP}",
        "SLURM_JOB_NUM_NODES": "1",
        "OMPI_COMM_WORLD_LOCAL_RANK": "0",
        "SLURM_NTASKS_PER_NODE": "${DGXNGPU}",
        "LOGFILE_BASE": "${LOGDIR}/${DATESTAMP}",
        "VOLS": "-v $DATADIR:/data -v $LOGDIR:/results",
        "MLPERF_HOST_OS": "cat /etc/issue | head -1 | cut -f1-3 -d\" \" / cat /etc/dgx-release | grep -E \"DGX_PRETTY_NAME|DGX_OTA_VERSION\" |cut -f2 -d= |cut -f2 -d '\"' |paste -sd' '",
        "CONTNAME": "mpi_${SLURM_JOB_ID}",
        "SRUN": "srun --mem=0 -n $SLURM_JOB_NUM_NODES --ntasks-per-node=1",
        "SRUNl": "$SRUN -l",
        "KVSTORE": "horovod",
        "DEBUG": "0",
        "TAGOUTPUT": "--tag-output",
        "MPIRUN": "mpirun --allow-run-as-root --bind-to none $TAGOUTPUT ${VARS[@]//-e/-x} --launch-agent \"docker exec $CONTNAME orted\"",
        "PULL": "0",
        "DOCKERRUN_ARGS": "--rm --net=host --uts=host --ipc=host --ulimit stack=67108864 --ulimit memlock=-1 --security-opt seccomp=unconfined -u $CONTAINER_UID:$CONTAINER_GID $IBDEVICES"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": null,
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": "${DGXNGPU}",
        "cores_per_socket": "20",
        "gpus_per_task": null,
        "exclusive": "True",
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "12:00:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": "0",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}