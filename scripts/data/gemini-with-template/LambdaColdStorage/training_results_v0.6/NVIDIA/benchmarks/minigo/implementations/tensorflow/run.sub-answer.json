{
    "application": "minigo",
    "software": [
        "nvidia-docker",
        "docker",
        "mpirun",
        "orte",
        "openmpi",
        "tensorflow",
        "python",
        "bash",
        "date",
        "od",
        "grep",
        "sed",
        "tr",
        "cut",
        "paste",
        "mkdir",
        "rm",
        "cp",
        "chown",
        "chmod",
        "sync",
        "sysctl",
        "timeout",
        "sleep",
        "tar",
        "cat"
    ],
    "modules": [],
    "environment_variables": {
        "CONT": "mlperf-nvidia:reinforcement",
        "BENCHMARK": "reinforcement",
        "BENCHMARK_NAME": "MINIGO",
        "DATADIR": "/raid/data",
        "LOGDIR": "/raid/results/reinforcement",
        "NEXP": "1",
        "SEED": "generated_random_number",
        "SYSLOGGING": "1",
        "SYS_LOG_GET": "'import mlperf_compliance; from minigo.ml_perf.mlperf_log_utils import mlperf_submission_log; mlperf_submission_log(mlperf_compliance.constants.$BENCHMARK_NAME)'",
        "DGXSYSTEM": "DGX1",
        "IBDEVICES": "defined_in_config_file",
        "INSLURM": "1",
        "SLURM_JOB_ID": "generated_timestamp",
        "SLURM_NNODES": "1",
        "OMPI_COMM_WORLD_LOCAL_RANK": "0",
        "SLURM_NTASKS_PER_NODE": "defined_in_config_file",
        "LOGFILE_BASE": "/raid/results/reinforcement/generated_timestamp",
        "CONTVOLS": "-v /raid/data:/data -v /raid/results:/results",
        "NV_GPU": "defined_in_config_file",
        "DOCKEREXEC": "env NV_GPU=defined_in_config_file nvidia-docker run --init --rm --net=host --uts=host --ipc=host --ulimit stack=67108864 --ulimit memlock=-1 --name=cont_generated_timestamp --security-opt seccomp=unconfined defined_in_config_file",
        "MLPERF_HOST_OS": "defined_in_config_file",
        "SHARED_DIR_EXCHANGE": "/raid/results/reinforcement/scratch/multinode_generated_timestamp",
        "CONTAINER_UID": "0",
        "CONTAINER_GID": "0",
        "MASTER_IP": "hostname",
        "PORT": "random_port_number",
        "SSH": "ssh -q -o UserKnownHostsFile=/dev/null -o StrictHostKeyChecking=no hostname",
        "SRUN": "srun --mem=0 -N 1 -n 1 -w hostname",
        "MULTI_NODE": "defined_in_script",
        "DOCKERENV": "defined_in_script"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "1",
        "tasks": "1",
        "ntasks_per_code": "1",
        "gpus": "defined_in_config_file",
        "gpus_per_node": "defined_in_config_file",
        "cores_per_socket": "20",
        "gpus_per_task": "1",
        "exclusive": "true",
        "cpus_per_gpu": "defined_in_config_file",
        "gpu_type": "defined_in_config_file",
        "time": "12:00:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": "0",
        "sockets_per_node": "1",
        "ntasks_per_socket": "1",
        "mem_per_gpu": "defined_in_config_file",
        "mem_per_cpu": "defined_in_config_file",
        "gres_flags": ""
    },
    "versions": {}
}