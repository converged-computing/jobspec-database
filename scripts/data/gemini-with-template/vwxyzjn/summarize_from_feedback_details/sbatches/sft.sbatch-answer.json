{
    "application": "slurm",
    "software": [
        "poetry",
        "accelerate",
        "deepspeed"
    ],
    "modules": [
        "cuda/12.2"
    ],
    "environment_variables": {
        "WANDB_TAGS": "refactor-chosen-rejected3,no-tag-$(git rev-parse --short HEAD)",
        "MODEL_INDEX": "$(($SLURM_ARRAY_TASK_ID / 4))",
        "SEED_INDEX": "$(($SLURM_ARRAY_TASK_ID % 4))",
        "MODEL": "${MODELS[$MODEL_INDEX]}",
        "SEED": "${SEEDS[$SEED_INDEX]}",
        "LR": "3e-6",
        "REWARD_MODEL_PATH": "models/$MODEL/reward_model_$SEED",
        "SFT_MODEL_PATH": "models/$MODEL/sft_model_$SEED",
        "POLICY_MODEL_PATH": "models/$MODEL/policy_model_$SEED",
        "DPO_POLICY_MODEL_PATH": "models/$MODEL/dpo_policy_model_$SEED",
        "local_rollout_forward_batch_size": "64",
        "gradient_accumulation_steps": "4",
        "local_micro_batch_size": "16",
        "local_eval_batch_size": "8"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "10",
        "tasks": "1",
        "ntasks_per_code": null,
        "gpus": "8",
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": "8",
        "exclusive": true,
        "cpus_per_gpu": "10",
        "gpu_type": null,
        "time": null,
        "ntasks_per_node": null,
        "nodes": null,
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": ""
    },
    "versions": {}
}