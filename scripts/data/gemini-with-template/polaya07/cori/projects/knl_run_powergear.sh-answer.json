{
    "application": "slurm",
    "software": [
        "gearshifft_fftwwrappers",
        "powercap_plot"
    ],
    "modules": [
        "PrgEnv-gnu",
        "cray-fftw",
        "boost"
    ],
    "environment_variables": {
