{
    "application": "gearshifft",
    "software": [
        "gearshifft_fftw",
        "gearshifft_fftwwrappers",
        "powercap_plot"
    ],
    "modules": [
        "PrgEnv-intel",
        "PrgEnv-gnu",
        "cray-fftw",
        "boost"
    ],
    "environment_variables": {
        "BOOST_VER": "1.67.0",
        "FFTW_VER": "3.3.6.5",
        "CURDIR": "$HOME/projects/gearshifft",
        "FEXUNIQUE": "$CURDIR/share/gearshifft/extents_uni.conf",
