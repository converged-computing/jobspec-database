{
    "application": "slurm",
    "software": [
        "python",
        "apptainer",
        "docker",
        "rocm-smi",
        "nvidia-smi"
    ],
    "modules": [
        "cray-python",
        "rocm",
        "open-ce/1.1.3-py38-0",
        "cuda/11.0.2"
