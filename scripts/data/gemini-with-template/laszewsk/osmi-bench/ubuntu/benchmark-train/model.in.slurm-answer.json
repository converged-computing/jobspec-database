        "WITH_CONTAINER": "NONE",
        "CONTAINER_DIR": "../../images",
        "MODELS_DIR": "./models",
        "RESULT_DIR": "pwd",
        "MODEL": "{experiment.model}",
        "CONTAINER": "$CONTAINER_DIR/cloudmesh-tfs-23-10-nv.sif"
    },
    "resources": {
