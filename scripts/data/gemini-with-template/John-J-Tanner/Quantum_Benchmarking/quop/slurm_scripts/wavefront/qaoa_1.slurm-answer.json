        "OMP_NUM_THREADS": "$SLURM_CPUS_PER_TASK",
        "OMP_PLACES": "cores",
        "OMP_PROC_BIND": "close",
        "SLURM_NTASKS": "8",
        "SLURM_CPUS_PER_TASK": "1",
        "SLURM_GPUS": "1"
