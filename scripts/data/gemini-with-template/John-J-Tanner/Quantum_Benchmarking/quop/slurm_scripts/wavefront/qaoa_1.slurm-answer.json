{
    "application": "slurm",
    "software": [
        "wavefront",
        "python3",
        "hipcc",
        "hipTT",
        "hipfort",
        "hipfftND",
        "rocm",
        "craype-accel-amd-gfx90a",
        "MPICH"
    ],
    "modules": [
        "rocm",
        "craype-accel-amd-gfx90a"
