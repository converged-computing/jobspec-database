{
    "application": "python",
    "software": [
        "conda",
        "mpiexec",
        "nvidia-smi",
        "pip",
        "python3"
    ],
    "modules": [
        "conda"
    ],
    "environment_variables": {
        "NCPU_PER_RANK": "getconf _NPROCESSORS_ONLN",
        "ALCF_RESOURCE": "NONE",
        "NRANKS": "wc -l < \"${COBALT_NODEFILE}\"",
        "NGPU_PER_RANK": "nvidia-smi -L | wc -l",
        "NGPUS": "(${NRANKS} * ${NGPU_PER_RANK})",
        "VENV_PREFIX": "2022-07-21",
        "MPI_COMMAND": "which mpirun",
        "MPI_FLAGS": "-x LD_LIBRARY_PATH -x PATH --verbose -n ${NGPUS} -npernode ${NGPU_PER_RANK} --hostfile ${HOSTFILE}",
        "HOSTFILE": "${COBALT_NODEFILE}",
        "NCPUS": "(${NRANKS} * ${NCPU_PER_RANK})",
        "MAIN": "${DIR}/main.py",
        "SET_AFFINITY": "${DIR}/affinity.sh",
        "LOGDIR": "${DIR}/logs",
        "LOGFILE": "${LOGDIR}/${TSTAMP}-${HOST}_ngpu${NGPUS}.log",
        "VENV_DIR": "${ROOT}/venvs/${ALCF_RESOURCE}/${VENV_PREFIX}",
        "NCCL_DEBUG": "INFO",
        "KMP_SETTINGS": "TRUE",
        "OMP_NUM_THREADS": "16",
        "TF_ENABLE_AUTO_MIXED_PRECISION": "1",
        "EXEC": "${MPI_COMMAND} ${MPI_FLAGS} ${SET_AFFINITY} $(which python3) ${MAIN}"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": null,
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": null,
        "ntasks_per_node": null,
        "nodes": null,
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}