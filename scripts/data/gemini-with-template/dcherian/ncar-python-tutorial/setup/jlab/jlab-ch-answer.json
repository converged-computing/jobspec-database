{
    "application": "jupyterlab",
    "software": [
        "jupyter lab",
        "matlab"
    ],
    "modules": [
        "matlab/${MATLAB_VERSION}",
        "py-matlab"
    ],
    "environment_variables": {
        "JOB_ACCOUNT": "/glade/u/apps/ch/opt/usr/bin/getacct.sh",
        "MATLAB_API_PATH": "~/matlab-python/${MATLAB_VERSION}/lib/python${python_version}/site-packages",
        "JUPYTER_RUNTIME_DIR": "${WORKDIR}"
    },
    "resources": {
        "nodes": "${NODES}",
        "cpus_per_task": "${CORES}",
        "memory": "${memory}",
        "time": "${walltime}",
        "gres": ""
    },
    "versions": {
        "MATLAB_VERSION": "${MATLAB_VERSION}"
    }
}