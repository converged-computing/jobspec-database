{
    "application": "mpi",
    "software": [
        "mpirun",
        "epirust"
    ],
    "modules": [
        "${MPI_MODULE}"
    ],
    "environment_variables": {
        "OMP_NUM_THREADS": "$SLURM_CPUS_PER_TASK",
        "EPIRUST_CONFIG_NAME": "1m_100",
        "CURRENT_DATE": "$(date -u | sed -r 's/ +/::/g')",
        "SHARED_DIR": "/home/ubuntu",
        "EPIRUST_OUTPUT_DIR": "${SHARED_DIR}/${EPIRUST_CONFIG_NAME}_${CURRENT_DATE}",
        "EPIRUST_CONFIG": "${SHARED_DIR}/epirust/engine/config/${EPIRUST_CONFIG_NAME}.json",
        "RUST_BACKTRACE": "full"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "2",
        "tasks": "$SLURM_NTASKS",
        "ntasks_per_code": "",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "",
        "ntasks_per_node": "20",
        "nodes": "15",
        "memory": "",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {}
}