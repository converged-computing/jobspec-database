{
    "application": "slurm",
    "software": [
        "R",
        "gcc",
        "java"
    ],
    "modules": [
        "r",
        "gcc/4.8.5",
        "java"
    ],
    "environment_variables": {
        "DEBUGME": "<%= Sys.getenv(\"DEBUGME\") %>"
    },
    "resources": {
        "cpus_per_task": "<%= resources$ncpus %>",
        "memory": "<%= resources$memory %>",
        "time": "<%= ceiling(resources$walltime / 60) %>",
        "qos": "<%= resources$qos %>",
        "account": "<%= resources$account %>",
        "partition": "<%= resources$partition %>"
    },
    "versions": {}
}