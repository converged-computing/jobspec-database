{
    "application": "snakemake",
    "software": [
        "snakemake",
        "conda",
        "python"
    ],
    "modules": [],
    "environment_variables": {
        "log_dir": "$(pwd)",
        "log_file": "logs/metagenotate-analysis.log.txt",
        "num_jobs": "60",
        "latency_wait": "15",
        "restart_times": "10",
        "output_dir": "$(grep \"output_dir\" < config.yaml | grep -v \"#\" | cut -d ' ' -f2 | sed 's/\"//g')",
        "list_files": "$(grep \"list_files\" < config.yaml | grep -v \"#\" | cut -d ' ' -f2 | sed 's/\"//g')",
        "snakemake_file_dir": "${output_dir}/snakemake_files"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "1",
        "tasks": "1",
        "ntasks_per_code": "",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "7-00:00:00",
        "ntasks_per_node": "",
        "nodes": "1",
        "memory": "5G",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {}
}