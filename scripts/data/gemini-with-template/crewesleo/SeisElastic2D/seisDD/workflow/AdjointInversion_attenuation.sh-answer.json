{
    "application": "specfem3d",
    "software": [
        "specfem3d",
        "data_misfit.exe",
        "sum_kernel.exe",
        "xsmooth_sem",
        "optimization.exe",
        "model_update.exe",
        "prepare_data.sh",
        "Adjoint.sh",
        "Adjoint_attenuation.sh"
    ],
    "modules": [],
    "environment_variables": {
        "user": "whoami",
        "SUBMIT_DIR": "SLURM_SUBMIT_DIR",
        "SCRIPTS_DIR": "$package_path/scripts",
        "SUBMIT_RESULT": "$SUBMIT_DIR/RESULTS/$job/Scale${Wscale}_${measurement_list}_${misfit_type_list}",
        "working_path": "$SUBMIT_DIR",
        "WORKING_DIR": "$working_path/$Job_title/specfem/",
        "DISK_DIR": "$working_path/$Job_title/output/",
        "velocity_dir": "$target_velocity_dir",
        "smooth_x": "$(echo $(echo \"$sigma_x $iter $sigma_x_rate\" | awk '{ print $1-($2-1)*$3 }'))",
        "smooth_z": "$(echo $(echo \"$sigma_z $iter $sigma_z_rate\" | awk '{ print $1-($2-1)*$3 }'))",
        "compute_adjoint": "true",
        "step_length": "0.0",
        "file": "$DISK_DIR/data_misfit/search_status.dat",
        "is_cont": "$(awk -v \"line=1\" 'NR==line { print $1 }' $file)",
        "is_done": "$(awk -v \"line=2\" 'NR==line { print $1 }' $file)",
        "is_brak": "$(awk -v \"line=3\" 'NR==line { print $1 }' $file)",
        "optimal_step_length": "$(awk -v \"line=5\" 'NR==line { print $1 }' $file)"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "$NPROC_SPECFEM",
        "tasks": "32",
        "ntasks_per_code": "32",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "03:30:00",
        "ntasks_per_node": "",
        "nodes": "16",
        "memory": "",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {}
}