{
    "application": "specfem3D",
    "software": [
        "specfem3D",
        "data_misfit.exe",
        "sum_kernel.exe",
        "xsmooth_sem",
        "optimization.exe",
        "model_update.exe",
        "Adjoint_attenuation.sh"
    ],
    "modules": [],
    "environment_variables": {
        "user": "whoami",
        "SUBMIT_DIR": "$SLURM_SUBMIT_DIR",
        "SCRIPTS_DIR": "$package_path/scripts",
        "SUBMIT_RESULT": "$SUBMIT_DIR/RESULTS/$job/Scale${Wscale}_${measurement_list}_${misfit_type_list}",
        "working_path": "$SUBMIT_DIR",
        "WORKING_DIR": "$working_path/$Job_title/specfem/",
        "DISK_DIR": "$working_path/$Job_title/output/",
        "smooth_x": "awk '{ print $1-($2-1)*$3 }'",
        "smooth_z": "awk '{ print $1-($2-1)*$3 }'",
        "velocity_dir": "$DISK_DIR/m_current",
        "compute_adjoint": "true",
        "step_length": "0.0",
        "file": "$DISK_DIR/data_misfit/search_status.dat",
        "is_cont": "awk -v \"line=1\" 'NR==line { print $1 }'",
        "is_done": "awk -v \"line=2\" 'NR==line { print $1 }'",
        "is_brak": "awk -v \"line=3\" 'NR==line { print $1 }'",
        "optimal_step_length": "awk -v \"line=5\" 'NR==line { print $1 }'",
        "linearsearch_direction": "0",
        "GPU_MODE": null
    },
    "resources": {
        "gres": null,
        "cpus_per_task": "$NPROC_SPECFEM",
        "tasks": "32",
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "03:00:00",
        "ntasks_per_node": null,
        "nodes": "32",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}