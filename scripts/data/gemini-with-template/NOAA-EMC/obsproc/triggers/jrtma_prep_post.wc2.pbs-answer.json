{
    "application": "obsproc",
    "software": [
        "grib_util",
        "netcdf",
        "intel",
        "craype",
        "cray-mpich",
        "cray-pals",
        "cfp",
        "bufr_dump",
        "prepobs"
    ],
    "modules": [],
    "environment_variables": {
        "envir": "prod",
        "cyc": "%CC%",
        "cycM": "%CCM%",
        "DESC": "%DESC%",
        "JTYP": "%JTYP%",
        "job": "${JTYP}_dump_$cyc",
        "jobid": "$job.$PBS_JOBID",
        "PDY": "%PDY%",
        "obsproc_ver": "v1.0",
        "obsproc_ver_pckg": "v1.0.0",
        "prepobs_ver": "1.0.0",
        "obsNET": "obsproc",
        "PACKAGEROOTpara": "/lfs/h1/ops/para/packages",
        "HOMEobsproc": "${userROOT}/install/obsproc-rmprep",
        "VERSION_FILE": "${HOMEobsproc}/versions/run.ver",
        "SENDECF": "NO",
        "SENDDBN": "NO",
        "SENDSDM": "NO",
        "DATAROOT": "/lfs/h2/emc/stmp/$USER",
        "jlogfile": "/lfs/h2/emc/stmp/${USER}/${JTYP}.${PDY}.jlogfile",
        "COMIN": "${DATAROOT}/CRON/${DESC}/com/${obsNET}/$obsproc_ver/${JTYP}.${PDY}",
        "COMOUT": "${DATAROOT}/CRON/${DESC}/com/${obsNET}/$obsproc_ver/${JTYP}.${PDY}",
        "KEEPDATA": "YES",
        "LOUD": "ON",
        "DEBUG_LEVEL": "3"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": "1",
        "tasks": "1",
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "0:10:00",
        "ntasks_per_node": null,
        "nodes": "1",
        "memory": "1gb",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "grib_util": "${grib_util_ver}",
        "netcdf": "${netcdf_ver}",
        "intel": "${intel_ver}",
        "craype": "${craype_ver}",
        "cray-mpich": "${cray_mpich_ver}",
        "cray-pals": "${cray_pals_ver}",
        "cfp": "${cfp_ver}",
        "bufr_dump": "${bufr_dump_ver}",
        "prepobs": "${prepobs_ver}"
    }
}