{
    "application": "obsproc",
    "software": [
        "bufr_dump",
        "nco"
    ],
    "modules": [
        "libjpeg",
        "grib_util",
        "netcdf",
        "intel",
        "craype",
        "cray-mpich",
        "cray-pals",
        "cfp",
        "bufr_dump"
    ],
    "environment_variables": {
        "envir": "prod",
        "cyc": "%CC%",
        "DESC": "%DESC%",
        "JTYP": "%JTYP%",
        "job": "${JTYP}_dump_$cyc",
        "jobid": "$job.$PBS_JOBID",
        "tmmark": "%TMMARK%",
        "PDY": "%PDY%",
        "userROOT": "/lfs/h2/emc/obsproc/noscrub/$USER",
        "obsproc_ver": "v1.0",
        "obsproc_ver_pckg": "v1.0.0",
        "bufr_dump_ver": "1.0.0",
        "obsNET": "obsproc",
        "PACKAGEROOTpara": "/lfs/h1/ops/para/packages",
        "HOMEobsproc": "${userROOT}/install/obsproc-rmprep",
        "VERSION_FILE": "$HOMEobsproc/versions/run.ver",
        "SENDECF": "NO",
        "SENDDBN": "NO",
        "SENDSDM": "NO",
        "TANK": "$DCOMROOT",
        "DATAROOT": "/lfs/h2/emc/stmp/$USER",
        "jlogfile": "/lfs/h2/emc/stmp/$USER/${JTYP}.$PDY.jlogfile",
        "COMOUT_ROOT": "/lfs/h2/emc/stmp/$USER/CRON/${DESC}/com",
        "COMOUT": "${COMOUT_ROOT}/${obsNET}/${obsproc_ver}/${JTYP}.${PDY}",
        "LOUD": "ON",
        "KEEPDATA": "YES",
        "DEBUG_LEVEL": "3"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "3",
        "tasks": "1",
        "ntasks_per_code": "",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "00:20:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": "6000MB",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {
        "grib_util_ver": "${grib_util_ver}",
        "netcdf_ver": "${netcdf_ver}",
        "intel_ver": "${intel_ver}",
        "craype_ver": "${craype_ver}",
        "cray_mpich_ver": "${cray_mpich_ver}",
        "cray_pals_ver": "${cray_pals_ver}",
        "cfp_ver": "${cfp_ver}",
        "bufr_dump_ver": "${bufr_dump_ver}"
    }
}