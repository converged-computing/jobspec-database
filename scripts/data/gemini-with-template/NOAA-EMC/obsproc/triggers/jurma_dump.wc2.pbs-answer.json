{
    "application": "obsproc",
    "software": [
        "libjpeg",
        "grib_util",
        "netcdf",
        "intel",
        "craype",
        "cray-mpich",
        "cray-pals",
        "cfp",
        "bufr_dump",
        "JOBSPROC_URMA_DUMP"
    ],
    "modules": [
        "libjpeg",
        "grib_util/${grib_util_ver}",
        "netcdf/${netcdf_ver}",
        "intel/${intel_ver}",
        "craype/${craype_ver}",
        "cray-mpich/${cray_mpich_ver}",
        "cray-pals/${cray_pals_ver}",
        "cfp/${cfp_ver}",
        "bufr_dump/${bufr_dump_ver}"
    ],
    "environment_variables": {
        "envir": "prod",
        "cyc": "%CC%",
        "DESC": "%DESC%",
        "JTYP": "%JTYP%",
        "job": "${JTYP}_${cyc}",
        "PDY": "%PDY%",
        "userROOT": "/lfs/h2/emc/obsproc/noscrub/$USER",
        "obsproc_ver": "v1.0",
        "obsproc_ver_pckg": "v1.0.0",
        "bufr_dump_ver": "1.0.0",
        "obsNET": "obsproc",
        "PACKAGEROOTpara": "/lfs/h1/ops/para/packages",
        "HOMEobsproc": "${userROOT}/install/obsproc-rmprep",
        "VERSION_FILE": "${HOMEobsproc}/versions/run.ver",
        "SENDECF": "NO",
        "SENDDBN": "NO",
        "SENDSDM": "NO",
        "TANK": "$DCOMROOT",
        "DATAROOT": "/lfs/h2/emc/stmp/${USER}",
        "jlogfile": "/lfs/h2/emc/stmp/${USER}/${JTYP}.${PDY}.jlogfile",
        "COMOUT_ROOT": "${DATAROOT}/CRON/${DESC}/com",
        "LOUD": "on",
        "DEBUG_LEVEL": "3",
        "KEEPDATA": "YES"
    },
    "resources": {
        "gres": "vscatter",
        "cpus_per_task": "3",
        "tasks": "1",
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "00:15:00",
        "ntasks_per_node": null,
        "nodes": "1",
        "memory": "4gb",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "grib_util_ver": null,
        "netcdf_ver": null,
        "intel_ver": null,
        "craype_ver": null,
        "cray_mpich_ver": null,
        "cray_pals_ver": null,
        "cfp_ver": null,
        "bufr_dump_ver": null
    }
}