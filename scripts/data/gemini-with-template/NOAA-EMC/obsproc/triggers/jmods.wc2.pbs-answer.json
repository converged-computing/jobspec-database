{
    "application": "obsproc",
    "software": [
        "grib_util",
        "netcdf",
        "intel",
        "craype",
        "cray-mpich",
        "cray-pals",
        "cfp",
        "bufr_dump"
    ],
    "modules": [
        "grib_util/${grib_util_ver}",
        "netcdf/${netcdf_ver}",
        "intel/${intel_ver}",
        "craype/${craype_ver}",
        "cray-mpich/${cray_mpich_ver}",
        "cray-pals/${cray_pals_ver}",
        "cfp/${cfp_ver}",
        "bufr_dump/${bufr_dump_ver}"
    ],
    "environment_variables": {
        "envir": "prod",
        "cyc": "%CC%",
        "DESC": "%DESC%",
        "JTYP": "%JTYP%",
        "job": "${JTYP}_${cyc}",
        "PDY": "%PDY%",
        "userROOT": "/lfs/h2/emc/obsproc/noscrub/$USER",
        "obsproc_ver": "v1.0",
        "obsproc_ver_pckg": "v1.0.0",
        "bufr_dump_ver": "1.0.0",
        "obsNET": "obsproc",
        "PACKAGEROOTpara": "/lfs/h1/ops/para/packages",
        "HOMEobsproc": "${userROOT}/install/obsproc-rmprep",
        "VERSION_FILE": "${HOMEobsproc}/versions/run.ver",
        "SENDECF": "NO",
        "SENDDBN": "NO",
        "TANK": "$DCOMROOT",
        "DATAROOT": "/lfs/h2/emc/stmp/${USER}",
        "jlogfile": "/lfs/h2/emc/stmp/${USER}/${JTYP}.${PDY}.jlogfile",
        "COMOUT": "${DATAROOT}/CRON/${DESC}/com/${obsNET}/$obsproc_ver/${JTYP}.${PDY}",
        "MODSDIR": "${DATAROOT}/CRON/${DESC}/dcom/${JTYP}",
        "DEBUG_LEVEL": "3",
        "KEEPDATA": "YES"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "2",
        "tasks": "1",
        "ntasks_per_code": "1",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "1:00:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": "4gb",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {
        "grib_util_ver": "${grib_util_ver}",
        "netcdf_ver": "${netcdf_ver}",
        "intel_ver": "${intel_ver}",
        "craype_ver": "${craype_ver}",
        "cray_mpich_ver": "${cray_mpich_ver}",
        "cray_pals_ver": "${cray_pals_ver}",
        "cfp_ver": "${cfp_ver}",
        "bufr_dump_ver": "${bufr_dump_ver}"
    }
}