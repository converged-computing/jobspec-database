{
    "application": "obsproc",
    "software": [
        "grib_util",
        "netcdf",
        "intel",
        "craype",
        "cray-mpich",
        "cray-pals",
        "cfp",
        "bufr_dump"
    ],
    "modules": [
        "grib_util",
        "netcdf",
        "intel",
        "craype",
        "cray-mpich",
        "cray-pals",
        "cfp",
        "bufr_dump"
    ],
    "environment_variables": {
        "envir": "prod",
        "cyc": "%CC%",
        "DESC": "%DESC%",
        "JTYP": "%JTYP%",
        "job": "${JTYP}_dump_post_$cyc",
        "jobid": "$job.$PBS_JOBID",
        "PDY": "%PDY%",
        "PDYm1": "$(date -d \"${PDY} 1 day ago\" +%Y%m%d)",
        "obsNET": "obsproc",
        "obsproc_ver": "v1.0",
        "obsproc_ver_pckg": "v1.0.0",
        "bufr_dump_ver": "1.0.0",
        "HOMEobsproc": "${userROOT}/install/obsproc-rmprep",
        "VERSION_FILE": "$HOMEobsproc/versions/run.ver",
        "SENDECF": "NO",
        "SENDDBN": "NO",
        "SENDSDM": "NO",
        "TANK": "$DCOMROOT",
        "DATAROOT": "/lfs/h2/emc/stmp/$USER",
        "jlogfile": "/lfs/h2/emc/stmp/$USER/${JTYP}.$PDY.jlogfile",
        "DEBUG_LEVEL": "3",
        "LOUD": "ON",
        "KEEPDATA": "YES",
        "COMPONENT": "atmos",
        "COMIN_ROOT": "/lfs/h2/emc/stmp/$USER/CRON/${DESC}/com",
        "COMOUT_ROOT": "/lfs/h2/emc/stmp/$USER/CRON/${DESC}/com",
        "comin": "${COMIN_ROOT}/${obsNET}/${obsproc_ver}/${JTYP}.${PDY}/${cyc}/${COMPONENT}",
        "comin_m1": "${COMIN_ROOT}/${obsNET}/${obsproc_ver}/${JTYP}.${PDYm1}/${cyc}/${COMPONENT}",
        "COPY": "YES",
        "HOMEarch": "${COMOUT_ROOT}/${obsNET}/${obsproc_ver}/sdm_rtdm/gfs",
        "AVGDarch_IN": "$HOMEarch/avgdata",
        "AVGDarch_OUT": "$HOMEarch/avgdata",
        "OBCNTarch": "$HOMEarch/obcount_30day"
    },
    "resources": {
        "gres": "vscatter",
        "cpus_per_task": "3",
        "tasks": "1",
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "00:15:00",
        "ntasks_per_node": null,
        "nodes": "1",
        "memory": "4gb",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "grib_util_ver": null,
        "netcdf_ver": null,
        "intel_ver": null,
        "craype_ver": null,
        "cray_mpich_ver": null,
        "cray_pals_ver": null,
        "cfp_ver": null,
        "bufr_dump_ver": "1.0.0"
    }
}