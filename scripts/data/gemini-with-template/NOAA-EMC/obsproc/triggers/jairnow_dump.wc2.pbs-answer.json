{
    "application": "obsproc",
    "software": [
        "grib_util",
        "netcdf",
        "bufr_dump",
        "intel",
        "craype",
        "cray-mpich",
        "cray-pals",
        "cfp"
    ],
    "modules": [],
    "environment_variables": {
        "envir": "prod",
        "cyc": "%CC%",
        "JTYP": "%JTYP%",
        "DESC": "%DESC%",
        "job": "${JTYP}_dump_$cyc",
        "jobid": "job.$PBS_JOBID",
        "model": "hourly",
        "PDY": "%PDY%",
        "userROOT": "/lfs/h2/emc/obsproc/noscrub/$USER",
        "obsproc_ver": "v1.0",
        "obsproc_ver_pckg": "v1.0.0",
        "obsNET": "obsproc",
        "PACKAGEROOTpara": "/lfs/h1/ops/para/packages",
        "HOMEobsproc": "${userROOT}/install/obsproc-rmprep",
        "VERSION_FILE": "${HOMEobsproc}/versions/run.ver",
        "SENDECF": "NO",
        "SENDSDM": "NO",
        "SENDDBN": "NO",
        "TANK": "$DCOMROOT",
        "DATAROOT": "/lfs/h2/emc/stmp/$USER",
        "jlogfile": "/lfs/h2/emc/stmp/$USER/${JTYP}_dump.$PDY.jlogfile",
        "COMOUT_ROOT": "${DATAROOT}/CRON/${DESC}/com",
        "COMOUT": "${COMOUT_ROOT}/obsproc/${obsproc_ver}/${model}.${PDY}",
        "DEBUG_LEVEL": "3",
        "KEEPDATA": "YES",
        "LOUD": "ON"
    },
    "resources": {
        "gres": "place=vscatter",
        "cpus_per_task": "3",
        "tasks": "1",
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "00:30:00",
        "ntasks_per_node": null,
        "nodes": "1",
        "memory": "1000MB",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "grib_util_ver": null,
        "netcdf_ver": null,
        "intel_ver": null,
        "craype_ver": null,
        "cray_mpich_ver": null,
        "cray_pals_ver": null,
        "cfp_ver": null,
        "bufr_dump_ver": null
    }
}