{
    "application": "LSF",
    "software": [
        "NWPS"
    ],
    "modules": [
        "prod_envir",
        "prod_util",
        "grib_util/1.0.5",
        "PrgEnv-intel",
        "iobuf",
        "python/3.6.3",
        "cfp-intel-sandybridge"
    ],
    "environment_variables": {
        "NODES": "2",
        "IOBUF_PARAMS": "*:count=4:size=32M:prefetch=1:preflush=1",
        "FORT_BUFFERED": "true",
        "NWPSplatform": "DEVWCOSS",
        "MP_PGMMODEL": "mpmd",
        "MP_CSS_INTERRUPT": "yes",
        "WFO": "%WFO%",
        "siteid": "${WFO}",
        "SITEID": "${WFO^^}",
        "N": "n",
        "KEEPDATA": "YES",
        "model": "nwps",
        "MODEL": "${model^^}",
        "envir": "para",
        "DEVWCOSS_USER": "$(whoami)",
        "COMROOT": "/gpfs/hps3/ptmp/${DEVWCOSS_USER}/data/retro/com",
        "job": "pnwps_${WFO}_fc_cgn_ret",
        "jobid": "pnwps_${WFO}_fc_cgn_ret.$$",
        "jlogfile": "${COMROOT}/logs/${envir}/jlogfile.${job}",
        "WCOSS_SYSTEM": "$( echo $COMROOTp1 | cut -c7 )d1",
        "WCOSS_SYSTEM_COM": "$( echo $COMROOTp1 | cut -c7-9 )",
        "NWROOT": "${NWPSdir}",
        "VERSION_FILE": "${NWROOT}/dev/nwps_test.ver",
        "ECFNAME": "jnwps_prep.ecf"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "",
        "tasks": "",
        "ntasks_per_code": "",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "5:30",
        "ntasks_per_node": "",
        "nodes": "2",
        "memory": "3000",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {
        "nwps_ver": ""
    }
}