{
    "application": "LSF",
    "software": [
        "NWPS",
        "HDF5",
        "NetCDF",
        "python",
        "jasper",
        "png",
        "zlib",
        "nco",
        "cfp",
        "iobuf"
    ],
    "modules": [
        "prod_envir",
        "prod_util",
        "PrgEnv-intel",
        "iobuf",
        "HDF5-serial-intel-haswell/1.8.9",
        "NetCDF-intel-haswell/4.2",
        "python/3.6.3",
        "jasper-gnu-haswell/1.900.1",
        "png-intel-haswell/1.2.49",
        "zlib-intel-haswell/1.2.7",
        "nco-gnu-haswell/4.4.4",
        "cfp-intel-sandybridge"
    ],
    "environment_variables": {
        "NODES": "2",
        "IOBUF_PARAMS": "*:count=4:size=32M:prefetch=1:preflush=1",
        "FORT_BUFFERED": "true",
        "NWPSplatform": "DEVWCOSS",
        "MP_PGMMODEL": "mpmd",
        "MP_CSS_INTERRUPT": "yes",
        "WFO": "%WFO%",
        "siteid": "${WFO}",
        "SITEID": "${WFO^^}",
        "N": "1",
        "KEEPDATA": "YES",
        "model": "nwps",
        "MODEL": "${model^^}",
        "envir": "para",
        "DEVWCOSS_USER": "$(whoami)",
        "COMROOT": "/gpfs/hps3/ptmp/${DEVWCOSS_USER}/data/retro/com",
        "job": "pnwps_${WFO}_fc_cg1_ret",
        "jobid": "pnwps_${WFO}_fc_cg1_ret.$$",
        "jlogfile": "${COMROOT}/logs/${envir}/jlogfile.${job}",
        "WCOSS_SYSTEM": "$( echo $COMROOTp1 | cut -c7 )d1",
        "WCOSS_SYSTEM_COM": "$( echo $COMROOTp1 | cut -c7-9 )",
        "NWROOT": "${NWPSdir}",
        "VERSION_FILE": "${NWROOT}/dev/nwps_test.ver",
        "ECFNAME": "jnwps_prep.ecf"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": null,
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "4:15",
        "ntasks_per_node": null,
        "nodes": "2",
        "memory": "3000",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "NWPS": "${nwps_ver}"
    }
}