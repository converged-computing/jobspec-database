{
    "application": "ncep_post",
    "software": [
        "aprun",
        "module load"
    ],
    "modules": [
        "PrgEnv-intel",
        "ESMF-intel-haswell",
        "cfp-intel-sandybridge",
        "iobuf",
        "craype-hugepages2M",
        "craype-haswell",
        "prod_envir",
        "prod_util",
        "grib_util",
        "crtm-intel"
    ],
    "environment_variables": {
        "NODES": "8",
        "ntasks": "96",
        "ptile": "12",
        "threads": "1",
        "MP_LABELIO": "yes",
        "OMP_NUM_THREADS": "1",
        "APRUN": "aprun -j 1 -n96 -N12 -d1 -cc depth",
        "svndir": "/u/Wen.Meng/noscrubc/ncep_post/post_fv3gfs_flat",
        "hwrf_ver": "v11.0.5",
        "CDATE": "CURRENTDATE",
        "PDY": "CURRENTDATE | cut -c1-8",
        "cyc": "CURRENTDATE | cut -c9-10",
        "cycle": "t${cyc}z",
        "envir": "prod",
        "NET": "gfs",
        "RUN": "RRR",
        "job": "RRR_post_${cyc}",
        "pid": "$$",
        "jobid": "${job}.${pid}",
        "jlogfile": "/gpfs/hps3/ptmp/$LOGNAME/nceppost/logs/jlogfile.${job}.${pid}",
        "DATA": "/gpfs/hps/stmp/$LOGNAME/test/$jobid",
        "SENDCOM": "YES",
        "SENDDBN": "NO",
        "RERUN": "NO",
        "VERBOSE": "YES",
        "HOMEgfs": "${svndir}",
        "COMIN": "$COMINP/${RUN}.${PDY}/${cyc}",
        "COMOUT": "/gpfs/hps3/ptmp/$LOGNAME/com2/gfs/test/${RUN}.$PDY/${cyc}",
        "FIXglobal": "/gpfs/hps3/emc/global/noscrub/emc.glopara/git/fv3gfs/master/global_shared.v15.0.0/fix",
        "FIXgfs": "/gpfs/hps3/emc/global/noscrub/emc.glopara/git/fv3gfs/master/global_shared.v15.0.0/fix",
        "nemsioget": "/gpfs/hps3/emc/global/noscrub/emc.glopara/git/fv3gfs/master/global_shared.v15.0.0/exec/nemsio_get",
        "POSTGRB2TBL": "/gpfs/hps/nco/ops/nwprod/lib/g2tmpl/v1.5.0/src/params_grib2_tbl_new",
        "POSTGPEXEC": "${HOMEgfs}/exec/ncep_post",
        "PARMpost": "${HOMEgfs}/parm",
        "PGBF": "NO",
        "PGB1F": "YES",
        "FLXF": "NO",
        "GOESF": "NO",
        "GTGF": "NO",
        "KEEPDATA": "NO",
        "post_times": "$allfhr"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": "1",
        "tasks": "96",
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "00:30",
        "ntasks_per_node": "12",
        "nodes": "8",
        "memory": "1000",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "prod_util": "1.0.23",
        "grib_util": "1.0.3",
        "crtm-intel": "2.2.5"
    }
}