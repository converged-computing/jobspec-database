{
    "application": "juicer",
    "software": [
        "bwa",
        "awk",
        "sort",
        "java",
        "df",
        "split",
        "zcat",
        "mv",
        "cp",
        "tail",
        "chmod"
    ],
    "modules": [],
    "environment_variables": {
        "juicer_version": "1.5.6",
        "juiceDir": "/opt/juicer",
        "queue": "normal",
        "queue_time": "1200",
        "long_queue": "long",
        "long_queue_time": "3600",
        "splitsize": "90000000",
        "read1str": "_R1",
        "read2str": "_R2",
        "groupname": "a$(date +%s)",
        "topDir": "$(pwd)",
        "site": "MboI",
        "genomeID": "hg19",
        "shortreadend": "0",
        "about": "",
        "nofrag": "1",
        "justexact": "0",
        "splitdir": "${topDir}/splits",
        "donesplitdir": "${topDir}/done_splits",
        "fastqdir": "${topDir}/fastq/*_R*.fastq*",
        "outputdir": "${topDir}/aligned",
        "tmpdir": "${topDir}/HIC_tmp",
        "debugdir": "${topDir}/debug",
        "threads": "$(getconf _NPROCESSORS_ONLN)",
        "threadstring": "-t $threads",
        "alloc_mem": "12000",
        "gzipped": "0",
        "usegzip": "0",
        "name": "",
        "name1": "",
        "name2": "",
        "ext": "",
        "ligation": "GATCGATC",
        "site_file": "${juiceDir}/restriction_sites/${genomeID}_${site}.txt",
        "genomePath": "$genomeID"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": null,
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": null,
        "ntasks_per_node": null,
        "nodes": null,
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}