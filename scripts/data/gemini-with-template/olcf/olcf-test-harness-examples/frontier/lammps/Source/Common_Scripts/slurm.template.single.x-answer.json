{
    "application": "lammps",
    "software": [
        "log_binary_execution_time.py",
        "check_executable_driver.py",
        "test_harness_driver.py",
        "find_n_close_factors.py",
        "setup_env.sh"
    ],
    "modules": [
        "rocm",
        "craype-accel-amd-${RGT_GPU_ARCH}"
    ],
    "environment_variables": {
        "SCRIPTS_DIR": "__scripts_dir__",
        "WORK_DIR": "__working_dir__",
        "RESULTS_DIR": "__results_dir__",
        "HARNESS_ID": "__harness_id__",
        "BUILD_DIR": "__build_dir__",
        "LAMMPS_ATOMS_PER_TASK": "__LAMMPS_ATOMS_PER_TASK__",
        "LAMMPS_EXE": "${BUILD_DIR}/lammps/bin/lmp_${RGT_GPU_ARCH}",
        "LAMMPS_BENCHMARK": "__LAMMPS_BENCHMARK__",
        "RGT_TASKS_PER_NODE": "__RGT_TASKS_PER_NODE__",
        "RGT_GPU_ARCH": "__RGT_GPU_ARCH__",
        "RGT_LAMMPS_THREADS_PER_TASK": "__RGT_LAMMPS_THREADS_PER_TASK__",
        "NO_RFP": "__NO_RFP__",
        "MPICH_GPU_SUPPORT_ENABLED": "0",
        "OMP_NUM_THREADS": "${RGT_LAMMPS_THREADS_PER_TASK}",
        "OMP_PLACES": "threads",
        "OMP_PROC_BIND": "true",
        "LMP_PKG_KOKKOS_CMD": "__LMP_PKG_KOKKOS_CMD__",
        "steps_cmd": "__steps_cmd__",
        "default_systems_per_task": "__default_systems_per_task__",
        "atoms_per_system": "__atoms_per_system__"
    },
    "resources": {
        "gres": "gpu",
        "cpus_per_task": "${RGT_LAMMPS_THREADS_PER_TASK}",
        "tasks": "${RGT_TASKS_PER_NODE}",
        "ntasks_per_code": "${RGT_TASKS_PER_NODE}",
        "gpus": "${RGT_TASKS_PER_NODE}",
        "gpus_per_node": "${RGT_TASKS_PER_NODE}",
        "cores_per_socket": null,
        "gpus_per_task": "1",
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": "${RGT_GPU_ARCH}",
        "time": "__walltime__",
        "ntasks_per_node": "${RGT_TASKS_PER_NODE}",
        "nodes": "__nodes__",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": "gpu:__nodes__"
    },
    "versions": {}
