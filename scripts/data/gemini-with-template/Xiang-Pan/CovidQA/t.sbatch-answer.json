        "gpus_per_node": "2",
        "cores_per_socket": null,
        "gpus_per_task": "2",
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "168:00:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": "32G",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}