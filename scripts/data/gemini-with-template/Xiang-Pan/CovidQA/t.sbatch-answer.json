{
    "application": "slurm",
    "software": [
        "conda",
        "python",
        "roberta_base_ce_2gpu.sh",
        "main_dist.py"
    ],
    "modules": [
        "anaconda3/2020.11",
        "covidqa"
    ],
    "environment_variables": {
        "MASTER": "/bin/hostname -s",
