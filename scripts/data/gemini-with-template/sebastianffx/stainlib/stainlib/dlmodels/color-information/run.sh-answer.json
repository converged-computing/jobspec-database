    "environment_variables": {
        "MPICC": "mpicc",
        "MPICXX": "mpicpc",
        "HOROVOD_MPICXX_SHOW": "mpicxx --showme:link",
        "HOROVOD_WITH_PYTORCH": "1",
        "HOROVOD_CUDA_HOME": "$CUDA_HOME",
        "HOROVOD_CUDA_INCLUDE": "$CUDA_HOME/include",
        "HOROVOD_CUDA_LIB": "$CUDA_HOME/lib64",
        "HOROVOD_NCCL_HOME": "$EBROOTNCCL",
        "HOROVOD_GPU_ALLREDUCE": "NCCL",
        "HOROVOD_GPU_BROADCAST": "NCCL",
        "HOROVOD_WITHOUT_GLOO": "1",
        "HOROVOD_WITHOUT_TENSORFLOW": "1",
        "PATH": "$HOME/virtualenvs/$VENV_NAME/bin:$PATH",
        "LD_LIBRARY_PATH": "$HOME/virtualenvs/$VENV_NAME/lib:$LD_LIBRARY_PATH",
        "CPATH": "$HOME/virtualenvs/$VENV_NAME/include:$CPATH"
