{
    "application": "horovod",
    "software": [
        "python",
        "torch",
        "torchvision",
        "horovod",
        "openslide-python",
        "opencv-python",
        "six",
        "tqdm",
        "Pillow",
        "scikit-learn",
        "scikit-image",
