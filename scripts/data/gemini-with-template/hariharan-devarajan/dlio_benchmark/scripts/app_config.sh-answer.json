{
    "application": "dlio_benchmark",
    "software": [
        "python",
        "aprun",
        "darshan",
        "tensorflow",
        "mpi"
    ],
    "modules": [
        "/soft/datascience/tensorflow/tf2.2-craympi.sh",
        "/soft/perftools/darshan/darshan-3.1.8/lib/libdarshan.so"
    ],
    "environment_variables": {
        "PYTHONPATH": "$DLIO_ROOT:$PYTHONPATH",
        "OMP_NUM_THREADS": "$PROCESS_DISTANCE",
        "DXT_ENABLE_IO_TRACE": "1",
        "LD_PRELOAD": "$DARSHAN_PRELOAD"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": "64",
        "tasks": "$NRANKS",
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "1:00:00",
        "ntasks_per_node": "$RANKS_PER_NODE",
        "nodes": "$COBALT_JOBSIZE",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "tensorflow": "2.2",
        "darshan": "3.1.8"
    }
}