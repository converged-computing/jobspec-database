{
    "application": "dlio_benchmark",
    "software": [
        "aprun",
        "python",
        "darshan",
        "mpich"
    ],
    "modules": [
        "/soft/datascience/tensorflow/tf2.2-craympi.sh"
    ],
    "environment_variables": {
        "MPICH_MAX_THREAD_SAFETY": "multiple",
        "PYTHONPATH": "${DLIO_ROOT}:${PYTHONPATH}",
        "COBALT_JOBSIZE": "1",
        "CURRENT_DIR": "`pwd`",
        "DLIO_ROOT": "`dirname $CURRENT_DIR`",
        "DARSHAN_PRELOAD": "/soft/perftools/darshan/darshan-3.1.8/lib/libdarshan.so",
        "DATA_DIR": "/projects/datascience/dhari/dlio_datasets",
        "APP_DATA_DIR": "${DATA_DIR}/frnn",
        "OMP_NUM_THREADS": "$PROCESS_DISTANCE",
        "DXT_ENABLE_IO_TRACE": "1",
        "LD_PRELOAD": "$DARSHAN_PRELOAD"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": "64",
        "tasks": "$NRANKS",
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "3:00:00",
        "ntasks_per_node": "$RANKS_PER_NODE",
        "nodes": "$COBALT_JOBSIZE",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}