{
    "application": "cleverleaf",
    "software": [
        "cleverleaf-apollo-release",
        "cleverleaf-normal-release",
        "cleverleaf-traced-release",
        "sosd_stop.sh"
    ],
    "modules": [],
    "environment_variables": {
        "EXPERIMENT_JOB_TITLE": "COMPARE.0001.cleverleaf",
        "APPLICATION_RANKS": "1",
        "SOS_AGGREGATOR_COUNT": "1",
        "EXPERIMENT_NODE_COUNT": "2",
        "EXPERIMENT_BASE": "/p/lustre2/${USER}/experiments/apollo",
