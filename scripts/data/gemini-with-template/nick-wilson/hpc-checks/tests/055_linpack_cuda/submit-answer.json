{
    "application": "pbs",
    "software": [
        "intelmpi",
        "mkl",
        "memhog",
        "nvidia-smi",
        "mpirun",
        "xhpl",
        "numastat",
        "dmesg"
    ],
    "modules": [
        "cuda/10.1",
        "intelmpi",
        "mkl"
    ],
    "environment_variables": {
        "PATH": "${PATH}:/app/pbs/bin:/opt/pbs/bin",
        "KMP_AFFINITY": "granularity=fine,compact",
        "MKL_NUM_THREADS": "$CPU_CORES_PER_GPU",
        "MKL_DYNAMIC": "FALSE",
        "LD_LIBRARY_PATH": "$HPL_DIR/lib:$LD_LIBRARY_PATH",
        "CUDA_DGEMM_SPLIT": "0.70",
        "CUDA_DTRSM_SPLIT": "0.60",
        "CPU_CORES_PER_GPU": "24",
        "HPL_DIR": "/app/benchmarks/linpack-cuda-10.1",
        "failmetric": "900.0"
    },
    "resources": {
        "gres": "gpu",
        "cpus_per_task": "24",
        "tasks": "1",
        "ntasks_per_code": "1",
        "gpus": "1",
        "gpus_per_node": "1",
        "cores_per_socket": null,
        "gpus_per_task": "1",
        "exclusive": null,
        "cpus_per_gpu": "24",
        "gpu_type": null,
        "time": "1:00:00",
        "ntasks_per_node": "1",
        "nodes": "${target}",
        "memory": "96gb",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": ""
    },
    "versions": {}
}