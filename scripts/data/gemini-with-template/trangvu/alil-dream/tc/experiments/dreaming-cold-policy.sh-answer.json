{
    "application": "python",
    "software": [
        "ALIL-dreaming.py"
    ],
    "modules": [
        "cuda/9.0",
        "python/3.6.2",
        "tensorflow/1.12.0-python3.6-gcc5"
    ],
    "environment_variables": {
        "CUDA_VISIBLE_DEVICES": "0",
        "CUDA_CACHE_PATH": "/tmp/nv-$DATE",
        "CACHE_PATH": "/tmp/nv-$DATE",
        "ROOT_DIR": "`cd ..&&pwd`",
        "DATE": "`date '+%Y%m%d-%H%M%S'`",
        "SRC_PATH": "$ROOT_DIR",
        "DATA_DIR": "$ROOT_DIR'/datadir'",
        "OUT_DIR": "$ROOT_DIR/results",
        "POLICY_PATH": "$ROOT_DIR'/policy/'${POLICY_PATHS[$index]}",
        "DATASET_NAME": "${DATASETS[$index]}",
        "EXP_NAME": "${EXPS[$index]}",
        "EMBEDING_FILE": "${DATASET_NAME}_w2v.txt",
        "TEXT_DATA_DIR": "$DATA_DIR'/'$DATASET_NAME'/'$EXP_NAME",
        "TEST_SET": "$DATA_DIR'/'$DATASET_NAME'/'$EXP_NAME'_test'",
        "EMB_DIM": "${DIMS[$index]}"
    },
    "resources": {
        "gres": "gpu:1",
        "cpus_per_task": "1",
        "tasks": "1",
        "ntasks_per_code": "1",
        "gpus": "1",
        "gpus_per_node": "1",
        "cores_per_socket": null,
        "gpus_per_task": "1",
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "5-00:00:00",
        "ntasks_per_node": "1",
        "nodes": null,
        "memory": "50000",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": "50000",
        "gres_flags": null
    },
    "versions": {}
}