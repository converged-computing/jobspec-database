{
    "application": "amber",
    "software": [
        "pmemd.cuda"
    ],
    "modules": [
        "amber_gpu",
        "amber_on_ims"
    ],
    "environment_variables": {
        "prefix": "$TEMPLATE_PROJECT_PREFIX",
        "PMEMD": "pmemd.cuda",
        "work": "/local/${PJM_JOBID}[${PJM_BULKNUM}]",
        "run": "${specify_job_id:0:2}",
        "smp": "${specify_job_id:2:1}",
        "output": "$prefix/4nve/output/${run}_${smp}",
        "trajectory_num": "$(((smp+1)*5000000))",
        "restart_file": "$prefix/3sampling/output/$run/sampling/md.rst_${trajectory_num}"
    },
    "resources": {
        "gres": "gpu=1",
        "cpus_per_task": "8",
        "tasks": "1",
        "ntasks_per_code": "1",
        "gpus": "1",
        "gpus_per_node": "1",
        "cores_per_socket": null,
        "gpus_per_task": "1",
        "exclusive": null,
        "cpus_per_gpu": "8",
        "gpu_type": null,
        "time": "01:00:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": "1",
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}