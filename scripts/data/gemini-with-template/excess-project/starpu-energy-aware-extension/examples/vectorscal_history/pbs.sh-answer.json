{
    "application": "starpu",
    "software": [
        "starpu",
        "libmf",
        "intel mkl",
        "mpi",
        "cuda",
        "gcc"
    ],
    "modules": [
        "compiler/intel/parallel_studio_2017",
        "amd/app-sdk/3.0.124.132-GA",
        "mpi/mpich/3.1-gnu-4.9.2",
        "compiler/cuda/7.0",
        "numlib/intel/mkl/11.1",
        "compiler/gnu/4.9.2"
    ],
    "environment_variables": {
        "ROOT": "/nas_home/hpcfapix/starpu-energy-extension",
        "LIB": "/nas_home/hpcfapix/starpu-energy-extension/src",
        "LIB_MF": "/opt/mf/stable/16.6/lib/",
        "PKG_CONFIG_PATH": "${PKG_CONFIG_PATH}:${ROOT}/bin/starpu/lib/pkgconfig",
        "LD_LIBRARY_PATH": "${LD_LIBRARY_PATH}:${ROOT}/bin/starpu/lib:$LIB:${LIB_MF}",
        "DBKEY_FILE": "/nas_home/hpcfapix/.mf/dbkey/${PBS_JOBID}",
        "DBKEY": "$(cat ${DBKEY_FILE})",
        "PBS_USER": "hpcfapix",
        "MF_USER": "hpcfapix",
        "MF_TASKID": "${PBS_JOBNAME}",
        "MF_EXPID": "${DBKEY}",
        "STARPU_NCPU": "1",
        "STARPU_NCUDA": "0",
        "STARPU_NOPENCL": "0",
        "STARPU_SCHED": "dmda",
        "STARPU_CALIBRATE": "1",
        "EXECUTABLE": "${ROOT}/examples/vectorscal_history/vector_scal"
