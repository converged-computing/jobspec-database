{
    "application": "slurm",
    "software": [
        "python",
        "conda",
        "hydra",
        "slurm_launcher",
        "local_launcher",
        "fsps"
    ],
    "modules": [
        "lang/python/anaconda/3.9.7-2021.12-tensorflow.2.7.0"
    ],
    "environment_variables": {
        "SPS_HOME": "$(pwd)/deps/fsps",
        "PYTORCH_CUDA_ALLOC_CONF": "max_split_size_mb:256",
        "NCCL_BLOCKING_WAIT": "1",
        "MASTER_ADDR": "$(hostname | cut -d. -f1)",
        "MASTER_PORT": "${PORTS[0]:-$((32768+RANDOM%28231))}",
        "COMM_PORT": "${PORTS[1]:-$((32768+RANDOM%28231))}"
    },
    "resources": {
        "gres": "A100:1",
        "cpus_per_task": "8",
        "tasks": "4",
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": "1",
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": "A100",
        "time": "0-10:00:00",
        "ntasks_per_node": null,
        "nodes": null,
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": "32G",
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}