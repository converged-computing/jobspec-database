{
    "application": "mpi",
    "software": [
        "python",
        "mpiexec",
        "launcher.sh",
        "main.py"
    ],
    "modules": [
        "darshan/darshan-openmpi-gcc"
    ],
    "environment_variables": {
        "PYTHONPATH": "/home/hzheng/PolarisAT_eagle/dlio_ml_workloads/:$PYTHONPATH",
        "RDMAV_HUGEPAGES_SAFE": "1",
        "LD_PRELOAD": "$HOME//PolarisAT/pyenvs/ml_workload/lib/libdlio_profiler_preload.so",
        "MAX_EPOCHS": "${EPOCHS:-5}",
        "QUALITY_THRESHOLD": "0.908",
        "START_EVAL_AT": "10",
        "EVALUATE_EVERY": "8",
        "LEARNING_RATE": "0.8",
        "LR_WARMUP_EPOCHS": "200",
        "DATASET_DIR": "${DATA_DIR:-\"/eagle/datasets/unet3d/\"}",
        "BATCH_SIZE": "${BATCH_SIZE:-7}",
        "GRADIENT_ACCUMULATION_STEPS": "1",
        "NUM_WORKERS": "${NUM_WORKERS:-4}",
        "SLEEP": "${SLEEP:--1}",
        "OUTPUT_DIR": "${OUTPUT_DIR:-\"results/\"}",
        "NPROC": "${NPROC:-1}",
        "PPN": "${PPN:-4}"
    },
    "resources": {
        "gres": "gpu_a100",
        "cpus_per_task": "64",
        "tasks": "1",
        "ntasks_per_code": "1",
        "gpus": "1",
        "gpus_per_node": "1",
        "cores_per_socket": null,
        "gpus_per_task": "1",
        "exclusive": null,
        "cpus_per_gpu": "64",
        "gpu_type": "a100",
        "time": "1:00:00",
        "ntasks_per_node": "4",
        "nodes": "1",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}