{
    "application": "pbs",
    "software": [
        "JNOS_OFS_AWS",
        "awscli"
    ],
    "modules": [
        "envvar",
        "PrgEnv-intel",
        "craype",
        "intel",
        "cray-pals",
        "libjpeg",
        "python",
        "prod_envir",
        "prod_util",
        "grib_util"
    ],
    "environment_variables": {
        "envir": "dev",
        "OFS": "cbofs",
        "cyc": "12",
        "job": "cbofs_aws_12",
        "platform": "ptmp",
        "PDY": "20210824",
        "KEEPDATA": "YES",
        "SENDCOM": "NO",
        "SENDDBN": "NO",
        "SENDSMS": "NO",
        "PACKAGEROOT": "/lfs/h1/nos/nosofs/noscrub/$LOGNAME/packages",
        "COMPATH": "/lfs/h1/ops/prod/com",
        "COMROOT": "/lfs/h1/nos/ptmp/$LOGNAME/com",
        "DCOMROOT": "/lfs/h1/ops/prod/dcom",
        "DATAROOT": "/lfs/h1/nos/ptmp/$LOGNAME/work/${nosofs_ver}/${OFS}",
        "pbsid": "${PBS_JOBID%%.*}"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": "8",
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "1:30:00",
        "ntasks_per_node": null,
        "nodes": "1",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "nosofs_ver": "${nosofs_ver}",
        "envvars_ver": "${envvars_ver}",
        "PrgEnv_intel_ver": "${PrgEnv_intel_ver}",
        "craype_ver": "${craype_ver}",
        "intel_ver": "${intel_ver}",
        "cray_mpich_ver": "${cray_mpich_ver}",
        "cray_pals_ver": "${cray_pals_ver}",
        "libjpeg_ver": "${libjpeg_ver}",
        "python_ver": "${python_ver}",
        "prod_envir_ver": "${prod_envir_ver}",
        "prod_util_ver": "${prod_util_ver}",
        "grib_util_ver": "${grib_util_ver}"
    }
}