{
    "application": "JNOS_OFS_AWS",
    "software": [
        "cray-pals",
        "cray-mpich",
        "intel",
        "craype",
        "PrgEnv-intel",
        "envvar",
        "prod_util",
        "grib_util",
        "awscli",
        "libjpeg",
        "python",
        "prod_envir"
    ],
    "modules": [
        "cray-pals/${cray_pals_ver}",
        "cray-mpich/${cray_mpich_ver}",
        "intel/${intel_ver}",
        "craype/${craype_ver}",
        "PrgEnv-intel/${PrgEnv_intel_ver}",
        "envvar/${envvars_ver}",
        "libjpeg/${libjpeg_ver}",
        "python/${python_ver}",
        "prod_envir/${prod_envir_ver}",
        "prod_util/${prod_util_ver}",
        "grib_util/${grib_util_ver}",
        "awscli/1.16.308"
    ],
    "environment_variables": {
        "envir": "dev",
        "OFS": "ciofs",
        "cyc": "06",
        "job": "ciofs_aws_06",
        "platform": "ptmp",
        "PDY": "20210824",
        "KEEPDATA": "YES",
        "SENDCOM": "NO",
        "SENDDBN": "NO",
        "SENDSMS": "NO",
        "PACKAGEROOT": "/lfs/h1/nos/nosofs/noscrub/$LOGNAME/packages",
        "COMPATH": "/lfs/h1/ops/prod/com",
        "COMROOT": "/lfs/h1/nos/ptmp/$LOGNAME/com",
        "DCOMROOT": "/lfs/h1/ops/prod/dcom",
        "DATAROOT": "/lfs/h1/nos/ptmp/$LOGNAME/work/${nosofs_ver}/${OFS}",
        "pbsid": "${PBS_JOBID%%.*}"
    },
    "resources": {
        "gres": "place=vscatter",
        "cpus_per_task": "8",
        "tasks": "1",
        "ntasks_per_code": "1",
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "1:30:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "nosofs_ver": "${nosofs_ver}",
        "cray_mpich_ver": "${cray_mpich_ver}",
        "cray_pals_ver": "${cray_pals_ver}",
        "intel_ver": "${intel_ver}",
        "craype_ver": "${craype_ver}",
        "PrgEnv_intel_ver": "${PrgEnv_intel_ver}",
        "envvars_ver": "${envvars_ver}",
        "libjpeg_ver": "${libjpeg_ver}",
        "python_ver": "${python_ver}",
        "prod_envir_ver": "${prod_envir_ver}",
        "prod_util_ver": "${prod_util_ver}",
        "grib_util_ver": "${grib_util_ver}"
    },
    "job_script": "/lfs/h1/nos/nosofs/noscrub/$LOGNAME/packages/nosofs.${nosofs_ver}/jobs/JNOS_OFS_AWS"
}