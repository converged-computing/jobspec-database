{
    "application": "aprun",
    "software": [
        "python",
        "miniconda",
        "mpich"
    ],
    "modules": [
        "miniconda-3/latest"
    ],
    "environment_variables": {
        "KMP_BLOCKTIME": "0",
        "KMP_AFFINITY": "granularity=fine,compact,1,0",
        "MPICH_GNI_FORK_MODE": "FULLCOPY",
        "OMP_NUM_THREADS": "$threads_per_rank",
        "PYTHONPATH": ""
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "$threads_per_core",
        "tasks": "$total_ranks",
        "ntasks_per_code": "$ranks_per_node",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "",
        "ntasks_per_node": "$ranks_per_node",
        "nodes": "$nodes",
        "memory": "",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {}
}