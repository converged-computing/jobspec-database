{
    "application": "magpie",
    "software": [
        "hadoop",
        "hbase",
        "phoenix",
        "zookeeper"
    ],
    "modules": [],
    "environment_variables": {
        "MAGPIE_SCRIPTS_HOME": "${HOME}/magpie",
        "MAGPIE_LOCAL_DIR": "/tmp/${USER}/magpie",
        "MAGPIE_JOB_TYPE": "phoenix",
        "MAGPIE_STARTUP_TIME": "30",
        "MAGPIE_SHUTDOWN_TIME": "30",
        "MAGPIE_ONE_TIME_RUN": "yes",
        "MAGPIE_PRE_JOB_RUN": "${MAGPIE_SCRIPTS_HOME}/scripts/pre-job-run-scripts/my-pre-job-script",
        "MAGPIE_POST_JOB_RUN": "${MAGPIE_SCRIPTS_HOME}/scripts/post-job-run-scripts/my-post-job-script",
        "MAGPIE_PRE_EXECUTE_RUN": "${MAGPIE_SCRIPTS_HOME}/scripts/pre-job-run-scripts/my-pre-job-script",
        "MAGPIE_POST_EXECUTE_RUN": "${MAGPIE_SCRIPTS_HOME}/scripts/post-job-run-scripts/my-post-job-script",
        "MAGPIE_ENVIRONMENT_VARIABLE_SCRIPT": "${HOME}/my-job-env",
        "MAGPIE_ENVIRONMENT_VARIABLE_SCRIPT_SHELL": "/bin/bash",
        "MAGPIE_REMOTE_CMD": "ssh",
        "MAGPIE_REMOTE_CMD_OPTS": "",
        "JAVA_HOME": "/usr/lib/jvm/jre-1.7.0/",
        "HADOOP_SETUP": "yes",
        "HADOOP_SETUP_TYPE": "HDFS",
        "HADOOP_VERSION": "2.9.1",
        "HADOOP_HOME": "${HOME}/hadoop-${HADOOP_VERSION}",
        "HADOOP_LOCAL_DIR": "/tmp/${USER}/hadoop",
        "HADOOP_CONF_FILES": "${HOME}/myconf",
        "HADOOP_DAEMON_HEAP_MAX": "2000",
        "HADOOP_NAMENODE_DAEMON_HEAP_MAX": "2000",
        "HADOOP_ENVIRONMENT_EXTRA_PATH": "${HOME}/hadoop-my-environment",
        "HADOOP_JOB": "terasort",
        "HADOOP_MAX_TASKS_PER_NODE": "8",
        "HADOOP_DEFAULT_MAP_TASKS": "8",
        "HADOOP_DEFAULT_REDUCE_TASKS": "8",
        "HADOOP_CHILD_HEAPSIZE": "2048",
        "HADOOP_CHILD_MAP_HEAPSIZE": "2048",
        "HADOOP_CHILD_REDUCE_HEAPSIZE": "4096",
        "HADOOP_CHILD_MAP_CONTAINER_BUFFER": "256",
        "HADOOP_CHILD_REDUCE_CONTAINER_BUFFER": "512",
        "HADOOP_MAPREDUCE_SLOWSTART": "0.05",
        "YARN_RESOURCE_MEMORY": "32768",
        "YARN_VMEM_CHECK": "false",
        "YARN_PMEM_CHECK": "false",
        "HADOOP_COMPRESSION": "yes",
        "HADOOP_IO_SORT_FACTOR": "10",
        "HADOOP_IO_SORT_MB": "100",
        "HADOOP_PARALLEL_COPIES": "10",
        "HADOOP_TERASORT_SIZE": "50000000",
        "HADOOP_TERAGEN_MAP_COUNT": "4",
        "HADOOP_TERASORT_REDUCER_COUNT": "4",
        "HADOOP_TERASORT_CLEAR_CACHE": "no",
        "HADOOP_TERASORT_OUTPUT_REPLICATION": "1",
        "HADOOP_TERASORT_RUN_TERACHECKSUM": "no",
        "HADOOP_TERASORT_RUN_TERAVALIDATE": "no",
        "HADOOP_DECOMMISSION_HDFS_NODE_SIZE": "8",
        "HADOOP_FILESYSTEM_MODE": "hdfsoverlustre",
        "HADOOP_LOCAL_FILESYSTEM_BLOCKSIZE": "33554432",
        "HADOOP_HDFS_REPLICATION": "3",
        "HADOOP_HDFS_BLOCKSIZE": "134217728",
        "HADOOP_HDFS_PATH": "/ssd/${USER}/hdfs",
        "HADOOP_HDFS_PATH_CLEAR": "yes",
        "HADOOP_HDFSOVERLUSTRE_PATH": "/lustre/${USER}/hdfsoverlustre/",
        "HADOOP_HDFSOVERLUSTRE_REMOVE_LOCKS": "yes",
        "HADOOP_HDFSOVERNETWORKFS_PATH": "/networkfs/${USER}/hdfsovernetworkfs/",
        "HADOOP_HDFSOVERNETWORKFS_REMOVE_LOCKS": "yes",
        "HADOOP_RAWNETWORKFS_PATH": "/lustre/${USER}/rawnetworkfs/",
        "HADOOP_LOCALSTORE": "/ssd/${USER}/localstore/",
        "HADOOP_LOCALSTORE_CLEAR": "yes",
        "HBASE_SETUP": "yes",
        "HBASE_VERSION": "1.4.9",
        "HBASE_HOME": "${HOME}/hbase-${HBASE_VERSION}",
        "HBASE_LOCAL_DIR": "/tmp/${USER}/hbase",
        "HBASE_CONF_FILES": "${HOME}/myconf",
        "HBASE_MASTER_DAEMON_HEAP_MAX": "1000",
        "HBASE_REGIONSERVER_DAEMON_HEAP_MAX": "16000",
        "HBASE_ENVIRONMENT_EXTRA_PATH": "${HOME}/hbase-my-environment",
        "HBASE_JOB": "performanceeval",
        "HBASE_START_THRIFT_SERVER": "no",
        "HBASE_MAJOR_COMPACTION_ON_SHUTDOWN": "yes",
        "HBASE_PERFORMANCEEVAL_MODE": "sequential-thread",
        "HBASE_PERFORMANCEEVAL_ROW_COUNT": "1048576",
        "HBASE_PERFORMANCEEVAL_CLIENT_COUNT": "1",
        "PHOENIX_SETUP": "yes",
        "PHOENIX_VERSION": "4.14.0-HBase-1.4",
        "PHOENIX_HOME": "${HOME}/apache-phoenix-${PHOENIX_VERSION}-bin",
        "PHOENIX_LOCAL_DIR": "/tmp/${USER}/phoenix",
        "PHOENIX_CONF_FILES": "${HOME}/myconf",
        "PHOENIX_START_QUERYSERVER": "no",
        "PHOENIX_JOB": "performanceeval",
        "PHOENIX_SCRIPT_PATH": "${HOME}/my-phoenix-script",
        "PHOENIX_SCRIPT_OPTS": "",
        "PHOENIX_QUERYSERVER_EXTRA_OPTS": "-Djava.io.tmpdir=${PHOENIX_LOCAL_JOB_DIR}/tmp",
        "PHOENIX_PERFORMANCEEVAL_ROW_COUNT": "100000",
        "ZOOKEEPER_SETUP": "yes",
        "ZOOKEEPER_REPLICATION_COUNT": "3",
        "ZOOKEEPER_SHARE_NODES": "yes",
        "ZOOKEEPER_JOB": "zookeeperruok",
        "ZOOKEEPER_VERSION": "3.4.14",
        "ZOOKEEPER_HOME": "${HOME}/zookeeper-${ZOOKEEPER_VERSION}",
        "ZOOKEEPER_CONF_FILES": "${HOME}/myconf",
        "ZOOKEEPER_DATA_DIR": "/lustre/${USER}/zookeeper",
        "ZOOKEEPER_DATA_DIR_CLEAR": "yes",
        "ZOOKEEPER_DATA_DIR_TYPE": "networkfs",
        "ZOOKEEPER_LOCAL_DIR": "/tmp/${USER}/zookeeper",
        "ZOOKEEPER_TICKTIME": "2000",
        "ZOOKEEPER_INITLIMIT": "10",
        "ZOOKEEPER_SYNCLIMIT": "5",
        "MPIRUN_OPTIONS": "-genvall -genv MV2_USE_APM 0"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "",
        "tasks": "",
        "ntasks_per_code": "",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "true",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "<my_time_in_minutes>",
        "ntasks_per_node": "1",
        "nodes": "<my_node_count>",
        "memory": "",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {}
}