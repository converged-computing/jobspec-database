        "MASTER_PORT": "6000",
        "WORLD_SIZE": "$SLURM_NPROCS",
        "LAUNCHER": "python -u",
        "PROGRAM": "torch-distributed-gpu-test.py",
        "CMD": "$LAUNCHER $PROGRAM",
        "TMPDIR": "/scratch",
        "NCCL_DEBUG": "INFO",
        "CUDA_LAUNCH_BLOCKING": "1",
        "NCCL_ASYNC_ERROR_HANDLING": "1",
        "RANK": "$SLURM_PROCID",
