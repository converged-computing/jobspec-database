{
    "application": "surfex",
    "software": [
        "MASTER",
        "openmpi",
        "grib_api",
        "eccodes",
        "rttov31",
        "rrtm"
    ],
    "modules": [
        "mpiauto",
        "ICC_2018.5.274/eccodes-2.17.0"
    ],
    "environment_variables": {
        "MYLIB": "48t1_main.01%jpdup",
        "TESTDIR": "test directory",
        "OUTPUTDIR": "output directory",
        "OMP_NUM_THREADS": "1",
        "DR_HOOK": "1",
        "DR_HOOK_IGNORE_SIGNALS": "-1",
        "DR_HOOK_NOT_MPI": "1",
        "DR_HOOK_SILENT": "1",
        "DR_HOOK_OPT": "",
        "EC_PROFILE_HEAP": "0",
        "EC_PROFILE_MEM": "0",
        "EC_MPI_ATEXIT": "0",
        "DR_HOOK_SHOW_PROCESS_OPTIONS": "0",
        "EC_MEMINFO": "0",
        "TVSEARCHPATH": "SOURCE",
        "HOMEPACK": "$HOME/pack",
        "SOURCE": "$HOMEPACK/$MYLIB/src/local",
        "LOADIR": "$HOMEPACK/$MYLIB/bin",
        "TMPDIR": "$HOME/tmp",
        "TMPLOC": "$TMPDIR/rundir.$$",
        "TMPWAIT": "$TMPDIR/wait_queue.$$",
        "RTTOV_COEFDIR": "$PWD",
        "CNMEXP": "FPOS",
        "NPROC": "4",
        "NSTRIN": "4",
        "NSTROUT": "1",
        "NPRTRW_NPRTRV": "  NPRTRW=$NPROC,\n  NPRTRV=1,",
        "MPILIB": "openmpi",
        "MPIRUN": "$(echo $(dirname $(dirname $(ldd MASTER | grep openmpi| tail -1 | awk '{print $3}'))))/bin/orterun --oversubscribe -np $NPROC",
        "GRIB_API": "$(dirname $(dirname $(ldd MASTER | grep grib_api | head -1 | awk '{print $3}') 2>/dev/null) 2>/dev/null)",
        "ECCODES": "$(dirname $(dirname $(ldd MASTER | grep eccodes | head -1 | awk '{print $3}') 2>/dev/null) 2>/dev/null)",
        "GRIB_SAMPLES_PATH": "$GRIB_API/share/grib_api/ifs_samples/grib1",
        "GRIB_DEFINITION_PATH": "$GRIB_API/share/grib_api/definitions",
        "ECCODES_SAMPLES_PATH": "/opt/softs/libraries/ICC_2018.5.274/eccodes-2.17.0/share/eccodes/ifs_samples/grib1",
        "ECCODES_DEFINITION_PATH": "/opt/softs/libraries/ICC_2018.5.274/eccodes-2.17.0/share/eccodes/definitions",
        "NNODES": "$SLURM_JOB_NUM_NODES",
        "MPITASKS_PER_NODE": "((SLURM_NTASKS/SLURM_JOB_NUM_NODES))",
        "MPI_TASKS": "$SLURM_NTASKS",
        "OMP_STACKSIZE": "4G",
        "KMP_STACKSIZE": "4G",
        "KMP_MONITOR_STACKSIZE": "4G",
        "MPL_MBX_SIZE": "2048000000",
        "OPENBLAS_NUM_THREADS": "1",
        "MKL_CBWR": "AUTO,STRICT",
        "MKL_NUM_THREADS": "1",
        "MKL_DEBUG_CPU_TYPE": "5"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "",
        "tasks": "4",
        "ntasks_per_code": "",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "00:10:00",
        "ntasks_per_node": "",
        "nodes": "1",
        "memory": "20000",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {}
}