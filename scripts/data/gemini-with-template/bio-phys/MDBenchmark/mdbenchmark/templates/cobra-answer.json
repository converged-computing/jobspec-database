{
    "application": "slurm",
    "software": [
        "gmx_mpi",
        "namd2"
    ],
    "modules": [
        "intel",
        "impi",
        "cuda",
        "{{ module }}"
    ],
    "environment_variables": {
        "OMP_NUM_THREADS": "$SLURM_CPUS_PER_TASK",
        "OMP_PLACES": "threads",
        "SLURM_HINT": "multithread"
    },
    "resources": {
        "gres": "gpu:rtx5000:2",
        "cpus_per_task": "{{ number_of_threads }}",
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": "2",
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": "rtx5000",
        "time": "{{ formatted_time }}",
        "ntasks_per_node": "{{ number_of_ranks }}",
        "nodes": "{{ n_nodes }}",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}