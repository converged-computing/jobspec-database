{
    "application": "um-atmos",
    "software": [
        "aprun",
        "wrapper",
        "um-atmos.exe"
    ],
    "modules": [],
    "environment_variables": {
        "UMDIR": "",
        "CUMFDIR": "$CYLC_TASK_WORK_PATH",
        "DATAW": "./",
        "DATAM": "./",
        "INPUT_DATA": "./",
        "FLUME_IOS_NPROC": "0",
        "UM_ATM_NPROCX": "8",
        "UM_ATM_NPROCY": "5",
        "TOTAL_MPI_TASKS": "$((UM_ATM_NPROCX*UM_ATM_NPROCY + FLUME_IOS_NPROC))",
        "OMP_NUM_THREADS": "2",
        "OMP_STACKSIZE": "2g",
        "STASHMASTER": "./",
        "CORES_PER_NODE": "40",
        "NUMA_REGIONS_PER_NODE": "2",
        "HYPERTHREADS": "2",
        "MPI_TASKS_PER_NODE": "$((CORES_PER_NODE*HYPERTHREADS/OMP_NUM_THREADS))",
        "ROSE_LAUNCHER_PREOPTS": "-ss -n $TOTAL_MPI_TASKS -N $MPI_TASKS_PER_NODE -S $((MPI_TASKS_PER_NODE/NUMA_REGIONS_PER_NODE)) -d $OMP_NUM_THREADS -j $HYPERTHREADS",
        "MPICH_COLL_SYNC": "MPI_Gatherv",
        "FASTRUN": "false",
        "ATMOS_KEEP_MPP_STDOUT": "true",
        "COUPLER": "",
        "DR_HOOK": "0",
        "DR_HOOK_CATCH_SIGNALS": "0",
        "DR_HOOK_IGNORE_SIGNALS": "0",
        "DR_HOOK_OPT": "wallprof,self,time",
        "DR_HOOK_PROFILE": "./drhook.prof.cray.%d",
        "DR_HOOK_PROFILE_LIMIT": "-10.0",
        "DR_HOOK_PROFILE_PROC": "-1",
        "ENS_MEMBER": "0",
        "HISTORY": "./atmos.xhist",
        "PRINT_STATUS": "PrStatus_Min",
        "RCF_PRINTSTATUS": "PrStatus_Normal",
        "RCF_TIMER": "false",
        "RECON_KEEP_MPP_STDOUT": "true",
        "RUNID": "atmos",
        "SPECTRAL_FILE_DIR": "./spectral",
        "PATH": "$SRC_DIR/bin/:$PATH",
        "LD_LIBRARY_PATH": "$BUILD_DIR/shumlib/lib/:$LD_LIBRARY_PATH",
        "ATMOS_LAUNCHER": "aprun $ROSE_LAUNCHER_PREOPTS"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": "2",
        "tasks": "$((UM_ATM_NPROCX*UM_ATM_NPROCY + FLUME_IOS_NPROC))",
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "00:30:00",
        "ntasks_per_node": "$((CORES_PER_NODE*HYPERTHREADS/OMP_NUM_THREADS))",
        "nodes": "1",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {}
}