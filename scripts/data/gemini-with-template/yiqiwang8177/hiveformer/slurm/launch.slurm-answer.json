{
    "application": "hiveformer",
    "software": [
        "singularity",
        "xvfb-run",
        "python3.9"
    ],
    "modules": [
        "singularity"
    ],
    "environment_variables": {
        "XDG_RUNTIME_DIR": "$SCRATCH/tmp/runtime-$SLURM_JOBID",
        "hf_dir": "$HOME/src/hiveformer",
        "xp": "$xp:-$hf_dir/xp/",
        "log_dir": "$hf_dir/logs",
        "seed": "$seed:-0",
        "dataset": "$dataset:-$hf_dir/datasets/pkg/seed-$seed",
        "name": "${name:-${prefix}${tasks}}",
        "valset": "$valset:-$hf_dir/datasets/pkg/seed-$((($seed + 1) % $num_seeds))"
    },
    "resources": {
        "gres": "gpu:1",
        "cpus_per_task": "1",
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": "1",
        "gpus_per_node": "1",
        "cores_per_socket": null,
        "gpus_per_task": "1",
        "exclusive": null,
        "cpus_per_gpu": "10",
        "gpu_type": "v100",
        "time": "20:00:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "python": "3.9"
    }
}