{
    "application": "obsproc",
    "software": [
        "libjpeg",
        "grib_util",
        "netcdf",
        "intel",
        "craype",
        "cray-mpich",
        "cray-pals",
        "cfp",
        "bufr_dump"
    ],
    "modules": [
        "libjpeg",
        "grib_util/${grib_util_ver}",
        "netcdf/${netcdf_ver}",
        "intel/${intel_ver}",
        "craype/${craype_ver}",
        "cray-mpich/${cray_mpich_ver}",
        "cray-pals/${cray_pals_ver}",
        "cfp/${cfp_ver}",
        "bufr_dump/${bufr_dump_ver}"
    ],
    "environment_variables": {
        "envir": "prod",
        "cyc": "%CC%",
        "DESC": "%DESC%",
        "JTYP": "%JTYP%",
        "job": "${JTYP}_dump_post_$cyc",
        "jobid": "$job.$PBS_JOBID",
        "PDY": "%PDY%",
        "PDYm1": "$(date -d \"${PDY} 1 day ago\" +%Y%m%d)",
        "obsproc_ver": "v1.1",
        "obsproc_ver_pckg": "v1.1.2",
        "bufr_dump_ver": "1.0.0",
        "obsNET": "obsproc",
        "HOMEobsproc": "${userROOT}/install/obsproc-rel.v1.0.0",
        "VERSION_FILE": "$HOMEobsproc/versions/run.ver",
        "SENDECF": "NO",
        "SENDDBN": "NO",
        "SENDSDM": "NO",
        "TANK": "$DCOMROOT",
        "DATAROOT": "/lfs/h2/emc/stmp/$USER",
        "jlogfile": "$DATAROOT/${JTYP}.$PDY.jlogfile",
        "DEBUG_LEVEL": "3",
        "LOUD": "ON",
        "KEEPDATA": "YES",
        "COMIN_ROOT": "${DATAROOT}/CRON/${DESC}/com",
        "COMOUT_ROOT": "${DATAROOT}/CRON/${DESC}/com"
    },
    "resources": {
        "gres": null,
        "cpus_per_task": "3",
        "tasks": "1",
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "0:30:00",
        "ntasks_per_node": null,
        "nodes": "1",
        "memory": "500mb",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "grib_util_ver": "${grib_util_ver}",
        "netcdf_ver": "${netcdf_ver}",
        "intel_ver": "${intel_ver}",
        "craype_ver": "${craype_ver}",
        "cray_mpich_ver": "${cray_mpich_ver}",
        "cray_pals_ver": "${cray_pals_ver}",
        "cfp_ver": "${cfp_ver}",
        "bufr_dump_ver": "${bufr_dump_ver}"
    }
}