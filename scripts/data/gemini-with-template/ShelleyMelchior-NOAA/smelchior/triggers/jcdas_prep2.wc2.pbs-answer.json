{
    "application": "obsproc",
    "software": [
        "grib_util",
        "netcdf",
        "bufr_dump",
        "prepobs",
        "intel",
        "craype",
        "cray-mpich",
        "cray-pals",
        "cfp"
    ],
    "modules": [
        "grib_util/${grib_util_ver}",
        "netcdf/${netcdf_ver}",
        "bufr_dump/${bufr_dump_ver}",
        "intel/${intel_ver}",
        "craype/${craype_ver}",
        "cray-mpich/${cray_mpich_ver}",
        "cray-pals/${cray_pals_ver}",
        "cfp/${cfp_ver}",
        "prepobs/${prepobs_ver}"
    ],
    "environment_variables": {
        "envir": "prod",
        "cyc": "%CC%",
        "cycle": "t${cyc}z",
        "DESC": "%DESC%",
        "JTYP": "%JTYP%",
        "job": "${JTYP}_prep2_$cyc",
        "jobid": "$job.$PBS_JOBID",
        "PDY": "%PDY%",
        "userROOT": "/lfs/h2/emc/obsproc/noscrub/$USER",
        "obsNET": "obsproc",
        "obsproc_ver": "v1.1",
        "obsproc_ver_pckg": "v1.1.2",
        "prepobs_ver": "1.0.0",
        "PACKAGEROOTpara": "/lfs/h1/ops/para/packages",
        "HOMEobsproc": "${userROOT}/install/obsproc-rel.v1.0.0",
        "VERSION_FILE": "${HOMEobsproc}/versions/run.ver",
        "SENDECF": "NO",
        "SENDSDM": "NO",
        "SENDDBN": "NO",
        "DATAROOT": "/lfs/h2/emc/stmp/${USER}",
        "jlogfile": "/lfs/h2/emc/stmp/${USER}/${JTYP}.$PDY.jlogfile",
        "COMIN_ROOT": "/lfs/h2/emc/stmp/$USER/CRON/${DESC}/com",
        "COMOUT_ROOT": "/lfs/h2/emc/stmp/$USER/CRON/${DESC}/com",
        "DEBUG_LEVEL": "3",
        "KEEPDATA": "YES",
        "LOUD": "ON"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "16",
        "tasks": "1",
        "ntasks_per_code": "",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "0:30:00",
        "ntasks_per_node": "",
        "nodes": "1",
        "memory": "10gb",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {
        "grib_util_ver": "${grib_util_ver}",
        "netcdf_ver": "${netcdf_ver}",
        "bufr_dump_ver": "${bufr_dump_ver}",
        "prepobs_ver": "${prepobs_ver}",
        "intel_ver": "${intel_ver}",
        "craype_ver": "${craype_ver}",
        "cray_mpich_ver": "${cray_mpich_ver}",
        "cray_pals_ver": "${cray_pals_ver}",
        "cfp_ver": "${cfp_ver}"
    }
}