{
    "application": "spack",
    "software": [
        "slepc@3.16.0",
        "petsc@3.16.1",
        "py-petsc4py@3.16.1",
        "py-slepc4py@3.16.0",
        "metis@5.1.0",
        "gcc@10.2.0"
    ],
    "modules": [
        "slurm",
        "spack"
    ],
    "environment_variables": {
        "LOCAL_TIME": "$(date +'%Y%m%dT%H%M%S%z')",
        "UNIX_TIME": "$(date +'%s')",
        "LOCAL_SCRATCH_DIR": "/scratch/${USER}/job_${SLURM_JOB_ID}",
        "TMPDIR": "${LOCAL_SCRATCH_DIR}",
        "SYSTEM_NAME": "expanse",
        "SPACK_VERSION": "0.17.3",
        "SPACK_INSTANCE_NAME": "gpu",
        "SPACK_INSTANCE_VERSION": "b",
        "SPACK_INSTANCE_DIR": "/cm/shared/apps/spack/${SPACK_VERSION}/${SPACK_INSTANCE_NAME}/${SPACK_INSTANCE_VERSION}",
        "SLURM_JOB_SCRIPT": "$(scontrol show job ${SLURM_JOB_ID} | awk -F= '/Command=/{print $2}')",
        "SLURM_JOB_MD5SUM": "$(md5sum ${SLURM_JOB_SCRIPT})",
        "SCHEDULER_MODULE": "slurm",
        "SPACK_PACKAGE": "py-slepc4py@3.16.0",
        "SPACK_COMPILER": "gcc@10.2.0",
        "SPACK_VARIANTS": "",
        "SPACK_DEPENDENCIES": "^py-petsc4py@3.16.1/$(spack find --format '{hash:7}' py-petsc4py@3.16.1 % ${SPACK_COMPILER} ~mpi) ^slepc@3.16.0/$(spack find --format '{hash:7}' slepc@3.16.0 % ${SPACK_COMPILER} ^petsc@3.16.1 ~mpi ~complex)",
        "SPACK_SPEC": "${SPACK_PACKAGE} % ${SPACK_COMPILER} ${SPACK_VARIANTS} ${SPACK_DEPENDENCIES}"
    },
    "resources": {
        "gres": "gpu:1",
        "cpus_per_task": "10",
        "tasks": "1",
        "ntasks_per_code": "1",
        "gpus": "1",
        "gpus_per_node": "1",
        "cores_per_socket": null,
        "gpus_per_task": "1",
        "exclusive": null,
        "cpus_per_gpu": "10",
        "gpu_type": null,
        "time": "00:30:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": "93G",
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "spack": "0.17.3"
    }
}