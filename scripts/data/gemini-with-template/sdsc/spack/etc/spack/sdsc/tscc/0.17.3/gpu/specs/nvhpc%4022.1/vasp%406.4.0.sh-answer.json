{
    "application": "spack",
    "software": [
        "vasp@6.4.0",
        "nvhpc@22.1",
        "qd@2.3.13",
        "intel-mkl@2020.4.304",
        "openmpi@4.0.5"
    ],
    "modules": [
        "slurm",
        "nvhpc@22.1"
    ],
    "environment_variables": {
        "LOCAL_TIME": "$(date +'%Y%m%dT%H%M%S%z')",
        "UNIX_TIME": "$(date +'%s')",
        "SYSTEM_NAME": "tscc",
        "SPACK_VERSION": "0.17.3",
        "SPACK_INSTANCE_NAME": "gpu",
        "SPACK_INSTANCE_DIR": "/cm/shared/apps/spack/${SPACK_VERSION}/${SPACK_INSTANCE_NAME}",
        "SLURM_JOB_SCRIPT": "$(scontrol show job ${SLURM_JOB_ID} | awk -F= '/Command=/{print $2}')",
        "SLURM_JOB_MD5SUM": "$(md5sum ${SLURM_JOB_SCRIPT})",
        "SCHEDULER_MODULE": "slurm",
        "COMPILER_MODULE": "nvhpc@22.1",
        "SPACK_PACKAGE": "vasp@6.4.0",
        "SPACK_COMPILER": "nvhpc@22.1",
        "SPACK_VARIANTS": "+acc +openmp +scalapack ~vaspsol",
        "SPACK_DEPENDENCIES": "^qd@2.3.13 % ${SPACK_COMPILER} ^intel-mkl@2020.4.304 % ${SPACK_COMPILER}   ^openmpi@4.0.5 % ${SPACK_COMPILER}",
        "SPACK_SPEC": "${SPACK_PACKAGE} % ${SPACK_COMPILER} ${SPACK_VARIANTS} ${SPACK_DEPENDENCIES}"
    },
    "resources": {
        "gres": "gpu1",
        "cpus_per_task": "8",
        "tasks": "1",
        "ntasks_per_code": "1",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "02:00:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": "",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {
        "spack": "0.17.3"
    }
}