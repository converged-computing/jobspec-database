        "SCHEDULER_MODULE": "slurm/tscc/current",
        "INTEL_LICENSE_FILE": "40000@elprado.sdsc.edu:40200@elprado.sdsc.edu",
        "SPACK_PACKAGE": "superlu-dist@7.1.1",
        "SPACK_COMPILER": "intel@19.1.1.217",
        "SPACK_VARIANTS": "~cuda ~int64 ~ipo ~openmp +shared",
        "SPACK_DEPENDENCIES": "^openblas@0.3.17/$(spack find --format '{hash:7}' openblas@0.3.17 % ${SPACK_COMPILER} ~ilp64 threads=none) ^parmetis@4.0.3/$(spack find --format '{hash:7}' parmetis@4.0.3 % ${SPACK_COMPILER} ^openmpi@4.1.3)",
        "SPACK_SPEC": "${SPACK_PACKAGE} % ${SPACK_COMPILER} ${SPACK_VARIANTS} ${SPACK_DEPENDENCIES}"
    },
