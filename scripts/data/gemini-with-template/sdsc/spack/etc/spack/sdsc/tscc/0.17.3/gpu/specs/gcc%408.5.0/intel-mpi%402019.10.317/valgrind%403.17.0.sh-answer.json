{
    "application": "spack",
    "software": [
        "valgrind@3.17.0",
        "gcc@8.5.0",
        "boost@1.77.0",
        "intel-mpi@2019.10.317"
    ],
    "modules": [
        "slurm"
    ],
    "environment_variables": {
        "LOCAL_TIME": "date +'%Y%m%dT%H%M%S%z'",
        "UNIX_TIME": "date +'%s'",
        "SYSTEM_NAME": "tscc",
        "SPACK_VERSION": "0.17.3",
        "SPACK_INSTANCE_NAME": "gpu",
        "SPACK_INSTANCE_DIR": "/cm/shared/apps/spack/${SPACK_VERSION}/${SPACK_INSTANCE_NAME}",
        "SLURM_JOB_SCRIPT": "scontrol show job ${SLURM_JOB_ID} | awk -F= '/Command=/{print $2}'",
        "SLURM_JOB_MD5SUM": "md5sum ${SLURM_JOB_SCRIPT}",
        "SCHEDULER_MODULE": "slurm",
        "SPACK_PACKAGE": "valgrind@3.17.0",
        "SPACK_COMPILER": "gcc@8.5.0",
        "SPACK_VARIANTS": "+boost +mpi +only64bit +ubsan",
        "SPACK_DEPENDENCIES": "^boost@1.77.0/$(spack find --format '{hash:7}' boost@1.77.0 % ${SPACK_COMPILER} +mpi ^intel-mpi@2019.10.317)",
        "SPACK_SPEC": "${SPACK_PACKAGE} % ${SPACK_COMPILER} ${SPACK_VARIANTS} ${SPACK_DEPENDENCIES}"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "8",
        "tasks": null,
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "00:30:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "valgrind": "3.17.0",
        "gcc": "8.5.0",
        "boost": "1.77.0",
        "intel-mpi": "2019.10.317"
    }
}