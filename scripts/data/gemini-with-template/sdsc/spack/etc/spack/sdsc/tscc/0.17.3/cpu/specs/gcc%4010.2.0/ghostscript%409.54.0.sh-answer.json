{
    "application": "spack",
    "software": [
        "ghostscript",
        "meson",
        "librsvg",
        "gcc",
        "ninja",
        "py-setuptools",
        "python",
        "bzip2",
        "expat",
        "libbsd",
        "libmd",
        "gdbm",
        "readline",
        "ncurses",
        "gettext",
        "libiconv",
        "libxml2",
        "xz",
        "zlib",
        "tar",
        "libffi",
        "openssl",
        "sqlite",
        "util-linux-uuid"
    ],
    "modules": [
        "slurm"
    ],
    "environment_variables": {
        "LOCAL_TIME": "$(date +'%Y%m%dT%H%M%S%z')",
        "UNIX_TIME": "$(date +'%s')",
        "SYSTEM_NAME": "tscc",
        "SPACK_VERSION": "0.17.3",
        "SPACK_INSTANCE_NAME": "cpu",
        "SPACK_INSTANCE_DIR": "/cm/shared/apps/spack/${SPACK_VERSION}/${SPACK_INSTANCE_NAME}",
        "SLURM_JOB_SCRIPT": "$(scontrol show job ${SLURM_JOB_ID} | awk -F= '/Command=/{print $2}')",
        "SLURM_JOB_MD5SUM": "$(md5sum ${SLURM_JOB_SCRIPT})",
        "SCHEDULER_MODULE": "slurm",
        "SPACK_PACKAGE": "ghostscript@9.54.0",
        "SPACK_COMPILER": "gcc@10.2.0",
        "SPACK_VARIANTS": "~tesseract",
        "SPACK_DEPENDENCIES": "^meson@0.60.0/$(spack find --format '{hash:7}' meson@0.60.0 % ${SPACK_COMPILER}) ^librsvg@2.42.7",
        "SPACK_SPEC": "${SPACK_PACKAGE} % ${SPACK_COMPILER} ${SPACK_VARIANTS} ${SPACK_DEPENDENCIES}"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "8",
        "tasks": "",
        "ntasks_per_code": "",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "04:00:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": "",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {
        "ghostscript": "9.54.0",
        "meson": "0.60.0",
        "librsvg": "2.42.7",
        "gcc": "10.2.0",
        "ninja": "1.10.2",
        "py-setuptools": "58.2.0",
        "python": "3.8.12",
        "bzip2": "1.0.8",
        "expat": "2.4.1",
        "libbsd": "0.11.3",
        "libmd": "1.0.3",
        "gdbm": "1.21",
        "readline": "8.1",
        "ncurses": "6.2",
        "gettext": "0.21",
        "libiconv": "1.16",
        "libxml2": "2.9.12",
        "xz": "5.2.5",
        "zlib": "1.2.11",
        "tar": "1.34",
        "libffi": "3.3",
        "openssl": "1.1.1k",
        "sqlite": "3.36.0",
        "util-linux-uuid": "2.36.2"
    }
}