{
    "application": "spack",
    "software": [
        "ffmpeg"
    ],
    "modules": [
        "slurm",
        "spack"
    ],
    "environment_variables": {
        "LOCAL_TIME": "date +'%Y%m%dT%H%M%S%z'",
        "UNIX_TIME": "date +'%s'",
        "SYSTEM_NAME": "'tscc'",
        "SPACK_VERSION": "'0.17.3'",
        "SPACK_INSTANCE_NAME": "'gpu'",
        "SPACK_INSTANCE_DIR": "'/cm/shared/apps/spack/${SPACK_VERSION}/${SPACK_INSTANCE_NAME}'",
        "SLURM_JOB_SCRIPT": "scontrol show job ${SLURM_JOB_ID} | awk -F= '/Command=/{print $2}'",
        "SLURM_JOB_MD5SUM": "md5sum ${SLURM_JOB_SCRIPT}",
        "SCHEDULER_MODULE": "'slurm'",
        "SPACK_PACKAGE": "'ffmpeg@4.3.2'",
        "SPACK_COMPILER": "'gcc@10.2.0'",
        "SPACK_VARIANTS": "'~X ~avresample +bzlib ~drawtext +gpl ~libaom ~libmp3lame ~libopenjpeg ~libopus ~libsnappy ~libspeex ~libssh ~libvorbis ~libvpx ~libwebp ~libzmq ~lzma ~nonfree ~openssl ~sdl2 +shared +version3'",
        "SPACK_DEPENDENCIES": "''",
        "SPACK_SPEC": "${SPACK_PACKAGE} % ${SPACK_COMPILER} ${SPACK_VARIANTS} ${SPACK_DEPENDENCIES}"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "8",
        "tasks": "1",
        "ntasks_per_code": "1",
        "gpus": "1",
        "gpus_per_node": "1",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "00:30:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": "",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {
        "ffmpeg": "4.3.2",
        "gcc": "10.2.0",
        "spack": "0.17.3"
    }
}