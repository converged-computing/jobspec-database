{
    "application": "valgrind",
    "software": [
        "spack@0.17.3",
        "intel@19.1.3.304",
        "intel-mpi@2019.10.317",
        "boost",
        "mpi",
        "valgrind@3.17.0"
    ],
    "modules": [
        "slurm"
    ],
    "environment_variables": {
        "LOCAL_TIME": "$(date +'%Y%m%dT%H%M%S%z')",
        "UNIX_TIME": "$(date +'%s')",
        "LOCAL_SCRATCH_DIR": "/scratch/${USER}/job_${SLURM_JOB_ID}",
        "TMPDIR": "${LOCAL_SCRATCH_DIR}",
        "SYSTEM_NAME": "expanse",
        "SPACK_VERSION": "0.17.3",
        "SPACK_INSTANCE_NAME": "cpu",
        "SPACK_INSTANCE_VERSION": "b",
        "SPACK_INSTANCE_DIR": "/cm/shared/apps/spack/${SPACK_VERSION}/${SPACK_INSTANCE_NAME}/${SPACK_INSTANCE_VERSION}",
        "SLURM_JOB_SCRIPT": "$(scontrol show job ${SLURM_JOB_ID} | awk -F= '/Command=/{print $2}')",
        "SLURM_JOB_MD5SUM": "$(md5sum ${SLURM_JOB_SCRIPT})",
        "SCHEDULER_MODULE": "slurm",
        "INTEL_LICENSE_FILE": "40000@elprado.sdsc.edu:40200@elprado.sdsc.edu",
        "SPACK_PACKAGE": "valgrind@3.17.0",
        "SPACK_COMPILER": "intel@19.1.3.304",
        "SPACK_VARIANTS": "~boost +mpi +only64bit +ubsan",
        "SPACK_DEPENDENCIES": "^intel-mpi@2019.10.317/$(spack find --format '{hash:7}' intel-mpi@2019.10.317 % ${SPACK_COMPILER})",
        "SPACK_SPEC": "${SPACK_PACKAGE} % ${SPACK_COMPILER} ${SPACK_VARIANTS} ${SPACK_DEPENDENCIES}"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "16",
        "tasks": "1",
        "ntasks_per_code": "",
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": "",
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "00:30:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": "32G",
        "sockets_per_node": "",
        "ntasks_per_socket": "",
        "mem_per_gpu": "",
        "mem_per_cpu": "",
        "gres_flags": ""
    },
    "versions": {
        "spack": "0.17.3",
        "intel": "19.1.3.304",
        "intel-mpi": "2019.10.317",
        "valgrind": "3.17.0"
    }
}