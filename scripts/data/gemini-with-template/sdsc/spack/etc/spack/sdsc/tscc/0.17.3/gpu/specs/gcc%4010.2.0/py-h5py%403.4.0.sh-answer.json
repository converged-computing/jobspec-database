{
    "application": "spack",
    "software": [
        "py-h5py",
        "py-numpy",
        "openblas",
        "hdf5"
    ],
    "modules": [
        "slurm"
    ],
    "environment_variables": {
        "LOCAL_TIME": "date +'%Y%m%dT%H%M%S%z'",
        "UNIX_TIME": "date +'%s'",
        "SYSTEM_NAME": "'tscc'",
        "SPACK_VERSION": "'0.17.3'",
        "SPACK_INSTANCE_NAME": "'gpu'",
        "SPACK_INSTANCE_DIR": "\"/cm/shared/apps/spack/${SPACK_VERSION}/${SPACK_INSTANCE_NAME}\"",
        "SLURM_JOB_SCRIPT": "scontrol show job ${SLURM_JOB_ID} | awk -F= '/Command=/{print $2}'",
        "SLURM_JOB_MD5SUM": "md5sum ${SLURM_JOB_SCRIPT}",
        "SCHEDULER_MODULE": "'slurm'",
        "SPACK_PACKAGE": "'py-h5py@3.4.0'",
        "SPACK_COMPILER": "'gcc@10.2.0'",
        "SPACK_VARIANTS": "'~mpi'",
        "SPACK_DEPENDENCIES": "^py-numpy@1.20.3/$(spack find --format '{hash:7}' py-numpy@1.20.3 % ${SPACK_COMPILER}) ^openblas@0.3.17/$(spack find --format '{hash:7}' openblas@0.3.17 % ${SPACK_COMPILER} ~ilp64 threads=none) ^hdf5@1.10.7/$(spack find --format '{hash:7}' hdf5@1.10.7 % ${SPACK_COMPILER} ~mpi)",
        "SPACK_SPEC": "${SPACK_PACKAGE} % ${SPACK_COMPILER} ${SPACK_VARIANTS} ${SPACK_DEPENDENCIES}"
    },
    "resources": {
        "gres": "gpu1",
        "cpus_per_task": "8",
        "tasks": "1",
        "ntasks_per_code": null,
        "gpus": null,
        "gpus_per_node": null,
        "cores_per_socket": null,
        "gpus_per_task": null,
        "exclusive": null,
        "cpus_per_gpu": null,
        "gpu_type": null,
        "time": "00:30:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": null
    },
    "versions": {
        "py-h5py": "3.4.0",
        "py-numpy": "1.20.3",
        "openblas": "0.3.17",
        "hdf5": "1.10.7",
        "gcc": "10.2.0"
    }
}