{
    "application": "trinity",
    "software": [
        "spack",
        "trinity",
        "hisat2",
        "boost",
        "intel-mpi",
        "kallisto",
        "hwloc"
    ],
    "modules": [
        "slurm"
    ],
    "environment_variables": {
        "LOCAL_TIME": "date +'%Y%m%dT%H%M%S%z'",
        "UNIX_TIME": "date +'%s'",
        "SYSTEM_NAME": "'tscc'",
        "SPACK_VERSION": "'0.17.3'",
        "SPACK_INSTANCE_NAME": "'cpu'",
        "SPACK_INSTANCE_DIR": "'/cm/shared/apps/spack/${SPACK_VERSION}/${SPACK_INSTANCE_NAME}'",
        "SLURM_JOB_SCRIPT": "scontrol show job ${SLURM_JOB_ID} | awk -F= '/Command=/{print $2}'",
        "SLURM_JOB_MD5SUM": "md5sum ${SLURM_JOB_SCRIPT}",
        "SCHEDULER_MODULE": "'slurm'",
        "SPACK_PACKAGE": "'trinity@2.15.1.FULL'",
        "SPACK_COMPILER": "'gcc@10.2.0'",
        "SPACK_VARIANTS": "''",
        "SPACK_DEPENDENCIES": "'^boost@1.77.0/$(spack find --format '{hash:7}' boost@1.77.0 % ${SPACK_COMPILER} ~mpi) ^intel-mpi@2019.10.317 ^kallisto@0.46.2/$(spack find --format '{hash:7}' kallisto@0.46.2 % ${SPACK_COMPILER}) ^hwloc@2.6.0/$(spack find --format '{hash:7}' hwloc@2.6.0 % ${SPACK_COMPILER})'",
        "SPACK_SPEC": "${SPACK_PACKAGE} % ${SPACK_COMPILER} ${SPACK_VARIANTS} ${SPACK_DEPENDENCIES}"
    },
    "resources": {
        "gres": "",
        "cpus_per_task": "8",
        "tasks": "1",
        "ntasks_per_code": null,
        "gpus": "",
        "gpus_per_node": "",
        "cores_per_socket": "",
        "gpus_per_task": "",
        "exclusive": null,
        "cpus_per_gpu": "",
        "gpu_type": "",
        "time": "00:30:00",
        "ntasks_per_node": "1",
        "nodes": "1",
        "memory": null,
        "sockets_per_node": null,
        "ntasks_per_socket": null,
        "mem_per_gpu": null,
        "mem_per_cpu": null,
        "gres_flags": ""
    },
    "versions": {
        "spack": "0.17.3",
        "trinity": "2.15.1",
        "boost": "1.77.0",
        "intel-mpi": "2019.10.317",
        "kallisto": "0.46.2",
        "hwloc": "2.6.0",
        "gcc": "10.2.0",
        "hisat2": "2.2.0"
    }
